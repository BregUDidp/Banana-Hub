local extraTab = Window:CreateTab("|| -  ‡πÇ‡∏ã‡∏ô‡∏≠‡∏°‡∏ï‡∏∞‡πÅ‡∏•‡∏∞‡∏Ü‡πà‡∏≤‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï", "tree-pine")
local Section2 = extraTab:CreateSection("[  üå≤  ]  ‡πÇ‡∏ã‡∏ô‡∏≠‡∏°‡∏ï‡∏∞")

extraTab:CreateButton({Name = "[ üíñ ]  ‡∏£‡∏±‡∏ô ‡∏≠‡∏°‡∏ï‡∏∞", Callback = function() loadstring(game:HttpGet("https://raw.githubusercontent.com/BregUDidp/Banana-Hub/refs/heads/main/god%20mode"))() end})

local Section2 = extraTab:CreateSection("[  üå≤  ]  ‡πÇ‡∏ã‡∏ô‡∏Ü‡πà‡∏≤‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï")

-- ‡∏ö‡∏£‡∏¥‡∏Å‡∏≤‡∏£‡∏´‡∏•‡∏±‡∏Å
local Players = game:GetService("Players")
local Workspace = game:GetService("Workspace")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local player = Players.LocalPlayer
local Characters = Workspace:WaitForChild("Characters")

-- ‡∏ï‡∏±‡∏ß‡πÅ‡∏õ‡∏£ Kill Aura
local killAuraEnabled = false
local killAuraRange = 100
local killAuraCount = 0
local killAuraHitDelay = 0
local selectedMode = "Auto (Priority)"

-- ‡∏£‡∏≤‡∏¢‡∏Å‡∏≤‡∏£‡∏≠‡∏≤‡∏ß‡∏∏‡∏ò‡∏ó‡∏±‡πâ‡∏á‡∏´‡∏°‡∏î
local allWeapons = {
    "Scythe","Spear","Infernal Sword", "Morningstar", "Katana", "Laser Sword", "Ice Sword",
    "Trident", "Poison Spear", "Infernal Sword", "Cultist King Mace",
    "Obsidiron Hammer", "Scythe", "Vampire Scythe", "Old Axe", "Strong Axe", "Good Axe"
}

-- ‡∏´‡∏≤‡∏≠‡∏≤‡∏ß‡∏∏‡∏ò‡∏ï‡∏≤‡∏°‡πÇ‡∏´‡∏°‡∏î
local function getWeapon()
    if not player:FindFirstChild("Inventory") then return nil end

    if selectedMode == "Auto (Priority)" then
        local priority = {"Scythe","Infernal Sword","Strong Axe","Good Axe", "Vampire Scythe", "Laser Sword", "Ice Sword", "Poison Spear"}
        for _, w in ipairs(priority) do
            local tool = player.Inventory:FindFirstChild(w)
            if tool then return tool end
        end
    elseif selectedMode:find("Only") then
        local name = selectedMode:gsub(" Only", "")
        return player.Inventory:FindFirstChild(name)
    elseif selectedMode == "All Weapons (Random)" then
        local available = {}
        for _, w in ipairs(allWeapons) do
            local tool = player.Inventory:FindFirstChild(w)
            if tool then table.insert(available, tool) end
        end
        if #available > 0 then return available[math.random(1, #available)] end
    end

    -- fallback
    for _, w in ipairs(allWeapons) do
        local tool = player.Inventory:FindFirstChild(w)
        if tool then return tool end
    end
    return nil
end

-- ‡∏™‡πà‡∏á‡∏î‡∏≤‡πÄ‡∏°‡∏à
local function damage(target, tool)
    if not tool then return end
    local remoteEvents = ReplicatedStorage:FindFirstChild("RemoteEvents")
    if not remoteEvents then return end
    local evt = remoteEvents:FindFirstChild("ToolDamageObject")
    if not evt then return end

    local hrp = target:FindFirstChild("HumanoidRootPart")
    if not hrp then return end

    evt:InvokeServer(target, tool, tostring(killAuraCount).."_7367831688", hrp.CFrame)
end

-- Toggle
extraTab:CreateToggle({
    Name = "[ ‚öîÔ∏è ]  ‡πÄ‡∏õ‡∏¥‡∏î/‡∏õ‡∏¥‡∏î ‡∏Å‡∏≤‡∏£‡∏Ü‡πà‡∏≤‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï",
    CurrentValue = false,
    Callback = function(v)
        killAuraEnabled = v
        Rayfield:Notify({Title = "Kill Aura", Content = v and "‡πÄ‡∏õ‡∏¥‡∏î‡πÅ‡∏•‡πâ‡∏ß!" or "‡∏õ‡∏¥‡∏î‡πÅ‡∏•‡πâ‡∏ß", Duration = 2})
    end
})

-- Slider ‡∏£‡∏∞‡∏¢‡∏∞
extraTab:CreateSlider({
    Name = "[ üìè ]  ‡∏£‡∏∞‡∏¢‡∏∞‡∏Å‡∏≤‡∏£‡πÇ‡∏à‡∏°‡∏ï‡∏µ",
    Range = {50, 150},
    Increment = 5,
    CurrentValue = 100,
    Callback = function(v) killAuraRange = v end
})

-- Slider ‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß‡∏ï‡∏µ
extraTab:CreateSlider({
    Name = "[ üìè ]  ‡∏õ‡∏£‡∏±‡∏ö‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß‡∏ï‡∏µ",
    Range = {0, 0.5},
    Increment = 0.05,
    CurrentValue = 0,
    Callback = function(v)
        killAuraHitDelay = v
        Rayfield:Notify({Title = "Speed", Content = "Delay: "..v.."s", Duration = 1.5})
    end
})

-- Dropdown ‡πÇ‡∏´‡∏°‡∏î‡∏≠‡∏≤‡∏ß‡∏∏‡∏ò
local modes = {"Auto (Priority)", "Good Axe Only", "Vampire Scythe Only", "Laser Sword Only", "All Weapons (Random)"}
extraTab:CreateDropdown({
    Name = "[ ‚öîÔ∏è ]  ‡πÇ‡∏´‡∏°‡∏î‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡∏≠‡∏≤‡∏ß‡∏∏‡∏ò",
    Options = modes,
    CurrentOption = {"Auto (Priority)"},
    Callback = function(v) selectedMode = v[1] end
})

-- Label ‡∏™‡∏ñ‡∏¥‡∏ï‡∏¥
local statusLabel = extraTab:CreateLabel("Kills: 0 | Range: 100 | Speed: 0s | Weapon: -")

-- Reset Stats
extraTab:CreateButton({
    Name = "[ üîÑÔ∏è ]  ‡∏£‡∏µ‡πÄ‡∏ü‡∏£‡∏ä‡∏™‡πÅ‡∏ï‡πá‡∏î",
    Callback = function()
        killAuraCount = 0
        statusLabel:Set("Kills: 0 | Range: "..killAuraRange.." | Speed: "..killAuraHitDelay.."s | Weapon: Ready")
    end
})

-- Auto Equip
extraTab:CreateToggle({
    Name = "[ ‚ôæÔ∏è ]  ‡∏≠‡∏≠‡πÇ‡∏ï‡πâ‡∏Å‡∏≤‡∏£‡∏ñ‡∏∑‡∏≠‡∏≠‡∏≤‡∏ß‡∏∏‡∏ò",
    CurrentValue = false,
    Callback = function(enabled)
        task.spawn(function()
            while enabled and task.wait(0.5) do
                local tool = getWeapon()
                if tool and player.Character then
                    local hum = player.Character:FindFirstChild("Humanoid")
                    if hum then hum:EquipTool(tool) end
                end
            end
        end)
    end
})

-- === ‡∏•‡∏π‡∏õ‡∏´‡∏•‡∏±‡∏Å Kill Aura ===
task.spawn(function()
    while task.wait() do
        if not killAuraEnabled then
            statusLabel:Set("OFF | Kills: "..killAuraCount)
            task.wait(0.5)
            continue
        end

        local weapon = getWeapon()
        local myChar = player.Character
        if not myChar or not myChar:FindFirstChild("HumanoidRootPart") or not weapon then
            statusLabel:Set("Kills: "..killAuraCount.." | ‡πÑ‡∏°‡πà‡∏°‡∏µ‡∏≠‡∏≤‡∏ß‡∏∏‡∏ò")
            task.wait(0.5)
            continue
        end

        local myHRP = myChar.HumanoidRootPart
        statusLabel:Set("Kills: "..killAuraCount.." | "..weapon.Name.." | R:"..killAuraRange)

        for _, v in ipairs(Characters:GetChildren()) do
            if v:IsA("Model") and v ~= myChar and v:FindFirstChild("HumanoidRootPart") and not v.Name:find(" Body") then
                local dist = (v.HumanoidRootPart.Position - myHRP.Position).Magnitude
                if dist <= killAuraRange then
                    killAuraCount += 1
                    damage(v, weapon)
                    task.wait(killAuraHitDelay)
                end
            end
        end
    end
end)

local ExTab = Window:CreateTab("|| -  ‡∏î‡∏∂‡∏á‡∏Ç‡∏≠‡∏á‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï", "flame-kindling")
local Section3 = ExTab:CreateSection("[ üå≤ ]  ‡πÇ‡∏ã‡∏ô‡∏ß‡∏≤‡∏õ")

-- Services
local Players = cloneref(game:GetService("Players"))
local ReplicatedStorage = cloneref(game:GetService("ReplicatedStorage"))
local Workspace = cloneref(game:GetService("Workspace"))

-- Services
local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer
local RunService = game:GetService("RunService")

-- Variables
local Character = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
local HumanoidRootPart = Character:WaitForChild("HumanoidRootPart")
local GodModeEnabled = false

-- Teleport Positions (Approximate; adjust based on your server/map)
local Positions = {
    Stronghold = Vector3.new(1500, 50, 1200),  -- Common spawn; check map diamond icon
    Campfire = Vector3.new(0, 10, 0),           -- Your base
    Traders = Vector3.new(-500, 20, 300)        -- Trader NPC area
}

-- TP Function
local function teleportTo(pos, name)
    if HumanoidRootPart then
        HumanoidRootPart.CFrame = CFrame.new(pos)
        Rayfield:Notify({
            Title = "‡∏ß‡∏≤‡∏õ‡∏™‡∏≥‡πÄ‡∏£‡πá‡∏à [‚úÖ]",
            Content = "Teleported to " .. name .. "!",
            Duration = 3,
            Image = 4483362458
        })
    else
        Rayfield:Notify({
            Title = "Error",
            Content = "Character not loaded!",
            Duration = 3,
            Image = 4483362458
        })
    end
end

ExTab:CreateButton({
    Name = "[  ‚õ∫  ]  ‡∏ß‡∏≤‡∏õ‡πÑ‡∏õ‡πÅ‡∏Ñ‡∏°‡πÑ‡∏ü",
    Callback = function()
        teleportTo(Positions.Campfire, "Campfire")
    end
})

