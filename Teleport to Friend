local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()
 
local Window = Rayfield:CreateWindow({
   Name = "   |  Teleport   [ V-8.1 ]",
   Icon = "webhook",
   LoadingTitle = "[ üï∏Ô∏è ]  Akari Hub  [ üï∏Ô∏è ]",
   LoadingSubtitle = "____________________________________________________________________________________________________________________________________________________________________________________________________________________________________________________________________________________________________________",
   ShowText = "üï∏Ô∏è",
   Theme = "DarkBlue",
   ToggleUIKeybind = "F",
   DisableRayfieldPrompts = true,
   DisableBuildWarnings = true, -- Prevents Rayfield from warning when the script has a version mismatch with the interface
   ConfigurationSaving = {
      Enabled = true,
      FolderName = nil, -- Create a custom folder for your hub/game
      FileName = "Big Hub"
   },
   Discord = {
      Enabled = false, -- Prompt the user to join your Discord server if their executor supports it
      Invite = "noinvitelink", -- The Discord invite code, do not include discord.gg/. E.g. discord.gg/ ABCD would be ABCD
      RememberJoins = true
   },
   KeySystem = false,
   KeySettings = {
      Title = "",
      Subtitle = "",
      Note = "",
      FileName = "",
      SaveKey = true,
      GrabKeyFromSite = false,
      Key = {"üï∏Ô∏è"}
   }
})

-- === ‡πÅ‡∏ó‡πá‡∏ö‡∏ß‡∏≤‡∏õ + ‡∏•‡∏≤‡∏Å‡∏®‡∏û (‡πÄ‡∏ß‡∏≠‡∏£‡πå‡∏ä‡∏±‡∏ô‡πÉ‡∏´‡∏°‡πà: Auto TP to Friend ‡πÑ‡∏°‡πà‡∏ï‡πâ‡∏≠‡∏á‡∏Å‡∏î‡∏õ‡∏∏‡πà‡∏°) ===
local friendTab = Window:CreateTab("|| -  ‡∏ß‡∏≤‡∏õ‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï", "door-open")
local Section1 = friendTab:CreateSection("[ üë• ]  ‡πÇ‡∏ã‡∏ô‡∏ß‡∏≤‡∏õ‡πÑ‡∏õ‡∏´‡∏≤‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏ô (‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï‡∏¥)")

local Players = game:GetService("Players")
local player = Players.LocalPlayer
local autoTPConnection

-- ‡∏ï‡∏±‡∏ß‡πÅ‡∏õ‡∏£‡πÄ‡∏Å‡πá‡∏ö‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏ô‡∏ó‡∏µ‡πà‡πÄ‡∏•‡∏∑‡∏≠‡∏Å
local selectedFriendName = nil

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï‡∏£‡∏≤‡∏¢‡∏ä‡∏∑‡πà‡∏≠‡πÉ‡∏ô Dropdown
local function updatePlayerList()
    local list = {}
    for _, p in ipairs(Players:GetPlayers()) do
        if p ~= player then
            table.insert(list, p.DisplayName .. " (@" .. p.Name .. ")")
        end
    end
    return list
end

-- Dropdown ‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏ô (‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡πÅ‡∏•‡πâ‡∏ß‡∏ß‡∏≤‡∏õ‡∏ó‡∏±‡∏ô‡∏ó‡∏µ)
local friendDropdown = friendTab:CreateDropdown({
    Name = "[ üë• ]  ‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏ô (‡∏ß‡∏≤‡∏õ‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï‡∏¥)",
    Options = updatePlayerList(),
    CurrentOption = {""},
    MultipleOptions = false,
    Callback = function(Option)
        if Option[1] == nil or Option[1] == "" then
            selectedFriendName = nil
            if autoTPConnection then autoTPConnection:Disconnect() end
            Rayfield:Notify({
                Title = "‡∏´‡∏¢‡∏∏‡∏î‡∏ß‡∏≤‡∏õ‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï‡∏¥",
                Content = "‡∏¢‡∏Å‡πÄ‡∏•‡∏¥‡∏Å‡∏Å‡∏≤‡∏£‡∏ß‡∏≤‡∏õ‡πÑ‡∏õ‡∏´‡∏≤‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏ô",
                Duration = 3
            })
            return
        end

        local friendName = Option[1]:match("@(.+)%)") or Option[1]
        selectedFriendName = friendName

        local targetPlayer = Players:FindFirstChild(friendName)
        if not targetPlayer or not targetPlayer.Character or not targetPlayer.Character:FindFirstChild("HumanoidRootPart") then
            Rayfield:Notify({
                Title = "‚ùå ‡πÑ‡∏°‡πà‡∏û‡∏ö‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏ô",
                Content = friendName .. " ‡∏¢‡∏±‡∏á‡πÑ‡∏°‡πà‡πÇ‡∏´‡∏•‡∏î‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£‡∏´‡∏£‡∏∑‡∏≠‡πÑ‡∏°‡πà‡∏≠‡∏≠‡∏ô‡πÑ‡∏•‡∏ô‡πå",
                Duration = 4
            })
            return
        end

        Rayfield:Notify({
            Title = "üöÄ ‡∏ß‡∏≤‡∏õ‡πÑ‡∏õ‡∏´‡∏≤ " .. targetPlayer.DisplayName,
            Content = "‡∏Å‡∏≥‡∏•‡∏±‡∏á‡∏ß‡∏≤‡∏õ‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï‡∏¥‡∏ó‡∏∏‡∏Å ‡πÜ 0.5 ‡∏ß‡∏¥‡∏ô‡∏≤‡∏ó‡∏µ",
            Duration = 4
        })

        -- ‡∏¢‡∏Å‡πÄ‡∏•‡∏¥‡∏Å‡∏Å‡∏≤‡∏£‡πÄ‡∏ä‡∏∑‡πà‡∏≠‡∏°‡∏ï‡πà‡∏≠‡πÄ‡∏Å‡πà‡∏≤ (‡∏ñ‡πâ‡∏≤‡∏°‡∏µ)
        if autoTPConnection then autoTPConnection:Disconnect() end

        -- ‡∏™‡∏£‡πâ‡∏≤‡∏á‡∏Å‡∏≤‡∏£‡∏ß‡∏≤‡∏õ‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï‡∏¥
        autoTPConnection = game:GetService("RunService").Heartbeat:Connect(function()
            if not selectedFriendName then return end

            local me = player.Character or player.CharacterAdded:Wait()
            local myHRP = me:FindFirstChild("HumanoidRootPart")
            if not myHRP then return end

            local target = Players:FindFirstChild(selectedFriendName)
            if target and target.Character then
                local targetHRP = target.Character:FindFirstChild("HumanoidRootPart")
                if targetHRP then
                    myHRP.CFrame = targetHRP.CFrame + Vector3.new(0, 4, 0) -- ‡∏ß‡∏≤‡∏õ‡πÄ‡∏´‡∏ô‡∏∑‡∏≠‡∏´‡∏±‡∏ß‡∏ô‡∏¥‡∏î‡∏ô‡∏∂‡∏á
                end
            end
        end)
    end
})

local Section1 = friendTab:CreateSection("[ üë• ]  ‡πÇ‡∏ã‡∏ô‡∏•‡∏ö‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏ô")

-- ‡∏õ‡∏∏‡πà‡∏°‡∏´‡∏¢‡∏∏‡∏î‡∏ß‡∏≤‡∏õ/‡∏≠‡∏≠‡∏Å‡∏à‡∏≤‡∏Å‡∏Å‡∏≤‡∏£‡∏ï‡∏¥‡∏î‡∏ï‡∏≤‡∏° (‡∏Å‡∏î‡∏õ‡∏∏‡πà‡∏°‡πÅ‡∏î‡∏á‡πÄ‡∏î‡πà‡∏ô ‡πÜ)
friendTab:CreateButton({
    Name = "[ üõë ] ‡∏´‡∏¢‡∏∏‡∏î‡∏ß‡∏≤‡∏õ‡πÑ‡∏õ‡∏´‡∏≤‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏ô",
    Callback = function()
        selectedFriendName = nil
        
        if autoTPConnection then
            autoTPConnection:Disconnect()
            autoTPConnection = nil
        end
        
        -- ‡∏£‡∏µ‡πÄ‡∏ã‡πá‡∏ï Dropdown ‡πÉ‡∏´‡πâ‡∏Å‡∏•‡∏±‡∏ö‡∏°‡∏≤‡πÄ‡∏õ‡πá‡∏ô‡∏Ñ‡πà‡∏≤‡∏ß‡πà‡∏≤‡∏á
        friendDropdown:Set({""})
        
        Rayfield:Notify({
            Title = "üõë ‡∏´‡∏¢‡∏∏‡∏î‡∏ß‡∏≤‡∏õ‡∏™‡∏≥‡πÄ‡∏£‡πá‡∏à",
            Content = "‡∏¢‡∏Å‡πÄ‡∏•‡∏¥‡∏Å‡∏Å‡∏≤‡∏£‡∏ï‡∏¥‡∏î‡∏ï‡∏≤‡∏°‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏ô‡πÅ‡∏•‡πâ‡∏ß",
            Duration = 4,
            Image = 4483362458
        })
    end
})

local Section1 = friendTab:CreateSection("[ üë• ]  ‡πÇ‡∏ã‡∏ô‡∏£‡∏µ‡πÄ‡∏ü‡∏£‡∏ä‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏ô")

-- ‡∏õ‡∏∏‡πà‡∏°‡∏£‡∏µ‡πÄ‡∏ü‡∏£‡∏ä‡∏£‡∏≤‡∏¢‡∏ä‡∏∑‡πà‡∏≠
friendTab:CreateButton({
    Name = "[ üîÑ ]  ‡∏£‡∏µ‡πÄ‡∏ü‡∏£‡∏ä‡∏£‡∏≤‡∏¢‡∏ä‡∏∑‡πà‡∏≠‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏ô",
    Callback = function()
        friendDropdown:Refresh(updatePlayerList(), true)
        Rayfield:Notify({Title = "‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï‡πÅ‡∏•‡πâ‡∏ß ‚úÖ", Content = "‡∏£‡∏µ‡πÄ‡∏ü‡∏£‡∏ä‡∏£‡∏≤‡∏¢‡∏ä‡∏∑‡πà‡∏≠‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏ô‡πÄ‡∏£‡∏µ‡∏¢‡∏ö‡∏£‡πâ‡∏≠‡∏¢", Duration = 3})
    end
})

-- ‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï‡∏¥‡πÄ‡∏°‡∏∑‡πà‡∏≠‡∏°‡∏µ‡∏Ñ‡∏ô‡πÄ‡∏Ç‡πâ‡∏≤/‡∏≠‡∏≠‡∏Å
Players.PlayerAdded:Connect(function()
    friendDropdown:Refresh(updatePlayerList(), true)
end)

Players.PlayerRemoving:Connect(function()
    task.wait(1) -- ‡∏£‡∏≠‡πÉ‡∏´‡πâ‡∏≠‡∏≠‡∏Å‡∏à‡∏£‡∏¥‡∏á ‡πÜ
    friendDropdown:Refresh(updatePlayerList(), true)
end)

-- ‡∏´‡∏¢‡∏∏‡∏î‡∏ß‡∏≤‡∏õ‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï‡∏¥‡πÄ‡∏°‡∏∑‡πà‡∏≠‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£‡∏ï‡∏≤‡∏¢/‡∏£‡∏µ‡∏™‡∏õ‡∏≤‡∏ß‡∏ô‡πå
player.CharacterRemoving:Connect(function()
    if autoTPConnection then
        autoTPConnection:Disconnect()
        autoTPConnection = nil
    end
end)
