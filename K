    Callback = function()
        -- ‡∏•‡∏π‡∏Å‡∏≠‡∏°‡∏™‡∏µ‡πÄ‡∏Ç‡∏µ‡∏¢‡∏ß Sparkles ‡πÄ‡∏ó‡πÄ‡∏•‡∏û‡∏≠‡∏£‡πå‡∏ï (‡πÅ‡∏™‡∏á‡πÄ‡∏Ç‡∏µ‡∏¢‡∏ß‡∏ó‡∏±‡πâ‡∏á‡∏•‡∏π‡∏Å + Effect)
local tool = Instance.new("Tool")
tool.Name = "üç≠"
tool.ToolTip = "[ üç≠ ]  Teleport Tool"
tool.RequiresHandle = true

local player = game.Players.LocalPlayer

-- ‡∏™‡∏£‡πâ‡∏≤‡∏á Handle ‡∏•‡∏π‡∏Å‡∏≠‡∏°‡∏™‡∏µ‡πÄ‡∏Ç‡∏µ‡∏¢‡∏ß
