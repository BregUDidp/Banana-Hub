   Name = "[ üç≠ ]   Teleport Tools   [  -  Toggle  -  ]",
   CurrentValue = false,
   Flag = "ToggleCandy",
   Callback = function(Value)
      if Value then
         CreateTeleportCandy()
         
         -- ‡πÅ‡∏™‡∏î‡∏á Label ‡∏™‡∏µ‡πÄ‡∏Ç‡∏µ‡∏¢‡∏ß‡∏ß‡πà‡∏≤‡πÇ‡∏´‡∏•‡∏î‡πÄ‡∏™‡∏£‡πá‡∏à‡πÅ‡∏•‡πâ‡∏ß
         StatusLabel:Set(
            '<font color="rgb(0, 255, 100)"><b>ON :  -  Teleport Warping Tools</b></font>'
         )
         
         Rayfield:Notify({
            Title = "[ üç≠ ]  Teleport Tools ON",
            Content = "[ ‚úÖ ]  Teleport Tools ON",
            Duration = 1,
            Image = "lollipop",
         })
      else
         RemoveTeleportCandy()
         
         -- ‡πÅ‡∏™‡∏î‡∏á Label ‡∏™‡∏µ‡πÅ‡∏î‡∏á/‡πÄ‡∏´‡∏•‡∏∑‡∏≠‡∏á‡∏ß‡πà‡∏≤‡∏õ‡∏¥‡∏î‡πÅ‡∏•‡πâ‡∏ß
         StatusLabel:Set(
            '<font color="rgb(255, 70, 70)"><b>OFF :  -  Teleport Warping Tools</b></font>'
         )
         
         Rayfield:Notify({
            Title = "[ üç≠ ]  Teleport Tools OFF",
            Content = "[ üõë ]  Teleport Tools OFF",
            Duration = 1,
            Image = "lollipop",
         })
         
         -- ‡∏´‡∏•‡∏±‡∏á‡∏à‡∏≤‡∏Å 5 ‡∏ß‡∏¥‡∏ô‡∏≤‡∏ó‡∏µ ‡∏£‡∏µ‡πÄ‡∏ã‡πá‡∏ï Label ‡∏Å‡∏•‡∏±‡∏ö‡πÑ‡∏õ‡∏™‡∏ñ‡∏≤‡∏ô‡∏∞‡∏õ‡∏Å‡∏ï‡∏¥ (optional)
         task.delay(5, function()
            StatusLabel:Set(
               '<font color="rgb(138, 43, 179)"><b> --- </b></font>'
            )
         end)
      end
   end,
})

local Paragraph = BTab:CreateParagraph({Title = "Teleport Tool For Player / Friend", Content = "Teleport Tool Is Good For Building"})

local Section = BTab:CreateSection("")

local Player = game.Players.LocalPlayer
local Backpack = Player:WaitForChild("Backpack")

local NormalWalkSpeed = 17      -- ‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß‡∏õ‡∏Å‡∏ï‡∏¥
local BoostWalkSpeed = 30       -- ‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß‡∏ï‡∏≠‡∏ô‡∏ñ‡∏∑‡∏≠ (‡∏õ‡∏£‡∏±‡∏ö‡πÑ‡∏î‡πâ‡∏ï‡∏≤‡∏°‡∏ï‡πâ‡∏≠‡∏á‡∏Å‡∏≤‡∏£)

-- ‡∏ï‡∏±‡∏ß‡πÅ‡∏õ‡∏£‡πÄ‡∏Å‡πá‡∏ö Tool
local RecoilBike = nil

-- ‡∏™‡∏£‡πâ‡∏≤‡∏á Label ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡πÅ‡∏™‡∏î‡∏á‡∏™‡∏ñ‡∏≤‡∏ô‡∏∞ (‡∏ó‡∏≥‡∏Ñ‡∏£‡∏±‡πâ‡∏á‡πÄ‡∏î‡∏µ‡∏¢‡∏ß ‡∏≠‡∏¢‡∏π‡πà‡∏ô‡∏≠‡∏Å Toggle ‡πÄ‡∏û‡∏∑‡πà‡∏≠‡πÉ‡∏´‡πâ‡∏≠‡∏¢‡∏π‡πà‡∏ñ‡∏≤‡∏ß‡∏£‡πÉ‡∏ô Tab)
local StatusLabel = BTab:CreateLabel(
   '<font color="rgb(138, 43, 179)"><b> --- </b></font>'
)

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏™‡∏£‡πâ‡∏≤‡∏á Tool ‡∏à‡∏±‡∏Å‡∏£‡∏¢‡∏≤‡∏ô‡∏™‡∏µ‡∏°‡πà‡∏ß‡∏á + Speed Boost
local function CreateRecoilBike()
    if RecoilBike then return end -- ‡πÑ‡∏°‡πà‡πÉ‡∏´‡πâ‡∏™‡∏£‡πâ‡∏≤‡∏á‡∏ã‡πâ‡∏≥

    local tool = Instance.new("Tool")
    tool.Name = "üö≤"
    tool.ToolTip = "[ üö≤ ]  Recoil Tools Speed Boost"
    tool.RequiresHandle = true

    -- Handle ‡∏•‡∏π‡∏Å‡∏≠‡∏°/‡∏•‡∏π‡∏Å‡∏ö‡∏≠‡∏•‡∏™‡∏µ‡∏°‡πà‡∏ß‡∏á
    local handle = Instance.new("Part")
    handle.Name = "Handle"
    handle.Size = Vector3.new(2.2, 2.2, 2.2)
    handle.Shape = Enum.PartType.Ball
    handle.Material = Enum.Material.ForceField
    handle.Color = Color3.fromRGB(138, 43, 179)
    handle.Transparency = 0.15
    handle.CanCollide = false
    handle.Parent = tool

    -- Sparkles ‡∏™‡∏µ‡∏°‡πà‡∏ß‡∏á
    local sparkles = Instance.new("Sparkles")
    sparkles.SparkleColor = Color3.fromRGB(180, 80, 255)
    sparkles.Enabled = false
    sparkles.Parent = handle

    -- ParticleEmitter ‡∏°‡πà‡∏ß‡∏á‡∏™‡∏ß‡∏¢‡∏á‡∏≤‡∏°
    local particles = Instance.new("ParticleEmitter")
    particles.Texture = "rbxassetid://241650934"
    particles.Color = ColorSequence.new({
        ColorSequenceKeypoint.new(0, Color3.fromRGB(138, 43, 179)),
        ColorSequenceKeypoint.new(1, Color3.fromRGB(200, 120, 255))
    })
    particles.Transparency = NumberSequence.new({NumberSequenceKeypoint.new(0,0.3), NumberSequenceKeypoint.new(1,1)})
    particles.Size = NumberSequence.new(0.5, 0)
    particles.Lifetime = NumberRange.new(1.2, 2.8)
    particles.Rate = 0
    particles.Speed = NumberRange.new(4, 8)
    particles.SpreadAngle = Vector2.new(50, 50)
    particles.Parent = handle

    -- ‡πÅ‡∏™‡∏á‡∏°‡πà‡∏ß‡∏á
    local light = Instance.new("PointLight")
    light.Color = Color3.fromRGB(180, 80, 255)
    light.Brightness = 0
    light.Range = 0
    light.Parent = handle

    -- ‡πÄ‡∏™‡∏µ‡∏¢‡∏á‡∏ï‡∏≠‡∏ô‡∏ñ‡∏∑‡∏≠
    tool.Equipped:Connect(function()
        sparkles.Enabled = false
        particles.Rate = 60
        light.Brightness = 5

        -- ‡πÄ‡∏û‡∏¥‡πà‡∏°‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß
        if Player.Character and Player.Character:FindFirstChild("Humanoid") then
            Player.Character.Humanoid.WalkSpeed = BoostWalkSpeed
        end

        -- ‡πÄ‡∏™‡∏µ‡∏¢‡∏á‡πÄ‡∏ó‡πà ‡πÜ
        local equipSound = Instance.new("Sound")
        equipSound.SoundId = "rbxassetid://2865227271"
        equipSound.Volume = 0.6
        equipSound.Parent = Player.Character and Player.Character:FindFirstChild("HumanoidRootPart")
        if equipSound.Parent then
            equipSound:Play()
            game.Debris:AddItem(equipSound, 3)
        end
    end)

    tool.Unequipped:Connect(function()
        sparkles.Enabled = true
        particles.Rate = 30
        light.Brightness = 3

        -- ‡∏Ñ‡∏∑‡∏ô‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß‡∏õ‡∏Å‡∏ï‡∏¥
        if Player.Character and Player.Character:FindFirstChild("Humanoid") then
            Player.Character.Humanoid.WalkSpeed = NormalWalkSpeed
        end
    end)

    -- ‡πÄ‡∏Å‡πá‡∏ö‡πÑ‡∏ß‡πâ‡πÉ‡∏ô‡∏ï‡∏±‡∏ß‡πÅ‡∏õ‡∏£
    RecoilBike = tool
    tool.Parent = Backpack
end

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏•‡∏ö Tool
local function RemoveRecoilBike()
    if RecoilBike and RecoilBike.Parent then
        RecoilBike:Destroy()
        RecoilBike = nil
    end
end

BTab:CreateToggle({
   Name = "[ üö≤ ]   Recoil Tool   [  -  Speed  30  -  ]",
   CurrentValue = false,
   Flag = "ToggleBike",
   Callback = function(Value)
      if Value then
         CreateRecoilBike()
         
         -- ‡πÅ‡∏™‡∏î‡∏á Label ‡∏™‡∏µ‡πÄ‡∏Ç‡∏µ‡∏¢‡∏ß‡∏ß‡πà‡∏≤‡πÇ‡∏´‡∏•‡∏î‡πÄ‡∏™‡∏£‡πá‡∏à‡πÅ‡∏•‡πâ‡∏ß
         StatusLabel:Set(
            '<font color="rgb(0, 255, 100)"><b>ON :  -  Recoil Speed Tools</b></font>'
         )
         
         Rayfield:Notify({
            Title = "[ üö≤ ]  Recoil Tools Running ON",
            Content = "[ ‚úÖ ]  Recoil Tools Running ON",
            Duration = 1,
            Image = "bike",
         })
      else
         RemoveRecoilBike()
         
         -- ‡∏Ñ‡∏∑‡∏ô‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß‡∏õ‡∏Å‡∏ï‡∏¥‡∏ó‡∏±‡∏ô‡∏ó‡∏µ (‡∏õ‡πâ‡∏≠‡∏á‡∏Å‡∏±‡∏ô‡∏Ñ‡πâ‡∏≤‡∏á)
         if Player.Character and Player.Character:FindFirstChild("Humanoid") then
            Player.Character.Humanoid.WalkSpeed = NormalWalkSpeed
         end
         
         -- ‡πÅ‡∏™‡∏î‡∏á Label ‡∏™‡∏µ‡πÅ‡∏î‡∏á‡∏ß‡πà‡∏≤‡∏õ‡∏¥‡∏î‡πÅ‡∏•‡πâ‡∏ß
         StatusLabel:Set(
            '<font color="rgb(255, 70, 70)"><b>OFF :  -  Recoil Speed Tools</b></font>'
         )
         
         Rayfield:Notify({
            Title = "[ üö≤ ]  Recoil Tools Running OFF",
            Content = "[ üõë ]  Recoil Tools Running OFF",
            Duration = 1,
            Image = "bike",
         })
         
         -- ‡∏´‡∏•‡∏±‡∏á‡∏à‡∏≤‡∏Å 5 ‡∏ß‡∏¥‡∏ô‡∏≤‡∏ó‡∏µ ‡∏£‡∏µ‡πÄ‡∏ã‡πá‡∏ï Label ‡∏Å‡∏•‡∏±‡∏ö‡πÑ‡∏õ‡∏™‡∏ñ‡∏≤‡∏ô‡∏∞‡∏õ‡∏Å‡∏ï‡∏¥
         task.delay(5, function()
            StatusLabel:Set(
               '<font color="rgb(138, 43, 179)"><b> --- </b></font>'
            )
         end)
      end
   end,
})

local Paragraph = BTab:CreateParagraph({Title = "Recoil Tool For Player / Friend", Content = "Recoil Tool Is Good For Speed Flash"})

local Section = BTab:CreateSection("")

--// === TELEPORT TAB ===
local friendTab = Window:CreateTab("||   - Trolling Player", "bug-play")
local Section = friendTab:CreateSection("")

local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local player = Players.LocalPlayer

-- ‡∏ï‡∏±‡∏ß‡πÅ‡∏õ‡∏£‡πÄ‡∏Å‡πá‡∏ö‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á‡πÄ‡∏î‡∏¥‡∏°‡∏Å‡πà‡∏≠‡∏ô‡∏ß‡∏≤‡∏õ
local savedCFrame = nil

-- ‡∏ï‡∏±‡∏ß‡πÅ‡∏õ‡∏£‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö Auto Teleport
local autoTPConnection = nil
local autoTargetName = nil

-- ‡∏ï‡∏±‡∏ß‡πÅ‡∏õ‡∏£‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö ESP
local currentESPHighlight = nil
local espTargetPlayer = nil
local espConnection = nil

-- ‡∏ï‡∏±‡∏ß‡πÅ‡∏õ‡∏£‡∏™‡∏µ ESP (‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏ï‡πâ‡∏ô‡πÄ‡∏Ç‡∏µ‡∏¢‡∏ß‡∏≠‡πà‡∏≠‡∏ô)
local espFillColor = Color3.fromRGB(138, 43, 179)
local espOutlineColor = Color3.fromRGB(138, 43, 179) -- ‡∏Ç‡∏≠‡∏ö‡∏Ç‡∏≤‡∏ß‡∏Ñ‡∏á‡∏ó‡∏µ‡πà ‡∏´‡∏£‡∏∑‡∏≠‡∏à‡∏∞‡∏õ‡∏£‡∏±‡∏ö‡πÑ‡∏î‡πâ‡∏î‡πâ‡∏ß‡∏¢

-- ===== ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏ä‡πà‡∏ß‡∏¢ =====
local function getPlayerList()
    local list = {}
    for _, p in ipairs(Players:GetPlayers()) do
        if p ~= player then
            table.insert(list, string.format("%s (%s)", p.DisplayName, p.Name))
        end
    end
    table.sort(list)
    return list
end

local function getPlayerFromSelection(selected)
    if not selected or selected == "" then return nil end
    local friendName = selected:match("%((.-)%)") or selected
    return Players:FindFirstChild(friendName)
end

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡πÄ‡∏õ‡∏¥‡∏î ESP (‡πÉ‡∏ä‡πâ‡∏™‡∏µ‡∏õ‡∏±‡∏à‡∏à‡∏∏‡∏ö‡∏±‡∏ô)
local function enableESP(targetPlayer)
    if not targetPlayer or not targetPlayer.Character then return end
    
    -- ‡∏•‡∏ö Highlight ‡πÄ‡∏Å‡πà‡∏≤‡∏Å‡πà‡∏≠‡∏ô (‡∏Å‡∏£‡∏ì‡∏µ respawn)
    if currentESPHighlight and currentESPHighlight.Parent then
        currentESPHighlight:Destroy()
    end
    local highlight = Instance.new("Highlight")
    highlight.Parent = targetPlayer.Character
    highlight.FillColor = espFillColor
    highlight.FillTransparency = 0.5
    highlight.OutlineColor = espOutlineColor
    highlight.OutlineTransparency = 0
    highlight.DepthMode = Enum.HighlightDepthMode.AlwaysOnTop
    highlight.Enabled = true
    currentESPHighlight = highlight
    espTargetPlayer = targetPlayer
end

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏õ‡∏¥‡∏î ESP
local function disableESP()
    if currentESPHighlight then
        currentESPHighlight:Destroy()
        currentESPHighlight = nil
    end
    if espConnection then
        espConnection:Disconnect()
        espConnection = nil
    end
    espTargetPlayer = nil
end

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï‡∏™‡∏µ ESP (‡πÄ‡∏°‡∏∑‡πà‡∏≠‡πÄ‡∏õ‡∏•‡∏µ‡πà‡∏¢‡∏ô‡∏™‡∏µ‡πÉ‡∏ô Color Picker)
local function updateESPColor()
    if currentESPHighlight and currentESPHighlight.Parent then
        currentESPHighlight.FillColor = espFillColor
        currentESPHighlight.OutlineColor = espOutlineColor
    end
end

local friendDropdown = friendTab:CreateDropdown({
    Name = "[ üë• ]  Dropdown Player Choose Player",
    Options = getPlayerList(),
    CurrentOption = {""},
    Flag = "FriendSelect",
    Callback = function(Option)
        if currentESPHighlight then
            local selected = friendDropdown.CurrentOption[1]
            local newTarget = getPlayerFromSelection(selected)
            disableESP()
            if newTarget then
                enableESP(newTarget)

                
            end
        end
    end
})

-- ‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï Dropdown ‡πÄ‡∏°‡∏∑‡πà‡∏≠‡∏°‡∏µ‡∏Ñ‡∏ô‡πÄ‡∏Ç‡πâ‡∏≤-‡∏≠‡∏≠‡∏Å
Players.PlayerAdded:Connect(function()
    friendDropdown:Refresh(getPlayerList(), true)
end)

Players.PlayerRemoving:Connect(function(p)
    task.wait(1)
    friendDropdown:Refresh(getPlayerList(), true)
   
    if espTargetPlayer and p == espTargetPlayer then
        disableESP()
    end
end)

local Section = friendTab:CreateSection("")

-- ===== ‡∏õ‡∏∏‡πà‡∏°‡∏ß‡∏≤‡∏õ‡∏ò‡∏£‡∏£‡∏°‡∏î‡∏≤ =====
friendTab:CreateButton({
    Name = "[ üìå ]  Teleport To Player - Friend",
    Callback = function()
        local selected = friendDropdown.CurrentOption[1]
        if not selected or selected == "" then return end

        local targetPlayer = getPlayerFromSelection(selected)
        local char = player.Character or player.CharacterAdded:Wait()
        local hrp = char:WaitForChild("HumanoidRootPart", 5)
        if not hrp or not targetPlayer or not targetPlayer.Character then return end
        local targetHRP = targetPlayer.Character:FindFirstChild("HumanoidRootPart")
        if targetHRP then
            savedCFrame = hrp.CFrame
            hrp.CFrame = targetHRP.CFrame + Vector3.new(0, 3, 0)
        end
    end
})

friendTab:CreateButton({
    Name = "[ üìû ]  Teleport To Player   [ - 1 Sec - ]",
    Callback = function()
        local selected = friendDropdown.CurrentOption[1]
        if not selected or selected == "" then
            return
        end

        local friendName = selected:match("%((.-)%)") or selected
        local targetPlayer = Players:FindFirstChild(friendName)
        local char = player.Character or player.CharacterAdded:Wait()
        local hrp = char:WaitForChild("HumanoidRootPart", 5)

        if not hrp then
            return
        end

        if targetPlayer and targetPlayer.Character then
            local targetHRP = targetPlayer.Character:FindFirstChild("HumanoidRootPart")
            if targetHRP then
                -- ‡∏ö‡∏±‡∏ô‡∏ó‡∏∂‡∏Å‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á‡πÄ‡∏î‡∏¥‡∏°
                savedCFrame = hrp.CFrame

                -- ‡∏ß‡∏≤‡∏õ‡πÑ‡∏õ‡∏´‡∏≤‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏ô (‡∏¢‡∏Å‡∏Ç‡∏∂‡πâ‡∏ô‡∏ô‡∏¥‡∏î‡∏´‡∏ô‡πà‡∏≠‡∏¢)
                hrp.CFrame = targetHRP.CFrame + Vector3.new(0, 5, 0)

                -- ‡∏£‡∏≠‡πÄ‡∏ü‡∏£‡∏°‡∏ô‡∏¥‡∏î‡∏ô‡∏∂‡∏á‡πÄ‡∏û‡∏∑‡πà‡∏≠‡πÉ‡∏´‡πâ‡πÄ‡∏´‡πá‡∏ô‡∏Å‡∏≤‡∏£‡∏ß‡∏≤‡∏õ‡∏ä‡∏±‡∏î‡πÄ‡∏à‡∏ô (‡πÑ‡∏°‡πà‡∏Å‡∏£‡∏∞‡∏ï‡∏∏‡∏Å)
                task.wait()

                -- ‡∏Å‡∏•‡∏±‡∏ö‡∏à‡∏∏‡∏î‡πÄ‡∏î‡∏¥‡∏°‡∏ó‡∏±‡∏ô‡∏ó‡∏µ
                if savedCFrame and hrp and hrp.Parent then
                    hrp.CFrame = savedCFrame
                end

                return
            end
        end
    end
})

friendTab:CreateToggle({
    Name = "[ ü™ë ]  Auto Teleport to Player Sit Head",
    CurrentValue = false,
    Flag = "AutoTPToggle",
    Callback = function(Value)
        local selected = friendDropdown.CurrentOption[1]
        if not selected or selected == "" then
           
            Rayfield.Flags["AutoTPToggle"]:Set(false)
            return
        end
        local friendName = selected:match("%((.-)%)") or selected
        local targetPlayer = Players:FindFirstChild(friendName)
        if not targetPlayer then
           
            Rayfield.Flags["AutoTPToggle"]:Set(false)
            return
        end
        -- ‡∏õ‡∏¥‡∏î Auto TP ‡πÄ‡∏Å‡πà‡∏≤
        if autoTPConnection then
            autoTPConnection:Disconnect()
            autoTPConnection = nil
        end
        autoTargetName = nil
        if Value then
            -- ‡∏ö‡∏±‡∏ô‡∏ó‡∏∂‡∏Å‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á‡∏õ‡∏±‡∏à‡∏à‡∏∏‡∏ö‡∏±‡∏ô (‡∏Å‡πà‡∏≠‡∏ô‡πÄ‡∏£‡∏¥‡πà‡∏° Auto TP)
            local char = player.Character or player.CharacterAdded:Wait()
            local hrp = char:WaitForChild("HumanoidRootPart", 5)
            if hrp then
                savedCFrame = hrp.CFrame
            end
            autoTargetName = friendName
            -- ‡∏™‡∏£‡πâ‡∏≤‡∏á Connection ‡∏´‡∏•‡∏±‡∏Å‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö Auto Teleport
            autoTPConnection = RunService.Heartbeat:Connect(function()
                if not autoTargetName then return end
                local target = Players:FindFirstChild(autoTargetName)
                if not target or not target.Character then return end
                local targetHRP = target.Character:FindFirstChild("HumanoidRootPart")
                if not targetHRP then return end
                local myChar = player.Character
                if not myChar then return end
                local myHRP = myChar:FindFirstChild("HumanoidRootPart")
                if not myHRP then return end
                -- ‡∏ß‡∏≤‡∏õ‡πÑ‡∏õ‡∏´‡∏≤‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢ (‡∏™‡∏π‡∏á‡∏Ç‡∏∂‡πâ‡∏ô‡∏ô‡∏¥‡∏î‡∏´‡∏ô‡πà‡∏≠‡∏¢‡πÄ‡∏û‡∏∑‡πà‡∏≠‡πÑ‡∏°‡πà‡∏ï‡∏¥‡∏î‡∏û‡∏∑‡πâ‡∏ô)
                myHRP.CFrame = targetHRP.CFrame + Vector3.new(0, 4, 0)
            end)
           
        else
            -- ‡∏õ‡∏¥‡∏î Auto TP
            autoTargetName = nil
           
        end
    end
})

-- ‡πÄ‡∏û‡∏¥‡πà‡∏°: ‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï Auto TP ‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï‡∏¥‡πÄ‡∏°‡∏∑‡πà‡∏≠‡πÄ‡∏õ‡∏•‡∏µ‡πà‡∏¢‡∏ô‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡πÉ‡∏ô Dropdown (‡∏Ç‡∏ì‡∏∞‡πÄ‡∏õ‡∏¥‡∏î Toggle ‡∏≠‡∏¢‡∏π‡πà)
friendDropdown.Callback = function(selected)
    local currentToggleValue = Rayfield.Flags["AutoTPToggle"].CurrentValue
    -- ‡∏ñ‡πâ‡∏≤ Auto TP ‡∏Å‡∏≥‡∏•‡∏±‡∏á‡πÄ‡∏õ‡∏¥‡∏î‡∏≠‡∏¢‡∏π‡πà ‡πÉ‡∏´‡πâ‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢‡πÉ‡∏´‡∏°‡πà‡∏ó‡∏±‡∏ô‡∏ó‡∏µ
    if currentToggleValue then
        Rayfield.Flags["AutoTPToggle"]:Set(false) -- ‡∏õ‡∏¥‡∏î‡∏ä‡∏±‡πà‡∏ß‡∏Ñ‡∏£‡∏≤‡∏ß
        task.wait(0.1)
        Rayfield.Flags["AutoTPToggle"]:Set(true) -- ‡πÄ‡∏õ‡∏¥‡∏î‡πÉ‡∏´‡∏°‡πà ‚Üí ‡∏à‡∏∞‡πÉ‡∏ä‡πâ‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢‡πÉ‡∏´‡∏°‡πà
    end
    -- ‡∏ñ‡πâ‡∏≤‡∏°‡∏µ ESP ‡πÄ‡∏õ‡∏¥‡∏î‡∏≠‡∏¢‡∏π‡πà‡∏Å‡∏±‡∏ö‡∏Ñ‡∏ô‡πÄ‡∏Å‡πà‡∏≤ ‡∏Å‡πá‡∏≠‡∏≤‡∏à‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï‡πÑ‡∏î‡πâ‡∏ó‡∏µ‡πà‡∏ô‡∏µ‡πà‡πÄ‡∏´‡∏°‡∏∑‡∏≠‡∏ô‡∏Å‡∏±‡∏ô (‡πÅ‡∏ï‡πà‡∏ï‡∏≠‡∏ô‡∏ô‡∏µ‡πâ‡πÅ‡∏¢‡∏Å‡∏õ‡∏∏‡πà‡∏°)
end

local Paragraph = friendTab:CreateParagraph({Title = "Teleport To Player / Friend Normal / Auto", Content = "Teleport To Player / Friend Normal / Become Auto"})

local Section = friendTab:CreateSection("")

-- ‡∏ï‡∏±‡∏ß‡πÅ‡∏õ‡∏£‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö Hitbox ‡πÄ‡∏â‡∏û‡∏≤‡∏∞‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢
local TargetHitboxEnabled = false
local TargetHitboxSize = 20
local TargetHitboxConnection = nil
local CurrentTargetPlayer = nil  -- ‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡∏ó‡∏µ‡πà‡∏ñ‡∏π‡∏Å‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡∏õ‡∏±‡∏à‡∏à‡∏∏‡∏ö‡∏±‡∏ô

-- ‡∏™‡∏µ‡πÄ‡∏Ç‡∏µ‡∏¢‡∏ß‡πÄ‡∏î‡∏µ‡∏¢‡∏ß‡∏Å‡∏±‡∏ö ESP ‡∏Ç‡∏≠‡∏á‡∏Ñ‡∏∏‡∏ì
local TargetHitboxColor = Color3.fromRGB(138, 43, 179)

friendTab:CreateToggle({
    Name = "[ ‚öîÔ∏è ]  Hitbox Boxing Enable Target",
    CurrentValue = false,
    Flag = "TargetHitboxToggle",
    Callback = function(Value)
        TargetHitboxEnabled = Value
        
        -- ‡∏î‡∏∂‡∏á‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡∏ó‡∏µ‡πà‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡∏à‡∏≤‡∏Å Dropdown
        local selected = friendDropdown.CurrentOption[1]
        local targetPlayer = getPlayerFromSelection(selected)
        
        if not targetPlayer then
            Rayfield.Flags["TargetHitboxToggle"]:Set(false)
            
            return
        end
        
        CurrentTargetPlayer = targetPlayer
        
        if Value then
            -- ‡πÄ‡∏õ‡∏¥‡∏î Hitbox ‡πÄ‡∏â‡∏û‡∏≤‡∏∞‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢
            local function applyHitbox(char)
                if not char then return end
                pcall(function()
                    local hrp = char:FindFirstChild("HumanoidRootPart")
                    if hrp then
                        hrp.Size = Vector3.new(TargetHitboxSize, TargetHitboxSize, TargetHitboxSize)
                        hrp.Transparency = 0.7
                        hrp.Color = TargetHitboxColor
                        hrp.Material = "Neon"
                        hrp.CanCollide = false
                    end
                end)
            end
            
            -- ‡πÉ‡∏ä‡πâ RenderStepped ‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï‡∏ï‡πà‡∏≠‡πÄ‡∏ô‡∏∑‡πà‡∏≠‡∏á (‡∏Å‡∏£‡∏ì‡∏µ‡πÄ‡∏Ñ‡∏•‡∏∑‡πà‡∏≠‡∏ô‡∏ó‡∏µ‡πà‡∏´‡∏£‡∏∑‡∏≠‡πÄ‡∏õ‡∏•‡∏µ‡πà‡∏¢‡∏ô‡∏™‡πà‡∏ß‡∏ô)
            TargetHitboxConnection = RunService.RenderStepped:Connect(function()
                if not TargetHitboxEnabled or not CurrentTargetPlayer or CurrentTargetPlayer ~= targetPlayer then return end
                if CurrentTargetPlayer.Character then
                    applyHitbox(CurrentTargetPlayer.Character)
                end
            end)
            
            -- ‡πÉ‡∏ä‡πâ‡∏ï‡∏≠‡∏ô‡∏ô‡∏µ‡πâ‡πÄ‡∏•‡∏¢
            if targetPlayer.Character then
                applyHitbox(targetPlayer.Character)
            end
            
            -- ‡∏£‡∏≠‡∏á‡∏£‡∏±‡∏ö respawn
            targetPlayer.CharacterAdded:Connect(function(newChar)
                if TargetHitboxEnabled and CurrentTargetPlayer == targetPlayer then
                    task.wait(0.5)  -- ‡∏£‡∏≠‡πÉ‡∏´‡πâ‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£‡πÇ‡∏´‡∏•‡∏î‡πÄ‡∏™‡∏£‡πá‡∏à
                    applyHitbox(newChar)
                end
            end)
            
            
        else
            -- ‡∏õ‡∏¥‡∏î Hitbox
            if TargetHitboxConnection then
                TargetHitboxConnection:Disconnect()
                TargetHitboxConnection = nil
            end
            
            -- Reset Hitbox ‡∏Ç‡∏≠‡∏á‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢‡∏Å‡∏•‡∏±‡∏ö‡∏Ñ‡πà‡∏≤‡πÄ‡∏î‡∏¥‡∏°
            if targetPlayer.Character then
                pcall(function()
                    local hrp = targetPlayer.Character:FindFirstChild("HumanoidRootPart")
                    if hrp then
                        hrp.Size = Vector3.new(2, 2, 1)
                        hrp.Transparency = 1
                        hrp.Material = "Plastic"
                        hrp.CanCollide = false
                    end
                end)
            end
            
            CurrentTargetPlayer = nil
            
           
        end
    end,
})

friendTab:CreateSlider({
    Name = "[ üìè ]  Slider Hitbox Player Size",
    Range = {20, 5000},
    Increment = 1,
    Suffix = " Hitbox Size",
    CurrentValue = 20,
    Flag = "TargetHitboxSize",
    Callback = function(Value)
        TargetHitboxSize = Value
        
        -- ‡∏ñ‡πâ‡∏≤‡πÄ‡∏õ‡∏¥‡∏î‡∏≠‡∏¢‡∏π‡πà ‡πÉ‡∏´‡πâ‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï‡∏Ç‡∏ô‡∏≤‡∏î‡∏ó‡∏±‡∏ô‡∏ó‡∏µ
        if TargetHitboxEnabled and CurrentTargetPlayer and CurrentTargetPlayer.Character then
            pcall(function()
                local hrp = CurrentTargetPlayer.Character:FindFirstChild("HumanoidRootPart")
                if hrp then
                    hrp.Size = Vector3.new(TargetHitboxSize, TargetHitboxSize, TargetHitboxSize)
                end
            end)
        end
    end,
})

-- ‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï‡∏¥‡πÄ‡∏°‡∏∑‡πà‡∏≠‡πÄ‡∏õ‡∏•‡∏µ‡πà‡∏¢‡∏ô‡∏Ñ‡∏ô‡πÉ‡∏ô Dropdown (‡∏ñ‡πâ‡∏≤ Toggle ‡πÄ‡∏õ‡∏¥‡∏î‡∏≠‡∏¢‡∏π‡πà)
friendDropdown.Callback = function(selected)
    local currentToggleValue = Rayfield.Flags["TargetHitboxToggle"].CurrentValue
    
    -- ‡∏ñ‡πâ‡∏≤ Toggle ‡πÄ‡∏õ‡∏¥‡∏î‡∏≠‡∏¢‡∏π‡πà ‡πÉ‡∏´‡πâ‡∏õ‡∏¥‡∏î‡∏Å‡πà‡∏≠‡∏ô‡πÅ‡∏•‡πâ‡∏ß‡πÄ‡∏õ‡∏¥‡∏î‡πÉ‡∏´‡∏°‡πà‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢
    if currentToggleValue then
        Rayfield.Flags["TargetHitboxToggle"]:Set(false)
        task.wait(0.1)
        Rayfield.Flags["TargetHitboxToggle"]:Set(true)
    end
    
    -- ‡∏™‡πà‡∏ß‡∏ô‡πÄ‡∏î‡∏¥‡∏°‡∏Ç‡∏≠‡∏á‡∏Ñ‡∏∏‡∏ì (‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö Auto TP ‡πÅ‡∏•‡∏∞ ESP)
    local currentAutoTPValue = Rayfield.Flags["AutoTPToggle"].CurrentValue
    if currentAutoTPValue then
        Rayfield.Flags["AutoTPToggle"]:Set(false)
        task.wait(0.1)
        Rayfield.Flags["AutoTPToggle"]:Set(true)
    end
end

local Paragraph = friendTab:CreateParagraph({Title = "ESP / Hitbox Boxing For Kill Player And Reset", Content = "ESP / Hitbox Boxing For Kill Player And Reseting"})

local Section = friendTab:CreateSection("")

-- ===== ‡∏ï‡∏±‡∏ß‡πÅ‡∏õ‡∏£‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö ESP ‡∏ó‡∏±‡πâ‡∏á‡∏™‡∏≠‡∏á‡πÅ‡∏ö‡∏ö =====
local currentESPHighlight = nil -- ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö Highlight ‡∏ó‡∏±‡πâ‡∏á‡∏Ñ‡∏π‡πà
local currentESPDrawings = {} -- ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö Drawing (Box + Name) ‡πÄ‡∏â‡∏û‡∏≤‡∏∞ ESP ‡∏õ‡∏Å‡∏ï‡∏¥
local espTargetPlayer = nil
local espConnection = nil

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏™‡∏£‡πâ‡∏≤‡∏á/‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï Highlight
local function enableHighlight(targetPlayer)
    if not targetPlayer or not targetPlayer.Character then return end
   
    -- ‡∏•‡∏ö Highlight ‡πÄ‡∏Å‡πà‡∏≤
    if currentESPHighlight and currentESPHighlight.Parent then
        currentESPHighlight:Destroy()
    end
   
    local highlight = Instance.new("Highlight")
    highlight.Parent = targetPlayer.Character
    highlight.FillColor = espFillColor
    highlight.FillTransparency = 0.5
    highlight.OutlineColor = espOutlineColor
    highlight.OutlineTransparency = 0
    highlight.DepthMode = Enum.HighlightDepthMode.AlwaysOnTop
    highlight.Enabled = true
   
    currentESPHighlight = highlight
    espTargetPlayer = targetPlayer
end

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏™‡∏£‡πâ‡∏≤‡∏á Drawing ESP (Box + Name + Health)
local function enableDrawingESP(targetPlayer)
    if not targetPlayer or not targetPlayer.Character then return end
   
    -- ‡∏•‡∏ö Drawing ‡πÄ‡∏Å‡πà‡∏≤
    for _, drawing in pairs(currentESPDrawings) do
        if drawing then drawing:Remove() end
    end
    currentESPDrawings = {}
   
    local box = Drawing.new("Square")
    box.Thickness = 2
    box.Filled = false
    box.Color = espFillColor
    box.Visible = false
   
    local nameText = Drawing.new("Text")
    nameText.Size = 16
    nameText.Center = true
    nameText.Outline = true
    nameText.Color = Color3.fromRGB(138, 43, 179)
    nameText.Visible = false
   
    currentESPDrawings = {Box = box, Name = nameText}
   
    -- ‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï‡∏ó‡∏∏‡∏Å‡πÄ‡∏ü‡∏£‡∏°
    if espConnection then espConnection:Disconnect() end
    espConnection = RunService.RenderStepped:Connect(function()
        if not targetPlayer.Character or not targetPlayer.Character:FindFirstChild("HumanoidRootPart") or not targetPlayer.Character:FindFirstChild("Head") or not targetPlayer.Character:FindFirstChild("Humanoid") then
            box.Visible = false
            nameText.Visible = false
            return
        end
       
        local root = targetPlayer.Character.HumanoidRootPart
        local head = targetPlayer.Character.Head
        local humanoid = targetPlayer.Character.Humanoid
       
        local screenPos, onScreen = workspace.CurrentCamera:WorldToViewportPoint(root.Position)
       
        if onScreen then
            local top = workspace.CurrentCamera:WorldToViewportPoint(head.Position + Vector3.new(0, 3, 0))
            local bottom = workspace.CurrentCamera:WorldToViewportPoint(head.Position - Vector3.new(0, 3, 0))
            local sizeY = (top - bottom).Magnitude
           
            box.Size = Vector2.new(sizeY * 2, sizeY * 3.5)
            box.Position = Vector2.new(screenPos.X - box.Size.X / 2, screenPos.Y - box.Size.Y / 2)
            box.Visible = true
           
            nameText.Text = targetPlayer.DisplayName .. " [ " .. math.floor(humanoid.Health) .. " ]"
            nameText.Position = Vector2.new(screenPos.X, screenPos.Y - box.Size.Y / 2 - 20)
            nameText.Visible = true
        else
            box.Visible = false
            nameText.Visible = false
        end
    end)
end

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏õ‡∏¥‡∏î‡∏ó‡∏∏‡∏Å‡∏≠‡∏¢‡πà‡∏≤‡∏á
local function disableAllESP()
    if currentESPHighlight then
        currentESPHighlight:Destroy()
        currentESPHighlight = nil
    end
    for _, drawing in pairs(currentESPDrawings) do
        if drawing then drawing:Remove() end
    end
    currentESPDrawings = {}
    if espConnection then
        espConnection:Disconnect()
        espConnection = nil
    end
    espTargetPlayer = nil
end

-- ===== ‡∏õ‡∏∏‡πà‡∏° ESP ‡∏õ‡∏Å‡∏ï‡∏¥ (Highlight + Box + Name) =====
friendTab:CreateButton({
    Name = "[ üëÅÔ∏è ]  ESP Box Dropdown Player",
    Callback = function()
        local selected = friendDropdown.CurrentOption[1]
        local targetPlayer = getPlayerFromSelection(selected)
       
        if not targetPlayer then
            disableAllESP()
           
            return
        end
       
        -- ‡∏ñ‡πâ‡∏≤‡πÄ‡∏õ‡∏¥‡∏î‡∏≠‡∏¢‡∏π‡πà‡πÅ‡∏•‡πâ‡∏ß‡∏Å‡∏±‡∏ö‡∏Ñ‡∏ô‡πÄ‡∏î‡∏µ‡∏¢‡∏ß‡∏Å‡∏±‡∏ô ‚Üí ‡∏õ‡∏¥‡∏î
        if espTargetPlayer == targetPlayer and currentESPHighlight and next(currentESPDrawings) then
            disableAllESP()
            return
        end
       
        -- ‡∏õ‡∏¥‡∏î‡πÄ‡∏Å‡πà‡∏≤ ‡πÅ‡∏•‡πâ‡∏ß‡πÄ‡∏õ‡∏¥‡∏î‡πÉ‡∏´‡∏°‡πà
        disableAllESP()
        enableHighlight(targetPlayer)
        enableDrawingESP(targetPlayer)
       
        -- ‡∏ï‡∏¥‡∏î‡∏ï‡∏≤‡∏° respawn
        targetPlayer.CharacterAdded:Connect(function()
            if espTargetPlayer == targetPlayer then
                task.wait(0.5)
                enableHighlight(targetPlayer)
                enableDrawingESP(targetPlayer)
            end
        end)
    end
})

friendTab:CreateButton({
    Name = "[ üí° ]  ESP Highlight Dropdown Player",
    Callback = function()
        local selected = friendDropdown.CurrentOption[1]
        local targetPlayer = getPlayerFromSelection(selected)
       
        if not targetPlayer then
            disableAllESP()
           
            return
        end
       
        -- ‡∏ñ‡πâ‡∏≤‡πÄ‡∏õ‡∏¥‡∏î Highlight ‡∏≠‡∏¢‡∏π‡πà‡∏Ñ‡∏ô‡πÄ‡∏î‡∏µ‡∏¢‡∏ß‡∏Å‡∏±‡∏ô ‚Üí ‡∏õ‡∏¥‡∏î
        if espTargetPlayer == targetPlayer and currentESPHighlight and not next(currentESPDrawings) then
            disableAllESP()
            return
        end
       
        -- ‡∏õ‡∏¥‡∏î‡πÄ‡∏Å‡πà‡∏≤ ‡πÅ‡∏•‡πâ‡∏ß‡πÄ‡∏õ‡∏¥‡∏î Highlight ‡πÄ‡∏â‡∏û‡∏≤‡∏∞
        disableAllESP()
        enableHighlight(targetPlayer)
       
        -- ‡∏ï‡∏¥‡∏î‡∏ï‡∏≤‡∏° respawn (‡πÄ‡∏â‡∏û‡∏≤‡∏∞ Highlight)
        targetPlayer.CharacterAdded:Connect(function()
            if espTargetPlayer == targetPlayer and not next(currentESPDrawings) then
                task.wait(0.5)
                enableHighlight(targetPlayer)
            end
        end)
    end
})

game:GetService("RunService").RenderStepped:Connect(UpdateESP)

local Paragraph = friendTab:CreateParagraph({Title = "ESP To Player / Friend Normal", Content = "ESP To Player / Friend Become Normal"})

local Section = friendTab:CreateSection("")

-- ===== ‡∏õ‡∏∏‡πà‡∏°‡πÉ‡∏´‡∏°‡πà: ‡∏Å‡∏•‡∏±‡∏ö‡πÑ‡∏õ‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á‡πÄ‡∏î‡∏¥‡∏° =====
friendTab:CreateButton({
    Name = "[ üîô ]  Return To Original Position",
    Callback = function()
        if not savedCFrame then
            -- ‡∏ñ‡πâ‡∏≤‡∏¢‡∏±‡∏á‡πÑ‡∏°‡πà‡∏°‡∏µ‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á‡∏ó‡∏µ‡πà‡∏ö‡∏±‡∏ô‡∏ó‡∏∂‡∏Å‡πÑ‡∏ß‡πâ (‡∏¢‡∏±‡∏á‡πÑ‡∏°‡πà‡πÄ‡∏Ñ‡∏¢‡∏ß‡∏≤‡∏£‡πå‡∏õ)
            warn("No saved position to return to!")
            return
        end

        local char = player.Character or player.CharacterAdded:Wait()
        local hrp = char:FindFirstChild("HumanoidRootPart")
        
        if not hrp then
            warn("HumanoidRootPart not found!")
            return
        end

        -- ‡∏ß‡∏≤‡∏£‡πå‡∏õ‡∏Å‡∏•‡∏±‡∏ö‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á‡πÄ‡∏î‡∏¥‡∏°‡∏≠‡∏¢‡πà‡∏≤‡∏á‡∏£‡∏ß‡∏î‡πÄ‡∏£‡πá‡∏ß‡πÅ‡∏•‡∏∞‡∏ô‡∏∏‡πà‡∏°‡∏ô‡∏ß‡∏•
        hrp.CFrame = savedCFrame
    end
})

local Paragraph = friendTab:CreateParagraph({Title = "Return To Your Point", Content = "Return To Your Point And Save Point"})

local Section = friendTab:CreateSection("")

--// === ESP TAB (Local ESP - Rainbow Edition) ===
local espTab = Window:CreateTab("||   - Solution", "flask-conical")
local Section = espTab:CreateSection("")

local MouseTPEnabled = false
local Mouse = game.Players.LocalPlayer:GetMouse()
local Player = game.Players.LocalPlayer

-- ‡∏™‡∏£‡πâ‡∏≤‡∏á Sound ‡πÑ‡∏ß‡πâ‡πÉ‡∏ä‡πâ‡∏ã‡πâ‡∏≥‡πÑ‡∏î‡πâ‡πÄ‡∏£‡∏∑‡πà‡∏≠‡∏¢ ‡πÜ
local teleportSound = Instance.new("Sound")
teleportSound.SoundId = "rbxassetid://4861638982"
teleportSound.Volume = 0.7

local Toggle = espTab:CreateToggle({
    Name = "[ üëÜ ]  Clicking Teleport   [ - Backpack - ]",
    CurrentValue = false,
    Flag = "MouseTP",
    Callback = function(Value)
        MouseTPEnabled = Value
        Rayfield:Notify({
            Title = Value and   "[ üëÜ ]  Teleport Clicking On" or "[ üëÜ ]  Teleport Clicking Off",
            Content = Value and "[ ‚úÖ ]  Click Teleport On" or "[ üõë ]  Click Teleport Off",
            Duration = 1,
            Image = "mouse"
        })
    end,
})

-- ‡∏Å‡∏î‡∏Ñ‡∏•‡∏¥‡∏Å = ‡∏ß‡∏≤‡∏£‡πå‡∏õ‡∏ó‡∏±‡∏ô‡∏ó‡∏µ + ‡πÄ‡∏™‡∏µ‡∏¢‡∏á‡∏î‡∏±‡∏á‡∏û‡∏£‡πâ‡∏≠‡∏°‡∏Å‡∏±‡∏ô
Mouse.Button1Down:Connect(function()
    if MouseTPEnabled and Player.Character and Player.Character:FindFirstChild("HumanoidRootPart") then
        local targetPos = Mouse.Hit.Position + Vector3.new(0, 5, 0)
        
        -- ‡∏ß‡∏≤‡∏£‡πå‡∏õ‡∏Å‡πà‡∏≠‡∏ô‡πÄ‡∏•‡∏¢ ‡πÑ‡∏°‡πà‡∏ï‡πâ‡∏≠‡∏á‡∏£‡∏≠
        Player.Character.HumanoidRootPart.CFrame = CFrame.new(targetPos)
        
        -- ‡πÄ‡∏•‡πà‡∏ô‡πÄ‡∏™‡∏µ‡∏¢‡∏á‡∏ó‡∏±‡∏ô‡∏ó‡∏µ (‡πÄ‡∏™‡∏µ‡∏¢‡∏á‡∏à‡∏∞‡∏î‡∏±‡∏á‡∏à‡∏≤‡∏Å‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á‡πÉ‡∏´‡∏°‡πà‡πÄ‡∏•‡∏¢)
        teleportSound.Parent = Player.Character.HumanoidRootPart
        teleportSound:Play()
        
        -- ‡∏´‡∏•‡∏±‡∏á‡∏à‡∏≤‡∏Å‡πÄ‡∏•‡πà‡∏ô‡πÄ‡∏™‡∏£‡πá‡∏à ‡∏•‡∏ö‡∏≠‡∏≠‡∏Å‡πÄ‡∏û‡∏∑‡πà‡∏≠‡πÑ‡∏°‡πà‡πÉ‡∏´‡πâ‡∏Å‡∏¥‡∏ô memory
        task.delay(3, function()
            teleportSound.Parent = nil
        end)
    end
end)

-- ‡∏£‡∏µ‡πÇ‡∏´‡∏•‡∏î‡∏™‡∏ñ‡∏≤‡∏ô‡∏∞‡∏ï‡∏≠‡∏ô‡πÄ‡∏Å‡∏¥‡∏î‡πÉ‡∏´‡∏°‡πà
Player.CharacterAdded:Connect(function()
    task.wait(1)
    MouseTPEnabled = Rayfield.Flags.MouseTP.CurrentValue
end)

local Paragraph = espTab:CreateParagraph({Title = "Clicking Teleport For Player / Friend", Content = "Clicking Teleport Is Good For Building"})

local Section = espTab:CreateSection("")

local PurpleTool = {
    Enabled = false,
    Current = nil,
    Highlight = nil,
    Connections = {},
}

-- ‡∏™‡∏µ‡∏°‡πà‡∏ß‡∏á‡∏ó‡∏µ‡πà‡∏ï‡πâ‡∏≠‡∏á‡∏Å‡∏≤‡∏£ (138, 43, 179)
local PURPLE_COLOR = Color3.fromRGB(138, 43, 179)

local function CreateHighlight(tool)
    if PurpleTool.Highlight then PurpleTool.Highlight:Destroy() end
    PurpleTool.Highlight = Instance.new("Highlight")
    PurpleTool.Highlight.FillColor = PURPLE_COLOR
    PurpleTool.Highlight.OutlineColor = PURPLE_COLOR
    PurpleTool.Highlight.FillTransparency = 0.3
    PurpleTool.Highlight.OutlineTransparency = 0
    PurpleTool.Highlight.Parent = tool
    PurpleTool.Highlight.Adornee = tool:FindFirstChild("Handle") or tool
end

local function ApplyPurpleColor()
    if not PurpleTool.Enabled or not PurpleTool.Current or not PurpleTool.Highlight then return end
    
    -- ‡∏ï‡∏±‡πâ‡∏á‡∏Ñ‡πà‡∏≤‡∏™‡∏µ Highlight
    PurpleTool.Highlight.FillColor = PURPLE_COLOR
    PurpleTool.Highlight.OutlineColor = PURPLE_COLOR
    
    -- ‡πÄ‡∏õ‡∏•‡∏µ‡πà‡∏¢‡∏ô‡∏™‡∏µ‡∏ó‡∏∏‡∏Å Part ‡πÉ‡∏ô Tool (‡∏¢‡∏Å‡πÄ‡∏ß‡πâ‡∏ô HumanoidRootPart)
    for _, part in pairs(PurpleTool.Current:GetDescendants()) do
        if part:IsA("BasePart") and part.Name ~= "HumanoidRootPart" then
            part.Color = PURPLE_COLOR
        end
    end
end

local function OnEquipped(tool)
    if not PurpleTool.Enabled then return end
    PurpleTool.Current = tool
    task.wait(0.1)
    CreateHighlight(tool)
    ApplyPurpleColor()  -- ‡πÉ‡∏ä‡πâ‡∏™‡∏µ‡∏°‡πà‡∏ß‡∏á‡∏ó‡∏±‡∏ô‡∏ó‡∏µ
end

local function OnUnequipped()
    if PurpleTool.Highlight then 
        PurpleTool.Highlight:Destroy() 
        PurpleTool.Highlight = nil 
    end
    PurpleTool.Current = nil
end

local function SetupTool(char)
    -- ‡∏•‡πâ‡∏≤‡∏á connection ‡πÄ‡∏Å‡πà‡∏≤
    for _, c in pairs(PurpleTool.Connections) do c:Disconnect() end
    PurpleTool.Connections = {}

    local function ConnectTool(tool)
        table.insert(PurpleTool.Connections, tool.Equipped:Connect(function() OnEquipped(tool) end))
        table.insert(PurpleTool.Connections, tool.Unequipped:Connect(OnUnequipped))
    end

    -- ‡πÄ‡∏ä‡∏∑‡πà‡∏≠‡∏° Tool ‡∏ó‡∏µ‡πà‡∏≠‡∏¢‡∏π‡πà‡πÉ‡∏ô‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£‡∏ï‡∏≠‡∏ô‡∏ô‡∏µ‡πâ
    for _, child in pairs(char:GetChildren()) do
        if child:IsA("Tool") then ConnectTool(child) end
    end
    table.insert(PurpleTool.Connections, char.ChildAdded:Connect(function(c)
        if c:IsA("Tool") then ConnectTool(c) end
    end))

    -- ‡πÄ‡∏ä‡∏∑‡πà‡∏≠‡∏° Tool ‡πÉ‡∏ô Backpack
    local backpack = LocalPlayer:WaitForChild("Backpack")
    for _, child in pairs(backpack:GetChildren()) do
        if child:IsA("Tool") then ConnectTool(child) end
    end
    table.insert(PurpleTool.Connections, backpack.ChildAdded:Connect(function(c)
        if c:IsA("Tool") then ConnectTool(c) end
    end))
end

local function TogglePurpleTool(state)
    PurpleTool.Enabled = state
    if state then
        Rayfield:Notify({
           Title = "[ ‚úÖ ]  Purple Tools On",
           Content = "[ üü™ ]  Purple Tools Finish",
           Duration = 1,
           Image = "grape",
         })
        if LocalPlayer.Character then SetupTool(LocalPlayer.Character) end
    else
        Rayfield:Notify({
           Title = "[ ‚ùå ] Purple Tools Off",
           Content = "[ üü™ ] Purple Tools Finish",
           Duration = 1,
           Image = "grape",
         })
        if PurpleTool.Highlight then 
            PurpleTool.Highlight:Destroy() 
            PurpleTool.Highlight = nil 
        end
        PurpleTool.Current = nil
    end
end

-- ‡∏£‡∏≠‡∏á‡∏£‡∏±‡∏ö‡∏Å‡∏≤‡∏£‡πÄ‡∏Å‡∏¥‡∏î‡πÉ‡∏´‡∏°‡πà
LocalPlayer.CharacterAdded:Connect(SetupTool)
if LocalPlayer.Character then SetupTool(LocalPlayer.Character) end

-- Toggle ‡πÉ‡∏ô UI (‡πÄ‡∏õ‡∏•‡∏µ‡πà‡∏¢‡∏ô‡∏ä‡∏∑‡πà‡∏≠‡πÉ‡∏´‡πâ‡πÄ‡∏Ç‡πâ‡∏≤‡∏Å‡∏±‡∏ö‡∏ò‡∏µ‡∏°‡∏°‡πà‡∏ß‡∏á)
espTab:CreateToggle({
    Name = "[ üü™ ]  Purple Glowing Tool   [ - Permanent Purple - ]",
    CurrentValue = false,
    Callback = function(v) 
        TogglePurpleTool(v) 
    end
})

local RainbowGun = {
    Enabled = false,
    Current = nil,
    Highlight = nil,
    Hue = 0,
    Connections = {},
    Loop = nil
}

local function CreateHighlight(tool)
    if RainbowGun.Highlight then RainbowGun.Highlight:Destroy() end
    RainbowGun.Highlight = Instance.new("Highlight")
    RainbowGun.Highlight.FillTransparency = 0.3
    RainbowGun.Highlight.OutlineTransparency = 0
    RainbowGun.Highlight.Parent = tool
    RainbowGun.Highlight.Adornee = tool:FindFirstChild("Handle") or tool
end

local function UpdateRainbowGun()
    if not RainbowGun.Enabled or not RainbowGun.Current or not RainbowGun.Highlight then return end
    RainbowGun.Hue = (RainbowGun.Hue + 0.01) % 1
    local color = Color3.fromHSV(RainbowGun.Hue, 1, 1)
    RainbowGun.Highlight.FillColor = color
    RainbowGun.Highlight.OutlineColor = color
    for _, part in pairs(RainbowGun.Current:GetDescendants()) do
        if part:IsA("BasePart") and part.Name ~= "HumanoidRootPart" then
            part.Color = color
        end
    end
end

local function OnEquipped(tool)
    if not RainbowGun.Enabled then return end
    RainbowGun.Current = tool
    task.wait(0.1)
    CreateHighlight(tool)
end

local function OnUnequipped()
    if RainbowGun.Highlight then RainbowGun.Highlight:Destroy() RainbowGun.Highlight = nil end
    RainbowGun.Current = nil
end

local function SetupGun(char)
    for _, c in pairs(RainbowGun.Connections) do c:Disconnect() end
    RainbowGun.Connections = {}

    local function ConnectTool(tool)
        table.insert(RainbowGun.Connections, tool.Equipped:Connect(function() OnEquipped(tool) end))
        table.insert(RainbowGun.Connections, tool.Unequipped:Connect(OnUnequipped))
    end

    for _, child in pairs(char:GetChildren()) do
        if child:IsA("Tool") then ConnectTool(child) end
    end
    table.insert(RainbowGun.Connections, char.ChildAdded:Connect(function(c)
        if c:IsA("Tool") then ConnectTool(c) end
    end))

    local backpack = LocalPlayer:WaitForChild("Backpack")
    for _, child in pairs(backpack:GetChildren()) do
        if child:IsA("Tool") then ConnectTool(child) end
    end
    table.insert(RainbowGun.Connections, backpack.ChildAdded:Connect(function(c)
        if c:IsA("Tool") then ConnectTool(c) end
    end))
end

local function ToggleRainbowGun(state)
    RainbowGun.Enabled = state
    if state then
        Rayfield:Notify({
           Title = "[ ‚úÖ ]  Rainbow Tools On",
           Content = "[ üåà ]  Rainbow Tools Finish",
           Duration = 1,
           Image = "rainbow",
         })
        if not RainbowGun.Loop then
            RainbowGun.Loop = RunService.Heartbeat:Connect(UpdateRainbowGun)
        end
        if LocalPlayer.Character then SetupGun(LocalPlayer.Character) end
    else
        Rayfield:Notify({
           Title = "[ ‚ùå ]  Rainbow Tools Off",
           Content = "[ üåà ]  Rainbow Tools Finish",
           Duration = 1,
           Image = "rainbow",
         })
        if RainbowGun.Loop then RainbowGun.Loop:Disconnect() RainbowGun.Loop = nil end
        if RainbowGun.Highlight then RainbowGun.Highlight:Destroy() RainbowGun.Highlight = nil end
        RainbowGun.Current = nil
    end
end

LocalPlayer.CharacterAdded:Connect(SetupGun)
if LocalPlayer.Character then SetupGun(LocalPlayer.Character) end

espTab:CreateToggle({
    Name = "[ üåà ]  Rainbow Mountain Tool   [ - Permanent Rainbow - ]",
    CurrentValue = false,
    Callback = function(v) ToggleRainbowGun(v) end
})

Players.PlayerAdded:Connect(function(p)
    if ESPEnabled and p ~= LocalPlayer then
        p.CharacterAdded:Connect(function() createESP(p) end)
    end
end)

Players.PlayerRemoving:Connect(function(p)
    if ESPObjects[p] then
        for _, obj in pairs(ESPObjects[p]) do
            if typeof(obj) == "Instance" and obj.Remove then
                obj:Remove()
            end
        end
        ESPObjects[p] = nil
    end
end)

local Paragraph = espTab:CreateParagraph({Title = "Purple / RainbowTool For Cool Style", Content = "Purple / Rainbow Tool Is Good For Style Gun / Sword"})

RunService.RenderStepped:Connect(updateESP)

