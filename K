local Player = game.Players.LocalPlayer
local Backpack = Player:WaitForChild("Backpack")

-- ‡∏ï‡∏±‡∏ß‡πÅ‡∏õ‡∏£‡πÄ‡∏Å‡πá‡∏ö Tool ‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏à‡∏±‡∏î‡∏Å‡∏≤‡∏£‡πÄ‡∏õ‡∏¥‡∏î/‡∏õ‡∏¥‡∏î
local TeleportCandy = nil

-- ‡∏™‡∏£‡πâ‡∏≤‡∏á Label ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡πÅ‡∏™‡∏î‡∏á‡∏™‡∏ñ‡∏≤‡∏ô‡∏∞ (‡∏ó‡∏≥‡∏Ñ‡∏£‡∏±‡πâ‡∏á‡πÄ‡∏î‡∏µ‡∏¢‡∏ß ‡∏≠‡∏¢‡∏π‡πà‡∏ô‡∏≠‡∏Å Toggle)
local StatusLabel = BTab:CreateLabel(
   '<font color="rgb(138, 43, 179)"><b> --- </b></font>'
)

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏™‡∏£‡πâ‡∏≤‡∏á‡πÄ‡∏Ñ‡∏£‡∏∑‡πà‡∏≠‡∏á‡∏°‡∏∑‡∏≠‡∏•‡∏π‡∏Å‡∏≠‡∏°‡πÄ‡∏ó‡πÄ‡∏•‡∏û‡∏≠‡∏£‡πå‡∏ï
local function CreateTeleportCandy()
    if TeleportCandy then return end -- ‡∏õ‡πâ‡∏≠‡∏á‡∏Å‡∏±‡∏ô‡∏Å‡∏≤‡∏£‡∏™‡∏£‡πâ‡∏≤‡∏á‡∏ã‡πâ‡∏≥

    local tool = Instance.new("Tool")
    tool.Name = " üç≠ "
    tool.ToolTip = "[ üç≠ ]  Teleport Tools Warping"
    tool.RequiresHandle = true

    -- Handle ‡∏•‡∏π‡∏Å‡∏≠‡∏°
    local handle = Instance.new("Part")
    handle.Name = "Handle"
    handle.Size = Vector3.new(2.2, 2.2, 2.2)
    handle.Shape = Enum.PartType.Ball
    handle.Material = Enum.Material.ForceField
    handle.Color = Color3.fromRGB(138, 43, 179)
    handle.Transparency = 0.15
    handle.CanCollide = false
    handle.Parent = tool

    -- Sparkles ‡∏°‡πà‡∏ß‡∏á
    local sparkles = Instance.new("Sparkles")
    sparkles.SparkleColor = Color3.fromRGB(200, 100, 255)
    sparkles.Enabled = false
    sparkles.Parent = handle

    -- ParticleEmitter ‡∏°‡πà‡∏ß‡∏á‡∏™‡∏ß‡∏¢‡∏á‡∏≤‡∏°
    local particles = Instance.new("ParticleEmitter")
    particles.Texture = "rbxassetid://241650934"
    particles.Color = ColorSequence.new({
        ColorSequenceKeypoint.new(0, Color3.fromRGB(138, 43, 179)),
        ColorSequenceKeypoint.new(1, Color3.fromRGB(200, 120, 255))
    })
    particles.Transparency = NumberSequence.new({NumberSequenceKeypoint.new(0,0.2), NumberSequenceKeypoint.new(1,1)})
    particles.Size = NumberSequence.new(0.5, 0)
    particles.Lifetime = NumberRange.new(1.2, 2.8)
    particles.Rate = 25
    particles.Speed = NumberRange.new(3, 6)
    particles.SpreadAngle = Vector2.new(60, 60)
    particles.Parent = handle

    -- ‡πÅ‡∏™‡∏á‡∏°‡πà‡∏ß‡∏á‡∏£‡∏≠‡∏ö‡∏ï‡∏±‡∏ß
    local light = Instance.new("PointLight")
    light.Color = Color3.fromRGB(180, 80, 255)
    light.Brightness = 0
    light.Range = 0
    light.Parent = handle

    -- ‡πÄ‡∏™‡∏µ‡∏¢‡∏á‡πÄ‡∏ó‡πÄ‡∏•‡∏û‡∏≠‡∏£‡πå‡∏ï
    local teleportSound = Instance.new("Sound")
    teleportSound.SoundId = "rbxassetid://4861638982"
    teleportSound.Volume = 0.8
    teleportSound.Parent = handle

    -- ‡πÄ‡∏™‡∏µ‡∏¢‡∏á‡∏ï‡∏≠‡∏ô‡∏ñ‡∏∑‡∏≠
    tool.Equipped:Connect(function()
        local equipSound = Instance.new("Sound")
        equipSound.SoundId = "rbxassetid://2865227271"
        equipSound.Volume = 0.6
        equipSound.Parent = Player.Character and Player.Character:FindFirstChild("HumanoidRootPart")
        if equipSound.Parent then
            equipSound:Play()
            game.Debris:AddItem(equipSound, 3)
        end

        sparkles.Enabled = false
        particles.Rate = 60
        light.Brightness = 5
    end)

    tool.Unequipped:Connect(function()
        sparkles.Enabled = true
        particles.Rate = 25
        light.Brightness = 3
    end)

    -- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡πÄ‡∏ó‡πÄ‡∏•‡∏û‡∏≠‡∏£‡πå‡∏ï‡πÄ‡∏°‡∏∑‡πà‡∏≠‡∏Ñ‡∏•‡∏¥‡∏Å
    tool.Activated:Connect(function()
        if not Player.Character or not Player.Character:FindFirstChild("HumanoidRootPart") then return end
        
        teleportSound:Play()
        
        local mouse = Player:GetMouse()
        local targetPos = mouse.Hit.Position + Vector3.new(0, 3.5, 0)
        
        local char = Player.Character
        for _, part in pairs(char:GetDescendants()) do
            if part:IsA("BasePart") and part ~= char.HumanoidRootPart then
                part.Transparency = 0.4
                task.delay(0.25, function()
                    if part and part.Parent then
                        part.Transparency = 0
                    end
                end)
            end
        end
        
        char.HumanoidRootPart.CFrame = CFrame.new(targetPos)
    end)

    TeleportCandy = tool
    tool.Parent = Backpack
end

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏•‡∏ö‡πÄ‡∏Ñ‡∏£‡∏∑‡πà‡∏≠‡∏á‡∏°‡∏∑‡∏≠
local function RemoveTeleportCandy()
    if TeleportCandy and TeleportCandy.Parent then
        TeleportCandy:Destroy()
        TeleportCandy = nil
    end
end

