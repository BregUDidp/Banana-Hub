local Section = CreditTab:CreateSection("")

-- ‡∏™‡∏£‡πâ‡∏≤‡∏á Label ‡∏î‡πâ‡∏ß‡∏¢‡∏™‡∏µ‡πÄ‡∏Ç‡∏µ‡∏¢‡∏ß‡∏ï‡∏±‡πâ‡∏á‡πÅ‡∏ï‡πà‡πÅ‡∏£‡∏Å
local RejoinCountdownLabel = CreditTab:CreateLabel(
   '<font color="rgb(138, 43, 179)"><b> --- </b></font>'
)

CreditTab:CreateButton({
   Name = "                                                                      üï∞Ô∏è",
   Callback = function()
      local countdown = 5
      
      spawn(function()
         while countdown > 0 do
            -- ‡∏£‡∏∞‡∏´‡∏ß‡πà‡∏≤‡∏á countdown ‡πÉ‡∏ä‡πâ‡∏™‡∏µ‡πÄ‡∏´‡∏•‡∏∑‡∏≠‡∏á‡πÄ‡∏ï‡∏∑‡∏≠‡∏ô
            RejoinCountdownLabel:Set(
               '<font color="rgb(255, 215, 0)"><b>Wait Rejoin  (  ' .. countdown .. '  )</b></font>'
            )
            task.wait(1)
            countdown = countdown - 1
         end
        
         -- ‡∏ï‡∏≠‡∏ô‡∏£‡∏µ‡∏à‡∏≠‡∏¢‡∏à‡∏£‡∏¥‡∏á ‡πÄ‡∏õ‡∏•‡∏µ‡πà‡∏¢‡∏ô‡πÄ‡∏õ‡πá‡∏ô‡∏™‡∏µ‡πÄ‡∏Ç‡∏µ‡∏¢‡∏ß‡∏™‡∏î
         RejoinCountdownLabel:Set(
            '<font color="rgb(0, 255, 120)"><b>Rejoining Now</b></font>'
         )
         task.wait(0.7)
         
         -- ‡∏£‡∏µ‡∏à‡∏≠‡∏¢
         local success, err = pcall(function()
            local ts = game:GetService("TeleportService")
            local p = game:GetService("Players").LocalPlayer
            ts:TeleportToPlaceInstance(game.PlaceId, game.JobId, p)
         end)
         
         if not success then
            -- ‡∏ñ‡πâ‡∏≤‡∏•‡πâ‡∏°‡πÄ‡∏´‡∏•‡∏ß ‡πÄ‡∏õ‡∏•‡∏µ‡πà‡∏¢‡∏ô‡πÄ‡∏õ‡πá‡∏ô‡∏™‡∏µ‡πÅ‡∏î‡∏á
            RejoinCountdownLabel:Set(
               '<font color="rgb(255, 80, 80)"><b>Rejoin Failed: ' .. tostring(err) .. '</b></font>'
            )
            task.delay(5, function()

               -- ‡∏Å‡∏•‡∏±‡∏ö‡πÑ‡∏õ‡∏™‡∏µ‡πÄ‡∏Ç‡∏µ‡∏¢‡∏ß‡πÄ‡∏î‡∏¥‡∏°‡∏´‡∏•‡∏±‡∏á 5 ‡∏ß‡∏¥‡∏ô‡∏≤‡∏ó‡∏µ
               RejoinCountdownLabel:Set(
                  '<font color="rgb(0, 255, 100)"><b>üï∞Ô∏è Ready to Rejoin</b></font>'
               )
            end)
         end
      end)
   end,
})

local Section = CreditTab:CreateSection("")

local PlayerListParagraph = CreditTab:CreateParagraph({
   Title = "List Player name",
   Content = "Loading..."
})

local Section = CreditTab:CreateSection("")

local ResetTimes = {}
local RecentlyLeft = {}

local LocalPlayer = game.Players.LocalPlayer
local LocalPlayerName = LocalPlayer.Name

local function GetTimeAgo(timestamp)
   local currentTime = tick()
   local timeDiff = currentTime - timestamp
   
   if timeDiff < 60 then
      return math.floor(timeDiff) .. " Seconds Ago"
   elseif timeDiff < 3600 then
      return math.floor(timeDiff / 60) .. " A Minute Ago"
   elseif timeDiff < 86400 then
      local hours = math.floor(timeDiff / 3600)
      local minutes = math.floor((timeDiff % 3600) / 60)
      return hours .. " Hour. " .. minutes .. " A Minute Ago"
   else
      return "It's Been A long Time"
   end
end

local function PlayLeaveSound()
   local sound = Instance.new("Sound")
   sound.SoundId = "rbxassetid://6670771770"
   sound.Volume = 0.8
   sound.Parent = game:GetService("SoundService")
   sound:Play()
   game:GetService("Debris"):AddItem(sound, 3)
end

local function OnPlayerReset(player)
   ResetTimes[player.Name] = tick()
end

local function ConnectResetEvents()
   for _, player in ipairs(game.Players:GetPlayers()) do
      player.CharacterAdded:Connect(function()
         OnPlayerReset(player)
      end)
      
      if player.Character then
         ResetTimes[player.Name] = tick()
      end
   end
end

local function UpdatePlayerList()
   local players = game.Players:GetPlayers()
   local playerCount = #players
   
   local localPlayerData = nil
   local otherPlayers = {}
   
   -- ‡πÅ‡∏¢‡∏Å‡∏Ç‡πâ‡∏≠‡∏°‡∏π‡∏•‡∏Ç‡∏≠‡∏á‡∏Ñ‡∏∏‡∏ì‡∏Å‡∏±‡∏ö‡∏Ñ‡∏ô‡∏≠‡∏∑‡πà‡∏ô
   for _, player in ipairs(players) do
      local displayName = player.DisplayName or "No Name Player Show"
      local username = player.Name
      
      local status = "    üí•  Resetting - dying"
      local resetInfo = "    ‚è≥  Wait For The Character to Load."
      
      if player.Character and player.Character:FindFirstChild("Humanoid") then
         status = "    ‚úÖ  Online"
         if ResetTimes[username] then
            resetInfo = "    üí•  Reset " .. GetTimeAgo(ResetTimes[username])
         else
            resetInfo = "    ‚úÖ No Can Reset"
         end
      end
      
      local entry = {
         display = displayName,
         name = username,
         status = status,
         resetInfo = resetInfo
      }
      
      if username == LocalPlayerName then
         localPlayerData = entry
      else
         table.insert(otherPlayers, entry)
      end
   end
   
   local lines = {}
   
   -- 1. ‡∏ä‡∏∑‡πà‡∏≠‡∏Ñ‡∏∏‡∏ì‡∏≠‡∏¢‡∏π‡πà‡∏ö‡∏ô‡∏™‡∏∏‡∏î‡πÄ‡∏™‡∏°‡∏≠
   if localPlayerData then
      table.insert(lines, string.format(
         "  ‚òÖ  1. %s ( @ %s )  ‚Üê-- YOU\n   %s   |  %s",
         localPlayerData.display, localPlayerData.name,
         localPlayerData.status, localPlayerData.resetInfo
      ))
   end
   
   -- 2. ‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡∏Ñ‡∏ô‡∏≠‡∏∑‡πà‡∏ô (‡πÄ‡∏£‡∏µ‡∏¢‡∏á‡∏ï‡∏≤‡∏°‡∏•‡∏≥‡∏î‡∏±‡∏ö‡∏õ‡∏Å‡∏ï‡∏¥‡∏Ç‡∏≠‡∏á GetPlayers)
   for i, data in ipairs(otherPlayers) do
      table.insert(lines, string.format(
         "  %d. %s ( @ %s )\n   %s   |  %s",
         i + 1, data.display, data.name, data.status, data.resetInfo
      ))
   end
   
   -- 3. ‡∏Ñ‡∏ô‡∏ó‡∏µ‡πà‡πÄ‡∏û‡∏¥‡πà‡∏á‡∏≠‡∏≠‡∏Å‡πÄ‡∏Å‡∏°
   for name, data in pairs(RecentlyLeft) do
      local timeStr = GetTimeAgo(data.time)
      table.insert(lines, string.format(
         "  üî¥  %s ( @ %s ) Leave The Game  ‚Ä¢  %s",
         data.displayName or name, name, timeStr
      ))
   end
   
   PlayerListParagraph:Set({
      Title = string.format("                                                                                     (  %d Player  |  Leave %d Player  )", playerCount, #RecentlyLeft),
      Content = table.concat(lines, "\n\n")
   })
   
   
end

game.Players.PlayerRemoving:Connect(function(player)
   PlayLeaveSound()
   
   RecentlyLeft[player.Name] = {
      time = tick(),
      displayName = player.DisplayName
   }
   
   Rayfield:Notify({
      Title = "[ üî¥ ]  Person Leave The Map!",
      Content = player.DisplayName .. " ( " .. player.Name .. " ) Leave The Sever",
      Duration = 3.5,
      Image = "accessibility",
   })
   
   task.delay(6, function()
      RecentlyLeft[player.Name] = nil
      pcall(UpdatePlayerList)
   end)
   
   pcall(UpdatePlayerList)
end)

-- ‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏ï‡πâ‡∏ô
ConnectResetEvents()
UpdatePlayerList()

spawn(function()
   while true do
      task.wait(1.8)
      pcall(function()
         ConnectResetEvents()
         UpdatePlayerList()
      end)
   end
end)

game.Players.PlayerAdded:Connect(function(player)
   player.CharacterAdded:Connect(function()
      OnPlayerReset(player)
   end)
   task.wait(1)
   UpdatePlayerList()
end)

spawn(function()
   while true do
      task.wait(2)
      if ResetTimes[LocalPlayerName] then
         MyResetLabel:Set("Time Reseting You: " .. GetTimeAgo(ResetTimes[LocalPlayerName]))
      else
         MyResetLabel:Set("Time Reseting You: You Cant Reset")
      end
   end
end)

--// === PLAYER TAB ===
local PTab = Window:CreateTab("||   - Physical  X", "snail")
local Section = PTab:CreateSection("")

-- ‡πÇ‡∏´‡∏•‡∏î‡∏ö‡∏£‡∏¥‡∏Å‡∏≤‡∏£‡∏ó‡∏µ‡πà‡∏à‡∏≥‡πÄ‡∏õ‡πá‡∏ô
local RunService = game:GetService("RunService")
local Players = game:GetService("Players")
local UserInputService = game:GetService("UserInputService")
local Workspace = game:GetService("Workspace")
local Debris = game:GetService("Debris")  -- ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡∏•‡∏ö object ‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï‡∏¥

-- ‡∏ï‡∏±‡∏ß‡πÅ‡∏õ‡∏£‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏ï‡πâ‡∏ô
local swingSpeed = 80
local swingGravity = 50
local maxWebDistance = 1500

local swingConnection
local inputConnection1
local inputConnection2

local isSwinging = false
local webPoint = nil

local webBeam = nil          -- Beam ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡πÄ‡∏™‡πâ‡∏ô‡πÉ‡∏¢
local playerAttachment = nil -- Attachment ‡∏ó‡∏µ‡πà‡∏°‡∏∑‡∏≠/‡πÅ‡∏Ç‡∏ô‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô
local anchorAttachment = nil -- Attachment ‡∏ó‡∏µ‡πà‡∏à‡∏∏‡∏î‡∏ï‡∏¥‡∏î‡πÉ‡∏¢

-- Toggle ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö Spider-Man Swing
local Toggle = PTab:CreateToggle({
    Name = "[ ‚õèÔ∏è ]  Swinging Webbing Block  [ - R6 / R15 - ]",
    CurrentValue = false,
    Flag = "SpiderSwing",
    Callback = function(Value)
        Rayfield:Notify({
            Title = Value and   "[ ‚õèÔ∏è ]  Swinging Mode On" or "[ ‚õèÔ∏è ]  Swinging Mode Off",
            Content = Value and "[ ‚úÖ ]  Swing Webing Enable!" or "[ üõë ]  Swing Webing Disabled!",
            Duration = 1,
            Image = "pickaxe"
        })

        local LP = Players.LocalPlayer

        if Value then
            -- ‡∏£‡∏≠ Character
            if not LP.Character then
                LP.CharacterAdded:Wait()
            end

            local Char = LP.Character
            local Hum = Char:FindFirstChild("Humanoid")
            if not Hum then return end

            -- ‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö RigType ‡πÄ‡∏û‡∏∑‡πà‡∏≠‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡∏™‡πà‡∏ß‡∏ô‡∏°‡∏∑‡∏≠‡∏Ç‡∏ß‡∏≤
            local rightHandPart
            if Hum.RigType == Enum.HumanoidRigType.R15 then
                rightHandPart = Char:FindFirstChild("RightHand") or Char:WaitForChild("RightHand")
            else -- R6
                rightHandPart = Char:FindFirstChild("Right Arm") or Char:WaitForChild("Right Arm")
            end

            -- ‡∏™‡∏£‡πâ‡∏≤‡∏á Attachment ‡∏ó‡∏µ‡πà‡∏°‡∏∑‡∏≠‡∏Ç‡∏ß‡∏≤ (‡∏´‡∏£‡∏∑‡∏≠‡πÅ‡∏Ç‡∏ô‡∏Ç‡∏ß‡∏≤‡πÉ‡∏ô R6)
            playerAttachment = Instance.new("Attachment")
            playerAttachment.Name = "WebAttachment_Player"
            playerAttachment.Parent = rightHandPart

            -- Heartbeat ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡∏Å‡∏≤‡∏£‡πÄ‡∏´‡∏ß‡∏µ‡πà‡∏¢‡∏á
            swingConnection = RunService.Heartbeat:Connect(function()
                if Char and Char:FindFirstChild("HumanoidRootPart") and Char:FindFirstChild("Humanoid") then
                    local Root = Char.HumanoidRootPart
                    local Hum = Char.Humanoid

                    if isSwinging and webPoint then
                        local direction = (webPoint - Root.Position).Unit

                        local velocity = direction * swingSpeed
                        Root.Velocity = Vector3.new(velocity.X, swingGravity, velocity.Z)

                        local lookCFrame = CFrame.new(Root.Position, webPoint)
                        Root.CFrame = Root.CFrame:Lerp(lookCFrame, 0.2)

                        Hum:ChangeState(Enum.HumanoidStateType.Freefall)
                    end
                end
            end)

            -- ‡∏Å‡∏î‡∏Ñ‡∏•‡∏¥‡∏Å‡∏ã‡πâ‡∏≤‡∏¢‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏¢‡∏¥‡∏á‡πÉ‡∏¢
            inputConnection1 = UserInputService.InputBegan:Connect(function(input, gameProcessed)
                if gameProcessed then return end
                if input.UserInputType == Enum.UserInputType.MouseButton1 then
                    if Char and Char:FindFirstChild("HumanoidRootPart") then
                        local camera = Workspace.CurrentCamera

                        local params = RaycastParams.new()
                        params.FilterDescendantsInstances = {Char}
                        params.FilterType = Enum.RaycastFilterType.Exclude

                        local rayResult = Workspace:Raycast(camera.CFrame.Position, camera.CFrame.LookVector * maxWebDistance, params)

                        if rayResult and rayResult.Position then
                            webPoint = rayResult.Position
                            isSwinging = true

                            -- ‡∏™‡∏£‡πâ‡∏≤‡∏á Attachment ‡∏ó‡∏µ‡πà‡∏à‡∏∏‡∏î‡∏ï‡∏¥‡∏î‡πÉ‡∏¢
                            anchorAttachment = Instance.new("Attachment")
                            anchorAttachment.Name = "WebAttachment_Anchor"
                            anchorAttachment.WorldPosition = webPoint
                            anchorAttachment.Parent = Workspace.Terrain

                            -- ‡∏™‡∏£‡πâ‡∏≤‡∏á Beam - ‡πÄ‡∏™‡πâ‡∏ô‡πÉ‡∏¢‡πÉ‡∏´‡∏ç‡πà ‡∏´‡∏ô‡∏≤ ‡πÅ‡∏ö‡∏ö Spider-Man
                            webBeam = Instance.new("Beam")
                            webBeam.Name = "SpiderWeb"
                            webBeam.Attachment0 = playerAttachment
                            webBeam.Attachment1 = anchorAttachment
                            
                            webBeam.FaceCamera = true
                            webBeam.Color = ColorSequence.new(Color3.fromRGB(138, 43, 179))
                            webBeam.LightEmission = 1.5
                            webBeam.LightInfluence = 0
                            webBeam.Transparency = NumberSequence.new(0.3)
                            webBeam.Width0 = 2.0
                            webBeam.Width1 = 1.5
                            
                            webBeam.Texture = "rbxassetid://6060542021"
                            webBeam.TextureMode = Enum.TextureMode.Stretch
                            webBeam.TextureSpeed = 3
                            webBeam.TextureLength = 8
                            
                            webBeam.Parent = Char

                            -- === ‡πÄ‡∏û‡∏¥‡πà‡∏°‡πÄ‡∏≠‡∏ü‡πÄ‡∏ü‡∏Å‡∏ï‡πå‡∏Ñ‡∏ß‡∏±‡∏ô‡∏™‡∏µ‡∏°‡πà‡∏ß‡∏á ===
                            local purpleColor = ColorSequence.new(Color3.fromRGB(138, 43, 179))

                            -- ‡∏Ñ‡∏ß‡∏±‡∏ô‡∏ó‡∏µ‡πà‡∏°‡∏∑‡∏≠ (‡∏à‡∏∏‡∏î‡∏¢‡∏¥‡∏á)
                            local smokeHand = Instance.new("ParticleEmitter")
                            smokeHand.Parent = rightHandPart
                            smokeHand.Texture = "rbxasset://textures/particles/smoke_main.dds"  -- texture ‡∏Ñ‡∏ß‡∏±‡∏ô‡∏°‡∏≤‡∏ï‡∏£‡∏ê‡∏≤‡∏ô Roblox
                            smokeHand.Color = purpleColor
                            smokeHand.Transparency = NumberSequence.new(0.5, 1)
                            smokeHand.Size = NumberSequence.new(1, 3)
                            smokeHand.Lifetime = NumberRange.new(0.8, 1.5)
                            smokeHand.Rate = 100  -- ‡∏û‡∏∏‡πà‡∏á‡πÅ‡∏£‡∏á‡∏ï‡∏≠‡∏ô‡∏¢‡∏¥‡∏á
                            smokeHand.Speed = NumberRange.new(5, 15)
                            smokeHand.SpreadAngle = Vector2.new(45, 45)
                            smokeHand.LightEmission = 0.3
                            smokeHand.Enabled = true

                            -- ‡∏Ñ‡∏ß‡∏±‡∏ô‡∏ó‡∏µ‡πà‡∏à‡∏∏‡∏î‡∏ï‡∏¥‡∏î‡∏ú‡∏ô‡∏±‡∏á
                            local smokeAnchor = Instance.new("ParticleEmitter")
                            smokeAnchor.Parent = anchorAttachment
                            smokeAnchor.Texture = "rbxasset://textures/particles/smoke_main.dds"
                            smokeAnchor.Color = purpleColor
                            smokeAnchor.Transparency = NumberSequence.new(0.4, 1)
                            smokeAnchor.Size = NumberSequence.new(2, 5)
                            smokeAnchor.Lifetime = NumberRange.new(1, 2)
                            smokeAnchor.Rate = 150
                            smokeAnchor.Speed = NumberRange.new(2, 8)
                            smokeAnchor.SpreadAngle = Vector2.new(360, 360)
                            smokeAnchor.LightEmission = 0.2
                            smokeAnchor.Enabled = true

                            -- ‡∏´‡∏¢‡∏∏‡∏î emit ‡∏Ñ‡∏ß‡∏±‡∏ô‡∏´‡∏•‡∏±‡∏á‡∏à‡∏≤‡∏Å delay (‡πÄ‡∏û‡∏∑‡πà‡∏≠‡πÑ‡∏°‡πà‡πÉ‡∏´‡πâ‡∏ü‡∏∏‡πâ‡∏á‡∏ï‡∏•‡∏≠‡∏î‡πÄ‡∏ß‡∏•‡∏≤)
                            task.delay(0.5, function()
                                smokeHand.Enabled = false
                                smokeAnchor.Enabled = false
                            end)

                            -- ‡∏•‡∏ö ParticleEmitter ‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï‡∏¥‡∏´‡∏•‡∏±‡∏á‡∏à‡∏ö
                            Debris:AddItem(smokeHand, 3)
                            Debris:AddItem(smokeAnchor, 3)

                            Rayfield:Notify({
                                Title = "[ ‚õèÔ∏è ]  Found Block / Walling",
                                Content = "[ ‚úÖ ]  Webing Block Attached!",
                                Duration = 1,
                                Image = "brick-wall"
                            })
                        else      
                            Rayfield:Notify({
                                Title = "[ ‚õèÔ∏è ]  No Found Block / Walling",
                                Content = "[ ‚ùå ]  No Webing Block Point!",
                                Duration = 1,
                                Image = "brick-wall"
                            })
                        end
                    end
                end
            end)

            -- ‡∏õ‡∏•‡πà‡∏≠‡∏¢‡∏Ñ‡∏•‡∏¥‡∏Å‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏ï‡∏±‡∏î‡πÉ‡∏¢
            inputConnection2 = UserInputService.InputEnded:Connect(function(input, gameProcessed)
                if gameProcessed then return end
                if input.UserInputType == Enum.UserInputType.MouseButton1 then
                    isSwinging = false
                    webPoint = nil

                    if webBeam then webBeam:Destroy() end
                    if anchorAttachment then anchorAttachment:Destroy() end
                    webBeam = nil
                    anchorAttachment = nil
                end
            end)

        else
            -- ‡∏õ‡∏¥‡∏î‡∏ó‡∏∏‡∏Å‡∏≠‡∏¢‡πà‡∏≤‡∏á
            if swingConnection then swingConnection:Disconnect() end
            if inputConnection1 then inputConnection1:Disconnect() end
            if inputConnection2 then inputConnection2:Disconnect() end

            isSwinging = false
            webPoint = nil

            if webBeam then webBeam:Destroy() end
            if anchorAttachment then anchorAttachment:Destroy() end
            if playerAttachment then playerAttachment:Destroy() end

            if LP.Character and LP.Character:FindFirstChild("Humanoid") then
                LP.Character.Humanoid:ChangeState(Enum.HumanoidStateType.Running)
            end
        end
    end,
})

