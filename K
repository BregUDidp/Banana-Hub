local Paragraph = PTab:CreateParagraph({Title = "Noclips For Walling And Hack Wall", Content = "Noclips For Walling And Hack Walling"})

-- ‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï‡∏•‡∏π‡∏õ
RS.RenderStepped:Connect(function()
    if flying and vv and gyro and hrp and humanoid then
        local cam = workspace.CurrentCamera
        if not cam then return end
 
        local flatForward = Vector3.new(cam.CFrame.LookVector.X,0,cam.CFrame.LookVector.Z)
        local forward = flatForward.Magnitude > 0 and flatForward.Unit or Vector3.zero
        local right = Vector3.new(cam.CFrame.RightVector.X,0,cam.CFrame.RightVector.Z)
        if right.Magnitude > 0 then right = right.Unit end
 
        local moveVec = Vector3.zero
        if UIS:IsKeyDown(Enum.KeyCode.W) then moveVec = moveVec + Vector3.new(0,0,1) end
        if UIS:IsKeyDown(Enum.KeyCode.S) then moveVec = moveVec + Vector3.new(0,0,-1) end
        if UIS:IsKeyDown(Enum.KeyCode.A) then moveVec = moveVec + Vector3.new(-1,0,0) end
        if UIS:IsKeyDown(Enum.KeyCode.D) then moveVec = moveVec + Vector3.new(1,0,0) end
        if UIS:IsKeyDown(Enum.KeyCode.Space) then moveVec = moveVec + Vector3.new(0,1,0) end
        if UIS:IsKeyDown(Enum.KeyCode.LeftControl) then moveVec = moveVec + Vector3.new(0,-1,0) end
 
        local desired = forward*moveVec.Z + right*moveVec.X + Vector3.new(0,moveVec.Y,0)
        if desired.Magnitude>0 then desired = desired.Unit*flySpeed end
        vv.Velocity = desired
 
        if desired.Magnitude>1 then
            local flatDir = Vector3.new(desired.X,0,desired.Z)
            if flatDir.Magnitude>0 then gyro.CFrame = CFrame.new(hrp.Position,hrp.Position+flatDir) end
        end
    end
end)
 
RS.Stepped:Connect(function()
    if noclipEnabled and char then
        for _,part in pairs(char:GetDescendants()) do
            if part:IsA("BasePart") then part.CanCollide=false end
        end
    end
end)
 
UIS.JumpRequest:Connect(function()
    if infJumpEnabled and humanoid then
        humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
    end
end)

UIS.JumpRequest:Connect(function()
    if infJumpEnabled and humanoid then
        humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
    end
end)
 
-- ‡∏£‡∏µ‡πÄ‡∏ã‡πá‡∏ï‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£‡πÄ‡∏°‡∏∑‡πà‡∏≠ respawn
player.CharacterAdded:Connect(function(c)
    char=c
    hrp=c:WaitForChild("HumanoidRootPart")
    humanoid=c:WaitForChild("Humanoid")
    stopFlying()
    toggleSpeed(false)
    noclipEnabled=false
    infJumpEnabled=false
    Rayfield:LoadConfiguration()
end)

local BTab = Window:CreateTab("||   - Backpack Tool", "backpack")
local Section = BTab:CreateSection("")
local Section = BTab:CreateSection("")

BTab:CreateButton({
    Name = "[ üç≠ ]  Teleport Tool   [ - Sword - ]",
    Callback = function()
        -- ‡∏•‡∏π‡∏Å‡∏≠‡∏°‡∏™‡∏µ‡∏°‡πà‡∏ß‡∏á Sparkles ‡πÄ‡∏ó‡πÄ‡∏•‡∏û‡∏≠‡∏£‡πå‡∏ï (‡πÅ‡∏™‡∏á‡∏°‡πà‡∏ß‡∏á‡∏ó‡∏±‡πâ‡∏á‡∏•‡∏π‡∏Å + Effect)
        local tool = Instance.new("Tool")
        tool.Name = "üç≠"
        tool.ToolTip = "[ üç≠ ]  Teleport Tool"
        tool.RequiresHandle = true

        local player = game.Players.LocalPlayer

        -- ‡∏™‡∏£‡πâ‡∏≤‡∏á Handle ‡∏•‡∏π‡∏Å‡∏≠‡∏°‡∏™‡∏µ‡∏°‡πà‡∏ß‡∏á
        local handle = Instance.new("Part")
        handle.Name = "Handle"
        handle.Size = Vector3.new(2.2, 2.2, 2.2)
        handle.Shape = Enum.PartType.Ball
        handle.Material = Enum.Material.ForceField
        handle.BrickColor = BrickColor.new("Royal purple")  -- ‡∏™‡∏µ‡πÉ‡∏Å‡∏•‡πâ‡πÄ‡∏Ñ‡∏µ‡∏¢‡∏á‡∏ó‡∏µ‡πà‡∏™‡∏∏‡∏î‡πÉ‡∏ô BrickColor
        handle.Color = Color3.fromRGB(138, 43, 179)        -- ‡∏ï‡∏±‡πâ‡∏á‡∏Ñ‡πà‡∏≤‡∏™‡∏µ‡∏à‡∏£‡∏¥‡∏á‡πÉ‡∏´‡πâ‡∏ï‡∏£‡∏á‡∏ï‡∏≤‡∏° RGB
        handle.Transparency = 0.15
        handle.CanCollide = false
        handle.Parent = tool

        -- Sparkles ‡∏™‡∏µ‡∏°‡πà‡∏ß‡∏á
        local sparkles = Instance.new("Sparkles")
        sparkles.SparkleColor = Color3.fromRGB(138, 43, 179)
        sparkles.Enabled = false
        sparkles.Parent = handle

        -- ParticleEmitter ‡∏™‡∏µ‡∏°‡πà‡∏ß‡∏á
        local particles = Instance.new("ParticleEmitter")
        particles.Texture = "rbxassetid://241650934"
        particles.Color = ColorSequence.new(
            Color3.fromRGB(138, 43, 179),
            Color3.fromRGB(180, 100, 200)  -- ‡∏°‡πà‡∏ß‡∏á‡∏≠‡πà‡∏≠‡∏ô‡∏•‡∏á‡∏ô‡∏¥‡∏î‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏Ñ‡∏ß‡∏≤‡∏°‡∏™‡∏ß‡∏¢
        )
        particles.Size = NumberSequence.new(0.4)
        particles.Lifetime = NumberRange.new(1.5, 2.5)
        particles.Rate = 20
        particles.SpreadAngle = Vector2.new(45, 45)
        particles.Speed = NumberRange.new(2)
        particles.Parent = handle

        -- PointLight ‡∏™‡∏µ‡∏°‡πà‡∏ß‡∏á
        local light = Instance.new("PointLight")
        light.Color = Color3.fromRGB(138, 43, 179)
        light.Brightness = 2
        light.Range = 10
        light.Parent = handle

        -- ‡πÄ‡∏™‡∏µ‡∏¢‡∏á‡πÄ‡∏ó‡πÄ‡∏•‡∏û‡∏≠‡∏£‡πå‡∏ï
        local teleportSound = Instance.new("Sound")
        teleportSound.SoundId = "rbxassetid://4861638982"
        teleportSound.Volume = 0.7
        teleportSound.Parent = handle

        -- ‡πÄ‡∏≠‡∏ü‡πÄ‡∏ü‡∏Å‡∏ï‡πå‡∏ï‡∏≠‡∏ô‡∏ñ‡∏∑‡∏≠/‡∏õ‡∏•‡πà‡∏≠‡∏¢
        tool.Equipped:Connect(function()
            sparkles.Enabled = false
            particles.Rate = 50
            light.Brightness = 0
        end)

        tool.Unequipped:Connect(function()
            sparkles.Enabled = false
            particles.Rate = 20
            light.Brightness = 2
        end)

        -- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡πÄ‡∏ó‡πÄ‡∏•‡∏û‡∏≠‡∏£‡πå‡∏ï
        tool.Activated:Connect(function()
            teleportSound:Play()
            local character = player.Character
            if character and character:FindFirstChild("HumanoidRootPart") then
                local mouse = player:GetMouse()
                local pos = mouse.Hit.Position + Vector3.new(0, 3, 0)
                character.HumanoidRootPart.CFrame = CFrame.new(pos)
            end
        end)

        -- ‡πÄ‡∏™‡∏µ‡∏¢‡∏á‡∏ï‡∏≠‡∏ô‡∏ñ‡∏∑‡∏≠‡πÄ‡∏Ñ‡∏£‡∏∑‡πà‡∏≠‡∏á‡∏°‡∏∑‡∏≠ (‡πÄ‡∏û‡∏¥‡πà‡∏°‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏ó‡πà)
        tool.Equipped:Connect(function()
            local equipSound = Instance.new("Sound")
            equipSound.SoundId = "rbxassetid://2865227271"
            equipSound.Volume = 0.5
            equipSound.Parent = player.Character:FindFirstChild("HumanoidRootPart")
            if equipSound.Parent then
                equipSound:Play()
                equipSound.Ended:Connect(function()
                    equipSound:Destroy()
                end)
            end
        end)

        -- ‡πÉ‡∏™‡πà‡∏•‡∏á Backpack
        player:WaitForChild("Backpack")
        tool.Parent = player.Backpack

        Rayfield:Notify({
            Title = "[ üç≠ ]  Teleport Tool",
            Content = "[ üç≠ ]  Finish You Got Teleport Tool  üèÅ",
            Duration = 1,
            Image = "lollipop",
        })
    end
})

local Paragraph = BTab:CreateParagraph({Title = "Teleport Tool For Player / Friend", Content = "Teleport Tool Is Good For Building"})

local Section = BTab:CreateSection("")

BTab:CreateButton({
    Name = "[ üö≤ ]  Recoil Tool  [ - 30 - ]   [ - Recoil - ]",
    Callback = function()
        local player = game.Players.LocalPlayer

        -- ‡∏™‡∏£‡πâ‡∏≤‡∏á Tool
        local tool = Instance.new("Tool")
        tool.Name = "üö≤"
        tool.ToolTip = "[ üö≤ ]  Recoil Speed"
        tool.RequiresHandle = true  -- ‡∏ï‡πâ‡∏≠‡∏á‡∏°‡∏µ Handle ‡πÄ‡∏û‡∏∑‡πà‡∏≠‡πÉ‡∏´‡πâ‡πÄ‡∏´‡πá‡∏ô‡∏•‡∏π‡∏Å‡∏≠‡∏°

        -- ‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß‡πÄ‡∏î‡∏¥‡∏ô‡∏õ‡∏Å‡∏ï‡∏¥ (‡∏™‡πà‡∏ß‡∏ô‡πÉ‡∏´‡∏ç‡πà‡∏Ñ‡∏∑‡∏≠ 18)
        local NormalWalkSpeed = 17

        -- ‡∏™‡∏£‡πâ‡∏≤‡∏á Handle ‡∏•‡∏π‡∏Å‡∏≠‡∏°‡∏™‡∏µ‡∏°‡πà‡∏ß‡∏á
        local handle = Instance.new("Part")
        handle.Name = "Handle"
        handle.Size = Vector3.new(2.2, 2.2, 2.2)
        handle.Shape = Enum.PartType.Ball
        handle.Material = Enum.Material.ForceField
        handle.Color = Color3.fromRGB(138, 43, 179)        -- ‡∏™‡∏µ‡∏°‡πà‡∏ß‡∏á‡∏ï‡∏£‡∏á‡∏ï‡∏≤‡∏° RGB
        handle.Transparency = 0.15
        handle.CanCollide = false
        handle.Parent = tool

        -- Sparkles ‡∏™‡∏µ‡∏°‡πà‡∏ß‡∏á
        local sparkles = Instance.new("Sparkles")
        sparkles.SparkleColor = Color3.fromRGB(138, 43, 179)
        sparkles.Enabled = false
        sparkles.Parent = handle

        -- ParticleEmitter ‡∏™‡∏µ‡∏°‡πà‡∏ß‡∏á
        local particles = Instance.new("ParticleEmitter")
        particles.Texture = "rbxassetid://241650934" -- 241650934 --
        particles.Color = ColorSequence.new(
            Color3.fromRGB(138, 43, 179),
            Color3.fromRGB(180, 100, 200)  -- ‡∏°‡πà‡∏ß‡∏á‡∏≠‡πà‡∏≠‡∏ô‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏Ñ‡∏ß‡∏≤‡∏°‡∏™‡∏ß‡∏¢
        )
        
        particles.Size = NumberSequence.new(0.4)
        particles.Lifetime = NumberRange.new(1.5, 2.5)
        particles.Rate = 20
        particles.SpreadAngle = Vector2.new(45, 45)
        particles.Speed = NumberRange.new(2)
        particles.Parent = handle

        -- PointLight ‡∏™‡∏µ‡∏°‡πà‡∏ß‡∏á
        local light = Instance.new("PointLight")
        light.Color = Color3.fromRGB(138, 43, 179)
        light.Brightness = 2
        light.Range = 10
        light.Parent = handle

        -- ‡πÄ‡∏≠‡∏ü‡πÄ‡∏ü‡∏Å‡∏ï‡πå‡∏ï‡∏≠‡∏ô‡∏ñ‡∏∑‡∏≠‡πÄ‡∏Ñ‡∏£‡∏∑‡πà‡∏≠‡∏á‡∏°‡∏∑‡∏≠
        tool.Equipped:Connect(function()
            if sparkles then sparkles.Enabled = false end
            particles.Rate = 50
            light.Brightness = 0

            -- ‡πÄ‡∏û‡∏¥‡πà‡∏°‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß‡πÄ‡∏î‡∏¥‡∏ô
            if player.Character and player.Character:FindFirstChild("Humanoid") then
                player.Character.Humanoid.WalkSpeed = 30
            end

            -- ‡πÄ‡∏™‡∏µ‡∏¢‡∏á‡∏ï‡∏≠‡∏ô‡∏ñ‡∏∑‡∏≠
            local equipSound = Instance.new("Sound")
            equipSound.SoundId = "rbxassetid://2865227271" -- 2865227271 --
            equipSound.Volume = 0.5
            equipSound.Parent = player.Character and player.Character:FindFirstChild("HumanoidRootPart")
            if equipSound.Parent then
                equipSound:Play()
                equipSound.Ended:Connect(function()
                    equipSound:Destroy()
                end)
            end
        end)

        tool.Unequipped:Connect(function()
            if sparkles then sparkles.Enabled = false end
            particles.Rate = 20
            light.Brightness = 2

            -- ‡∏Å‡∏•‡∏±‡∏ö‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß‡∏õ‡∏Å‡∏ï‡∏¥
            if player.Character and player.Character:FindFirstChild("Humanoid") then
                player.Character.Humanoid.WalkSpeed = NormalWalkSpeed
            end
        end)

        -- ‡πÉ‡∏™‡πà Tool ‡πÄ‡∏Ç‡πâ‡∏≤ Backpack
        player:WaitForChild("Backpack")
        tool.Parent = player.Backpack

        -- ‡πÅ‡∏à‡πâ‡∏á‡πÄ‡∏ï‡∏∑‡∏≠‡∏ô
        Rayfield:Notify({
            Title = "[ üö≤ ]  Recoil Tool",
            Content = "[ üö≤ ]  Finish You Got Recoil Tool  üèÅ",
            Duration = 1,
            Image = "bike",
        })
    end
})

local Paragraph = BTab:CreateParagraph({Title = "Recoil Tool For Player / Friend", Content = "Recoil Tool Is Good For Speed Flash"})

local Section = BTab:CreateSection("")

--// === TELEPORT TAB ===
local friendTab = Window:CreateTab("||   - Trolling Player", "bug-play")
local Section = friendTab:CreateSection("")

local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local player = Players.LocalPlayer

-- ‡∏ï‡∏±‡∏ß‡πÅ‡∏õ‡∏£‡πÄ‡∏Å‡πá‡∏ö‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á‡πÄ‡∏î‡∏¥‡∏°‡∏Å‡πà‡∏≠‡∏ô‡∏ß‡∏≤‡∏õ
local savedCFrame = nil

-- ‡∏ï‡∏±‡∏ß‡πÅ‡∏õ‡∏£‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö Auto Teleport
local autoTPConnection = nil
local autoTargetName = nil

-- ‡∏ï‡∏±‡∏ß‡πÅ‡∏õ‡∏£‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö ESP
local currentESPHighlight = nil
local espTargetPlayer = nil
local espConnection = nil

-- ‡∏ï‡∏±‡∏ß‡πÅ‡∏õ‡∏£‡∏™‡∏µ ESP (‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏ï‡πâ‡∏ô‡πÄ‡∏Ç‡∏µ‡∏¢‡∏ß‡∏≠‡πà‡∏≠‡∏ô)
local espFillColor = Color3.fromRGB(138, 43, 179)
local espOutlineColor = Color3.fromRGB(138, 43, 179) -- ‡∏Ç‡∏≠‡∏ö‡∏Ç‡∏≤‡∏ß‡∏Ñ‡∏á‡∏ó‡∏µ‡πà ‡∏´‡∏£‡∏∑‡∏≠‡∏à‡∏∞‡∏õ‡∏£‡∏±‡∏ö‡πÑ‡∏î‡πâ‡∏î‡πâ‡∏ß‡∏¢

-- ===== ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏ä‡πà‡∏ß‡∏¢ =====
local function getPlayerList()
    local list = {}
    for _, p in ipairs(Players:GetPlayers()) do
        if p ~= player then
            table.insert(list, string.format("%s (%s)", p.DisplayName, p.Name))
        end
    end
    table.sort(list)
    return list
end

local function getPlayerFromSelection(selected)
    if not selected or selected == "" then return nil end
    local friendName = selected:match("%((.-)%)") or selected
    return Players:FindFirstChild(friendName)
end

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡πÄ‡∏õ‡∏¥‡∏î ESP (‡πÉ‡∏ä‡πâ‡∏™‡∏µ‡∏õ‡∏±‡∏à‡∏à‡∏∏‡∏ö‡∏±‡∏ô)
local function enableESP(targetPlayer)
    if not targetPlayer or not targetPlayer.Character then return end
    
    -- ‡∏•‡∏ö Highlight ‡πÄ‡∏Å‡πà‡∏≤‡∏Å‡πà‡∏≠‡∏ô (‡∏Å‡∏£‡∏ì‡∏µ respawn)
    if currentESPHighlight and currentESPHighlight.Parent then
        currentESPHighlight:Destroy()
    end
    local highlight = Instance.new("Highlight")
    highlight.Parent = targetPlayer.Character
    highlight.FillColor = espFillColor
    highlight.FillTransparency = 0.5
    highlight.OutlineColor = espOutlineColor
    highlight.OutlineTransparency = 0
    highlight.DepthMode = Enum.HighlightDepthMode.AlwaysOnTop
    highlight.Enabled = true
    currentESPHighlight = highlight
    espTargetPlayer = targetPlayer
end

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏õ‡∏¥‡∏î ESP
local function disableESP()
    if currentESPHighlight then
        currentESPHighlight:Destroy()
        currentESPHighlight = nil
    end
    if espConnection then
        espConnection:Disconnect()
        espConnection = nil
    end
    espTargetPlayer = nil
end

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï‡∏™‡∏µ ESP (‡πÄ‡∏°‡∏∑‡πà‡∏≠‡πÄ‡∏õ‡∏•‡∏µ‡πà‡∏¢‡∏ô‡∏™‡∏µ‡πÉ‡∏ô Color Picker)
local function updateESPColor()
    if currentESPHighlight and currentESPHighlight.Parent then
        currentESPHighlight.FillColor = espFillColor
        currentESPHighlight.OutlineColor = espOutlineColor
    end
end

local friendDropdown = friendTab:CreateDropdown({
    Name = "[ üë• ]  Dropdown Player Choose Player",
    Options = getPlayerList(),
    CurrentOption = {""},
    Flag = "FriendSelect",
    Callback = function(Option)
        if currentESPHighlight then
            local selected = friendDropdown.CurrentOption[1]
            local newTarget = getPlayerFromSelection(selected)
            disableESP()
            if newTarget then
                enableESP(newTarget)

                
            end
        end
    end
})

-- ‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï Dropdown ‡πÄ‡∏°‡∏∑‡πà‡∏≠‡∏°‡∏µ‡∏Ñ‡∏ô‡πÄ‡∏Ç‡πâ‡∏≤-‡∏≠‡∏≠‡∏Å
Players.PlayerAdded:Connect(function()
    friendDropdown:Refresh(getPlayerList(), true)
end)

Players.PlayerRemoving:Connect(function(p)
    task.wait(1)
    friendDropdown:Refresh(getPlayerList(), true)
   
    if espTargetPlayer and p == espTargetPlayer then
        disableESP()
    end
end)

local Section = friendTab:CreateSection("")

-- ===== ‡∏õ‡∏∏‡πà‡∏°‡∏ß‡∏≤‡∏õ‡∏ò‡∏£‡∏£‡∏°‡∏î‡∏≤ =====
friendTab:CreateButton({
    Name = "[ üìå ]  Teleport To Player - Friend",
    Callback = function()
        local selected = friendDropdown.CurrentOption[1]
        if not selected or selected == "" then return end

        local targetPlayer = getPlayerFromSelection(selected)
        local char = player.Character or player.CharacterAdded:Wait()
        local hrp = char:WaitForChild("HumanoidRootPart", 5)
        if not hrp or not targetPlayer or not targetPlayer.Character then return end
        local targetHRP = targetPlayer.Character:FindFirstChild("HumanoidRootPart")
        if targetHRP then
            savedCFrame = hrp.CFrame
            hrp.CFrame = targetHRP.CFrame + Vector3.new(0, 3, 0)
        end
    end
})

friendTab:CreateButton({
    Name = "[ üìû ]  Teleport To Player   [ - 1 Sec - ]",
    Callback = function()
        local selected = friendDropdown.CurrentOption[1]
        if not selected or selected == "" then
            return
        end

        local friendName = selected:match("%((.-)%)") or selected
        local targetPlayer = Players:FindFirstChild(friendName)
        local char = player.Character or player.CharacterAdded:Wait()
        local hrp = char:WaitForChild("HumanoidRootPart", 5)

        if not hrp then
            return
        end

        if targetPlayer and targetPlayer.Character then
            local targetHRP = targetPlayer.Character:FindFirstChild("HumanoidRootPart")
            if targetHRP then
                -- ‡∏ö‡∏±‡∏ô‡∏ó‡∏∂‡∏Å‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á‡πÄ‡∏î‡∏¥‡∏°
                savedCFrame = hrp.CFrame

                -- ‡∏ß‡∏≤‡∏õ‡πÑ‡∏õ‡∏´‡∏≤‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏ô (‡∏¢‡∏Å‡∏Ç‡∏∂‡πâ‡∏ô‡∏ô‡∏¥‡∏î‡∏´‡∏ô‡πà‡∏≠‡∏¢)
                hrp.CFrame = targetHRP.CFrame + Vector3.new(0, 5, 0)

                -- ‡∏£‡∏≠‡πÄ‡∏ü‡∏£‡∏°‡∏ô‡∏¥‡∏î‡∏ô‡∏∂‡∏á‡πÄ‡∏û‡∏∑‡πà‡∏≠‡πÉ‡∏´‡πâ‡πÄ‡∏´‡πá‡∏ô‡∏Å‡∏≤‡∏£‡∏ß‡∏≤‡∏õ‡∏ä‡∏±‡∏î‡πÄ‡∏à‡∏ô (‡πÑ‡∏°‡πà‡∏Å‡∏£‡∏∞‡∏ï‡∏∏‡∏Å)
                task.wait()

                -- ‡∏Å‡∏•‡∏±‡∏ö‡∏à‡∏∏‡∏î‡πÄ‡∏î‡∏¥‡∏°‡∏ó‡∏±‡∏ô‡∏ó‡∏µ
                if savedCFrame and hrp and hrp.Parent then
                    hrp.CFrame = savedCFrame
                end

                return
            end
        end
    end
})

friendTab:CreateToggle({
    Name = "[ ü™ë ]  Auto Teleport to Player Sit Head",
    CurrentValue = false,
    Flag = "AutoTPToggle",
    Callback = function(Value)
        local selected = friendDropdown.CurrentOption[1]
        if not selected or selected == "" then
           
            Rayfield.Flags["AutoTPToggle"]:Set(false)
            return
        end
        local friendName = selected:match("%((.-)%)") or selected
        local targetPlayer = Players:FindFirstChild(friendName)
        if not targetPlayer then
           
            Rayfield.Flags["AutoTPToggle"]:Set(false)
            return
        end
        -- ‡∏õ‡∏¥‡∏î Auto TP ‡πÄ‡∏Å‡πà‡∏≤
        if autoTPConnection then
            autoTPConnection:Disconnect()
            autoTPConnection = nil
        end
        autoTargetName = nil
        if Value then
            -- ‡∏ö‡∏±‡∏ô‡∏ó‡∏∂‡∏Å‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á‡∏õ‡∏±‡∏à‡∏à‡∏∏‡∏ö‡∏±‡∏ô (‡∏Å‡πà‡∏≠‡∏ô‡πÄ‡∏£‡∏¥‡πà‡∏° Auto TP)
            local char = player.Character or player.CharacterAdded:Wait()
            local hrp = char:WaitForChild("HumanoidRootPart", 5)
            if hrp then
                savedCFrame = hrp.CFrame
            end
            autoTargetName = friendName
            -- ‡∏™‡∏£‡πâ‡∏≤‡∏á Connection ‡∏´‡∏•‡∏±‡∏Å‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö Auto Teleport
            autoTPConnection = RunService.Heartbeat:Connect(function()
                if not autoTargetName then return end
                local target = Players:FindFirstChild(autoTargetName)
                if not target or not target.Character then return end
                local targetHRP = target.Character:FindFirstChild("HumanoidRootPart")
                if not targetHRP then return end
                local myChar = player.Character
                if not myChar then return end
                local myHRP = myChar:FindFirstChild("HumanoidRootPart")
                if not myHRP then return end
                -- ‡∏ß‡∏≤‡∏õ‡πÑ‡∏õ‡∏´‡∏≤‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢ (‡∏™‡∏π‡∏á‡∏Ç‡∏∂‡πâ‡∏ô‡∏ô‡∏¥‡∏î‡∏´‡∏ô‡πà‡∏≠‡∏¢‡πÄ‡∏û‡∏∑‡πà‡∏≠‡πÑ‡∏°‡πà‡∏ï‡∏¥‡∏î‡∏û‡∏∑‡πâ‡∏ô)
                myHRP.CFrame = targetHRP.CFrame + Vector3.new(0, 4, 0)
            end)
           
        else
            -- ‡∏õ‡∏¥‡∏î Auto TP
            autoTargetName = nil
           
        end
    end
})

-- ‡πÄ‡∏û‡∏¥‡πà‡∏°: ‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï Auto TP ‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï‡∏¥‡πÄ‡∏°‡∏∑‡πà‡∏≠‡πÄ‡∏õ‡∏•‡∏µ‡πà‡∏¢‡∏ô‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡πÉ‡∏ô Dropdown (‡∏Ç‡∏ì‡∏∞‡πÄ‡∏õ‡∏¥‡∏î Toggle ‡∏≠‡∏¢‡∏π‡πà)
friendDropdown.Callback = function(selected)
    local currentToggleValue = Rayfield.Flags["AutoTPToggle"].CurrentValue
    -- ‡∏ñ‡πâ‡∏≤ Auto TP ‡∏Å‡∏≥‡∏•‡∏±‡∏á‡πÄ‡∏õ‡∏¥‡∏î‡∏≠‡∏¢‡∏π‡πà ‡πÉ‡∏´‡πâ‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢‡πÉ‡∏´‡∏°‡πà‡∏ó‡∏±‡∏ô‡∏ó‡∏µ
    if currentToggleValue then
        Rayfield.Flags["AutoTPToggle"]:Set(false) -- ‡∏õ‡∏¥‡∏î‡∏ä‡∏±‡πà‡∏ß‡∏Ñ‡∏£‡∏≤‡∏ß
        task.wait(0.1)
        Rayfield.Flags["AutoTPToggle"]:Set(true) -- ‡πÄ‡∏õ‡∏¥‡∏î‡πÉ‡∏´‡∏°‡πà ‚Üí ‡∏à‡∏∞‡πÉ‡∏ä‡πâ‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢‡πÉ‡∏´‡∏°‡πà
    end
    -- ‡∏ñ‡πâ‡∏≤‡∏°‡∏µ ESP ‡πÄ‡∏õ‡∏¥‡∏î‡∏≠‡∏¢‡∏π‡πà‡∏Å‡∏±‡∏ö‡∏Ñ‡∏ô‡πÄ‡∏Å‡πà‡∏≤ ‡∏Å‡πá‡∏≠‡∏≤‡∏à‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï‡πÑ‡∏î‡πâ‡∏ó‡∏µ‡πà‡∏ô‡∏µ‡πà‡πÄ‡∏´‡∏°‡∏∑‡∏≠‡∏ô‡∏Å‡∏±‡∏ô (‡πÅ‡∏ï‡πà‡∏ï‡∏≠‡∏ô‡∏ô‡∏µ‡πâ‡πÅ‡∏¢‡∏Å‡∏õ‡∏∏‡πà‡∏°)
end

local Paragraph = friendTab:CreateParagraph({Title = "Teleport To Player / Friend Normal / Auto", Content = "Teleport To Player / Friend Normal / Become Auto"})

local Section = friendTab:CreateSection("")

-- ‡∏ï‡∏±‡∏ß‡πÅ‡∏õ‡∏£‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö Hitbox ‡πÄ‡∏â‡∏û‡∏≤‡∏∞‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢
local TargetHitboxEnabled = false
local TargetHitboxSize = 20
local TargetHitboxConnection = nil
local CurrentTargetPlayer = nil  -- ‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡∏ó‡∏µ‡πà‡∏ñ‡∏π‡∏Å‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡∏õ‡∏±‡∏à‡∏à‡∏∏‡∏ö‡∏±‡∏ô

-- ‡∏™‡∏µ‡πÄ‡∏Ç‡∏µ‡∏¢‡∏ß‡πÄ‡∏î‡∏µ‡∏¢‡∏ß‡∏Å‡∏±‡∏ö ESP ‡∏Ç‡∏≠‡∏á‡∏Ñ‡∏∏‡∏ì
local TargetHitboxColor = Color3.fromRGB(138, 43, 179)

friendTab:CreateToggle({
    Name = "[ ‚öîÔ∏è ]  Hitbox Boxing Enable Target",
    CurrentValue = false,
    Flag = "TargetHitboxToggle",
    Callback = function(Value)
        TargetHitboxEnabled = Value
        
        -- ‡∏î‡∏∂‡∏á‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡∏ó‡∏µ‡πà‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡∏à‡∏≤‡∏Å Dropdown
        local selected = friendDropdown.CurrentOption[1]
        local targetPlayer = getPlayerFromSelection(selected)
        
        if not targetPlayer then
            Rayfield.Flags["TargetHitboxToggle"]:Set(false)
            
            return
        end
        
        CurrentTargetPlayer = targetPlayer
        
        if Value then
            -- ‡πÄ‡∏õ‡∏¥‡∏î Hitbox ‡πÄ‡∏â‡∏û‡∏≤‡∏∞‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢
            local function applyHitbox(char)
                if not char then return end
                pcall(function()
                    local hrp = char:FindFirstChild("HumanoidRootPart")
                    if hrp then
                        hrp.Size = Vector3.new(TargetHitboxSize, TargetHitboxSize, TargetHitboxSize)
                        hrp.Transparency = 0.7
                        hrp.Color = TargetHitboxColor
                        hrp.Material = "Neon"
                        hrp.CanCollide = false
                    end
                end)
            end
            
            -- ‡πÉ‡∏ä‡πâ RenderStepped ‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï‡∏ï‡πà‡∏≠‡πÄ‡∏ô‡∏∑‡πà‡∏≠‡∏á (‡∏Å‡∏£‡∏ì‡∏µ‡πÄ‡∏Ñ‡∏•‡∏∑‡πà‡∏≠‡∏ô‡∏ó‡∏µ‡πà‡∏´‡∏£‡∏∑‡∏≠‡πÄ‡∏õ‡∏•‡∏µ‡πà‡∏¢‡∏ô‡∏™‡πà‡∏ß‡∏ô)
            TargetHitboxConnection = RunService.RenderStepped:Connect(function()
                if not TargetHitboxEnabled or not CurrentTargetPlayer or CurrentTargetPlayer ~= targetPlayer then return end
                if CurrentTargetPlayer.Character then
                    applyHitbox(CurrentTargetPlayer.Character)
                end
            end)
            
            -- ‡πÉ‡∏ä‡πâ‡∏ï‡∏≠‡∏ô‡∏ô‡∏µ‡πâ‡πÄ‡∏•‡∏¢
            if targetPlayer.Character then
                applyHitbox(targetPlayer.Character)
            end
            
            -- ‡∏£‡∏≠‡∏á‡∏£‡∏±‡∏ö respawn
            targetPlayer.CharacterAdded:Connect(function(newChar)
                if TargetHitboxEnabled and CurrentTargetPlayer == targetPlayer then
                    task.wait(0.5)  -- ‡∏£‡∏≠‡πÉ‡∏´‡πâ‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£‡πÇ‡∏´‡∏•‡∏î‡πÄ‡∏™‡∏£‡πá‡∏à
                    applyHitbox(newChar)
                end
            end)
            
            
        else
            -- ‡∏õ‡∏¥‡∏î Hitbox
            if TargetHitboxConnection then
                TargetHitboxConnection:Disconnect()
                TargetHitboxConnection = nil
            end
            
            -- Reset Hitbox ‡∏Ç‡∏≠‡∏á‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢‡∏Å‡∏•‡∏±‡∏ö‡∏Ñ‡πà‡∏≤‡πÄ‡∏î‡∏¥‡∏°
            if targetPlayer.Character then
                pcall(function()
                    local hrp = targetPlayer.Character:FindFirstChild("HumanoidRootPart")
                    if hrp then
                        hrp.Size = Vector3.new(2, 2, 1)
                        hrp.Transparency = 1
                        hrp.Material = "Plastic"
                        hrp.CanCollide = false
                    end
                end)
            end
            
            CurrentTargetPlayer = nil
            
           
        end
    end,
})

friendTab:CreateSlider({
    Name = "[ üìè ]  Slider Hitbox Player Size",
    Range = {20, 5000},
    Increment = 1,
    Suffix = " Hitbox Size",
    CurrentValue = 20,
    Flag = "TargetHitboxSize",
    Callback = function(Value)
        TargetHitboxSize = Value
        
        -- ‡∏ñ‡πâ‡∏≤‡πÄ‡∏õ‡∏¥‡∏î‡∏≠‡∏¢‡∏π‡πà ‡πÉ‡∏´‡πâ‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï‡∏Ç‡∏ô‡∏≤‡∏î‡∏ó‡∏±‡∏ô‡∏ó‡∏µ
        if TargetHitboxEnabled and CurrentTargetPlayer and CurrentTargetPlayer.Character then
            pcall(function()
                local hrp = CurrentTargetPlayer.Character:FindFirstChild("HumanoidRootPart")
                if hrp then
                    hrp.Size = Vector3.new(TargetHitboxSize, TargetHitboxSize, TargetHitboxSize)
                end
            end)
        end
    end,
})

-- ‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï‡∏¥‡πÄ‡∏°‡∏∑‡πà‡∏≠‡πÄ‡∏õ‡∏•‡∏µ‡πà‡∏¢‡∏ô‡∏Ñ‡∏ô‡πÉ‡∏ô Dropdown (‡∏ñ‡πâ‡∏≤ Toggle ‡πÄ‡∏õ‡∏¥‡∏î‡∏≠‡∏¢‡∏π‡πà)
friendDropdown.Callback = function(selected)
    local currentToggleValue = Rayfield.Flags["TargetHitboxToggle"].CurrentValue
    
    -- ‡∏ñ‡πâ‡∏≤ Toggle ‡πÄ‡∏õ‡∏¥‡∏î‡∏≠‡∏¢‡∏π‡πà ‡πÉ‡∏´‡πâ‡∏õ‡∏¥‡∏î‡∏Å‡πà‡∏≠‡∏ô‡πÅ‡∏•‡πâ‡∏ß‡πÄ‡∏õ‡∏¥‡∏î‡πÉ‡∏´‡∏°‡πà‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢
    if currentToggleValue then
        Rayfield.Flags["TargetHitboxToggle"]:Set(false)
        task.wait(0.1)
        Rayfield.Flags["TargetHitboxToggle"]:Set(true)
    end
    
    -- ‡∏™‡πà‡∏ß‡∏ô‡πÄ‡∏î‡∏¥‡∏°‡∏Ç‡∏≠‡∏á‡∏Ñ‡∏∏‡∏ì (‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö Auto TP ‡πÅ‡∏•‡∏∞ ESP)
    local currentAutoTPValue = Rayfield.Flags["AutoTPToggle"].CurrentValue
    if currentAutoTPValue then
        Rayfield.Flags["AutoTPToggle"]:Set(false)
        task.wait(0.1)
        Rayfield.Flags["AutoTPToggle"]:Set(true)
    end
end

local Paragraph = friendTab:CreateParagraph({Title = "Hitbox Boxing For Kill Player And Reset", Content = "Hitbox Boxing For Kill Player And Reseting"})

local Section = friendTab:CreateSection("")

-- ===== ‡∏ï‡∏±‡∏ß‡πÅ‡∏õ‡∏£‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö ESP ‡∏ó‡∏±‡πâ‡∏á‡∏™‡∏≠‡∏á‡πÅ‡∏ö‡∏ö =====
local currentESPHighlight = nil -- ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö Highlight ‡∏ó‡∏±‡πâ‡∏á‡∏Ñ‡∏π‡πà
local currentESPDrawings = {} -- ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö Drawing (Box + Name) ‡πÄ‡∏â‡∏û‡∏≤‡∏∞ ESP ‡∏õ‡∏Å‡∏ï‡∏¥
local espTargetPlayer = nil
local espConnection = nil

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏™‡∏£‡πâ‡∏≤‡∏á/‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï Highlight
local function enableHighlight(targetPlayer)
    if not targetPlayer or not targetPlayer.Character then return end
   
    -- ‡∏•‡∏ö Highlight ‡πÄ‡∏Å‡πà‡∏≤
    if currentESPHighlight and currentESPHighlight.Parent then
        currentESPHighlight:Destroy()
    end
   
    local highlight = Instance.new("Highlight")
    highlight.Parent = targetPlayer.Character
    highlight.FillColor = espFillColor
    highlight.FillTransparency = 0.5
    highlight.OutlineColor = espOutlineColor
    highlight.OutlineTransparency = 0
    highlight.DepthMode = Enum.HighlightDepthMode.AlwaysOnTop
    highlight.Enabled = true
   
    currentESPHighlight = highlight
    espTargetPlayer = targetPlayer
end

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏™‡∏£‡πâ‡∏≤‡∏á Drawing ESP (Box + Name + Health)
local function enableDrawingESP(targetPlayer)
    if not targetPlayer or not targetPlayer.Character then return end
   
    -- ‡∏•‡∏ö Drawing ‡πÄ‡∏Å‡πà‡∏≤
    for _, drawing in pairs(currentESPDrawings) do
        if drawing then drawing:Remove() end
    end
    currentESPDrawings = {}
   
    local box = Drawing.new("Square")
    box.Thickness = 2
    box.Filled = false
    box.Color = espFillColor
    box.Visible = false
   
    local nameText = Drawing.new("Text")
    nameText.Size = 16
    nameText.Center = true
    nameText.Outline = true
    nameText.Color = Color3.fromRGB(138, 43, 179)
    nameText.Visible = false
   
    currentESPDrawings = {Box = box, Name = nameText}
   
    -- ‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï‡∏ó‡∏∏‡∏Å‡πÄ‡∏ü‡∏£‡∏°
    if espConnection then espConnection:Disconnect() end
    espConnection = RunService.RenderStepped:Connect(function()
        if not targetPlayer.Character or not targetPlayer.Character:FindFirstChild("HumanoidRootPart") or not targetPlayer.Character:FindFirstChild("Head") or not targetPlayer.Character:FindFirstChild("Humanoid") then
            box.Visible = false
            nameText.Visible = false
            return
        end
       
        local root = targetPlayer.Character.HumanoidRootPart
        local head = targetPlayer.Character.Head
        local humanoid = targetPlayer.Character.Humanoid
       
        local screenPos, onScreen = workspace.CurrentCamera:WorldToViewportPoint(root.Position)
       
        if onScreen then
            local top = workspace.CurrentCamera:WorldToViewportPoint(head.Position + Vector3.new(0, 3, 0))
            local bottom = workspace.CurrentCamera:WorldToViewportPoint(head.Position - Vector3.new(0, 3, 0))
            local sizeY = (top - bottom).Magnitude
           
            box.Size = Vector2.new(sizeY * 2, sizeY * 3.5)
            box.Position = Vector2.new(screenPos.X - box.Size.X / 2, screenPos.Y - box.Size.Y / 2)
            box.Visible = true
           
            nameText.Text = targetPlayer.DisplayName .. " [ " .. math.floor(humanoid.Health) .. " ]"
            nameText.Position = Vector2.new(screenPos.X, screenPos.Y - box.Size.Y / 2 - 20)
            nameText.Visible = true
        else
            box.Visible = false
            nameText.Visible = false
        end
    end)
end

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏õ‡∏¥‡∏î‡∏ó‡∏∏‡∏Å‡∏≠‡∏¢‡πà‡∏≤‡∏á
local function disableAllESP()
    if currentESPHighlight then
        currentESPHighlight:Destroy()
        currentESPHighlight = nil
    end
    for _, drawing in pairs(currentESPDrawings) do
        if drawing then drawing:Remove() end
    end
    currentESPDrawings = {}
    if espConnection then
        espConnection:Disconnect()
        espConnection = nil
    end
    espTargetPlayer = nil
end

-- ‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï‡∏™‡∏µ‡πÄ‡∏°‡∏∑‡πà‡∏≠‡πÄ‡∏õ‡∏•‡∏µ‡πà‡∏¢‡∏ô Color Picker
local function updateESPColor()
    if currentESPHighlight then
        currentESPHighlight.FillColor = espFillColor
        currentESPHighlight.OutlineColor = espOutlineColor
    end
    if currentESPDrawings.Box then
        currentESPDrawings.Box.Color = espFillColor
    end
end

-- ‡πÅ‡∏Å‡πâ Color Picker ‡πÉ‡∏´‡πâ‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï‡∏ó‡∏±‡πâ‡∏á‡∏™‡∏≠‡∏á‡πÅ‡∏ö‡∏ö
friendTab:CreateColorPicker({
    Name = "[ üé® ]  Choose Color ESP Player",
    Color = Color3.fromRGB(138, 43, 179),
    Flag = "ESPColorPicker",
    Callback = function(Value)
        espFillColor = Value
        updateESPColor()
    end
})

-- ===== ‡∏õ‡∏∏‡πà‡∏° ESP ‡∏õ‡∏Å‡∏ï‡∏¥ (Highlight + Box + Name) =====
friendTab:CreateButton({
    Name = "[ üëÅÔ∏è ]  ESP Box Dropdown Player",
    Callback = function()
        local selected = friendDropdown.CurrentOption[1]
        local targetPlayer = getPlayerFromSelection(selected)
       
        if not targetPlayer then
            disableAllESP()
           
            return
        end
       
        -- ‡∏ñ‡πâ‡∏≤‡πÄ‡∏õ‡∏¥‡∏î‡∏≠‡∏¢‡∏π‡πà‡πÅ‡∏•‡πâ‡∏ß‡∏Å‡∏±‡∏ö‡∏Ñ‡∏ô‡πÄ‡∏î‡∏µ‡∏¢‡∏ß‡∏Å‡∏±‡∏ô ‚Üí ‡∏õ‡∏¥‡∏î
        if espTargetPlayer == targetPlayer and currentESPHighlight and next(currentESPDrawings) then
            disableAllESP()
            return
        end
       
        -- ‡∏õ‡∏¥‡∏î‡πÄ‡∏Å‡πà‡∏≤ ‡πÅ‡∏•‡πâ‡∏ß‡πÄ‡∏õ‡∏¥‡∏î‡πÉ‡∏´‡∏°‡πà
        disableAllESP()
        enableHighlight(targetPlayer)
        enableDrawingESP(targetPlayer)
       
        -- ‡∏ï‡∏¥‡∏î‡∏ï‡∏≤‡∏° respawn
        targetPlayer.CharacterAdded:Connect(function()
            if espTargetPlayer == targetPlayer then
                task.wait(0.5)
                enableHighlight(targetPlayer)
                enableDrawingESP(targetPlayer)
            end
        end)
    end
})

-- ===== ‡∏õ‡∏∏‡πà‡∏° ESP Highlight ‡πÄ‡∏â‡∏û‡∏≤‡∏∞ =====
friendTab:CreateButton({
    Name = "[ üí° ]  ESP Highlight Dropdown Player",
    Callback = function()
        local selected = friendDropdown.CurrentOption[1]
        local targetPlayer = getPlayerFromSelection(selected)
       
        if not targetPlayer then
            disableAllESP()
           
            return
        end
       
        -- ‡∏ñ‡πâ‡∏≤‡πÄ‡∏õ‡∏¥‡∏î Highlight ‡∏≠‡∏¢‡∏π‡πà‡∏Ñ‡∏ô‡πÄ‡∏î‡∏µ‡∏¢‡∏ß‡∏Å‡∏±‡∏ô ‚Üí ‡∏õ‡∏¥‡∏î
        if espTargetPlayer == targetPlayer and currentESPHighlight and not next(currentESPDrawings) then
            disableAllESP()
            return
        end
       
        -- ‡∏õ‡∏¥‡∏î‡πÄ‡∏Å‡πà‡∏≤ ‡πÅ‡∏•‡πâ‡∏ß‡πÄ‡∏õ‡∏¥‡∏î Highlight ‡πÄ‡∏â‡∏û‡∏≤‡∏∞
        disableAllESP()
        enableHighlight(targetPlayer)
       
        -- ‡∏ï‡∏¥‡∏î‡∏ï‡∏≤‡∏° respawn (‡πÄ‡∏â‡∏û‡∏≤‡∏∞ Highlight)
        targetPlayer.CharacterAdded:Connect(function()
            if espTargetPlayer == targetPlayer and not next(currentESPDrawings) then
                task.wait(0.5)
                enableHighlight(targetPlayer)
            end
        end)
    end
})

game:GetService("RunService").RenderStepped:Connect(UpdateESP)

local Paragraph = friendTab:CreateParagraph({Title = "ESP To Player / Friend Normal", Content = "ESP To Player / Friend Become Normal"})

local Section = friendTab:CreateSection("")

-- ===== ‡∏õ‡∏∏‡πà‡∏°‡πÉ‡∏´‡∏°‡πà: ‡∏Å‡∏•‡∏±‡∏ö‡πÑ‡∏õ‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á‡πÄ‡∏î‡∏¥‡∏° =====
friendTab:CreateButton({
    Name = "[ üîô ]  Return To Original Position",
    Callback = function()
        if not savedCFrame then
            -- ‡∏ñ‡πâ‡∏≤‡∏¢‡∏±‡∏á‡πÑ‡∏°‡πà‡∏°‡∏µ‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á‡∏ó‡∏µ‡πà‡∏ö‡∏±‡∏ô‡∏ó‡∏∂‡∏Å‡πÑ‡∏ß‡πâ (‡∏¢‡∏±‡∏á‡πÑ‡∏°‡πà‡πÄ‡∏Ñ‡∏¢‡∏ß‡∏≤‡∏£‡πå‡∏õ)
            warn("No saved position to return to!")
            return
        end

        local char = player.Character or player.CharacterAdded:Wait()
        local hrp = char:FindFirstChild("HumanoidRootPart")
        
        if not hrp then
            warn("HumanoidRootPart not found!")
            return
        end

        -- ‡∏ß‡∏≤‡∏£‡πå‡∏õ‡∏Å‡∏•‡∏±‡∏ö‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á‡πÄ‡∏î‡∏¥‡∏°‡∏≠‡∏¢‡πà‡∏≤‡∏á‡∏£‡∏ß‡∏î‡πÄ‡∏£‡πá‡∏ß‡πÅ‡∏•‡∏∞‡∏ô‡∏∏‡πà‡∏°‡∏ô‡∏ß‡∏•
        hrp.CFrame = savedCFrame
    end
})

