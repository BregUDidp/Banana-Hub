local espSection = espTab:CreateSection("")

-- Variables
local HitboxEnabled = false
local OutlineColor = Color3.fromRGB(43, 179, 123)
local OutlineTransparency = 0
local FillTransparency = 0.7
local HitboxSize = 20
local HitboxTransparency = 0.3

-- Hitbox Functions
local function ApplyHitbox(player)
    if player == game.Players.LocalPlayer then return end
    local char = player.Character
    if not char then return end
    
    local head = char:FindFirstChild("Head")
    if head then
        head.Size = Vector3.new(HitboxSize, HitboxSize, HitboxSize)
        head.Material = Enum.Material.Neon
        head.Color = OutlineColor
        head.Transparency = HitboxTransparency
        head.CanCollide = false
    end
    
    local hrp = char:FindFirstChild("HumanoidRootPart")
    if hrp then
        hrp.Size = Vector3.new(HitboxSize/2, HitboxSize/2, 2)
        hrp.Material = Enum.Material.Neon
        hrp.Color = OutlineColor
        hrp.Transparency = HitboxTransparency
        hrp.CanCollide = false
    end
end

local function ResetHitbox(player)
    if player == game.Players.LocalPlayer then return end
    local char = player.Character
    if not char then return end
    
    local head = char:FindFirstChild("Head")
    if head then
        head.Size = Vector3.new(2, 1, 1)
        head.Material = Enum.Material.SmoothPlastic
        head.Color = Color3.fromRGB(163, 162, 165)
        head.Transparency = 0
        head.CanCollide = true
    end
    
    local hrp = char:FindFirstChild("HumanoidRootPart")
    if hrp then
        hrp.Size = Vector3.new(2, 2, 1)
        hrp.Material = Enum.Material.SmoothPlastic
        hrp.Transparency = 1
        hrp.CanCollide = false
    end
end

-- Toggle ‡πÄ‡∏î‡∏¥‡∏° (‡∏¢‡∏±‡∏á‡∏Ñ‡∏á‡πÑ‡∏ß‡πâ‡πÄ‡∏´‡∏°‡∏∑‡∏≠‡∏ô‡πÄ‡∏î‡∏¥‡∏°)
espTab:CreateToggle({
    Name = "[ ‚öîÔ∏è ]  Hitbox Boxing Player",
    CurrentValue = false,
    Flag = "HitboxToggle",
    Callback = function(Value)
        HitboxEnabled = Value
        if Value then
            spawn(function()
                while HitboxEnabled do
                    for _, player in pairs(game.Players:GetPlayers()) do
                        if player ~= game.Players.LocalPlayer and player.Character then
                            ApplyHitbox(player)
                        end
                    end
                    task.wait()
                end
            end)
            game.Players.PlayerAdded:Connect(function(plr)
                plr.CharacterAdded:Connect(function() ApplyHitbox(plr) end)
            end)
        else
            for _, player in pairs(game.Players:GetPlayers()) do
                ResetHitbox(player)
            end
        end
    end,
})

espTab:CreateKeybind({
    Name = "[ ‚å®Ô∏è ]  Hitbox Player Keybind",
    CurrentKeybind = "[ 1 ]",
    HoldToInteract = false,
    Flag = "HitboxKeybind",
    Callback = function()
        HitboxEnabled = not HitboxEnabled
        
        if HitboxEnabled then
            -- ‡πÄ‡∏õ‡∏¥‡∏î Hitbox
            spawn(function()
                while HitboxEnabled do
                    for _, player in pairs(game.Players:GetPlayers()) do
                        if player ~= game.Players.LocalPlayer and player.Character then
                            ApplyHitbox(player)
                        end
                    end
                    task.wait()
                end
            end)
            game.Players.PlayerAdded:Connect(function(plr)
                plr.CharacterAdded:Connect(function() ApplyHitbox(plr) end)
            end)
        else
            -- ‡∏õ‡∏¥‡∏î Hitbox
            for _, player in pairs(game.Players:GetPlayers()) do
                ResetHitbox(player)
            end
        end
        
        -- ‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï Toggle ‡πÉ‡∏ô UI ‡πÉ‡∏´‡πâ‡∏ï‡∏£‡∏á‡∏Å‡∏±‡∏ö‡∏™‡∏ñ‡∏≤‡∏ô‡∏∞‡∏à‡∏£‡∏¥‡∏á (‡∏ñ‡πâ‡∏≤‡∏ï‡πâ‡∏≠‡∏á‡∏Å‡∏≤‡∏£)
        RayfieldLibrary.Flags["HitboxToggle"]:Set(HitboxEnabled)
    end
})

-- Slider ‡πÄ‡∏î‡∏¥‡∏° (‡∏¢‡∏±‡∏á‡∏Ñ‡∏á‡πÑ‡∏ß‡πâ)
espTab:CreateSlider({
    Name = "[ üìè ]  Slider Hitbox Size",
    Range = {20, 2000},
    Increment = 1,
    Suffix = "Hitbox Size",
    CurrentValue = 20,
    Flag = "HitboxSize",
    Callback = function(Value)
        HitboxSize = Value
        if HitboxEnabled then
            for _, player in pairs(game.Players:GetPlayers()) do
                ApplyHitbox(player)
            end
        end
    end,
})

local Paragraph = espTab:CreateParagraph({Title = "Hitbox Player / Friend Sword", Content = "Hitbox Player / Friend Sword Are Muti Damage"})

local espSection = espTab:CreateSection("")

local ESPEnabled = false
local Drawings = {}
local Highlights = {}

-- ‡∏™‡∏£‡πâ‡∏≤‡∏á Drawing ESP (Box + Name)
local function CreateDrawingESP(player)
    if player == game.Players.LocalPlayer then return end
    
    local Box = Drawing.new("Square")
    Box.Thickness = 2
    Box.Filled = false
    Box.Color = Color3.fromRGB(43, 179, 123)
    Box.Visible = false

    local Name = Drawing.new("Text")
    Name.Size = 16
    Name.Center = true
    Name.Outline = true
    Name.Color = Color3.fromRGB(43, 179, 123)
    Name.Visible = false

    Drawings[player] = {Box = Box, Name = Name}
end

-- ‡∏™‡∏£‡πâ‡∏≤‡∏á Highlight ‡∏ö‡∏ô‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£
local function CreateHighlight(player)
    if player == game.Players.LocalPlayer then return end
    if player.Character then
        local highlight = Instance.new("Highlight")
        highlight.Parent = player.Character
        highlight.FillColor = Color3.fromRGB(43, 179, 123)
        highlight.OutlineColor = Color3.fromRGB(43, 179, 123)
        highlight.FillTransparency = 0.4
        highlight.OutlineTransparency = 0
        highlight.Enabled = false
        
        Highlights[player] = highlight
    end
end

-- ‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï Drawing ESP ‡∏ó‡∏∏‡∏Å‡πÄ‡∏ü‡∏£‡∏°
local function UpdateESP()
    if not ESPEnabled then return end
    
    for player, draws in pairs(Drawings) do
        if player.Character and player.Character:FindFirstChild("HumanoidRootPart") and player.Character:FindFirstChild("Head") and player.Character:FindFirstChild("Humanoid") then
            local root = player.Character.HumanoidRootPart
            local head = player.Character.Head
            local humanoid = player.Character.Humanoid

            local screenPos, onScreen = workspace.CurrentCamera:WorldToViewportPoint(root.Position)
            
            if onScreen then
                -- ‡∏Ñ‡∏≥‡∏ô‡∏ß‡∏ì‡∏Ç‡∏ô‡∏≤‡∏î Box
                local topPos = workspace.CurrentCamera:WorldToViewportPoint(head.Position + Vector3.new(0, 3, 0))
                local bottomPos = workspace.CurrentCamera:WorldToViewportPoint(head.Position - Vector3.new(0, 3, 0))
                local size = (topPos - bottomPos).Magnitude

                draws.Box.Size = Vector2.new(size * 2, size * 3.5)
                draws.Box.Position = Vector2.new(screenPos.X - draws.Box.Size.X / 2, screenPos.Y - draws.Box.Size.Y / 2)
                draws.Box.Visible = true

                -- ‡∏ä‡∏∑‡πà‡∏≠ + ‡πÄ‡∏•‡∏∑‡∏≠‡∏î
                draws.Name.Text = player.Name .. "  [ " .. math.floor(humanoid.Health) .. " ]"
                draws.Name.Position = Vector2.new(screenPos.X, screenPos.Y - draws.Box.Size.Y / 2 - 20)
                draws.Name.Visible = true
            else
                draws.Box.Visible = false
                draws.Name.Visible = false
            end
        else
            draws.Box.Visible = false
            draws.Name.Visible = false
        end
    end
end

-- Toggle ‡∏´‡∏•‡∏±‡∏Å (‡∏£‡∏ß‡∏° ESP + Highlight)
espTab:CreateToggle({
    Name = "[ üëÅÔ∏è ]  ESP Hitbox Player [ 43, 179, 123 ]  All",
    CurrentValue = false,
    Callback = function(Value)
        ESPEnabled = Value

        if Value then
            -- ‡∏™‡∏£‡πâ‡∏≤‡∏á ESP ‡πÅ‡∏•‡∏∞ Highlight ‡πÉ‡∏´‡πâ‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡∏ó‡∏±‡πâ‡∏á‡∏´‡∏°‡∏î
            for _, player in pairs(game.Players:GetPlayers()) do
                CreateDrawingESP(player)
                CreateHighlight(player)
                
                -- ‡∏£‡∏≠‡∏á‡∏£‡∏±‡∏ö‡∏Å‡∏≤‡∏£ respawn
                player.CharacterAdded:Connect(function()
                    task.wait(0.5)
                    CreateDrawingESP(player)
                    CreateHighlight(player)
                end)
            end

            -- ‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡πÉ‡∏´‡∏°‡πà‡πÄ‡∏Ç‡πâ‡∏≤‡∏°‡∏≤
            game.Players.PlayerAdded:Connect(function(player)
                player.CharacterAdded:Connect(function()
                    task.wait(0.5)
                    CreateDrawingESP(player)
                    CreateHighlight(player)
                end)
            end)

            -- ‡πÄ‡∏õ‡∏¥‡∏î Highlight ‡∏ó‡∏±‡πâ‡∏á‡∏´‡∏°‡∏î
            for _, highlight in pairs(Highlights) do
                if highlight and highlight.Parent then
                    highlight.Enabled = true
                end
            end

        else
            -- ‡∏õ‡∏¥‡∏î‡πÅ‡∏•‡∏∞‡∏•‡∏ö Drawing
            for _, draws in pairs(Drawings) do
                draws.Box:Remove()
                draws.Name:Remove()
            end
            Drawings = {}

            -- ‡∏õ‡∏¥‡∏î‡πÅ‡∏•‡∏∞‡∏•‡∏ö Highlight
            for _, highlight in pairs(Highlights) do
                if highlight and highlight.Parent then
                    highlight.Enabled = false
                    highlight:Destroy()
                end
            end
            Highlights = {}
        end
    end,
})

-- ‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï‡∏ó‡∏∏‡∏Å‡πÄ‡∏ü‡∏£‡∏°
game:GetService("RunService").RenderStepped:Connect(UpdateESP)

local ESPEnabled = false
local HighlightEnabled = false
local Drawings = {}
local Highlights = {}  -- ‡πÄ‡∏Å‡πá‡∏ö Highlight object

-- ‡∏™‡∏£‡πâ‡∏≤‡∏á Highlight
local function CreateHighlight(player)
    if player == game.Players.LocalPlayer then return end
    if player.Character then
        local highlight = Instance.new("Highlight")
        highlight.Parent = player.Character
        highlight.FillColor = Color3.fromRGB(43, 179, 123)
        highlight.OutlineColor = Color3.fromRGB(43, 179, 123)
        highlight.FillTransparency = 0.5
        highlight.OutlineTransparency = 0
        highlight.Enabled = false
        
        Highlights[player] = highlight
    end
end

-- Toggle Highlight
espTab:CreateToggle({
    Name = "[ üí° ]  ESP Highligh Player [ Highligh ]  All",
    CurrentValue = false,
    Callback = function(Value)
        HighlightEnabled = Value
        
        if Value then
            -- ‡∏™‡∏£‡πâ‡∏≤‡∏á Highlight ‡πÉ‡∏´‡πâ‡∏ó‡∏∏‡∏Å‡∏Ñ‡∏ô‡∏ó‡∏µ‡πà‡∏≠‡∏¢‡∏π‡πà‡πÉ‡∏ô‡πÄ‡∏Å‡∏°
            for _, player in pairs(game.Players:GetPlayers()) do
                CreateHighlight(player)
                player.CharacterAdded:Connect(function()
                    task.wait(0.5)  -- ‡∏£‡∏≠‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£‡πÇ‡∏´‡∏•‡∏î
                    CreateHighlight(player)
                end)
            end
            
            game.Players.PlayerAdded:Connect(function(player)
                player.CharacterAdded:Connect(function()
                    task.wait(0.5)
                    CreateHighlight(player)
                end)
            end)
        else
            -- ‡∏õ‡∏¥‡∏î‡πÅ‡∏•‡∏∞‡∏•‡∏ö Highlight ‡∏ó‡∏±‡πâ‡∏á‡∏´‡∏°‡∏î
            for _, highlight in pairs(Highlights) do
                if highlight and highlight.Parent then
                    highlight.Enabled = false
                    highlight:Destroy()
                end
            end
            Highlights = {}
        end
        
        -- ‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï‡∏™‡∏ñ‡∏≤‡∏ô‡∏∞ Enabled
        for _, highlight in pairs(Highlights) do
            if highlight and highlight.Parent then
                highlight.Enabled = Value
            end
        end
    end,
})

-- ‡∏™‡∏£‡πâ‡∏≤‡∏á ESP Drawing ‡πÄ‡∏°‡∏∑‡πà‡∏≠‡∏°‡∏µ‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡πÉ‡∏´‡∏°‡πà
for _, player in pairs(game.Players:GetPlayers()) do
    CreateDrawingESP(player)
    player.CharacterAdded:Connect(function()
        task.wait(0.5)
        CreateDrawingESP(player)
    end)
end

game.Players.PlayerAdded:Connect(function(player)
    player.CharacterAdded:Connect(function()
        task.wait(0.5)
        CreateDrawingESP(player)
    end)
end)

