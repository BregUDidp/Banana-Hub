local Paragraph = PTab:CreateParagraph({Title = "Noclips For Walling And Hack Wall", Content = "Noclips For Walling And Hack Walling"})

local BTab = Window:CreateTab("||   - Backpack Tool", "backpack")
local Section = BTab:CreateSection("")

local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local Debris = game:GetService("Debris")
local UserInputService = game:GetService("UserInputService")

local player = Players.LocalPlayer
local mouse = player:GetMouse()

local laserEnabled = false
local shooting = false
local leftEyeAtt, rightEyeAtt
local leftBeam, rightBeam
local targetAttLeft, targetAttRight
local leftParticle, rightParticle
local leftSmoke, rightSmoke
local leftEyeGlow, rightEyeGlow
local laserSound

local laserColor = Color3.fromRGB(138, 43, 179)
local darkPurple = Color3.fromRGB(100, 20, 160)
local brightPurple = Color3.fromRGB(180, 80, 255)

-- ‡∏ï‡∏±‡πâ‡∏á‡∏Ñ‡πà‡∏≤ Recoil & Damage
local headShake = 0.08          -- ‡∏Å‡∏≤‡∏£‡∏™‡∏±‡πà‡∏ô‡∏Ç‡∏≠‡∏á‡∏´‡∏±‡∏ß (recoil)
local damagePerTick = 0         -- ‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏™‡∏µ‡∏¢‡∏´‡∏≤‡∏¢‡∏ï‡πà‡∏≠‡∏Ñ‡∏£‡∏±‡πâ‡∏á
local damageInterval = 0.1      -- ‡∏ó‡∏∏‡∏Å 0.1 ‡∏ß‡∏¥‡∏ô‡∏≤‡∏ó‡∏µ
local lastDamageTick = 0

local function createAttachments()
    local character = player.Character or player.CharacterAdded:Wait()
    local head = character:WaitForChild("Head")
    
    leftEyeAtt = Instance.new("Attachment")
    leftEyeAtt.Name = "LeftEyeAttachment"
    leftEyeAtt.Position = Vector3.new(-0.3, 0.2, -0.5)
    leftEyeAtt.Parent = head
    
    rightEyeAtt = Instance.new("Attachment")
    rightEyeAtt.Name = "RightEyeAttachment"
    rightEyeAtt.Position = Vector3.new(0.3, 0.2, -0.5)
    rightEyeAtt.Parent = head
    
    targetAttLeft = Instance.new("Attachment")
    targetAttLeft.Parent = workspace.Terrain
    
    targetAttRight = Instance.new("Attachment")
    targetAttRight.Parent = workspace.Terrain

    -- ‡πÄ‡∏™‡∏µ‡∏¢‡∏á‡πÄ‡∏•‡πÄ‡∏ã‡∏≠‡∏£‡πå
    laserSound = Instance.new("Sound")
    laserSound.Name = "PurpleLaserSound"
    laserSound.SoundId = "rbxassetid://138087185"
    laserSound.Volume = 0.8
    laserSound.Looped = true
    laserSound.Parent = head
    laserSound:Stop()
end

local function createBeams()
    leftBeam = Instance.new("Beam")
    leftBeam.Attachment0 = leftEyeAtt
    leftBeam.Attachment1 = targetAttLeft
    leftBeam.Color = ColorSequence.new(laserColor)
    leftBeam.Width0 = 2.0
    leftBeam.Width1 = 2.0
    leftBeam.Texture = "rbxassetid://6060542021"
    leftBeam.TextureMode = Enum.TextureMode.Stretch
    leftBeam.TextureSpeed = 3
    leftBeam.LightEmission = 1
    leftBeam.Brightness = 8
    leftBeam.FaceCamera = true
    leftBeam.Segments = 20
    leftBeam.Enabled = false
    leftBeam.Parent = workspace
    
    rightBeam = leftBeam:Clone()
    rightBeam.Attachment0 = rightEyeAtt
    rightBeam.Attachment1 = targetAttRight
    rightBeam.Parent = workspace
end

local function createParticles()
    leftParticle = Instance.new("ParticleEmitter")
    leftParticle.Parent = leftEyeAtt
    leftParticle.Texture = "rbxasset://textures/particles/fire_main.dds"
    leftParticle.Color = ColorSequence.new({ColorSequenceKeypoint.new(0, darkPurple), ColorSequenceKeypoint.new(0.5, laserColor), ColorSequenceKeypoint.new(1, brightPurple)})
    leftParticle.LightEmission = 0.8
    leftParticle.Brightness = 8
    leftParticle.Size = NumberSequence.new({NumberSequenceKeypoint.new(0, 0.9), NumberSequenceKeypoint.new(0.5, 1.5), NumberSequenceKeypoint.new(1, 0)})
    leftParticle.Transparency = NumberSequence.new(0.3, 1)
    leftParticle.Lifetime = NumberRange.new(0.5, 0.9)
    leftParticle.Rate = 90
    leftParticle.Speed = NumberRange.new(15, 25)
    leftParticle.Enabled = false
    
    rightParticle = leftParticle:Clone()
    rightParticle.Parent = rightEyeAtt

    leftSmoke = Instance.new("ParticleEmitter")
    leftSmoke.Parent = leftEyeAtt
    leftSmoke.Texture = "rbxasset://textures/particles/smoke_main.dds"
    leftSmoke.Color = ColorSequence.new({ColorSequenceKeypoint.new(0, darkPurple), ColorSequenceKeypoint.new(1, Color3.fromRGB(140, 60, 200))})
    leftSmoke.LightEmission = 0.3
    leftSmoke.Brightness = 3
    leftSmoke.Size = NumberSequence.new(0.5, 1.3)
    leftSmoke.Transparency = NumberSequence.new(0.6, 1)
    leftSmoke.Lifetime = NumberRange.new(1.5, 2.8)
    leftSmoke.Rate = 18
    leftSmoke.Enabled = false
    
    rightSmoke = leftSmoke:Clone()
    rightSmoke.Parent = rightEyeAtt

    leftEyeGlow = Instance.new("ParticleEmitter")
    leftEyeGlow.Parent = leftEyeAtt
    leftEyeGlow.Texture = "rbxasset://textures/particles/fire_main.dds"
    leftEyeGlow.Color = ColorSequence.new(laserColor)
    leftEyeGlow.LightEmission = 0.9
    leftEyeGlow.Brightness = 7
    leftEyeGlow.Size = NumberSequence.new(0.4, 0.7)
    leftEyeGlow.Transparency = NumberSequence.new(0.4, 0.8)
    leftEyeGlow.Lifetime = NumberRange.new(0.3, 0.6)
    leftEyeGlow.Rate = 35
    leftEyeGlow.Speed = NumberRange.new(0, 3)
    leftEyeGlow.Enabled = false
    
    rightEyeGlow = leftEyeGlow:Clone()
    rightEyeGlow.Parent = rightEyeAtt
end

local function applyRecoil()
    -- ‡∏™‡∏±‡πà‡∏ô‡∏´‡∏±‡∏ß‡πÄ‡∏•‡πá‡∏Å‡∏ô‡πâ‡∏≠‡∏¢ (‡πÑ‡∏°‡πà‡∏°‡∏µ‡∏Å‡∏•‡πâ‡∏≠‡∏á)
    if player.Character and player.Character:FindFirstChild("Head") then
        local head = player.Character.Head
        local shake = CFrame.new(
            math.random(-headShake, headShake),
            math.random(-headShake, headShake),
            math.random(-headShake, headShake)
        )
        head.CFrame = head.CFrame * shake
    end
end

local function createImpactEffects(position)
    local att = Instance.new("Attachment")
    att.WorldPosition = position
    att.Parent = workspace.Terrain
    
    local fire = Instance.new("ParticleEmitter")
    fire.Parent = att
    fire.Texture = "rbxasset://textures/particles/fire_main.dds"
    fire.Color = ColorSequence.new({ColorSequenceKeypoint.new(0, darkPurple), ColorSequenceKeypoint.new(0.4, laserColor), ColorSequenceKeypoint.new(1, brightPurple)})
    fire.LightEmission = 0.9
    fire.Brightness = 10
    fire.Size = NumberSequence.new(1.8, 0)
    fire.Transparency = NumberSequence.new(0.2, 1)
    fire.Lifetime = NumberRange.new(1.0, 1.8)
    fire.Rate = 120
    fire.Speed = NumberRange.new(8, 18)
    fire.Acceleration = Vector3.new(0, 12, 0)
    fire.Enabled = true
    
    local smoke = Instance.new("ParticleEmitter")
    smoke.Parent = att
    smoke.Texture = "rbxasset://textures/particles/smoke_main.dds"
    smoke.Color = ColorSequence.new(darkPurple, Color3.fromRGB(140, 60, 200))
    smoke.Size = NumberSequence.new(1.5, 3.0)
    smoke.Transparency = NumberSequence.new(0.7, 1)
    smoke.Lifetime = NumberRange.new(2.0, 4.0)
    smoke.Rate = 50
    smoke.Enabled = true
    
    Debris:AddItem(att, 3)
end

local function updateLaser()
    if not laserEnabled or not shooting or not player.Character then return end
    local head = player.Character:FindFirstChild("Head")
    if not head then return end
    
    local originLeft = leftEyeAtt.WorldPosition
    local originRight = rightEyeAtt.WorldPosition
    local direction = (mouse.Hit.Position - head.Position).Unit * 500
    
    local rayParams = RaycastParams.new()
    rayParams.FilterDescendantsInstances = {player.Character}
    rayParams.FilterType = Enum.RaycastFilterType.Blacklist
    
    -- ‡∏ï‡∏≤‡∏ã‡πâ‡∏≤‡∏¢
    local resultLeft = workspace:Raycast(originLeft, direction, rayParams)
    if resultLeft then
        targetAttLeft.WorldPosition = resultLeft.Position
        createImpactEffects(resultLeft.Position)
        
        local humanoid = resultLeft.Instance:FindFirstAncestorOfClass("Model") and resultLeft.Instance:FindFirstAncestorOfClass("Model"):FindFirstChildOfClass("Humanoid")
        if humanoid and humanoid.Parent ~= player.Character then
            local now = tick()
            if now - lastDamageTick >= damageInterval then
                humanoid:TakeDamage(damagePerTick)
                lastDamageTick = now
            end
        end
    else
        targetAttLeft.WorldPosition = originLeft + direction
    end
    
    -- ‡∏ï‡∏≤‡∏Ç‡∏ß‡∏≤
    local resultRight = workspace:Raycast(originRight, direction, rayParams)
    if resultRight then
        targetAttRight.WorldPosition = resultRight.Position
        createImpactEffects(resultRight.Position)
        
        local humanoid = resultRight.Instance:FindFirstAncestorOfClass("Model") and resultLeft.Instance:FindFirstAncestorOfClass("Model"):FindFirstChildOfClass("Humanoid")
        if humanoid and humanoid.Parent ~= player.Character then
            local now = tick()
            if now - lastDamageTick >= damageInterval then
                humanoid:TakeDamage(damagePerTick)
                lastDamageTick = now
            end
        end
    else
        targetAttRight.WorldPosition = originRight + direction
    end
    
    applyRecoil()
end

-- ‡∏Ñ‡∏•‡∏¥‡∏Å‡∏ã‡πâ‡∏≤‡∏¢ = ‡∏¢‡∏¥‡∏á
UserInputService.InputBegan:Connect(function(input)
    if laserEnabled and input.UserInputType == Enum.UserInputType.MouseButton1 then
        shooting = true
        leftBeam.Enabled = true
        rightBeam.Enabled = true
        leftParticle.Enabled = true
        rightParticle.Enabled = true
        leftSmoke.Enabled = true
        rightSmoke.Enabled = true
        leftEyeGlow.Enabled = true
        rightEyeGlow.Enabled = true
        if laserSound then laserSound:Play() end
    end
end)

UserInputService.InputEnded:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        shooting = false
        leftBeam.Enabled = false
        rightBeam.Enabled = false
        leftParticle.Enabled = false
        rightParticle.Enabled = false
        leftSmoke.Enabled = false
        rightSmoke.Enabled = false
        leftEyeGlow.Enabled = false
        rightEyeGlow.Enabled = false
        if laserSound then laserSound:Stop() end
    end
end)

RunService.RenderStepped:Connect(updateLaser)

local function enableLaser()
    if player.Character then
        createAttachments()
        createBeams()
        createParticles()
    end
    
    laserEnabled = true
    
    Rayfield:Notify({
               Title = "[ üé• ]  Beamming Laser Player!",
               Content = "[ ‚úÖ ]  Beamming Laser On!",
               Duration = 1,
               Image = "telescope",
             })
end

local function disableLaser()
    laserEnabled = false
    shooting = false
    
    if leftBeam then leftBeam:Destroy() end
    if rightBeam then rightBeam:Destroy() end
    if leftEyeAtt then leftEyeAtt:Destroy() end
    if rightEyeAtt then rightEyeAtt:Destroy() end
    if targetAttLeft then targetAttLeft:Destroy() end
    if targetAttRight then targetAttRight:Destroy() end
    if leftParticle then leftParticle:Destroy() end
    if rightParticle then rightParticle:Destroy() end
    if leftSmoke then leftSmoke:Destroy() end
    if rightSmoke then rightSmoke:Destroy() end
    if leftEyeGlow then leftEyeGlow:Destroy() end
    if rightEyeGlow then rightEyeGlow:Destroy() end
    if laserSound then laserSound:Stop(); laserSound:Destroy() end
    
    Rayfield:Notify({
               Title = "[ üé• ]  Beamming Laser Player!",
               Content = "[ üõë ]  Beamming Laser Off!",
               Duration = 1,
               Image = "telescope",
             })
end

BTab:CreateButton({
    Name = "[ üé• ]  Beamming Laser Eyes   [ - Beamming Eyes - ]",
    Callback = function()
        if laserEnabled then
            disableLaser()
        else
            enableLaser()
        end
    end
})

player.CharacterAdded:Connect(function()
    if laserEnabled then
        disableLaser()
        task.wait(1)
        enableLaser()
    end
end)

local Paragraph = BTab:CreateParagraph({Title = "Laser Beamming Eyes For Laging Player", Content = "Laser Beamming Eyes For Laging Player And Style"})

local Section = BTab:CreateSection("")

BTab:CreateButton({
    Name = "[ üç≠ ]  Teleport Tool   [ - Sword - ]",
    Callback = function()
        -- ‡∏•‡∏π‡∏Å‡∏≠‡∏°‡∏™‡∏µ‡∏°‡πà‡∏ß‡∏á Sparkles ‡πÄ‡∏ó‡πÄ‡∏•‡∏û‡∏≠‡∏£‡πå‡∏ï (‡πÅ‡∏™‡∏á‡∏°‡πà‡∏ß‡∏á‡∏ó‡∏±‡πâ‡∏á‡∏•‡∏π‡∏Å + Effect)
        local tool = Instance.new("Tool")
        tool.Name = "üç≠"
        tool.ToolTip = "[ üç≠ ]  Teleport Tool"
        tool.RequiresHandle = true

        local player = game.Players.LocalPlayer

        -- ‡∏™‡∏£‡πâ‡∏≤‡∏á Handle ‡∏•‡∏π‡∏Å‡∏≠‡∏°‡∏™‡∏µ‡∏°‡πà‡∏ß‡∏á
        local handle = Instance.new("Part")
        handle.Name = "Handle"
        handle.Size = Vector3.new(2.2, 2.2, 2.2)
        handle.Shape = Enum.PartType.Ball
        handle.Material = Enum.Material.ForceField
        handle.BrickColor = BrickColor.new("Royal purple")  -- ‡∏™‡∏µ‡πÉ‡∏Å‡∏•‡πâ‡πÄ‡∏Ñ‡∏µ‡∏¢‡∏á‡∏ó‡∏µ‡πà‡∏™‡∏∏‡∏î‡πÉ‡∏ô BrickColor
        handle.Color = Color3.fromRGB(138, 43, 179)        -- ‡∏ï‡∏±‡πâ‡∏á‡∏Ñ‡πà‡∏≤‡∏™‡∏µ‡∏à‡∏£‡∏¥‡∏á‡πÉ‡∏´‡πâ‡∏ï‡∏£‡∏á‡∏ï‡∏≤‡∏° RGB
        handle.Transparency = 0.15
        handle.CanCollide = false
        handle.Parent = tool

        -- Sparkles ‡∏™‡∏µ‡∏°‡πà‡∏ß‡∏á
        local sparkles = Instance.new("Sparkles")
        sparkles.SparkleColor = Color3.fromRGB(138, 43, 179)
        sparkles.Enabled = false
        sparkles.Parent = handle

        -- ParticleEmitter ‡∏™‡∏µ‡∏°‡πà‡∏ß‡∏á
        local particles = Instance.new("ParticleEmitter")
        particles.Texture = "rbxassetid://241650934"
        particles.Color = ColorSequence.new(
            Color3.fromRGB(138, 43, 179),
            Color3.fromRGB(180, 100, 200)  -- ‡∏°‡πà‡∏ß‡∏á‡∏≠‡πà‡∏≠‡∏ô‡∏•‡∏á‡∏ô‡∏¥‡∏î‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏Ñ‡∏ß‡∏≤‡∏°‡∏™‡∏ß‡∏¢
        )

        particles.Size = NumberSequence.new(0.4)
        particles.Lifetime = NumberRange.new(1.5, 2.5)
        particles.Rate = 20
        particles.SpreadAngle = Vector2.new(45, 45)
        particles.Speed = NumberRange.new(2)
        particles.Parent = handle

        -- PointLight ‡∏™‡∏µ‡∏°‡πà‡∏ß‡∏á
        local light = Instance.new("PointLight")
        light.Color = Color3.fromRGB(138, 43, 179)
        light.Brightness = 2
        light.Range = 10
        light.Parent = handle

        -- ‡πÄ‡∏™‡∏µ‡∏¢‡∏á‡πÄ‡∏ó‡πÄ‡∏•‡∏û‡∏≠‡∏£‡πå‡∏ï
        local teleportSound = Instance.new("Sound")
        teleportSound.SoundId = "rbxassetid://4861638982"
        teleportSound.Volume = 0.7
        teleportSound.Parent = handle

        -- ‡πÄ‡∏≠‡∏ü‡πÄ‡∏ü‡∏Å‡∏ï‡πå‡∏ï‡∏≠‡∏ô‡∏ñ‡∏∑‡∏≠/‡∏õ‡∏•‡πà‡∏≠‡∏¢
        tool.Equipped:Connect(function()
            sparkles.Enabled = false
            particles.Rate = 50
            light.Brightness = 0
        end)

        tool.Unequipped:Connect(function()
            sparkles.Enabled = false
            particles.Rate = 20
            light.Brightness = 2
        end)

        -- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡πÄ‡∏ó‡πÄ‡∏•‡∏û‡∏≠‡∏£‡πå‡∏ï
        tool.Activated:Connect(function()
            teleportSound:Play()
            local character = player.Character
            if character and character:FindFirstChild("HumanoidRootPart") then
                local mouse = player:GetMouse()
                local pos = mouse.Hit.Position + Vector3.new(0, 3, 0)
                character.HumanoidRootPart.CFrame = CFrame.new(pos)
            end
        end)

        -- ‡πÄ‡∏™‡∏µ‡∏¢‡∏á‡∏ï‡∏≠‡∏ô‡∏ñ‡∏∑‡∏≠‡πÄ‡∏Ñ‡∏£‡∏∑‡πà‡∏≠‡∏á‡∏°‡∏∑‡∏≠ (‡πÄ‡∏û‡∏¥‡πà‡∏°‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏ó‡πà)
        tool.Equipped:Connect(function()
            local equipSound = Instance.new("Sound")
            equipSound.SoundId = "rbxassetid://2865227271"
            equipSound.Volume = 0.5
            equipSound.Parent = player.Character:FindFirstChild("HumanoidRootPart")
            if equipSound.Parent then
                equipSound:Play()
                equipSound.Ended:Connect(function()
                    equipSound:Destroy()
                end)
            end
        end)

        -- ‡πÉ‡∏™‡πà‡∏•‡∏á Backpack
        player:WaitForChild("Backpack")
        tool.Parent = player.Backpack

        Rayfield:Notify({
            Title = "[ üç≠ ]  Teleport Tool",
            Content = "[ üç≠ ]  Finish You Got Teleport Tool  üèÅ",
            Duration = 1,
            Image = "lollipop",
        })
    end
})

local Paragraph = BTab:CreateParagraph({Title = "Teleport Tool For Player / Friend", Content = "Teleport Tool Is Good For Building"})

local Section = BTab:CreateSection("")

BTab:CreateButton({
    Name = "[ üö≤ ]  Recoil Tool  [ - 30 - ]   [ - Recoil - ]",
    Callback = function()
        local player = game.Players.LocalPlayer

        -- ‡∏™‡∏£‡πâ‡∏≤‡∏á Tool
        local tool = Instance.new("Tool")
        tool.Name = "üö≤"
        tool.ToolTip = "[ üö≤ ]  Recoil Speed"
        tool.RequiresHandle = true  -- ‡∏ï‡πâ‡∏≠‡∏á‡∏°‡∏µ Handle ‡πÄ‡∏û‡∏∑‡πà‡∏≠‡πÉ‡∏´‡πâ‡πÄ‡∏´‡πá‡∏ô‡∏•‡∏π‡∏Å‡∏≠‡∏°

        -- ‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß‡πÄ‡∏î‡∏¥‡∏ô‡∏õ‡∏Å‡∏ï‡∏¥ (‡∏™‡πà‡∏ß‡∏ô‡πÉ‡∏´‡∏ç‡πà‡∏Ñ‡∏∑‡∏≠ 18)
        local NormalWalkSpeed = 17

        -- ‡∏™‡∏£‡πâ‡∏≤‡∏á Handle ‡∏•‡∏π‡∏Å‡∏≠‡∏°‡∏™‡∏µ‡∏°‡πà‡∏ß‡∏á
        local handle = Instance.new("Part")
        handle.Name = "Handle"
        handle.Size = Vector3.new(2.2, 2.2, 2.2)
        handle.Shape = Enum.PartType.Ball
        handle.Material = Enum.Material.ForceField
        handle.Color = Color3.fromRGB(138, 43, 179)        -- ‡∏™‡∏µ‡∏°‡πà‡∏ß‡∏á‡∏ï‡∏£‡∏á‡∏ï‡∏≤‡∏° RGB
        handle.Transparency = 0.15
        handle.CanCollide = false
        handle.Parent = tool

        -- Sparkles ‡∏™‡∏µ‡∏°‡πà‡∏ß‡∏á
        local sparkles = Instance.new("Sparkles")
        sparkles.SparkleColor = Color3.fromRGB(138, 43, 179)
        sparkles.Enabled = false
        sparkles.Parent = handle

        -- ParticleEmitter ‡∏™‡∏µ‡∏°‡πà‡∏ß‡∏á
        local particles = Instance.new("ParticleEmitter")
        particles.Texture = "rbxassetid://241650934" -- 241650934 --
        particles.Color = ColorSequence.new(
            Color3.fromRGB(138, 43, 179),
            Color3.fromRGB(180, 100, 200)  -- ‡∏°‡πà‡∏ß‡∏á‡∏≠‡πà‡∏≠‡∏ô‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏Ñ‡∏ß‡∏≤‡∏°‡∏™‡∏ß‡∏¢
        )
        
        particles.Size = NumberSequence.new(0.4)
        particles.Lifetime = NumberRange.new(1.5, 2.5)
        particles.Rate = 20
        particles.SpreadAngle = Vector2.new(45, 45)
        particles.Speed = NumberRange.new(2)
        particles.Parent = handle

        -- PointLight ‡∏™‡∏µ‡∏°‡πà‡∏ß‡∏á
        local light = Instance.new("PointLight")
        light.Color = Color3.fromRGB(138, 43, 179)
        light.Brightness = 2
        light.Range = 10
        light.Parent = handle

        -- ‡πÄ‡∏≠‡∏ü‡πÄ‡∏ü‡∏Å‡∏ï‡πå‡∏ï‡∏≠‡∏ô‡∏ñ‡∏∑‡∏≠‡πÄ‡∏Ñ‡∏£‡∏∑‡πà‡∏≠‡∏á‡∏°‡∏∑‡∏≠
        tool.Equipped:Connect(function()
            if sparkles then sparkles.Enabled = false end
            particles.Rate = 50
            light.Brightness = 0

            -- ‡πÄ‡∏û‡∏¥‡πà‡∏°‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß‡πÄ‡∏î‡∏¥‡∏ô
            if player.Character and player.Character:FindFirstChild("Humanoid") then
                player.Character.Humanoid.WalkSpeed = 30
            end

            -- ‡πÄ‡∏™‡∏µ‡∏¢‡∏á‡∏ï‡∏≠‡∏ô‡∏ñ‡∏∑‡∏≠
            local equipSound = Instance.new("Sound")
            equipSound.SoundId = "rbxassetid://2865227271" -- 2865227271 --
            equipSound.Volume = 0.5
            equipSound.Parent = player.Character and player.Character:FindFirstChild("HumanoidRootPart")
            if equipSound.Parent then
                equipSound:Play()
                equipSound.Ended:Connect(function()
                    equipSound:Destroy()
                end)
            end
        end)

        tool.Unequipped:Connect(function()
            if sparkles then sparkles.Enabled = false end
            particles.Rate = 20
            light.Brightness = 2

            -- ‡∏Å‡∏•‡∏±‡∏ö‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß‡∏õ‡∏Å‡∏ï‡∏¥
            if player.Character and player.Character:FindFirstChild("Humanoid") then
                player.Character.Humanoid.WalkSpeed = NormalWalkSpeed
            end
        end)

        -- ‡πÉ‡∏™‡πà Tool ‡πÄ‡∏Ç‡πâ‡∏≤ Backpack
        player:WaitForChild("Backpack")
        tool.Parent = player.Backpack

        -- ‡πÅ‡∏à‡πâ‡∏á‡πÄ‡∏ï‡∏∑‡∏≠‡∏ô
        Rayfield:Notify({
            Title = "[ üö≤ ]  Recoil Tool",
            Content = "[ üö≤ ]  Finish You Got Recoil Tool  üèÅ",
            Duration = 1,
            Image = "bike",
        })
    end
})

local Paragraph = BTab:CreateParagraph({Title = "Recoil Tool For Player / Friend", Content = "Recoil Tool Is Good For Speed Flash"})

--// === TELEPORT TAB ===
local friendTab = Window:CreateTab("||   - Trolling Player", "bug-play")
local Section = friendTab:CreateSection("")

local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local player = Players.LocalPlayer

-- ‡∏ï‡∏±‡∏ß‡πÅ‡∏õ‡∏£‡πÄ‡∏Å‡πá‡∏ö‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á‡πÄ‡∏î‡∏¥‡∏°‡∏Å‡πà‡∏≠‡∏ô‡∏ß‡∏≤‡∏õ
local savedCFrame = nil

-- ‡∏ï‡∏±‡∏ß‡πÅ‡∏õ‡∏£‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö Auto Teleport
local autoTPConnection = nil
local autoTargetName = nil

-- ‡∏ï‡∏±‡∏ß‡πÅ‡∏õ‡∏£‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö ESP
local currentESPHighlight = nil
local espTargetPlayer = nil
local espConnection = nil

-- ‡∏ï‡∏±‡∏ß‡πÅ‡∏õ‡∏£‡∏™‡∏µ ESP (‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏ï‡πâ‡∏ô‡πÄ‡∏Ç‡∏µ‡∏¢‡∏ß‡∏≠‡πà‡∏≠‡∏ô)
local espFillColor = Color3.fromRGB(138, 43, 179)
local espOutlineColor = Color3.fromRGB(138, 43, 179) -- ‡∏Ç‡∏≠‡∏ö‡∏Ç‡∏≤‡∏ß‡∏Ñ‡∏á‡∏ó‡∏µ‡πà ‡∏´‡∏£‡∏∑‡∏≠‡∏à‡∏∞‡∏õ‡∏£‡∏±‡∏ö‡πÑ‡∏î‡πâ‡∏î‡πâ‡∏ß‡∏¢

-- ===== ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏ä‡πà‡∏ß‡∏¢ =====
local function getPlayerList()
    local list = {}
    for _, p in ipairs(Players:GetPlayers()) do
        if p ~= player then
            table.insert(list, string.format("%s (%s)", p.DisplayName, p.Name))
        end
    end
    table.sort(list)
    return list
end

local function getPlayerFromSelection(selected)
    if not selected or selected == "" then return nil end
    local friendName = selected:match("%((.-)%)") or selected
    return Players:FindFirstChild(friendName)
end

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡πÄ‡∏õ‡∏¥‡∏î ESP (‡πÉ‡∏ä‡πâ‡∏™‡∏µ‡∏õ‡∏±‡∏à‡∏à‡∏∏‡∏ö‡∏±‡∏ô)
local function enableESP(targetPlayer)
    if not targetPlayer or not targetPlayer.Character then return end
    
    -- ‡∏•‡∏ö Highlight ‡πÄ‡∏Å‡πà‡∏≤‡∏Å‡πà‡∏≠‡∏ô (‡∏Å‡∏£‡∏ì‡∏µ respawn)
    if currentESPHighlight and currentESPHighlight.Parent then
        currentESPHighlight:Destroy()
    end
    local highlight = Instance.new("Highlight")
    highlight.Parent = targetPlayer.Character
    highlight.FillColor = espFillColor
    highlight.FillTransparency = 0.5
    highlight.OutlineColor = espOutlineColor
    highlight.OutlineTransparency = 0
    highlight.DepthMode = Enum.HighlightDepthMode.AlwaysOnTop
    highlight.Enabled = true
    currentESPHighlight = highlight
    espTargetPlayer = targetPlayer
end

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏õ‡∏¥‡∏î ESP
local function disableESP()
    if currentESPHighlight then
        currentESPHighlight:Destroy()
        currentESPHighlight = nil
    end
    if espConnection then
        espConnection:Disconnect()
        espConnection = nil
    end
    espTargetPlayer = nil
end

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï‡∏™‡∏µ ESP (‡πÄ‡∏°‡∏∑‡πà‡∏≠‡πÄ‡∏õ‡∏•‡∏µ‡πà‡∏¢‡∏ô‡∏™‡∏µ‡πÉ‡∏ô Color Picker)
local function updateESPColor()
    if currentESPHighlight and currentESPHighlight.Parent then
        currentESPHighlight.FillColor = espFillColor
        currentESPHighlight.OutlineColor = espOutlineColor
    end
end

local friendDropdown = friendTab:CreateDropdown({
    Name = "[ üë• ]  Dropdown Player Choose Player",
    Options = getPlayerList(),
    CurrentOption = {""},
    Flag = "FriendSelect",
    Callback = function(Option)
        if currentESPHighlight then
            local selected = friendDropdown.CurrentOption[1]
            local newTarget = getPlayerFromSelection(selected)
            disableESP()
            if newTarget then
                enableESP(newTarget)

                
            end
        end
    end
})

-- ‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï Dropdown ‡πÄ‡∏°‡∏∑‡πà‡∏≠‡∏°‡∏µ‡∏Ñ‡∏ô‡πÄ‡∏Ç‡πâ‡∏≤-‡∏≠‡∏≠‡∏Å
Players.PlayerAdded:Connect(function()
    friendDropdown:Refresh(getPlayerList(), true)
end)

Players.PlayerRemoving:Connect(function(p)
    task.wait(1)
    friendDropdown:Refresh(getPlayerList(), true)
   
    if espTargetPlayer and p == espTargetPlayer then
        disableESP()
    end
end)

local Section = friendTab:CreateSection("")

-- ===== ‡∏õ‡∏∏‡πà‡∏°‡∏ß‡∏≤‡∏õ‡∏ò‡∏£‡∏£‡∏°‡∏î‡∏≤ =====
friendTab:CreateButton({
    Name = "[ üìå ]  Teleport To Player - Friend",
    Callback = function()
        local selected = friendDropdown.CurrentOption[1]
        if not selected or selected == "" then return end

        local targetPlayer = getPlayerFromSelection(selected)
        local char = player.Character or player.CharacterAdded:Wait()
        local hrp = char:WaitForChild("HumanoidRootPart", 5)
        if not hrp or not targetPlayer or not targetPlayer.Character then return end
        local targetHRP = targetPlayer.Character:FindFirstChild("HumanoidRootPart")
        if targetHRP then
            savedCFrame = hrp.CFrame
            hrp.CFrame = targetHRP.CFrame + Vector3.new(0, 3, 0)
        end
    end
})

friendTab:CreateButton({
    Name = "[ üìû ]  Teleport To Player   [ - 1 Sec - ]",
    Callback = function()
        local selected = friendDropdown.CurrentOption[1]
        if not selected or selected == "" then
            return
        end

        local friendName = selected:match("%((.-)%)") or selected
        local targetPlayer = Players:FindFirstChild(friendName)
        local char = player.Character or player.CharacterAdded:Wait()
        local hrp = char:WaitForChild("HumanoidRootPart", 5)

        if not hrp then
            return
        end

        if targetPlayer and targetPlayer.Character then
            local targetHRP = targetPlayer.Character:FindFirstChild("HumanoidRootPart")
            if targetHRP then
                -- ‡∏ö‡∏±‡∏ô‡∏ó‡∏∂‡∏Å‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á‡πÄ‡∏î‡∏¥‡∏°
                savedCFrame = hrp.CFrame

                -- ‡∏ß‡∏≤‡∏õ‡πÑ‡∏õ‡∏´‡∏≤‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏ô (‡∏¢‡∏Å‡∏Ç‡∏∂‡πâ‡∏ô‡∏ô‡∏¥‡∏î‡∏´‡∏ô‡πà‡∏≠‡∏¢)
                hrp.CFrame = targetHRP.CFrame + Vector3.new(0, 5, 0)

                -- ‡∏£‡∏≠‡πÄ‡∏ü‡∏£‡∏°‡∏ô‡∏¥‡∏î‡∏ô‡∏∂‡∏á‡πÄ‡∏û‡∏∑‡πà‡∏≠‡πÉ‡∏´‡πâ‡πÄ‡∏´‡πá‡∏ô‡∏Å‡∏≤‡∏£‡∏ß‡∏≤‡∏õ‡∏ä‡∏±‡∏î‡πÄ‡∏à‡∏ô (‡πÑ‡∏°‡πà‡∏Å‡∏£‡∏∞‡∏ï‡∏∏‡∏Å)
                task.wait()

                -- ‡∏Å‡∏•‡∏±‡∏ö‡∏à‡∏∏‡∏î‡πÄ‡∏î‡∏¥‡∏°‡∏ó‡∏±‡∏ô‡∏ó‡∏µ
                if savedCFrame and hrp and hrp.Parent then
                    hrp.CFrame = savedCFrame
                end

                return
            end
        end
    end
})

friendTab:CreateToggle({
    Name = "[ ü™ë ]  Auto Teleport to Player Sit Head",
    CurrentValue = false,
    Flag = "AutoTPToggle",
    Callback = function(Value)
        local selected = friendDropdown.CurrentOption[1]
        if not selected or selected == "" then
           
            Rayfield.Flags["AutoTPToggle"]:Set(false)
            return
        end
        local friendName = selected:match("%((.-)%)") or selected
        local targetPlayer = Players:FindFirstChild(friendName)
        if not targetPlayer then
           
            Rayfield.Flags["AutoTPToggle"]:Set(false)
            return
        end
        -- ‡∏õ‡∏¥‡∏î Auto TP ‡πÄ‡∏Å‡πà‡∏≤
        if autoTPConnection then
            autoTPConnection:Disconnect()
            autoTPConnection = nil
        end
        autoTargetName = nil
        if Value then
            -- ‡∏ö‡∏±‡∏ô‡∏ó‡∏∂‡∏Å‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á‡∏õ‡∏±‡∏à‡∏à‡∏∏‡∏ö‡∏±‡∏ô (‡∏Å‡πà‡∏≠‡∏ô‡πÄ‡∏£‡∏¥‡πà‡∏° Auto TP)
            local char = player.Character or player.CharacterAdded:Wait()
            local hrp = char:WaitForChild("HumanoidRootPart", 5)
            if hrp then
                savedCFrame = hrp.CFrame
            end
            autoTargetName = friendName
            -- ‡∏™‡∏£‡πâ‡∏≤‡∏á Connection ‡∏´‡∏•‡∏±‡∏Å‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö Auto Teleport
            autoTPConnection = RunService.Heartbeat:Connect(function()
                if not autoTargetName then return end
                local target = Players:FindFirstChild(autoTargetName)
                if not target or not target.Character then return end
                local targetHRP = target.Character:FindFirstChild("HumanoidRootPart")
                if not targetHRP then return end
                local myChar = player.Character
                if not myChar then return end
                local myHRP = myChar:FindFirstChild("HumanoidRootPart")
                if not myHRP then return end
                -- ‡∏ß‡∏≤‡∏õ‡πÑ‡∏õ‡∏´‡∏≤‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢ (‡∏™‡∏π‡∏á‡∏Ç‡∏∂‡πâ‡∏ô‡∏ô‡∏¥‡∏î‡∏´‡∏ô‡πà‡∏≠‡∏¢‡πÄ‡∏û‡∏∑‡πà‡∏≠‡πÑ‡∏°‡πà‡∏ï‡∏¥‡∏î‡∏û‡∏∑‡πâ‡∏ô)
                myHRP.CFrame = targetHRP.CFrame + Vector3.new(0, 4, 0)
            end)
           
        else
            -- ‡∏õ‡∏¥‡∏î Auto TP
            autoTargetName = nil
           
        end
    end
})

-- ‡πÄ‡∏û‡∏¥‡πà‡∏°: ‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï Auto TP ‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï‡∏¥‡πÄ‡∏°‡∏∑‡πà‡∏≠‡πÄ‡∏õ‡∏•‡∏µ‡πà‡∏¢‡∏ô‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡πÉ‡∏ô Dropdown (‡∏Ç‡∏ì‡∏∞‡πÄ‡∏õ‡∏¥‡∏î Toggle ‡∏≠‡∏¢‡∏π‡πà)
friendDropdown.Callback = function(selected)
    local currentToggleValue = Rayfield.Flags["AutoTPToggle"].CurrentValue
    -- ‡∏ñ‡πâ‡∏≤ Auto TP ‡∏Å‡∏≥‡∏•‡∏±‡∏á‡πÄ‡∏õ‡∏¥‡∏î‡∏≠‡∏¢‡∏π‡πà ‡πÉ‡∏´‡πâ‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢‡πÉ‡∏´‡∏°‡πà‡∏ó‡∏±‡∏ô‡∏ó‡∏µ
    if currentToggleValue then
        Rayfield.Flags["AutoTPToggle"]:Set(false) -- ‡∏õ‡∏¥‡∏î‡∏ä‡∏±‡πà‡∏ß‡∏Ñ‡∏£‡∏≤‡∏ß
        task.wait(0.1)
        Rayfield.Flags["AutoTPToggle"]:Set(true) -- ‡πÄ‡∏õ‡∏¥‡∏î‡πÉ‡∏´‡∏°‡πà ‚Üí ‡∏à‡∏∞‡πÉ‡∏ä‡πâ‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢‡πÉ‡∏´‡∏°‡πà
    end
    -- ‡∏ñ‡πâ‡∏≤‡∏°‡∏µ ESP ‡πÄ‡∏õ‡∏¥‡∏î‡∏≠‡∏¢‡∏π‡πà‡∏Å‡∏±‡∏ö‡∏Ñ‡∏ô‡πÄ‡∏Å‡πà‡∏≤ ‡∏Å‡πá‡∏≠‡∏≤‡∏à‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï‡πÑ‡∏î‡πâ‡∏ó‡∏µ‡πà‡∏ô‡∏µ‡πà‡πÄ‡∏´‡∏°‡∏∑‡∏≠‡∏ô‡∏Å‡∏±‡∏ô (‡πÅ‡∏ï‡πà‡∏ï‡∏≠‡∏ô‡∏ô‡∏µ‡πâ‡πÅ‡∏¢‡∏Å‡∏õ‡∏∏‡πà‡∏°)
end

local Paragraph = friendTab:CreateParagraph({Title = "Teleport To Player / Friend Normal / Auto", Content = "Teleport To Player / Friend Normal / Become Auto"})

local Section = friendTab:CreateSection("")

-- ‡∏ï‡∏±‡∏ß‡πÅ‡∏õ‡∏£‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö Hitbox ‡πÄ‡∏â‡∏û‡∏≤‡∏∞‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢
local TargetHitboxEnabled = false
local TargetHitboxSize = 20
local TargetHitboxConnection = nil
local CurrentTargetPlayer = nil  -- ‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡∏ó‡∏µ‡πà‡∏ñ‡∏π‡∏Å‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡∏õ‡∏±‡∏à‡∏à‡∏∏‡∏ö‡∏±‡∏ô

-- ‡∏™‡∏µ‡πÄ‡∏Ç‡∏µ‡∏¢‡∏ß‡πÄ‡∏î‡∏µ‡∏¢‡∏ß‡∏Å‡∏±‡∏ö ESP ‡∏Ç‡∏≠‡∏á‡∏Ñ‡∏∏‡∏ì
local TargetHitboxColor = Color3.fromRGB(138, 43, 179)

friendTab:CreateToggle({
    Name = "[ ‚öîÔ∏è ]  Hitbox Boxing Enable Target",
    CurrentValue = false,
    Flag = "TargetHitboxToggle",
    Callback = function(Value)
        TargetHitboxEnabled = Value
        
        -- ‡∏î‡∏∂‡∏á‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡∏ó‡∏µ‡πà‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡∏à‡∏≤‡∏Å Dropdown
        local selected = friendDropdown.CurrentOption[1]
        local targetPlayer = getPlayerFromSelection(selected)
        
        if not targetPlayer then
            Rayfield.Flags["TargetHitboxToggle"]:Set(false)
            
            return
        end
        
        CurrentTargetPlayer = targetPlayer
        
        if Value then
            -- ‡πÄ‡∏õ‡∏¥‡∏î Hitbox ‡πÄ‡∏â‡∏û‡∏≤‡∏∞‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢
            local function applyHitbox(char)
                if not char then return end
                pcall(function()
                    local hrp = char:FindFirstChild("HumanoidRootPart")
                    if hrp then
                        hrp.Size = Vector3.new(TargetHitboxSize, TargetHitboxSize, TargetHitboxSize)
                        hrp.Transparency = 0.7
                        hrp.Color = TargetHitboxColor
                        hrp.Material = "Neon"
                        hrp.CanCollide = false
                    end
                end)
            end
            
            -- ‡πÉ‡∏ä‡πâ RenderStepped ‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï‡∏ï‡πà‡∏≠‡πÄ‡∏ô‡∏∑‡πà‡∏≠‡∏á (‡∏Å‡∏£‡∏ì‡∏µ‡πÄ‡∏Ñ‡∏•‡∏∑‡πà‡∏≠‡∏ô‡∏ó‡∏µ‡πà‡∏´‡∏£‡∏∑‡∏≠‡πÄ‡∏õ‡∏•‡∏µ‡πà‡∏¢‡∏ô‡∏™‡πà‡∏ß‡∏ô)
            TargetHitboxConnection = RunService.RenderStepped:Connect(function()
                if not TargetHitboxEnabled or not CurrentTargetPlayer or CurrentTargetPlayer ~= targetPlayer then return end
                if CurrentTargetPlayer.Character then
                    applyHitbox(CurrentTargetPlayer.Character)
                end
            end)
            
            -- ‡πÉ‡∏ä‡πâ‡∏ï‡∏≠‡∏ô‡∏ô‡∏µ‡πâ‡πÄ‡∏•‡∏¢
            if targetPlayer.Character then
                applyHitbox(targetPlayer.Character)
            end
            
            -- ‡∏£‡∏≠‡∏á‡∏£‡∏±‡∏ö respawn
            targetPlayer.CharacterAdded:Connect(function(newChar)
                if TargetHitboxEnabled and CurrentTargetPlayer == targetPlayer then
                    task.wait(0.5)  -- ‡∏£‡∏≠‡πÉ‡∏´‡πâ‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£‡πÇ‡∏´‡∏•‡∏î‡πÄ‡∏™‡∏£‡πá‡∏à
                    applyHitbox(newChar)
                end
            end)
            
            
        else
            -- ‡∏õ‡∏¥‡∏î Hitbox
            if TargetHitboxConnection then
                TargetHitboxConnection:Disconnect()
                TargetHitboxConnection = nil
            end
            
            -- Reset Hitbox ‡∏Ç‡∏≠‡∏á‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢‡∏Å‡∏•‡∏±‡∏ö‡∏Ñ‡πà‡∏≤‡πÄ‡∏î‡∏¥‡∏°
            if targetPlayer.Character then
                pcall(function()
                    local hrp = targetPlayer.Character:FindFirstChild("HumanoidRootPart")
                    if hrp then
                        hrp.Size = Vector3.new(2, 2, 1)
                        hrp.Transparency = 1
                        hrp.Material = "Plastic"
                        hrp.CanCollide = false
                    end
                end)
            end
            
            CurrentTargetPlayer = nil
            
           
        end
    end,
})

friendTab:CreateSlider({
    Name = "[ üìè ]  Slider Hitbox Player Size",
    Range = {20, 5000},
    Increment = 1,
    Suffix = " Hitbox Size",
    CurrentValue = 20,
    Flag = "TargetHitboxSize",
    Callback = function(Value)
        TargetHitboxSize = Value
        
        -- ‡∏ñ‡πâ‡∏≤‡πÄ‡∏õ‡∏¥‡∏î‡∏≠‡∏¢‡∏π‡πà ‡πÉ‡∏´‡πâ‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï‡∏Ç‡∏ô‡∏≤‡∏î‡∏ó‡∏±‡∏ô‡∏ó‡∏µ
        if TargetHitboxEnabled and CurrentTargetPlayer and CurrentTargetPlayer.Character then
            pcall(function()
                local hrp = CurrentTargetPlayer.Character:FindFirstChild("HumanoidRootPart")
                if hrp then
                    hrp.Size = Vector3.new(TargetHitboxSize, TargetHitboxSize, TargetHitboxSize)
                end
            end)
        end
    end,
})

-- ‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï‡∏¥‡πÄ‡∏°‡∏∑‡πà‡∏≠‡πÄ‡∏õ‡∏•‡∏µ‡πà‡∏¢‡∏ô‡∏Ñ‡∏ô‡πÉ‡∏ô Dropdown (‡∏ñ‡πâ‡∏≤ Toggle ‡πÄ‡∏õ‡∏¥‡∏î‡∏≠‡∏¢‡∏π‡πà)
friendDropdown.Callback = function(selected)
    local currentToggleValue = Rayfield.Flags["TargetHitboxToggle"].CurrentValue
    
    -- ‡∏ñ‡πâ‡∏≤ Toggle ‡πÄ‡∏õ‡∏¥‡∏î‡∏≠‡∏¢‡∏π‡πà ‡πÉ‡∏´‡πâ‡∏õ‡∏¥‡∏î‡∏Å‡πà‡∏≠‡∏ô‡πÅ‡∏•‡πâ‡∏ß‡πÄ‡∏õ‡∏¥‡∏î‡πÉ‡∏´‡∏°‡πà‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢
    if currentToggleValue then
        Rayfield.Flags["TargetHitboxToggle"]:Set(false)
        task.wait(0.1)
        Rayfield.Flags["TargetHitboxToggle"]:Set(true)
    end
    
    -- ‡∏™‡πà‡∏ß‡∏ô‡πÄ‡∏î‡∏¥‡∏°‡∏Ç‡∏≠‡∏á‡∏Ñ‡∏∏‡∏ì (‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö Auto TP ‡πÅ‡∏•‡∏∞ ESP)
    local currentAutoTPValue = Rayfield.Flags["AutoTPToggle"].CurrentValue
    if currentAutoTPValue then
        Rayfield.Flags["AutoTPToggle"]:Set(false)
        task.wait(0.1)
        Rayfield.Flags["AutoTPToggle"]:Set(true)
    end
end

local Paragraph = friendTab:CreateParagraph({Title = "ESP / Hitbox Boxing For Kill Player And Reset", Content = "ESP / Hitbox Boxing For Kill Player And Reseting"})

local Section = friendTab:CreateSection("")

-- ===== ‡∏ï‡∏±‡∏ß‡πÅ‡∏õ‡∏£‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö ESP ‡∏ó‡∏±‡πâ‡∏á‡∏™‡∏≠‡∏á‡πÅ‡∏ö‡∏ö =====
local currentESPHighlight = nil -- ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö Highlight ‡∏ó‡∏±‡πâ‡∏á‡∏Ñ‡∏π‡πà
local currentESPDrawings = {} -- ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö Drawing (Box + Name) ‡πÄ‡∏â‡∏û‡∏≤‡∏∞ ESP ‡∏õ‡∏Å‡∏ï‡∏¥
local espTargetPlayer = nil
local espConnection = nil

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏™‡∏£‡πâ‡∏≤‡∏á/‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï Highlight
local function enableHighlight(targetPlayer)
    if not targetPlayer or not targetPlayer.Character then return end
   
    -- ‡∏•‡∏ö Highlight ‡πÄ‡∏Å‡πà‡∏≤
    if currentESPHighlight and currentESPHighlight.Parent then
        currentESPHighlight:Destroy()
    end
   
    local highlight = Instance.new("Highlight")
    highlight.Parent = targetPlayer.Character
    highlight.FillColor = espFillColor
    highlight.FillTransparency = 0.5
    highlight.OutlineColor = espOutlineColor
    highlight.OutlineTransparency = 0
    highlight.DepthMode = Enum.HighlightDepthMode.AlwaysOnTop
    highlight.Enabled = true
   
    currentESPHighlight = highlight
    espTargetPlayer = targetPlayer
end

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏™‡∏£‡πâ‡∏≤‡∏á Drawing ESP (Box + Name + Health)
local function enableDrawingESP(targetPlayer)
    if not targetPlayer or not targetPlayer.Character then return end
   
    -- ‡∏•‡∏ö Drawing ‡πÄ‡∏Å‡πà‡∏≤
    for _, drawing in pairs(currentESPDrawings) do
        if drawing then drawing:Remove() end
    end
    currentESPDrawings = {}
   
    local box = Drawing.new("Square")
    box.Thickness = 2
    box.Filled = false
    box.Color = espFillColor
    box.Visible = false
   
    local nameText = Drawing.new("Text")
    nameText.Size = 16
    nameText.Center = true
    nameText.Outline = true
    nameText.Color = Color3.fromRGB(138, 43, 179)
    nameText.Visible = false
   
    currentESPDrawings = {Box = box, Name = nameText}
   
    -- ‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï‡∏ó‡∏∏‡∏Å‡πÄ‡∏ü‡∏£‡∏°
    if espConnection then espConnection:Disconnect() end
    espConnection = RunService.RenderStepped:Connect(function()
        if not targetPlayer.Character or not targetPlayer.Character:FindFirstChild("HumanoidRootPart") or not targetPlayer.Character:FindFirstChild("Head") or not targetPlayer.Character:FindFirstChild("Humanoid") then
            box.Visible = false
            nameText.Visible = false
            return
        end
       
        local root = targetPlayer.Character.HumanoidRootPart
        local head = targetPlayer.Character.Head
        local humanoid = targetPlayer.Character.Humanoid
       
        local screenPos, onScreen = workspace.CurrentCamera:WorldToViewportPoint(root.Position)
       
        if onScreen then
            local top = workspace.CurrentCamera:WorldToViewportPoint(head.Position + Vector3.new(0, 3, 0))
            local bottom = workspace.CurrentCamera:WorldToViewportPoint(head.Position - Vector3.new(0, 3, 0))
            local sizeY = (top - bottom).Magnitude
           
            box.Size = Vector2.new(sizeY * 2, sizeY * 3.5)
            box.Position = Vector2.new(screenPos.X - box.Size.X / 2, screenPos.Y - box.Size.Y / 2)
            box.Visible = true
           
            nameText.Text = targetPlayer.DisplayName .. " [ " .. math.floor(humanoid.Health) .. " ]"
            nameText.Position = Vector2.new(screenPos.X, screenPos.Y - box.Size.Y / 2 - 20)
            nameText.Visible = true
        else
            box.Visible = false
            nameText.Visible = false
        end
    end)
end

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏õ‡∏¥‡∏î‡∏ó‡∏∏‡∏Å‡∏≠‡∏¢‡πà‡∏≤‡∏á
local function disableAllESP()
    if currentESPHighlight then
        currentESPHighlight:Destroy()
        currentESPHighlight = nil
    end
    for _, drawing in pairs(currentESPDrawings) do
        if drawing then drawing:Remove() end
    end
    currentESPDrawings = {}
    if espConnection then
        espConnection:Disconnect()
        espConnection = nil
    end
    espTargetPlayer = nil
end

-- ===== ‡∏õ‡∏∏‡πà‡∏° ESP ‡∏õ‡∏Å‡∏ï‡∏¥ (Highlight + Box + Name) =====
friendTab:CreateButton({
    Name = "[ üëÅÔ∏è ]  ESP Box Dropdown Player",
    Callback = function()
        local selected = friendDropdown.CurrentOption[1]
        local targetPlayer = getPlayerFromSelection(selected)
       
        if not targetPlayer then
            disableAllESP()
           
            return
        end
       
        -- ‡∏ñ‡πâ‡∏≤‡πÄ‡∏õ‡∏¥‡∏î‡∏≠‡∏¢‡∏π‡πà‡πÅ‡∏•‡πâ‡∏ß‡∏Å‡∏±‡∏ö‡∏Ñ‡∏ô‡πÄ‡∏î‡∏µ‡∏¢‡∏ß‡∏Å‡∏±‡∏ô ‚Üí ‡∏õ‡∏¥‡∏î
        if espTargetPlayer == targetPlayer and currentESPHighlight and next(currentESPDrawings) then
            disableAllESP()
            return
        end
       
        -- ‡∏õ‡∏¥‡∏î‡πÄ‡∏Å‡πà‡∏≤ ‡πÅ‡∏•‡πâ‡∏ß‡πÄ‡∏õ‡∏¥‡∏î‡πÉ‡∏´‡∏°‡πà
        disableAllESP()
        enableHighlight(targetPlayer)
        enableDrawingESP(targetPlayer)
       
        -- ‡∏ï‡∏¥‡∏î‡∏ï‡∏≤‡∏° respawn
        targetPlayer.CharacterAdded:Connect(function()
            if espTargetPlayer == targetPlayer then
                task.wait(0.5)
                enableHighlight(targetPlayer)
                enableDrawingESP(targetPlayer)
            end
        end)
    end
})

friendTab:CreateButton({
    Name = "[ üí° ]  ESP Highlight Dropdown Player",
    Callback = function()
        local selected = friendDropdown.CurrentOption[1]
        local targetPlayer = getPlayerFromSelection(selected)
       
        if not targetPlayer then
            disableAllESP()
           
            return
        end
       
        -- ‡∏ñ‡πâ‡∏≤‡πÄ‡∏õ‡∏¥‡∏î Highlight ‡∏≠‡∏¢‡∏π‡πà‡∏Ñ‡∏ô‡πÄ‡∏î‡∏µ‡∏¢‡∏ß‡∏Å‡∏±‡∏ô ‚Üí ‡∏õ‡∏¥‡∏î
        if espTargetPlayer == targetPlayer and currentESPHighlight and not next(currentESPDrawings) then
            disableAllESP()
            return
        end
       
        -- ‡∏õ‡∏¥‡∏î‡πÄ‡∏Å‡πà‡∏≤ ‡πÅ‡∏•‡πâ‡∏ß‡πÄ‡∏õ‡∏¥‡∏î Highlight ‡πÄ‡∏â‡∏û‡∏≤‡∏∞
        disableAllESP()
        enableHighlight(targetPlayer)
       
        -- ‡∏ï‡∏¥‡∏î‡∏ï‡∏≤‡∏° respawn (‡πÄ‡∏â‡∏û‡∏≤‡∏∞ Highlight)
        targetPlayer.CharacterAdded:Connect(function()
            if espTargetPlayer == targetPlayer and not next(currentESPDrawings) then
                task.wait(0.5)
                enableHighlight(targetPlayer)
            end
        end)
    end
})

game:GetService("RunService").RenderStepped:Connect(UpdateESP)

local Paragraph = friendTab:CreateParagraph({Title = "ESP To Player / Friend Normal", Content = "ESP To Player / Friend Become Normal"})

local Section = friendTab:CreateSection("")

-- ===== ‡∏õ‡∏∏‡πà‡∏°‡πÉ‡∏´‡∏°‡πà: ‡∏Å‡∏•‡∏±‡∏ö‡πÑ‡∏õ‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á‡πÄ‡∏î‡∏¥‡∏° =====
friendTab:CreateButton({
    Name = "[ üîô ]  Return To Original Position",
    Callback = function()
        if not savedCFrame then
            -- ‡∏ñ‡πâ‡∏≤‡∏¢‡∏±‡∏á‡πÑ‡∏°‡πà‡∏°‡∏µ‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á‡∏ó‡∏µ‡πà‡∏ö‡∏±‡∏ô‡∏ó‡∏∂‡∏Å‡πÑ‡∏ß‡πâ (‡∏¢‡∏±‡∏á‡πÑ‡∏°‡πà‡πÄ‡∏Ñ‡∏¢‡∏ß‡∏≤‡∏£‡πå‡∏õ)
            warn("No saved position to return to!")
            return
        end

        local char = player.Character or player.CharacterAdded:Wait()
        local hrp = char:FindFirstChild("HumanoidRootPart")
        
        if not hrp then
            warn("HumanoidRootPart not found!")
            return
        end

        -- ‡∏ß‡∏≤‡∏£‡πå‡∏õ‡∏Å‡∏•‡∏±‡∏ö‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á‡πÄ‡∏î‡∏¥‡∏°‡∏≠‡∏¢‡πà‡∏≤‡∏á‡∏£‡∏ß‡∏î‡πÄ‡∏£‡πá‡∏ß‡πÅ‡∏•‡∏∞‡∏ô‡∏∏‡πà‡∏°‡∏ô‡∏ß‡∏•
        hrp.CFrame = savedCFrame
    end
})

local Paragraph = friendTab:CreateParagraph({Title = "Return To Your Point", Content = "Return To Your Point And Save Point"})

--// === ESP TAB (Local ESP - Rainbow Edition) ===
local espTab = Window:CreateTab("||   - Solution", "flask-conical")
local Section = espTab:CreateSection("")

local MouseTPEnabled = false
local Mouse = game.Players.LocalPlayer:GetMouse()
local Player = game.Players.LocalPlayer

-- ‡∏™‡∏£‡πâ‡∏≤‡∏á Sound ‡πÑ‡∏ß‡πâ‡πÉ‡∏ä‡πâ‡∏ã‡πâ‡∏≥‡πÑ‡∏î‡πâ‡πÄ‡∏£‡∏∑‡πà‡∏≠‡∏¢ ‡πÜ
local teleportSound = Instance.new("Sound")
teleportSound.SoundId = "rbxassetid://4861638982"
teleportSound.Volume = 0.7

local Toggle = espTab:CreateToggle({
    Name = "[ üëÜ ]  Clicking Teleport   [ - Backpack - ]",
    CurrentValue = false,
    Flag = "MouseTP",
    Callback = function(Value)
        MouseTPEnabled = Value
        Rayfield:Notify({
            Title = Value and   "[ üëÜ ]  Teleport Clicking On" or "[ üëÜ ]  Teleport Clicking Off",
            Content = Value and "[ ‚úÖ ]  Click Teleport On" or "[ üõë ]  Click Teleport Off",
            Duration = 1,
            Image = "mouse"
        })
    end,
})

-- ‡∏Å‡∏î‡∏Ñ‡∏•‡∏¥‡∏Å = ‡∏ß‡∏≤‡∏£‡πå‡∏õ‡∏ó‡∏±‡∏ô‡∏ó‡∏µ + ‡πÄ‡∏™‡∏µ‡∏¢‡∏á‡∏î‡∏±‡∏á‡∏û‡∏£‡πâ‡∏≠‡∏°‡∏Å‡∏±‡∏ô
Mouse.Button1Down:Connect(function()
    if MouseTPEnabled and Player.Character and Player.Character:FindFirstChild("HumanoidRootPart") then
        local targetPos = Mouse.Hit.Position + Vector3.new(0, 5, 0)
        
        -- ‡∏ß‡∏≤‡∏£‡πå‡∏õ‡∏Å‡πà‡∏≠‡∏ô‡πÄ‡∏•‡∏¢ ‡πÑ‡∏°‡πà‡∏ï‡πâ‡∏≠‡∏á‡∏£‡∏≠
        Player.Character.HumanoidRootPart.CFrame = CFrame.new(targetPos)
        
        -- ‡πÄ‡∏•‡πà‡∏ô‡πÄ‡∏™‡∏µ‡∏¢‡∏á‡∏ó‡∏±‡∏ô‡∏ó‡∏µ (‡πÄ‡∏™‡∏µ‡∏¢‡∏á‡∏à‡∏∞‡∏î‡∏±‡∏á‡∏à‡∏≤‡∏Å‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á‡πÉ‡∏´‡∏°‡πà‡πÄ‡∏•‡∏¢)
        teleportSound.Parent = Player.Character.HumanoidRootPart
        teleportSound:Play()
        
        -- ‡∏´‡∏•‡∏±‡∏á‡∏à‡∏≤‡∏Å‡πÄ‡∏•‡πà‡∏ô‡πÄ‡∏™‡∏£‡πá‡∏à ‡∏•‡∏ö‡∏≠‡∏≠‡∏Å‡πÄ‡∏û‡∏∑‡πà‡∏≠‡πÑ‡∏°‡πà‡πÉ‡∏´‡πâ‡∏Å‡∏¥‡∏ô memory
        task.delay(3, function()
            teleportSound.Parent = nil
        end)
    end
end)

-- ‡∏£‡∏µ‡πÇ‡∏´‡∏•‡∏î‡∏™‡∏ñ‡∏≤‡∏ô‡∏∞‡∏ï‡∏≠‡∏ô‡πÄ‡∏Å‡∏¥‡∏î‡πÉ‡∏´‡∏°‡πà
Player.CharacterAdded:Connect(function()
    task.wait(1)
    MouseTPEnabled = Rayfield.Flags.MouseTP.CurrentValue
end)

local Paragraph = espTab:CreateParagraph({Title = "Clicking Teleport For Player / Friend", Content = "Clicking Teleport Is Good For Building"})

local Section = espTab:CreateSection("")

local PurpleTool = {
    Enabled = false,
    Current = nil,
    Highlight = nil,
    Connections = {},
}

-- ‡∏™‡∏µ‡∏°‡πà‡∏ß‡∏á‡∏ó‡∏µ‡πà‡∏ï‡πâ‡∏≠‡∏á‡∏Å‡∏≤‡∏£ (138, 43, 179)
local PURPLE_COLOR = Color3.fromRGB(138, 43, 179)

local function CreateHighlight(tool)
    if PurpleTool.Highlight then PurpleTool.Highlight:Destroy() end
    PurpleTool.Highlight = Instance.new("Highlight")
    PurpleTool.Highlight.FillColor = PURPLE_COLOR
    PurpleTool.Highlight.OutlineColor = PURPLE_COLOR
    PurpleTool.Highlight.FillTransparency = 0.3
    PurpleTool.Highlight.OutlineTransparency = 0
    PurpleTool.Highlight.Parent = tool
    PurpleTool.Highlight.Adornee = tool:FindFirstChild("Handle") or tool
end

local function ApplyPurpleColor()
    if not PurpleTool.Enabled or not PurpleTool.Current or not PurpleTool.Highlight then return end
    
    -- ‡∏ï‡∏±‡πâ‡∏á‡∏Ñ‡πà‡∏≤‡∏™‡∏µ Highlight
    PurpleTool.Highlight.FillColor = PURPLE_COLOR
    PurpleTool.Highlight.OutlineColor = PURPLE_COLOR
    
    -- ‡πÄ‡∏õ‡∏•‡∏µ‡πà‡∏¢‡∏ô‡∏™‡∏µ‡∏ó‡∏∏‡∏Å Part ‡πÉ‡∏ô Tool (‡∏¢‡∏Å‡πÄ‡∏ß‡πâ‡∏ô HumanoidRootPart)
    for _, part in pairs(PurpleTool.Current:GetDescendants()) do
        if part:IsA("BasePart") and part.Name ~= "HumanoidRootPart" then
            part.Color = PURPLE_COLOR
        end
    end
end

local function OnEquipped(tool)
    if not PurpleTool.Enabled then return end
    PurpleTool.Current = tool
    task.wait(0.1)
    CreateHighlight(tool)
    ApplyPurpleColor()  -- ‡πÉ‡∏ä‡πâ‡∏™‡∏µ‡∏°‡πà‡∏ß‡∏á‡∏ó‡∏±‡∏ô‡∏ó‡∏µ
end

local function OnUnequipped()
    if PurpleTool.Highlight then 
        PurpleTool.Highlight:Destroy() 
        PurpleTool.Highlight = nil 
    end
    PurpleTool.Current = nil
end

local function SetupTool(char)
    -- ‡∏•‡πâ‡∏≤‡∏á connection ‡πÄ‡∏Å‡πà‡∏≤
    for _, c in pairs(PurpleTool.Connections) do c:Disconnect() end
    PurpleTool.Connections = {}

    local function ConnectTool(tool)
        table.insert(PurpleTool.Connections, tool.Equipped:Connect(function() OnEquipped(tool) end))
        table.insert(PurpleTool.Connections, tool.Unequipped:Connect(OnUnequipped))
    end

    -- ‡πÄ‡∏ä‡∏∑‡πà‡∏≠‡∏° Tool ‡∏ó‡∏µ‡πà‡∏≠‡∏¢‡∏π‡πà‡πÉ‡∏ô‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£‡∏ï‡∏≠‡∏ô‡∏ô‡∏µ‡πâ
    for _, child in pairs(char:GetChildren()) do
        if child:IsA("Tool") then ConnectTool(child) end
    end
    table.insert(PurpleTool.Connections, char.ChildAdded:Connect(function(c)
        if c:IsA("Tool") then ConnectTool(c) end
    end))

    -- ‡πÄ‡∏ä‡∏∑‡πà‡∏≠‡∏° Tool ‡πÉ‡∏ô Backpack
    local backpack = LocalPlayer:WaitForChild("Backpack")
    for _, child in pairs(backpack:GetChildren()) do
        if child:IsA("Tool") then ConnectTool(child) end
    end
    table.insert(PurpleTool.Connections, backpack.ChildAdded:Connect(function(c)
        if c:IsA("Tool") then ConnectTool(c) end
    end))
end

local function TogglePurpleTool(state)
    PurpleTool.Enabled = state
    if state then
        Rayfield:Notify({
           Title = "[ ‚úÖ ]  Purple Tools On",
           Content = "[ üü™ ]  Purple Tools Finish",
           Duration = 1,
           Image = "grape",
         })
        if LocalPlayer.Character then SetupTool(LocalPlayer.Character) end
    else
        Rayfield:Notify({
           Title = "[ ‚ùå ] Purple Tools Off",
           Content = "[ üü™ ] Purple Tools Finish",
           Duration = 1,
           Image = "grape",
         })
        if PurpleTool.Highlight then 
            PurpleTool.Highlight:Destroy() 
            PurpleTool.Highlight = nil 
        end
        PurpleTool.Current = nil
    end
end

-- ‡∏£‡∏≠‡∏á‡∏£‡∏±‡∏ö‡∏Å‡∏≤‡∏£‡πÄ‡∏Å‡∏¥‡∏î‡πÉ‡∏´‡∏°‡πà
LocalPlayer.CharacterAdded:Connect(SetupTool)
if LocalPlayer.Character then SetupTool(LocalPlayer.Character) end

-- Toggle ‡πÉ‡∏ô UI (‡πÄ‡∏õ‡∏•‡∏µ‡πà‡∏¢‡∏ô‡∏ä‡∏∑‡πà‡∏≠‡πÉ‡∏´‡πâ‡πÄ‡∏Ç‡πâ‡∏≤‡∏Å‡∏±‡∏ö‡∏ò‡∏µ‡∏°‡∏°‡πà‡∏ß‡∏á)
espTab:CreateToggle({
    Name = "[ üü™ ]  Purple Glowing Tool  [ - Permanent Purple - ]",
    CurrentValue = false,
    Callback = function(v) 
        TogglePurpleTool(v) 
    end
})

local RainbowGun = {
    Enabled = false,
    Current = nil,
    Highlight = nil,
    Hue = 0,
    Connections = {},
    Loop = nil
}

local function CreateHighlight(tool)
    if RainbowGun.Highlight then RainbowGun.Highlight:Destroy() end
    RainbowGun.Highlight = Instance.new("Highlight")
    RainbowGun.Highlight.FillTransparency = 0.3
    RainbowGun.Highlight.OutlineTransparency = 0
    RainbowGun.Highlight.Parent = tool
    RainbowGun.Highlight.Adornee = tool:FindFirstChild("Handle") or tool
end

local function UpdateRainbowGun()
    if not RainbowGun.Enabled or not RainbowGun.Current or not RainbowGun.Highlight then return end
    RainbowGun.Hue = (RainbowGun.Hue + 0.01) % 1
    local color = Color3.fromHSV(RainbowGun.Hue, 1, 1)
    RainbowGun.Highlight.FillColor = color
    RainbowGun.Highlight.OutlineColor = color
    for _, part in pairs(RainbowGun.Current:GetDescendants()) do
        if part:IsA("BasePart") and part.Name ~= "HumanoidRootPart" then
            part.Color = color
        end
    end
end

local function OnEquipped(tool)
    if not RainbowGun.Enabled then return end
    RainbowGun.Current = tool
    task.wait(0.1)
    CreateHighlight(tool)
end

local function OnUnequipped()
    if RainbowGun.Highlight then RainbowGun.Highlight:Destroy() RainbowGun.Highlight = nil end
    RainbowGun.Current = nil
end

local function SetupGun(char)
    for _, c in pairs(RainbowGun.Connections) do c:Disconnect() end
    RainbowGun.Connections = {}

    local function ConnectTool(tool)
        table.insert(RainbowGun.Connections, tool.Equipped:Connect(function() OnEquipped(tool) end))
        table.insert(RainbowGun.Connections, tool.Unequipped:Connect(OnUnequipped))
    end

    for _, child in pairs(char:GetChildren()) do
        if child:IsA("Tool") then ConnectTool(child) end
    end
    table.insert(RainbowGun.Connections, char.ChildAdded:Connect(function(c)
        if c:IsA("Tool") then ConnectTool(c) end
    end))

    local backpack = LocalPlayer:WaitForChild("Backpack")
    for _, child in pairs(backpack:GetChildren()) do
        if child:IsA("Tool") then ConnectTool(child) end
    end
    table.insert(RainbowGun.Connections, backpack.ChildAdded:Connect(function(c)
        if c:IsA("Tool") then ConnectTool(c) end
    end))
end

local function ToggleRainbowGun(state)
    RainbowGun.Enabled = state
    if state then
        Rayfield:Notify({
           Title = "[ ‚úÖ ]  Rainbow Tools On",
           Content = "[ üåà ]  Rainbow Tools Finish",
           Duration = 1,
           Image = "rainbow",
         })
        if not RainbowGun.Loop then
            RainbowGun.Loop = RunService.Heartbeat:Connect(UpdateRainbowGun)
        end
        if LocalPlayer.Character then SetupGun(LocalPlayer.Character) end
    else
        Rayfield:Notify({
           Title = "[ ‚ùå ]  Rainbow Tools Off",
           Content = "[ üåà ]  Rainbow Tools Finish",
           Duration = 1,
           Image = "rainbow",
         })
        if RainbowGun.Loop then RainbowGun.Loop:Disconnect() RainbowGun.Loop = nil end
        if RainbowGun.Highlight then RainbowGun.Highlight:Destroy() RainbowGun.Highlight = nil end
        RainbowGun.Current = nil
    end
end

LocalPlayer.CharacterAdded:Connect(SetupGun)
if LocalPlayer.Character then SetupGun(LocalPlayer.Character) end

espTab:CreateToggle({
    Name = "[ üåà ]  Rainbow Mountain Tool  [ - Permanent Rainbow - ]",
    CurrentValue = false,
    Callback = function(v) ToggleRainbowGun(v) end
})

Players.PlayerAdded:Connect(function(p)
    if ESPEnabled and p ~= LocalPlayer then
        p.CharacterAdded:Connect(function() createESP(p) end)
    end
end)

Players.PlayerRemoving:Connect(function(p)
    if ESPObjects[p] then
        for _, obj in pairs(ESPObjects[p]) do
            if typeof(obj) == "Instance" and obj.Remove then
                obj:Remove()
            end
        end
        ESPObjects[p] = nil
    end
end)

local Paragraph = espTab:CreateParagraph({Title = "Purple / RainbowTool For Cool Style", Content = "Purple / Rainbow Tool Is Good For Style Gun / Sword"})

RunService.RenderStepped:Connect(updateESP)

local Section = espTab:CreateSection("")

-- ‡πÇ‡∏´‡∏•‡∏î‡∏ö‡∏£‡∏¥‡∏Å‡∏≤‡∏£‡∏ó‡∏µ‡πà‡∏à‡∏≥‡πÄ‡∏õ‡πá‡∏ô
local RunService = game:GetService("RunService")
local Players = game:GetService("Players")
local UserInputService = game:GetService("UserInputService")
local Workspace = game:GetService("Workspace")
local Debris = game:GetService("Debris")  -- ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡∏•‡∏ö object ‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï‡∏¥

-- ‡∏ï‡∏±‡∏ß‡πÅ‡∏õ‡∏£‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏ï‡πâ‡∏ô
local swingSpeed = 80
local swingGravity = 50
local maxWebDistance = 1500

local swingConnection
local inputConnection1
local inputConnection2

local isSwinging = false
local webPoint = nil

local webBeam = nil          -- Beam ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡πÄ‡∏™‡πâ‡∏ô‡πÉ‡∏¢
local playerAttachment = nil -- Attachment ‡∏ó‡∏µ‡πà‡∏°‡∏∑‡∏≠/‡πÅ‡∏Ç‡∏ô‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô
local anchorAttachment = nil -- Attachment ‡∏ó‡∏µ‡πà‡∏à‡∏∏‡∏î‡∏ï‡∏¥‡∏î‡πÉ‡∏¢

-- Toggle ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö Spider-Man Swing
local Toggle = espTab:CreateToggle({
    Name = "[ ‚õèÔ∏è ]  Swinging Webbing Block  [ - R6 / R15 - ]",
    CurrentValue = false,
    Flag = "SpiderSwing",
    Callback = function(Value)
        Rayfield:Notify({
            Title = Value and   "[ ‚õèÔ∏è ]  Swinging Mode On" or "[ ‚õèÔ∏è ]  Swinging Mode Off",
            Content = Value and "[ ‚úÖ ]  Swing Webing Enable!" or "[ üõë ]  Swing Webing Disabled!",
            Duration = 1,
            Image = "pickaxe"
        })

        local LP = Players.LocalPlayer

        if Value then
            -- ‡∏£‡∏≠ Character
            if not LP.Character then
                LP.CharacterAdded:Wait()
            end

            local Char = LP.Character
            local Hum = Char:FindFirstChild("Humanoid")
            if not Hum then return end

            -- ‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö RigType ‡πÄ‡∏û‡∏∑‡πà‡∏≠‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡∏™‡πà‡∏ß‡∏ô‡∏°‡∏∑‡∏≠‡∏Ç‡∏ß‡∏≤
            local rightHandPart
            if Hum.RigType == Enum.HumanoidRigType.R15 then
                rightHandPart = Char:FindFirstChild("RightHand") or Char:WaitForChild("RightHand")
            else -- R6
                rightHandPart = Char:FindFirstChild("Right Arm") or Char:WaitForChild("Right Arm")
            end

            -- ‡∏™‡∏£‡πâ‡∏≤‡∏á Attachment ‡∏ó‡∏µ‡πà‡∏°‡∏∑‡∏≠‡∏Ç‡∏ß‡∏≤ (‡∏´‡∏£‡∏∑‡∏≠‡πÅ‡∏Ç‡∏ô‡∏Ç‡∏ß‡∏≤‡πÉ‡∏ô R6)
            playerAttachment = Instance.new("Attachment")
            playerAttachment.Name = "WebAttachment_Player"
            playerAttachment.Parent = rightHandPart

            -- Heartbeat ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡∏Å‡∏≤‡∏£‡πÄ‡∏´‡∏ß‡∏µ‡πà‡∏¢‡∏á
            swingConnection = RunService.Heartbeat:Connect(function()
                if Char and Char:FindFirstChild("HumanoidRootPart") and Char:FindFirstChild("Humanoid") then
                    local Root = Char.HumanoidRootPart
                    local Hum = Char.Humanoid

                    if isSwinging and webPoint then
                        local direction = (webPoint - Root.Position).Unit

                        local velocity = direction * swingSpeed
                        Root.Velocity = Vector3.new(velocity.X, swingGravity, velocity.Z)

                        local lookCFrame = CFrame.new(Root.Position, webPoint)
                        Root.CFrame = Root.CFrame:Lerp(lookCFrame, 0.2)

                        Hum:ChangeState(Enum.HumanoidStateType.Freefall)
                    end
                end
            end)

            -- ‡∏Å‡∏î‡∏Ñ‡∏•‡∏¥‡∏Å‡∏ã‡πâ‡∏≤‡∏¢‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏¢‡∏¥‡∏á‡πÉ‡∏¢
            inputConnection1 = UserInputService.InputBegan:Connect(function(input, gameProcessed)
                if gameProcessed then return end
                if input.UserInputType == Enum.UserInputType.MouseButton1 then
                    if Char and Char:FindFirstChild("HumanoidRootPart") then
                        local camera = Workspace.CurrentCamera

                        local params = RaycastParams.new()
                        params.FilterDescendantsInstances = {Char}
                        params.FilterType = Enum.RaycastFilterType.Exclude

                        local rayResult = Workspace:Raycast(camera.CFrame.Position, camera.CFrame.LookVector * maxWebDistance, params)

                        if rayResult and rayResult.Position then
                            webPoint = rayResult.Position
                            isSwinging = true

                            -- ‡∏™‡∏£‡πâ‡∏≤‡∏á Attachment ‡∏ó‡∏µ‡πà‡∏à‡∏∏‡∏î‡∏ï‡∏¥‡∏î‡πÉ‡∏¢
                            anchorAttachment = Instance.new("Attachment")
                            anchorAttachment.Name = "WebAttachment_Anchor"
                            anchorAttachment.WorldPosition = webPoint
                            anchorAttachment.Parent = Workspace.Terrain

                            -- ‡∏™‡∏£‡πâ‡∏≤‡∏á Beam - ‡πÄ‡∏™‡πâ‡∏ô‡πÉ‡∏¢‡πÉ‡∏´‡∏ç‡πà ‡∏´‡∏ô‡∏≤ ‡πÅ‡∏ö‡∏ö Spider-Man
                            webBeam = Instance.new("Beam")
                            webBeam.Name = "SpiderWeb"
                            webBeam.Attachment0 = playerAttachment
                            webBeam.Attachment1 = anchorAttachment
                            
                            webBeam.FaceCamera = true
                            webBeam.Color = ColorSequence.new(Color3.fromRGB(138, 43, 179))
                            webBeam.LightEmission = 1.5
                            webBeam.LightInfluence = 0
                            webBeam.Transparency = NumberSequence.new(0.3)
                            webBeam.Width0 = 2.0
                            webBeam.Width1 = 1.5
                            
                            webBeam.Texture = "rbxassetid://6060542021"
                            webBeam.TextureMode = Enum.TextureMode.Stretch
                            webBeam.TextureSpeed = 3
                            webBeam.TextureLength = 8
                            
                            webBeam.Parent = Char

                            -- === ‡πÄ‡∏û‡∏¥‡πà‡∏°‡πÄ‡∏≠‡∏ü‡πÄ‡∏ü‡∏Å‡∏ï‡πå‡∏Ñ‡∏ß‡∏±‡∏ô‡∏™‡∏µ‡∏°‡πà‡∏ß‡∏á ===
                            local purpleColor = ColorSequence.new(Color3.fromRGB(138, 43, 179))

                            -- ‡∏Ñ‡∏ß‡∏±‡∏ô‡∏ó‡∏µ‡πà‡∏°‡∏∑‡∏≠ (‡∏à‡∏∏‡∏î‡∏¢‡∏¥‡∏á)
                            local smokeHand = Instance.new("ParticleEmitter")
                            smokeHand.Parent = rightHandPart
                            smokeHand.Texture = "rbxasset://textures/particles/smoke_main.dds"  -- texture ‡∏Ñ‡∏ß‡∏±‡∏ô‡∏°‡∏≤‡∏ï‡∏£‡∏ê‡∏≤‡∏ô Roblox
                            smokeHand.Color = purpleColor
                            smokeHand.Transparency = NumberSequence.new(0.5, 1)
                            smokeHand.Size = NumberSequence.new(1, 3)
                            smokeHand.Lifetime = NumberRange.new(0.8, 1.5)
                            smokeHand.Rate = 100  -- ‡∏û‡∏∏‡πà‡∏á‡πÅ‡∏£‡∏á‡∏ï‡∏≠‡∏ô‡∏¢‡∏¥‡∏á
                            smokeHand.Speed = NumberRange.new(5, 15)
                            smokeHand.SpreadAngle = Vector2.new(45, 45)
                            smokeHand.LightEmission = 0.3
                            smokeHand.Enabled = true

                            -- ‡∏Ñ‡∏ß‡∏±‡∏ô‡∏ó‡∏µ‡πà‡∏à‡∏∏‡∏î‡∏ï‡∏¥‡∏î‡∏ú‡∏ô‡∏±‡∏á
                            local smokeAnchor = Instance.new("ParticleEmitter")
                            smokeAnchor.Parent = anchorAttachment
                            smokeAnchor.Texture = "rbxasset://textures/particles/smoke_main.dds"
                            smokeAnchor.Color = purpleColor
                            smokeAnchor.Transparency = NumberSequence.new(0.4, 1)
                            smokeAnchor.Size = NumberSequence.new(2, 5)
                            smokeAnchor.Lifetime = NumberRange.new(1, 2)
                            smokeAnchor.Rate = 150
                            smokeAnchor.Speed = NumberRange.new(2, 8)
                            smokeAnchor.SpreadAngle = Vector2.new(360, 360)
                            smokeAnchor.LightEmission = 0.2
                            smokeAnchor.Enabled = true

                            -- ‡∏´‡∏¢‡∏∏‡∏î emit ‡∏Ñ‡∏ß‡∏±‡∏ô‡∏´‡∏•‡∏±‡∏á‡∏à‡∏≤‡∏Å delay (‡πÄ‡∏û‡∏∑‡πà‡∏≠‡πÑ‡∏°‡πà‡πÉ‡∏´‡πâ‡∏ü‡∏∏‡πâ‡∏á‡∏ï‡∏•‡∏≠‡∏î‡πÄ‡∏ß‡∏•‡∏≤)
                            task.delay(0.5, function()
                                smokeHand.Enabled = false
                                smokeAnchor.Enabled = false
                            end)

                            -- ‡∏•‡∏ö ParticleEmitter ‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï‡∏¥‡∏´‡∏•‡∏±‡∏á‡∏à‡∏ö
                            Debris:AddItem(smokeHand, 3)
                            Debris:AddItem(smokeAnchor, 3)

                            Rayfield:Notify({
                                Title = "[ ‚úÖ ]  Webing Block Attached!",
                                Content = "[ ‚õèÔ∏è ]  Found Block / Walling",
                                Duration = 1,
                                Image = "brick-wall"
                            })
                        else      
                            Rayfield:Notify({
                                Title = "[ ‚ùå ]  No Webing Block Point!",
                                Content = "[ ‚õèÔ∏è ]  No Found Block / Walling",
                                Duration = 1,
                                Image = "brick-wall"
                            })
                        end
                    end
                end
            end)

            -- ‡∏õ‡∏•‡πà‡∏≠‡∏¢‡∏Ñ‡∏•‡∏¥‡∏Å‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏ï‡∏±‡∏î‡πÉ‡∏¢
            inputConnection2 = UserInputService.InputEnded:Connect(function(input, gameProcessed)
                if gameProcessed then return end
                if input.UserInputType == Enum.UserInputType.MouseButton1 then
                    isSwinging = false
                    webPoint = nil

                    if webBeam then webBeam:Destroy() end
                    if anchorAttachment then anchorAttachment:Destroy() end
                    webBeam = nil
                    anchorAttachment = nil
                end
            end)

        else
            -- ‡∏õ‡∏¥‡∏î‡∏ó‡∏∏‡∏Å‡∏≠‡∏¢‡πà‡∏≤‡∏á
            if swingConnection then swingConnection:Disconnect() end
            if inputConnection1 then inputConnection1:Disconnect() end
            if inputConnection2 then inputConnection2:Disconnect() end

            isSwinging = false
            webPoint = nil

            if webBeam then webBeam:Destroy() end
            if anchorAttachment then anchorAttachment:Destroy() end
            if playerAttachment then playerAttachment:Destroy() end

            if LP.Character and LP.Character:FindFirstChild("Humanoid") then
                LP.Character.Humanoid:ChangeState(Enum.HumanoidStateType.Running)
            end
        end
    end,
})

-- Slider ‡∏ï‡πà‡∏≤‡∏á ‡πÜ (‡πÄ‡∏´‡∏°‡∏∑‡∏≠‡∏ô‡πÄ‡∏î‡∏¥‡∏° ‡πÅ‡∏ï‡πà‡∏õ‡∏£‡∏±‡∏ö Notify ‡πÉ‡∏´‡πâ‡∏™‡∏ß‡∏¢‡∏Ç‡∏∂‡πâ‡∏ô)
espTab:CreateSlider({
    Name = "[ üìè ]  Swinging Webbing Speed",
    Range = {50, 5000},
    Increment = 5,
    Suffix = " Swing Speed",
    CurrentValue = 80,
    Flag = "SwingSpeed",
    Callback = function(Value)
        swingSpeed = Value
        Rayfield:Notify({
           Title = "[ üìè ]  Swinging Speed!",
           Content = "Swinging Speed: " .. Value,
           Duration = 1,
           Image = "ruler",
         })
    end
})

