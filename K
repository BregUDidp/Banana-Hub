-- ‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï‡∏•‡∏π‡∏õ
RS.RenderStepped:Connect(function()
    if flying and vv and gyro and hrp and humanoid then
        local cam = workspace.CurrentCamera
        if not cam then return end
 
        local flatForward = Vector3.new(cam.CFrame.LookVector.X,0,cam.CFrame.LookVector.Z)
        local forward = flatForward.Magnitude > 0 and flatForward.Unit or Vector3.zero
        local right = Vector3.new(cam.CFrame.RightVector.X,0,cam.CFrame.RightVector.Z)
        if right.Magnitude > 0 then right = right.Unit end
 
        local moveVec = Vector3.zero
        if UIS:IsKeyDown(Enum.KeyCode.W) then moveVec = moveVec + Vector3.new(0,0,1) end
        if UIS:IsKeyDown(Enum.KeyCode.S) then moveVec = moveVec + Vector3.new(0,0,-1) end
        if UIS:IsKeyDown(Enum.KeyCode.A) then moveVec = moveVec + Vector3.new(-1,0,0) end
        if UIS:IsKeyDown(Enum.KeyCode.D) then moveVec = moveVec + Vector3.new(1,0,0) end
        if UIS:IsKeyDown(Enum.KeyCode.Space) then moveVec = moveVec + Vector3.new(0,1,0) end
        if UIS:IsKeyDown(Enum.KeyCode.LeftControl) then moveVec = moveVec + Vector3.new(0,-1,0) end
 
        local desired = forward*moveVec.Z + right*moveVec.X + Vector3.new(0,moveVec.Y,0)
        if desired.Magnitude>0 then desired = desired.Unit*flySpeed end
        vv.Velocity = desired
 
        if desired.Magnitude>1 then
            local flatDir = Vector3.new(desired.X,0,desired.Z)
            if flatDir.Magnitude>0 then gyro.CFrame = CFrame.new(hrp.Position,hrp.Position+flatDir) end
        end
    end
end)
 
RS.Stepped:Connect(function()
    if noclipEnabled and char then
        for _,part in pairs(char:GetDescendants()) do
            if part:IsA("BasePart") then part.CanCollide=false end
        end
    end
end)
 
UIS.JumpRequest:Connect(function()
    if infJumpEnabled and humanoid then
        humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
    end
end)

UIS.JumpRequest:Connect(function()
    if infJumpEnabled and humanoid then
        humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
    end
end)
 
-- ‡∏£‡∏µ‡πÄ‡∏ã‡πá‡∏ï‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£‡πÄ‡∏°‡∏∑‡πà‡∏≠ respawn
player.CharacterAdded:Connect(function(c)
    char=c
    hrp=c:WaitForChild("HumanoidRootPart")
    humanoid=c:WaitForChild("Humanoid")
    stopFlying()
    toggleSpeed(false)
    noclipEnabled=false
    infJumpEnabled=false
    Rayfield:LoadConfiguration()
end)

--// === TELEPORT TAB ===
local friendTab = Window:CreateTab("||   - Teleport", "map-pin")
local Section1 = friendTab:CreateSection("")

local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local player = Players.LocalPlayer

-- ‡∏ï‡∏±‡∏ß‡πÅ‡∏õ‡∏£‡πÄ‡∏Å‡πá‡∏ö‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á‡πÄ‡∏î‡∏¥‡∏°‡∏Å‡πà‡∏≠‡∏ô‡∏ß‡∏≤‡∏õ
local savedCFrame = nil

-- ‡∏ï‡∏±‡∏ß‡πÅ‡∏õ‡∏£‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö Auto Teleport
local autoTPConnection = nil
local autoTargetName = nil

-- ===== ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏ä‡πà‡∏ß‡∏¢ =====
local function getPlayerList()
    local list = {}
    for _, p in ipairs(Players:GetPlayers()) do
        if p ~= player then
            table.insert(list, string.format("%s (%s)", p.DisplayName, p.Name))
        end
    end
    table.sort(list)
    return list
end

-- ===== Dropdown ‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô =====
local friendDropdown = friendTab:CreateDropdown({
    Name = "[ üë• ]  Dropdown Player",
    Options = getPlayerList(),
    CurrentOption = {""},
    Flag = "FriendSelect",
    Callback = function() end
})

-- ‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï Dropdown ‡πÄ‡∏°‡∏∑‡πà‡∏≠‡∏°‡∏µ‡∏Ñ‡∏ô‡πÄ‡∏Ç‡πâ‡∏≤-‡∏≠‡∏≠‡∏Å
Players.PlayerAdded:Connect(function() 
    friendDropdown:Refresh(getPlayerList(), true)
end)
Players.PlayerRemoving:Connect(function() 
    task.wait(1)
    friendDropdown:Refresh(getPlayerList(), true)
end)

-- ===== ‡∏õ‡∏∏‡πà‡∏°‡∏ß‡∏≤‡∏õ‡∏ò‡∏£‡∏£‡∏°‡∏î‡∏≤ (‡∏Ñ‡∏£‡∏±‡πâ‡∏á‡πÄ‡∏î‡∏µ‡∏¢‡∏ß) =====
friendTab:CreateButton({
    Name = "[ üìå ]  Teleport To Player",
    Callback = function()
        local selected = friendDropdown.CurrentOption[1]
        if not selected or selected == "" then
            return  -- ‡πÑ‡∏°‡πà‡πÅ‡∏à‡πâ‡∏á‡πÄ‡∏ï‡∏∑‡∏≠‡∏ô‡∏≠‡∏∞‡πÑ‡∏£‡πÄ‡∏•‡∏¢
        end

        local friendName = selected:match("%((.-)%)") or selected
        local targetPlayer = Players:FindFirstChild(friendName)
        local char = player.Character or player.CharacterAdded:Wait()
        local hrp = char:WaitForChild("HumanoidRootPart", 5)

        if not hrp then return end

        if targetPlayer and targetPlayer.Character then
            local targetHRP = targetPlayer.Character:FindFirstChild("HumanoidRootPart")
            if targetHRP then
                savedCFrame = hrp.CFrame
                hrp.CFrame = targetHRP.CFrame + Vector3.new(0, 5, 0)  -- ‡∏ß‡∏≤‡∏õ‡πÉ‡∏Å‡∏•‡πâ ‡πÜ (‡∏¢‡∏Å‡∏Ç‡∏∂‡πâ‡∏ô‡∏ô‡∏¥‡∏î‡∏´‡∏ô‡πà‡∏≠‡∏¢)
            end
        end
        -- ‡πÑ‡∏°‡πà‡∏°‡∏µ Notify
    end
})

local Paragraph = friendTab:CreateParagraph({Title = "Teleport To Player / Friend Normal", Content = "Teleport To Player / Friend Become Normal"})

local Section1 = friendTab:CreateSection("")

-- ===== ‡∏õ‡∏∏‡πà‡∏° Toggle Auto Teleport (‡πÄ‡∏õ‡∏¥‡∏î/‡∏õ‡∏¥‡∏î ‡∏™‡∏•‡∏±‡∏ö‡πÑ‡∏î‡πâ‡πÄ‡∏•‡∏¢) =====
friendTab:CreateToggle({
    Name = "[ ü™ë ]  Auto Teleport to Player",
    CurrentValue = false,
    Flag = "AutoTPToggle",
    Callback = function(Value)
        local selected = friendDropdown.CurrentOption[1]
        if not selected or selected == "" then
            Rayfield.Flags["AutoTPToggle"]:Set(false)  -- ‡∏õ‡∏¥‡∏î‡πÄ‡∏á‡∏µ‡∏¢‡∏ö ‡πÜ
            return
        end

        local friendName = selected:match("%((.-)%)") or selected
        local targetPlayer = Players:FindFirstChild(friendName)

        if Value == true then
            -- === ‡πÄ‡∏õ‡∏¥‡∏î Auto Teleport ===
            if not targetPlayer or not targetPlayer.Character then
                Rayfield.Flags["AutoTPToggle"]:Set(false)
                return
            end

            local char = player.Character or player.CharacterAdded:Wait()
            local hrp = char:FindFirstChild("HumanoidRootPart")
            if not hrp then 
                Rayfield.Flags["AutoTPToggle"]:Set(false)
                return 
            end

            -- ‡∏ö‡∏±‡∏ô‡∏ó‡∏∂‡∏Å‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á‡πÄ‡∏î‡∏¥‡∏° (‡πÄ‡∏á‡∏µ‡∏¢‡∏ö ‡πÜ)
            if not savedCFrame then
                savedCFrame = hrp.CFrame
            end

            autoTargetName = friendName

            -- ‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏ï‡∏¥‡∏î‡∏ï‡∏≤‡∏°‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï‡∏¥
            autoTPConnection = RunService.Heartbeat:Connect(function()
                if not autoTargetName then return end

                local target = Players:FindFirstChild(autoTargetName)
                if target and target.Character then
                    local targetHRP = target.Character:FindFirstChild("HumanoidRootPart")
                    if targetHRP and hrp then
                        hrp.CFrame = targetHRP.CFrame + Vector3.new(0, 4, 0)  -- ‡∏õ‡∏£‡∏±‡∏ö‡πÄ‡∏õ‡πá‡∏ô -4 ‡∏´‡∏£‡∏∑‡∏≠ -50 ‡πÑ‡∏î‡πâ‡∏ï‡∏≤‡∏°‡∏ï‡πâ‡∏≠‡∏á‡∏Å‡∏≤‡∏£
                    end
                end
            end)

        else
            -- === ‡∏õ‡∏¥‡∏î Auto Teleport ===
            if autoTPConnection then
                autoTPConnection:Disconnect()
                autoTPConnection = nil
                autoTargetName = nil
            end
        end
        -- ‡πÑ‡∏°‡πà‡∏°‡∏µ Notify ‡πÉ‡∏î ‡πÜ
    end
})

-- ===== ‡∏õ‡∏∏‡πà‡∏°‡∏Å‡∏•‡∏±‡∏ö‡∏à‡∏∏‡∏î‡πÄ‡∏î‡∏¥‡∏° =====
friendTab:CreateButton({
    Name = "[ üì∑ ]  Return To The Point",
    Callback = function()
        local char = player.Character or player.CharacterAdded:Wait()
        local hrp = char:FindFirstChild("HumanoidRootPart")

        if not hrp then return end

        if savedCFrame then
            hrp.CFrame = savedCFrame
        end
        -- ‡πÑ‡∏°‡πà‡∏°‡∏µ Notify
    end
})

local Paragraph = friendTab:CreateParagraph({Title = "Teleport To Player / Friend Auto", Content = "Teleport To Player / Friend Become Auto"})

-- ‡∏´‡∏¢‡∏∏‡∏î Auto ‡πÄ‡∏°‡∏∑‡πà‡∏≠‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£‡∏ï‡∏≤‡∏¢‡∏´‡∏£‡∏∑‡∏≠‡∏£‡∏µ‡∏™‡∏õ‡∏≤‡∏ß‡∏ô‡πå
player.CharacterRemoving:Connect(function()
    if autoTPConnection then
        autoTPConnection:Disconnect()
        autoTPConnection = nil
        autoTargetName = nil
        Rayfield.Flags["AutoTPToggle"]:Set(false)  -- ‡∏õ‡∏¥‡∏î Toggle ‡πÄ‡∏á‡∏µ‡∏¢‡∏ö ‡πÜ
    end
end)

-- ‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï‡∏¥‡πÄ‡∏°‡∏∑‡πà‡∏≠‡∏°‡∏µ‡∏Ñ‡∏ô‡πÄ‡∏Ç‡πâ‡∏≤/‡∏≠‡∏≠‡∏Å
Players.PlayerAdded:Connect(function()
    friendDropdown:Refresh(updatePlayerList(), true)
end)

Players.PlayerRemoving:Connect(function()
    task.wait(1)
    friendDropdown:Refresh(updatePlayerList(), true)
end)

-- ‡∏´‡∏¢‡∏∏‡∏î‡∏ß‡∏≤‡∏õ + ‡∏£‡∏µ‡πÄ‡∏ã‡πá‡∏ï‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á‡πÄ‡∏î‡∏¥‡∏°‡πÄ‡∏°‡∏∑‡πà‡∏≠‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£‡∏ï‡∏≤‡∏¢‡∏´‡∏£‡∏∑‡∏≠‡∏£‡∏µ‡∏™‡∏õ‡∏≤‡∏ß‡∏ô‡πå (‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏Ñ‡∏ß‡∏≤‡∏°‡∏õ‡∏•‡∏≠‡∏î‡∏†‡∏±‡∏¢)
player.CharacterRemoving:Connect(function()
    if autoTPConnection then
        autoTPConnection:Disconnect()
        autoTPConnection = nil
    end
    -- ‡πÑ‡∏°‡πà‡∏£‡∏µ‡πÄ‡∏ã‡πá‡∏ï originalPosition ‡∏ñ‡πâ‡∏≤‡∏≠‡∏¢‡∏≤‡∏Å‡πÄ‡∏Å‡πá‡∏ö‡πÑ‡∏ß‡πâ‡∏Ç‡πâ‡∏≤‡∏°‡∏Å‡∏≤‡∏£‡∏ï‡∏≤‡∏¢ ‡πÅ‡∏ï‡πà‡∏ñ‡πâ‡∏≤‡∏≠‡∏¢‡∏≤‡∏Å‡∏£‡∏µ‡πÄ‡∏ã‡πá‡∏ï‡πÉ‡∏´‡πâ uncomment ‡∏ö‡∏£‡∏£‡∏ó‡∏±‡∏î‡∏•‡πà‡∏≤‡∏á
    -- originalPosition = nil
end)

local Section1 = friendTab:CreateSection("")

--// === ESP TAB (Local ESP - Rainbow Edition) ===
local espTab = Window:CreateTab("||   - Attacks Smash", "drill")
local espSection = espTab:CreateSection("")

local ESPEnabled = false
local ESPObjects = {}
local rainbowHue = 0

local function getRainbowColor()
    rainbowHue = (rainbowHue + 0.001) % 1
    return Color3.fromHSV(rainbowHue, 1, 1)
end

local function createESP(player)
    if player == LocalPlayer then return end
    local char = player.Character
    if not char then return end

    local hrp = char:FindFirstChild("HumanoidRootPart") or char:FindFirstChild("Torso")
    if not hrp then return end

    -- Rainbow Box ESP
    local box = Drawing.new("Square")
    box.Thickness = 2
    box.Filled = false
    box.Transparency = 1
    box.Visible = false

    -- Rainbow Name ESP
    local name = Drawing.new("Text")
    name.Text = player.DisplayName
    name.Size = 16
    name.Center = true
    name.Outline = true
    name.Font = Drawing.Fonts.UI
    name.Visible = false

    -- Health Bar (Rainbow Border)
    local healthBG = Drawing.new("Square")
    healthBG.Thickness = 1
    healthBG.Filled = true
    healthBG.Color = Color3.fromRGB(0, 0, 0)
    healthBG.Transparency = 0.5
    healthBG.Visible = false

    local healthFG = Drawing.new("Square")
    healthFG.Thickness = 1
    healthFG.Filled = true
    healthFG.Visible = false

    ESPObjects[player] = {box = box, name = name, healthBG = healthBG, healthFG = healthFG, lastChar = char}
end

local function updateESP()
    if not ESPEnabled then return end

    local cam = Workspace.CurrentCamera
    local rainbowColor = getRainbowColor()

    for player, esp in pairs(ESPObjects) do
        local char = player.Character
        if char and char:FindFirstChild("Humanoid") and char:FindFirstChild("HumanoidRootPart") then
            local hrp = char.HumanoidRootPart
            local hum = char.Humanoid
            local head = char:FindFirstChild("Head")
            if not head then continue end

            local rootPos, onScreen = cam:WorldToViewportPoint(hrp.Position)
            local headPos = cam:WorldToViewportPoint(head.Position + Vector3.new(0, 0.5, 0))
            local legPos = cam:WorldToViewportPoint(hrp.Position - Vector3.new(0, 3, 0))

            if onScreen then
                local height = math.abs(headPos.Y - legPos.Y)
                local width = height * 0.5

                -- Rainbow Box
                esp.box.Size = Vector2.new(width, height)
                esp.box.Position = Vector2.new(rootPos.X - width/2, rootPos.Y - height/2)
                esp.box.Color = rainbowColor
                esp.box.Visible = true

                -- Rainbow Name
                esp.name.Position = Vector2.new(rootPos.X, rootPos.Y - height/2 - 20)
                esp.name.Color = rainbowColor
                esp.name.Visible = true

                -- Health Bar
                local health = hum.Health / hum.MaxHealth
                local barHeight = height
                local barWidth = 4

                esp.healthBG.Size = Vector2.new(barWidth, barHeight)
                esp.healthBG.Position = Vector2.new(rootPos.X - width/2 - barWidth - 2, rootPos.Y - height/2)

                esp.healthFG.Size = Vector2.new(barWidth, barHeight * health)
                esp.healthFG.Position = Vector2.new(rootPos.X - width/2 - barWidth - 2, rootPos.Y - height/2 + (barHeight * (1 - health)))
                esp.healthFG.Color = Color3.fromHSV(health * 0.33, 1, 1)

                esp.healthBG.Visible = true
                esp.healthFG.Visible = true
            else
                esp.box.Visible = false
                esp.name.Visible = false
                esp.healthBG.Visible = false
                esp.healthFG.Visible = false
            end
        else
            esp.box.Visible = false
            esp.name.Visible = false
            esp.healthBG.Visible = false
            esp.healthFG.Visible = false
        end
    end
end

