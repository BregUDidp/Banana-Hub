local Section = PTab:CreateSection("")

local SittingEnabled = false
local SitConnection = nil
local player = game.Players.LocalPlayer

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏ö‡∏±‡∏á‡∏Ñ‡∏±‡∏ö‡∏ô‡∏±‡πà‡∏á
local function ForceSit()
    local character = player.Character or player.CharacterAdded:Wait()
    local humanoid = character:FindFirstChildOfClass("Humanoid")
    
    if humanoid then
        -- ‡∏ö‡∏±‡∏á‡∏Ñ‡∏±‡∏ö‡πÉ‡∏´‡πâ Humanoid ‡∏ô‡∏±‡πà‡∏á (‡πÅ‡∏°‡πâ‡πÑ‡∏°‡πà‡∏°‡∏µ Seat)
        humanoid.Sit = true
        
        -- ‡∏õ‡πâ‡∏≠‡∏á‡∏Å‡∏±‡∏ô‡πÑ‡∏°‡πà‡πÉ‡∏´‡πâ‡∏•‡∏∏‡∏Å‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï‡∏¥
        if humanoid.SeatPart then
            humanoid.SeatPart = nil
        end
    end
end

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡πÄ‡∏õ‡∏¥‡∏î/‡∏õ‡∏¥‡∏î ‡∏Å‡∏≤‡∏£‡∏ô‡∏±‡πà‡∏á‡∏Ñ‡πâ‡∏≤‡∏á
local function ToggleSit(enabled)
    SittingEnabled = enabled
    
    if enabled then
        ForceSit()
        
        -- ‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï‡∏ó‡∏∏‡∏Å‡∏Ñ‡∏£‡∏±‡πâ‡∏á‡∏ó‡∏µ‡πà‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£‡πÄ‡∏Å‡∏¥‡∏î‡πÉ‡∏´‡∏°‡πà ‡∏´‡∏£‡∏∑‡∏≠‡πÄ‡∏õ‡∏•‡∏µ‡πà‡∏¢‡∏ô‡∏™‡∏ñ‡∏≤‡∏ô‡∏∞
        SitConnection = game:GetService("RunService").Heartbeat:Connect(function()
            if SittingEnabled then
                local character = player.Character
                if character then
                    local humanoid = character:FindFirstChildOfClass("Humanoid")
                    if humanoid and humanoid.Sit == false then
                        humanoid.Sit = true
                    end
                end
            end
        end)
        
        -- ‡∏ñ‡πâ‡∏≤‡∏ï‡∏≤‡∏¢‡πÅ‡∏•‡πâ‡∏ß‡πÄ‡∏Å‡∏¥‡∏î‡πÉ‡∏´‡∏°‡πà ‡πÉ‡∏´‡πâ‡∏ô‡∏±‡πà‡∏á‡∏ï‡πà‡∏≠‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï‡∏¥
        player.CharacterAdded:Connect(function()
            if SittingEnabled then
                task.wait(0.5) -- ‡∏£‡∏≠‡πÉ‡∏´‡πâ‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£‡πÇ‡∏´‡∏•‡∏î‡πÄ‡∏™‡∏£‡πá‡∏à
                ForceSit()
            end
        end)
        
        
        
    else
        -- ‡∏õ‡∏¥‡∏î‡∏Å‡∏≤‡∏£‡∏ô‡∏±‡πà‡∏á
        if SitConnection then
            SitConnection:Disconnect()
            SitConnection = nil
        end
        
        local character = player.Character
        if character then
            local humanoid = character:FindFirstChildOfClass("Humanoid")
            if humanoid then
                humanoid.Sit = false
            end
        end
        
        
    end
end

local Toggle = PTab:CreateToggle({
   Name = "[ ü™ë ]  Forced To Sitting",
   CurrentValue = false,
   Flag = "Toggle1",
   Callback = function(Value)
       ToggleSit(Value)
       Rayfield:Notify({
            Title = Value and   "[ ü™ë ]  Forced To Sitting ON" or "[ ü™ë ]  Forced To Sitting OFF",
            Content = Value and "[ ‚úÖ ]  Forced To Sitting ON" or "[ üõë ]  Forced To Sitting OFF",
            Duration = 1,
            Image = "rocking-chair"
        })
   end,
})

