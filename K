--// === ESP TAB (Local ESP - Rainbow Edition) ===
local espTab = Window:CreateTab("||   - Solution", "flask-conical")
local Section = espTab:CreateSection("")

local MouseTPEnabled = false
local Mouse = game.Players.LocalPlayer:GetMouse()
local Player = game.Players.LocalPlayer

-- ‡∏™‡∏£‡πâ‡∏≤‡∏á Sound ‡πÑ‡∏ß‡πâ‡πÉ‡∏ä‡πâ‡∏ã‡πâ‡∏≥‡πÑ‡∏î‡πâ‡πÄ‡∏£‡∏∑‡πà‡∏≠‡∏¢ ‡πÜ
local teleportSound = Instance.new("Sound")
teleportSound.SoundId = "rbxassetid://4861638982"
teleportSound.Volume = 0.7

local Toggle = espTab:CreateToggle({
    Name = "[ üëÜ ]  Clicking Teleport   [ - Backpack - ]",
    CurrentValue = false,
    Flag = "MouseTP",
    Callback = function(Value)
        MouseTPEnabled = Value
        Rayfield:Notify({
            Title = Value and   "[ üëÜ ]  Teleport Clicking On" or "[ üëÜ ]  Teleport Clicking Off",
            Content = Value and "[ ‚úÖ ]  Click Teleport On" or "[ üõë ]  Click Teleport Off",
            Duration = 1,
            Image = "mouse"
        })
    end,
})

-- ‡∏Å‡∏î‡∏Ñ‡∏•‡∏¥‡∏Å = ‡∏ß‡∏≤‡∏£‡πå‡∏õ‡∏ó‡∏±‡∏ô‡∏ó‡∏µ + ‡πÄ‡∏™‡∏µ‡∏¢‡∏á‡∏î‡∏±‡∏á‡∏û‡∏£‡πâ‡∏≠‡∏°‡∏Å‡∏±‡∏ô
Mouse.Button1Down:Connect(function()
    if MouseTPEnabled and Player.Character and Player.Character:FindFirstChild("HumanoidRootPart") then
        local targetPos = Mouse.Hit.Position + Vector3.new(0, 5, 0)
        
        -- ‡∏ß‡∏≤‡∏£‡πå‡∏õ‡∏Å‡πà‡∏≠‡∏ô‡πÄ‡∏•‡∏¢ ‡πÑ‡∏°‡πà‡∏ï‡πâ‡∏≠‡∏á‡∏£‡∏≠
        Player.Character.HumanoidRootPart.CFrame = CFrame.new(targetPos)
        
        -- ‡πÄ‡∏•‡πà‡∏ô‡πÄ‡∏™‡∏µ‡∏¢‡∏á‡∏ó‡∏±‡∏ô‡∏ó‡∏µ (‡πÄ‡∏™‡∏µ‡∏¢‡∏á‡∏à‡∏∞‡∏î‡∏±‡∏á‡∏à‡∏≤‡∏Å‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á‡πÉ‡∏´‡∏°‡πà‡πÄ‡∏•‡∏¢)
        teleportSound.Parent = Player.Character.HumanoidRootPart
        teleportSound:Play()
        
        -- ‡∏´‡∏•‡∏±‡∏á‡∏à‡∏≤‡∏Å‡πÄ‡∏•‡πà‡∏ô‡πÄ‡∏™‡∏£‡πá‡∏à ‡∏•‡∏ö‡∏≠‡∏≠‡∏Å‡πÄ‡∏û‡∏∑‡πà‡∏≠‡πÑ‡∏°‡πà‡πÉ‡∏´‡πâ‡∏Å‡∏¥‡∏ô memory
        task.delay(3, function()
            teleportSound.Parent = nil
        end)
    end
end)

-- ‡∏£‡∏µ‡πÇ‡∏´‡∏•‡∏î‡∏™‡∏ñ‡∏≤‡∏ô‡∏∞‡∏ï‡∏≠‡∏ô‡πÄ‡∏Å‡∏¥‡∏î‡πÉ‡∏´‡∏°‡πà
Player.CharacterAdded:Connect(function()
    task.wait(1)
    MouseTPEnabled = Rayfield.Flags.MouseTP.CurrentValue
end)

local Paragraph = espTab:CreateParagraph({Title = "Clicking Teleport For Player / Friend", Content = "Clicking Teleport Is Good For Building"})

local Section = espTab:CreateSection("")

local PurpleTool = {
    Enabled = false,
    Current = nil,
    Highlight = nil,
    Connections = {},
}

-- ‡∏™‡∏µ‡∏°‡πà‡∏ß‡∏á‡∏ó‡∏µ‡πà‡∏ï‡πâ‡∏≠‡∏á‡∏Å‡∏≤‡∏£ (138, 43, 179)
local PURPLE_COLOR = Color3.fromRGB(138, 43, 179)

local function CreateHighlight(tool)
    if PurpleTool.Highlight then PurpleTool.Highlight:Destroy() end
    PurpleTool.Highlight = Instance.new("Highlight")
    PurpleTool.Highlight.FillColor = PURPLE_COLOR
    PurpleTool.Highlight.OutlineColor = PURPLE_COLOR
    PurpleTool.Highlight.FillTransparency = 0.3
    PurpleTool.Highlight.OutlineTransparency = 0
    PurpleTool.Highlight.Parent = tool
    PurpleTool.Highlight.Adornee = tool:FindFirstChild("Handle") or tool
end

local function ApplyPurpleColor()
    if not PurpleTool.Enabled or not PurpleTool.Current or not PurpleTool.Highlight then return end
    
    -- ‡∏ï‡∏±‡πâ‡∏á‡∏Ñ‡πà‡∏≤‡∏™‡∏µ Highlight
    PurpleTool.Highlight.FillColor = PURPLE_COLOR
    PurpleTool.Highlight.OutlineColor = PURPLE_COLOR
    
    -- ‡πÄ‡∏õ‡∏•‡∏µ‡πà‡∏¢‡∏ô‡∏™‡∏µ‡∏ó‡∏∏‡∏Å Part ‡πÉ‡∏ô Tool (‡∏¢‡∏Å‡πÄ‡∏ß‡πâ‡∏ô HumanoidRootPart)
    for _, part in pairs(PurpleTool.Current:GetDescendants()) do
        if part:IsA("BasePart") and part.Name ~= "HumanoidRootPart" then
            part.Color = PURPLE_COLOR
        end
    end
end

local function OnEquipped(tool)
    if not PurpleTool.Enabled then return end
    PurpleTool.Current = tool
    task.wait(0.1)
    CreateHighlight(tool)
    ApplyPurpleColor()  -- ‡πÉ‡∏ä‡πâ‡∏™‡∏µ‡∏°‡πà‡∏ß‡∏á‡∏ó‡∏±‡∏ô‡∏ó‡∏µ
end

local function OnUnequipped()
    if PurpleTool.Highlight then 
        PurpleTool.Highlight:Destroy() 
        PurpleTool.Highlight = nil 
    end
    PurpleTool.Current = nil
end

local function SetupTool(char)
    -- ‡∏•‡πâ‡∏≤‡∏á connection ‡πÄ‡∏Å‡πà‡∏≤
    for _, c in pairs(PurpleTool.Connections) do c:Disconnect() end
    PurpleTool.Connections = {}

    local function ConnectTool(tool)
        table.insert(PurpleTool.Connections, tool.Equipped:Connect(function() OnEquipped(tool) end))
        table.insert(PurpleTool.Connections, tool.Unequipped:Connect(OnUnequipped))
    end

    -- ‡πÄ‡∏ä‡∏∑‡πà‡∏≠‡∏° Tool ‡∏ó‡∏µ‡πà‡∏≠‡∏¢‡∏π‡πà‡πÉ‡∏ô‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£‡∏ï‡∏≠‡∏ô‡∏ô‡∏µ‡πâ
    for _, child in pairs(char:GetChildren()) do
        if child:IsA("Tool") then ConnectTool(child) end
    end
    table.insert(PurpleTool.Connections, char.ChildAdded:Connect(function(c)
        if c:IsA("Tool") then ConnectTool(c) end
    end))

    -- ‡πÄ‡∏ä‡∏∑‡πà‡∏≠‡∏° Tool ‡πÉ‡∏ô Backpack
    local backpack = LocalPlayer:WaitForChild("Backpack")
    for _, child in pairs(backpack:GetChildren()) do
        if child:IsA("Tool") then ConnectTool(child) end
    end
    table.insert(PurpleTool.Connections, backpack.ChildAdded:Connect(function(c)
        if c:IsA("Tool") then ConnectTool(c) end
    end))
end

local function TogglePurpleTool(state)
    PurpleTool.Enabled = state
    if state then
        Rayfield:Notify({
           Title = "[ ‚úÖ ]  Purple Tools On",
           Content = "[ üü™ ]  Purple Tools Finish",
           Duration = 1,
           Image = "grape",
         })
        if LocalPlayer.Character then SetupTool(LocalPlayer.Character) end
    else
        Rayfield:Notify({
           Title = "[ ‚ùå ] Purple Tools Off",
           Content = "[ üü™ ] Purple Tools Finish",
           Duration = 1,
           Image = "grape",
         })
        if PurpleTool.Highlight then 
            PurpleTool.Highlight:Destroy() 
            PurpleTool.Highlight = nil 
        end
        PurpleTool.Current = nil
    end
end

-- ‡∏£‡∏≠‡∏á‡∏£‡∏±‡∏ö‡∏Å‡∏≤‡∏£‡πÄ‡∏Å‡∏¥‡∏î‡πÉ‡∏´‡∏°‡πà
LocalPlayer.CharacterAdded:Connect(SetupTool)
if LocalPlayer.Character then SetupTool(LocalPlayer.Character) end

-- Toggle ‡πÉ‡∏ô UI (‡πÄ‡∏õ‡∏•‡∏µ‡πà‡∏¢‡∏ô‡∏ä‡∏∑‡πà‡∏≠‡πÉ‡∏´‡πâ‡πÄ‡∏Ç‡πâ‡∏≤‡∏Å‡∏±‡∏ö‡∏ò‡∏µ‡∏°‡∏°‡πà‡∏ß‡∏á)
espTab:CreateToggle({
    Name = "[ üü™ ]  Purple Glowing Tool  [ - Permanent Purple - ]",
    CurrentValue = false,
    Callback = function(v) 
        TogglePurpleTool(v) 
    end
})

local RainbowGun = {
    Enabled = false,
    Current = nil,
    Highlight = nil,
    Hue = 0,
    Connections = {},
    Loop = nil
}

local function CreateHighlight(tool)
    if RainbowGun.Highlight then RainbowGun.Highlight:Destroy() end
    RainbowGun.Highlight = Instance.new("Highlight")
    RainbowGun.Highlight.FillTransparency = 0.3
    RainbowGun.Highlight.OutlineTransparency = 0
    RainbowGun.Highlight.Parent = tool
    RainbowGun.Highlight.Adornee = tool:FindFirstChild("Handle") or tool
end

local function UpdateRainbowGun()
    if not RainbowGun.Enabled or not RainbowGun.Current or not RainbowGun.Highlight then return end
    RainbowGun.Hue = (RainbowGun.Hue + 0.01) % 1
    local color = Color3.fromHSV(RainbowGun.Hue, 1, 1)
    RainbowGun.Highlight.FillColor = color
    RainbowGun.Highlight.OutlineColor = color
    for _, part in pairs(RainbowGun.Current:GetDescendants()) do
        if part:IsA("BasePart") and part.Name ~= "HumanoidRootPart" then
            part.Color = color
        end
    end
end

local function OnEquipped(tool)
    if not RainbowGun.Enabled then return end
    RainbowGun.Current = tool
    task.wait(0.1)
    CreateHighlight(tool)
end

local function OnUnequipped()
    if RainbowGun.Highlight then RainbowGun.Highlight:Destroy() RainbowGun.Highlight = nil end
    RainbowGun.Current = nil
end

local function SetupGun(char)
    for _, c in pairs(RainbowGun.Connections) do c:Disconnect() end
    RainbowGun.Connections = {}

    local function ConnectTool(tool)
        table.insert(RainbowGun.Connections, tool.Equipped:Connect(function() OnEquipped(tool) end))
        table.insert(RainbowGun.Connections, tool.Unequipped:Connect(OnUnequipped))
    end

    for _, child in pairs(char:GetChildren()) do
        if child:IsA("Tool") then ConnectTool(child) end
    end
    table.insert(RainbowGun.Connections, char.ChildAdded:Connect(function(c)
        if c:IsA("Tool") then ConnectTool(c) end
    end))

    local backpack = LocalPlayer:WaitForChild("Backpack")
    for _, child in pairs(backpack:GetChildren()) do
        if child:IsA("Tool") then ConnectTool(child) end
    end
    table.insert(RainbowGun.Connections, backpack.ChildAdded:Connect(function(c)
        if c:IsA("Tool") then ConnectTool(c) end
    end))
end

local function ToggleRainbowGun(state)
    RainbowGun.Enabled = state
    if state then
        Rayfield:Notify({
           Title = "[ ‚úÖ ]  Rainbow Tools On",
           Content = "[ üåà ]  Rainbow Tools Finish",
           Duration = 1,
           Image = "rainbow",
         })
        if not RainbowGun.Loop then
            RainbowGun.Loop = RunService.Heartbeat:Connect(UpdateRainbowGun)
        end
        if LocalPlayer.Character then SetupGun(LocalPlayer.Character) end
    else
        Rayfield:Notify({
           Title = "[ ‚ùå ]  Rainbow Tools Off",
           Content = "[ üåà ]  Rainbow Tools Finish",
           Duration = 1,
           Image = "rainbow",
         })
        if RainbowGun.Loop then RainbowGun.Loop:Disconnect() RainbowGun.Loop = nil end
        if RainbowGun.Highlight then RainbowGun.Highlight:Destroy() RainbowGun.Highlight = nil end
        RainbowGun.Current = nil
    end
end

LocalPlayer.CharacterAdded:Connect(SetupGun)
if LocalPlayer.Character then SetupGun(LocalPlayer.Character) end

espTab:CreateToggle({
    Name = "[ üåà ]  Rainbow Mountain Tool  [ - Permanent Rainbow - ]",
    CurrentValue = false,
    Callback = function(v) ToggleRainbowGun(v) end
})

Players.PlayerAdded:Connect(function(p)
    if ESPEnabled and p ~= LocalPlayer then
        p.CharacterAdded:Connect(function() createESP(p) end)
    end
end)

Players.PlayerRemoving:Connect(function(p)
    if ESPObjects[p] then
        for _, obj in pairs(ESPObjects[p]) do
            if typeof(obj) == "Instance" and obj.Remove then
                obj:Remove()
            end
        end
        ESPObjects[p] = nil
    end
end)

local Paragraph = espTab:CreateParagraph({Title = "Purple Tool For Cool Style", Content = "Purple / Rainbow Tool Is Good For Style"})

RunService.RenderStepped:Connect(updateESP)

local Section = espTab:CreateSection("")

-- ‡πÇ‡∏´‡∏•‡∏î‡∏ö‡∏£‡∏¥‡∏Å‡∏≤‡∏£‡∏ó‡∏µ‡πà‡∏à‡∏≥‡πÄ‡∏õ‡πá‡∏ô
local RunService = game:GetService("RunService")
local Players = game:GetService("Players")
local UserInputService = game:GetService("UserInputService")
local Workspace = game:GetService("Workspace")

-- ‡∏ï‡∏±‡∏ß‡πÅ‡∏õ‡∏£‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏ï‡πâ‡∏ô
local swingSpeed = 80
local swingGravity = 50
local maxWebDistance = 1500

local swingConnection
local inputConnection1
local inputConnection2

local isSwinging = false
local webPoint = nil

local webBeam = nil          -- Beam ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡πÄ‡∏™‡πâ‡∏ô‡πÉ‡∏¢
local playerAttachment = nil -- Attachment ‡∏ó‡∏µ‡πà‡∏°‡∏∑‡∏≠/‡πÅ‡∏Ç‡∏ô‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô
local anchorAttachment = nil -- Attachment ‡∏ó‡∏µ‡πà‡∏à‡∏∏‡∏î‡∏ï‡∏¥‡∏î‡πÉ‡∏¢

-- Toggle ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö Spider-Man Swing
local Toggle = espTab:CreateToggle({
    Name = "[ ‚õèÔ∏è ]  Swinging Webbing Block  [ - R6 / R15 - ]",
    CurrentValue = false,
    Flag = "SpiderSwing",
    Callback = function(Value)
        Rayfield:Notify({
            Title = Value and   "[ ‚õèÔ∏è ]  Swinging Mode On" or "[ ‚õèÔ∏è ]  Swinging Mode Off",
            Content = Value and "[ ‚úÖ ]  Swing Enable!" or "[ üõë ]  Swing Disabled!",
            Duration = 1,
            Image = "pickaxe"
        })

        local LP = Players.LocalPlayer

        if Value then
            -- ‡∏£‡∏≠ Character
            if not LP.Character then
                LP.CharacterAdded:Wait()
            end

            local Char = LP.Character
            local Hum = Char:FindFirstChild("Humanoid")
            if not Hum then return end

            -- ‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö RigType ‡πÄ‡∏û‡∏∑‡πà‡∏≠‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡∏™‡πà‡∏ß‡∏ô‡∏°‡∏∑‡∏≠‡∏Ç‡∏ß‡∏≤
            local rightHandPart
            if Hum.RigType == Enum.HumanoidRigType.R15 then
                rightHandPart = Char:FindFirstChild("RightHand") or Char:WaitForChild("RightHand")
            else -- R6
                rightHandPart = Char:FindFirstChild("Right Arm") or Char:WaitForChild("Right Arm")
            end

            -- ‡∏™‡∏£‡πâ‡∏≤‡∏á Attachment ‡∏ó‡∏µ‡πà‡∏°‡∏∑‡∏≠‡∏Ç‡∏ß‡∏≤ (‡∏´‡∏£‡∏∑‡∏≠‡πÅ‡∏Ç‡∏ô‡∏Ç‡∏ß‡∏≤‡πÉ‡∏ô R6)
            playerAttachment = Instance.new("Attachment")
            playerAttachment.Name = "WebAttachment_Player"
            playerAttachment.Parent = rightHandPart

            -- Heartbeat ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡∏Å‡∏≤‡∏£‡πÄ‡∏´‡∏ß‡∏µ‡πà‡∏¢‡∏á
            swingConnection = RunService.Heartbeat:Connect(function()
                if Char and Char:FindFirstChild("HumanoidRootPart") and Char:FindFirstChild("Humanoid") then
                    local Root = Char.HumanoidRootPart
                    local Hum = Char.Humanoid

                    if isSwinging and webPoint then
                        local direction = (webPoint - Root.Position).Unit

                        local velocity = direction * swingSpeed
                        Root.Velocity = Vector3.new(velocity.X, swingGravity, velocity.Z)

                        local lookCFrame = CFrame.new(Root.Position, webPoint)
                        Root.CFrame = Root.CFrame:Lerp(lookCFrame, 0.2)

                        Hum:ChangeState(Enum.HumanoidStateType.Freefall)
                    end
                end
            end)

            -- ‡∏Å‡∏î‡∏Ñ‡∏•‡∏¥‡∏Å‡∏ã‡πâ‡∏≤‡∏¢‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏¢‡∏¥‡∏á‡πÉ‡∏¢
            inputConnection1 = UserInputService.InputBegan:Connect(function(input, gameProcessed)
                if gameProcessed then return end
                if input.UserInputType == Enum.UserInputType.MouseButton1 then
                    if Char and Char:FindFirstChild("HumanoidRootPart") then
                        local camera = Workspace.CurrentCamera

                        local params = RaycastParams.new()
                        params.FilterDescendantsInstances = {Char}
                        params.FilterType = Enum.RaycastFilterType.Exclude

                        local rayResult = Workspace:Raycast(camera.CFrame.Position, camera.CFrame.LookVector * maxWebDistance, params)

                        if rayResult and rayResult.Position then
                            webPoint = rayResult.Position
                            isSwinging = true

                            -- ‡∏™‡∏£‡πâ‡∏≤‡∏á Attachment ‡∏ó‡∏µ‡πà‡∏à‡∏∏‡∏î‡∏ï‡∏¥‡∏î‡πÉ‡∏¢
                            anchorAttachment = Instance.new("Attachment")
                            anchorAttachment.Name = "WebAttachment_Anchor"
                            anchorAttachment.WorldPosition = webPoint
                            anchorAttachment.Parent = Workspace.Terrain

                            -- ‡∏™‡∏£‡πâ‡∏≤‡∏á Beam - ‡πÄ‡∏™‡πâ‡∏ô‡πÉ‡∏¢‡∏™‡∏µ‡∏°‡πà‡∏ß‡∏á‡πÄ‡∏Ç‡πâ‡∏° (138, 43, 179)
                            webBeam = Instance.new("Beam")
                            webBeam.Name = "SpiderWeb"
                            webBeam.Attachment0 = playerAttachment
                            webBeam.Attachment1 = anchorAttachment
                            
                            webBeam.Color = ColorSequence.new(Color3.fromRGB(138, 43, 179))
                            
                            webBeam.LightEmission = 1.5
                            webBeam.LightInfluence = 0
                            webBeam.Transparency = NumberSequence.new(0.3)
                            webBeam.Width0 = 0.4
                            webBeam.Width1 = 0.4
                            webBeam.Parent = Char

                            Rayfield:Notify({
                                Title = "[ ‚úÖ ]  Webing Block Attached!",
                                Content = "[ ‚õèÔ∏è ]  Found Block / Wall",
                                Duration = 1,
                                Image = "brick-wall"
                            })
                        else      
                            Rayfield:Notify({
                                Title = "[ ‚ùå ]  No Webing Block Point!",
                                Content = "[ ‚õèÔ∏è ]  No Found Block / Wall",
                                Duration = 1,
                                Image = "brick-wall"
                            })
                        end
                    end
                end
            end)

            -- ‡∏õ‡∏•‡πà‡∏≠‡∏¢‡∏Ñ‡∏•‡∏¥‡∏Å‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏ï‡∏±‡∏î‡πÉ‡∏¢
            inputConnection2 = UserInputService.InputEnded:Connect(function(input, gameProcessed)
                if gameProcessed then return end
                if input.UserInputType == Enum.UserInputType.MouseButton1 then
                    isSwinging = false
                    webPoint = nil

                    if webBeam then webBeam:Destroy() end
                    if anchorAttachment then anchorAttachment:Destroy() end
                    webBeam = nil
                    anchorAttachment = nil
                end
            end)

        else
            -- ‡∏õ‡∏¥‡∏î‡∏ó‡∏∏‡∏Å‡∏≠‡∏¢‡πà‡∏≤‡∏á
            if swingConnection then swingConnection:Disconnect() end
            if inputConnection1 then inputConnection1:Disconnect() end
            if inputConnection2 then inputConnection2:Disconnect() end

            isSwinging = false
            webPoint = nil

            if webBeam then webBeam:Destroy() end
            if anchorAttachment then anchorAttachment:Destroy() end
            if playerAttachment then playerAttachment:Destroy() end

            if LP.Character and LP.Character:FindFirstChild("Humanoid") then
                LP.Character.Humanoid:ChangeState(Enum.HumanoidStateType.Running)
            end
        end
    end,
})

-- Slider ‡∏ï‡πà‡∏≤‡∏á ‡πÜ (‡πÄ‡∏´‡∏°‡∏∑‡∏≠‡∏ô‡πÄ‡∏î‡∏¥‡∏°)
espTab:CreateSlider({
    Name = "[ üìè ]  Swinging Webbing Speed",
    Range = {50, 5000},
    Increment = 5,
    Suffix = " Swing Speed",
    CurrentValue = 80,
    Flag = "SwingSpeed",
    Callback = function(Value)
        swingSpeed = Value
    end
})

espTab:CreateSlider({
    Name = "[ üìè ]  Webbing Distance",
    Range = {50, 5000},
    Increment = 10,
    Suffix = " Swing Distance",
    CurrentValue = 1500,
    Flag = "WebDistance",
    Callback = function(Value)
        maxWebDistance = Value
    end
})

espTab:CreateSlider({
    Name = "[ ‚õèÔ∏è ]  Swinging Gravity Force",
    Range = {20, 5000},
    Increment = 5,
    Suffix = " Swing Jump",
    CurrentValue = 50,
    Flag = "SwingLift",
    Callback = function(Value)
        swingGravity = Value
    end
})

local Paragraph = espTab:CreateParagraph({Title = "Forging Block Player / Friend Sword", Content = "Forging Block Player / Friend Sword Are Multi Block"})

local Section = espTab:CreateSection("")

local HitboxEnabled = false
local HeadSize = 20
local connections = {}

espTab:CreateToggle({
    Name = "[ ‚öîÔ∏è ]  Hitbox Boxing Player   [ - Sword - ]   [ All ]",
    CurrentValue = false,
    Flag = "HitboxExpander",
    Callback = function(Value)
        HitboxEnabled = Value
        
        if Value then
            connections["Hitbox"] = game:GetService('RunService').RenderStepped:Connect(function()
                if not HitboxEnabled then return end
                
                for _, v in pairs(game:GetService('Players'):GetPlayers()) do
                    if v.Name ~= game.Players.LocalPlayer.Name and v.Character then
                        pcall(function()
                            local hrp = v.Character:FindFirstChild("HumanoidRootPart")
                            if hrp then
                                hrp.Size = Vector3.new(HeadSize, HeadSize, HeadSize)
                                hrp.Transparency = 0.7

                                -- ‡πÄ‡∏õ‡∏•‡∏µ‡πà‡∏¢‡∏ô‡πÄ‡∏õ‡πá‡∏ô‡∏™‡∏µ‡πÄ‡∏Ç‡∏µ‡∏¢‡∏ß RGB(138, 43, 179)
                                hrp.Color = Color3.fromRGB(138, 43, 179)
                                hrp.Material = "Neon"
                                hrp.CanCollide = false
                            end
                        end)
                    end
                end
            end)
            
            
        else
            if connections["Hitbox"] then
                connections["Hitbox"]:Disconnect()
            end
            
            -- Reset hitbox ‡∏Ç‡∏≠‡∏á‡∏ó‡∏∏‡∏Å‡∏Ñ‡∏ô‡∏Å‡∏•‡∏±‡∏ö‡∏Ñ‡πà‡∏≤‡πÄ‡∏î‡∏¥‡∏°
            for _, v in pairs(game:GetService('Players'):GetPlayers()) do
                if v.Character and v.Character:FindFirstChild("HumanoidRootPart") then
                    pcall(function()
                        local hrp = v.Character.HumanoidRootPart
                        hrp.Size = Vector3.new(2, 2, 1)
                        hrp.Transparency = 1
                        hrp.Material = "Plastic"
                        hrp.CanCollide = false
                    end)
                end
            end
            
           
        end
    end,
})

espTab:CreateSlider({
    Name = "[ üìè ]  Slider Hitbox Size   [ All ]",
    Range = {20, 5000},
    Increment = 1,
    Suffix = " Hitbox Boxing Size",
    CurrentValue = 20,
    Flag = "HeadSize",
    Callback = function(Value)
        HeadSize = Value
    end,
})

local Paragraph = espTab:CreateParagraph({Title = "Hitbox Player / Friend Sword", Content = "Hitbox Player / Friend Sword Are Multi Damage"})

local Section = espTab:CreateSection("")

local ESPEnabled = false
local Drawings = {}
local Highlights = {}

local RunService = game:GetService("RunService")
local Players = game:GetService("Players")

-- ‡∏™‡∏£‡πâ‡∏≤‡∏á‡∏´‡∏£‡∏∑‡∏≠‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï Drawing ESP ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö player
local function SetupDrawingESP(player)
    if player == Players.LocalPlayer then return end
    
    -- ‡∏•‡∏ö‡∏Ç‡∏≠‡∏á‡πÄ‡∏Å‡πà‡∏≤‡∏ñ‡πâ‡∏≤‡∏°‡∏µ
    if Drawings[player] then
        if Drawings[player].Box then Drawings[player].Box:Remove() end
        if Drawings[player].Name then Drawings[player].Name:Remove() end
    end
    
    local Box = Drawing.new("Square")
    Box.Thickness = 2
    Box.Filled = false
    Box.Color = Color3.fromRGB(138, 43, 179)
    Box.Visible = false

    local Name = Drawing.new("Text")
    Name.Size = 16
    Name.Center = true
    Name.Outline = true
    Name.Color = Color3.fromRGB(138, 43, 179)
    Name.Visible = false

    Drawings[player] = {Box = Box, Name = Name}
end

-- ‡∏™‡∏£‡πâ‡∏≤‡∏á‡∏´‡∏£‡∏∑‡∏≠‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï Highlight ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö player
local function SetupHighlight(player)
    if player == Players.LocalPlayer then return end
    
    -- ‡∏•‡∏ö‡∏Ç‡∏≠‡∏á‡πÄ‡∏Å‡πà‡∏≤‡∏ñ‡πâ‡∏≤‡∏°‡∏µ
    if Highlights[player] then
        if Highlights[player] and Highlights[player].Parent then
            Highlights[player]:Destroy()
        end
    end
    
    local character = player.Character
    if character then
        local highlight = Instance.new("Highlight")
        highlight.Parent = character
        highlight.FillColor = Color3.fromRGB(138, 43, 179)
        highlight.OutlineColor = Color3.fromRGB(138, 43, 179)
        highlight.FillTransparency = 0.4
        highlight.OutlineTransparency = 0
        highlight.Enabled = ESPEnabled  -- ‡πÄ‡∏õ‡∏¥‡∏î‡∏ó‡∏±‡∏ô‡∏ó‡∏µ‡∏ñ‡πâ‡∏≤ ESP ‡πÄ‡∏õ‡∏¥‡∏î‡∏≠‡∏¢‡∏π‡πà
        
        Highlights[player] = highlight
    end
end

-- ‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï Drawing ESP ‡∏ó‡∏∏‡∏Å‡πÄ‡∏ü‡∏£‡∏°
local function UpdateESP()
    if not ESPEnabled then return end
    
    for player, draws in pairs(Drawings) do
        local character = player.Character
        if character and character:FindFirstChild("HumanoidRootPart") and character:FindFirstChild("Head") and character:FindFirstChild("Humanoid") then
            local root = character.HumanoidRootPart
            local head = character.Head
            local humanoid = character.Humanoid
            
            if humanoid.Health <= 0 then
                draws.Box.Visible = false
                draws.Name.Visible = false
                continue
            end

            local screenPos, onScreen = workspace.CurrentCamera:WorldToViewportPoint(root.Position)
            
            if onScreen then

                -- ‡∏Ñ‡∏≥‡∏ô‡∏ß‡∏ì‡∏Ç‡∏ô‡∏≤‡∏î Box (‡∏õ‡∏£‡∏±‡∏ö‡πÉ‡∏´‡πâ‡πÅ‡∏°‡πà‡∏ô‡∏¢‡∏≥‡∏Ç‡∏∂‡πâ‡∏ô)
                local topPos = workspace.CurrentCamera:WorldToViewportPoint(head.Position + Vector3.new(0, 3, 0))
                local bottomPos = workspace.CurrentCamera:WorldToViewportPoint(root.Position - Vector3.new(0, 3, 0))
                local sizeY = math.abs(topPos.Y - bottomPos.Y)
                local sizeX = sizeY * 0.8

                draws.Box.Size = Vector2.new(sizeX, sizeY)
                draws.Box.Position = Vector2.new(screenPos.X - sizeX / 2, screenPos.Y - sizeY / 2)
                draws.Box.Visible = true

                -- ‡∏ä‡∏∑‡πà‡∏≠ + ‡πÄ‡∏•‡∏∑‡∏≠‡∏î
                draws.Name.Text = player.Name .. " [ " .. math.floor(humanoid.Health) .. " ]"
                draws.Name.Position = Vector2.new(screenPos.X, screenPos.Y - sizeY / 2 - 20)
                draws.Name.Visible = true
            else
                draws.Box.Visible = false
                draws.Name.Visible = false
            end
        else
            draws.Box.Visible = false
            draws.Name.Visible = false
        end
    end
end

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏à‡∏±‡∏î‡∏Å‡∏≤‡∏£‡πÄ‡∏°‡∏∑‡πà‡∏≠ character ‡πÇ‡∏´‡∏•‡∏î
local function OnCharacterAdded(player)
    if player == Players.LocalPlayer then return end
    
    -- ‡∏£‡∏≠‡πÉ‡∏´‡πâ character ‡πÇ‡∏´‡∏•‡∏î‡∏™‡πà‡∏ß‡∏ô‡∏™‡∏≥‡∏Ñ‡∏±‡∏ç
    local character = player.Character or player.CharacterAdded:Wait()
    character:WaitForChild("HumanoidRootPart")
    character:WaitForChild("Head")
    character:WaitForChild("Humanoid")
    
    SetupDrawingESP(player)
    SetupHighlight(player)
end

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡πÄ‡∏õ‡∏¥‡∏î ESP
local function EnableESP()
    -- ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡∏ó‡∏µ‡πà‡∏°‡∏µ‡∏≠‡∏¢‡∏π‡πà‡πÅ‡∏•‡πâ‡∏ß
    for _, player in pairs(Players:GetPlayers()) do
        if player.Character then
            OnCharacterAdded(player)
        end
        player.CharacterAdded:Connect(function()
            OnCharacterAdded(player)
        end)
    end
    
    -- ‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡πÉ‡∏´‡∏°‡πà‡πÄ‡∏Ç‡πâ‡∏≤‡∏°‡∏≤
    Players.PlayerAdded:Connect(function(player)
        player.CharacterAdded:Connect(function()
            OnCharacterAdded(player)
        end)
    end)
end

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏õ‡∏¥‡∏î ESP
local function DisableESP()
    -- ‡∏•‡∏ö Drawing ‡∏ó‡∏±‡πâ‡∏á‡∏´‡∏°‡∏î
    for player, draws in pairs(Drawings) do
        if draws.Box then draws.Box:Remove() end
        if draws.Name then draws.Name:Remove() end
    end
    Drawings = {}
    
    -- ‡∏õ‡∏¥‡∏î‡πÅ‡∏•‡∏∞‡∏•‡∏ö Highlight ‡∏ó‡∏±‡πâ‡∏á‡∏´‡∏°‡∏î
    for player, highlight in pairs(Highlights) do
        if highlight and highlight.Parent then
            highlight.Enabled = false
            highlight:Destroy()
        end
    end
    Highlights = {}
end

-- Toggle ‡∏´‡∏•‡∏±‡∏Å
espTab:CreateToggle({
    Name = "[ üëÅÔ∏è ]  ESP Hitbox Player  [ 138, 43, 179 ]  All",
    CurrentValue = false,
    Callback = function(Value)
        ESPEnabled = Value

        if Value then
            EnableESP()
        else
            DisableESP()
        end
    end,
})

local ESPEnabled = false
local Highlights = {}  -- [player] = highlight instance

local RunService = game:GetService("RunService")
local Players = game:GetService("Players")

-- ‡∏™‡∏£‡πâ‡∏≤‡∏á‡∏´‡∏£‡∏∑‡∏≠‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï Highlight ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö player (‡πÄ‡∏â‡∏û‡∏≤‡∏∞ Highlight ‡πÄ‡∏ó‡πà‡∏≤‡∏ô‡∏±‡πâ‡∏ô)
local function SetupHighlight(player)
    if player == Players.LocalPlayer then return end
    
    -- ‡∏•‡∏ö Highlight ‡πÄ‡∏Å‡πà‡∏≤‡∏ñ‡πâ‡∏≤‡∏°‡∏µ (‡∏õ‡πâ‡∏≠‡∏á‡∏Å‡∏±‡∏ô duplicate)
    if Highlights[player] then
        if Highlights[player] and Highlights[player].Parent then
            Highlights[player]:Destroy()
        end
    end
    
    local character = player.Character
    if character then
        local highlight = Instance.new("Highlight")
        highlight.Parent = character
        highlight.Adornee = character  -- ‡∏ó‡∏≥‡πÉ‡∏´‡πâ highlight ‡∏ï‡∏¥‡∏î‡∏Å‡∏±‡∏ö‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£‡πÄ‡∏ï‡πá‡∏°‡πÜ
        highlight.DepthMode = Enum.HighlightDepthMode.AlwaysOnTop  -- ‡∏°‡∏≠‡∏á‡πÄ‡∏´‡πá‡∏ô‡∏ó‡∏∞‡∏•‡∏∏‡∏Å‡∏≥‡πÅ‡∏û‡∏á
        highlight.FillColor = Color3.fromRGB(138, 43, 179)
        highlight.OutlineColor = Color3.fromRGB(138, 43, 179)
        highlight.FillTransparency = 0.4
        highlight.OutlineTransparency = 0
        highlight.Enabled = ESPEnabled  -- ‡πÄ‡∏õ‡∏¥‡∏î‡∏ó‡∏±‡∏ô‡∏ó‡∏µ‡∏ñ‡πâ‡∏≤ ESP ‡πÄ‡∏õ‡∏¥‡∏î‡∏≠‡∏¢‡∏π‡πà
        
        Highlights[player] = highlight
    end
end

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏à‡∏±‡∏î‡∏Å‡∏≤‡∏£‡πÄ‡∏°‡∏∑‡πà‡∏≠ character ‡πÇ‡∏´‡∏•‡∏î‡πÉ‡∏´‡∏°‡πà (respawn ‡∏´‡∏£‡∏∑‡∏≠‡πÄ‡∏Ç‡πâ‡∏≤‡∏°‡∏≤‡πÉ‡∏´‡∏°‡πà)
local function OnCharacterAdded(player)
    if player == Players.LocalPlayer then return end
    
    -- ‡∏£‡∏≠‡πÉ‡∏´‡πâ character ‡πÇ‡∏´‡∏•‡∏î‡∏™‡πà‡∏ß‡∏ô‡∏™‡∏≥‡∏Ñ‡∏±‡∏ç‡∏Ñ‡∏£‡∏ö
    local character = player.Character or player.CharacterAdded:Wait()
    character:WaitForChild("HumanoidRootPart", 5)
    character:WaitForChild("Head", 5)
    
    SetupHighlight(player)
end

-- ‡πÄ‡∏õ‡∏¥‡∏î Highlight ESP
local function EnableHighlightESP()
    -- ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡∏ó‡∏µ‡πà‡∏°‡∏µ‡∏≠‡∏¢‡∏π‡πà‡πÅ‡∏•‡πâ‡∏ß‡πÉ‡∏ô‡πÄ‡∏Å‡∏°
    for _, player in pairs(Players:GetPlayers()) do
        if player.Character then
            OnCharacterAdded(player)
        end
        -- ‡πÄ‡∏ä‡∏∑‡πà‡∏≠‡∏° event ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö respawn ‡πÉ‡∏ô‡∏≠‡∏ô‡∏≤‡∏Ñ‡∏ï
        player.CharacterAdded:Connect(function()
            OnCharacterAdded(player)
        end)
    end
    
    -- ‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡πÉ‡∏´‡∏°‡πà‡πÄ‡∏Ç‡πâ‡∏≤‡∏°‡∏≤‡πÉ‡∏ô‡πÄ‡∏Å‡∏°
    Players.PlayerAdded:Connect(function(newPlayer)
        newPlayer.CharacterAdded:Connect(function()
            OnCharacterAdded(newPlayer)
        end)
    end)
end  -- ‡πÄ‡∏û‡∏¥‡πà‡∏° end ‡∏ï‡∏£‡∏á‡∏ô‡∏µ‡πâ‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏õ‡∏¥‡∏î‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô

-- ‡∏õ‡∏¥‡∏î‡πÅ‡∏•‡∏∞‡∏•‡∏ö Highlight ‡∏ó‡∏±‡πâ‡∏á‡∏´‡∏°‡∏î
local function DisableHighlightESP()
    for player, highlight in pairs(Highlights) do
        if highlight and highlight.Parent then
            highlight.Enabled = false
            highlight:Destroy()
        end
    end
    Highlights = {}
end

-- Toggle ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö Highlight ‡πÄ‡∏ó‡πà‡∏≤‡∏ô‡∏±‡πâ‡∏ô
espTab:CreateToggle({
    Name = "[ üí° ]  ESP Highligh Player  [ Highligh ]  All",
    CurrentValue = false,
    Callback = function(Value)
        ESPEnabled = Value

        if Value then
            EnableHighlightESP()
        else
            DisableHighlightESP()
        end
    end,
})

-- ‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï‡∏ó‡∏∏‡∏Å‡πÄ‡∏ü‡∏£‡∏°
RunService.RenderStepped:Connect(UpdateESP)

local Paragraph = espTab:CreateParagraph({Title = "ESP Player Highligh / Name", Content = "ESP Player Highligh / Name For Hitbox"})

local Section = espTab:CreateSection("")

local coTab = Window:CreateTab("||   - Mapping Time", "map-pinned")
local Section = coTab:CreateSection("")

local Section = coTab:CreateSection("{ [ - FPS Check Loading - ] }")

local FPSLabel = coTab:CreateLabel("FPS:  Loading...")

local PingLabel = coTab:CreateLabel("Ping:  Loading...")

local RunService = game:GetService("RunService")
local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

local frameTimes = {}
local lastUpdate = tick()
local updateInterval = 1

RunService.RenderStepped:Connect(function(deltaTime)
    table.insert(frameTimes, deltaTime)
    
    if tick() - lastUpdate >= updateInterval then
        local totalTime = 0
        for _, time in ipairs(frameTimes) do
            totalTime += time
        end
        local avgDelta = totalTime / #frameTimes
        local fps = math.floor(1 / avgDelta + 0.5)
        
        local color = "üü¢"
        if fps < 30 then
            color = "üî¥"
        elseif fps < 60 then
            color = "üü°"
        end
        
        FPSLabel:Set("FPS:  (  " .. color .. "  =  " ..  fps .. "  )")
        
        -- ‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï Ping
        local ping = math.floor(LocalPlayer:GetNetworkPing() * 1000)
        local pingColor = ping < 50 and "üü¢" or (ping < 100 and "üü°" or "üî¥")
        PingLabel:Set("Ping:  (  " .. pingColor .. "  =  " .. ping .. " MS  )")
        
        -- ‡∏£‡∏µ‡πÄ‡∏ã‡πá‡∏ï‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡∏£‡∏≠‡∏ö‡∏ñ‡∏±‡∏î‡πÑ‡∏õ
        frameTimes = {}
        lastUpdate = tick()
    end
end)

local Section = coTab:CreateSection("")
local Section = coTab:CreateSection("{ [ - User Name:  All In One  Target - ] }")

local PlayerListParagraph = coTab:CreateParagraph({
    Title = "All Name Player In List",
    Content = "Loading..."
})

-- ‡∏ï‡∏≤‡∏£‡∏≤‡∏á‡πÄ‡∏Å‡πá‡∏ö‡∏Ç‡πâ‡∏≠‡∏°‡∏π‡∏•‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡∏ó‡∏µ‡πà‡∏≠‡∏≠‡∏Å‡πÑ‡∏õ‡∏ä‡∏±‡πà‡∏ß‡∏Ñ‡∏£‡∏≤‡∏ß (‡πÄ‡∏û‡∏∑‡πà‡∏≠‡πÅ‡∏™‡∏î‡∏á‡∏™‡∏µ‡πÅ‡∏î‡∏á 5 ‡∏ß‡∏¥)
local leavingPlayers = {}  -- {player = {timestamp = tick()}}

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï‡∏£‡∏≤‡∏¢‡∏ä‡∏∑‡πà‡∏≠‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô
local function UpdatePlayerList()
    local players = game.Players:GetPlayers()
    local nameList = {}
    
    local numberWidth = 5
    local currentTime = tick()
    
    -- ‡πÄ‡∏û‡∏¥‡πà‡∏°‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡∏ó‡∏µ‡πà‡∏¢‡∏±‡∏á‡∏≠‡∏¢‡∏π‡πà‡πÉ‡∏ô‡πÄ‡∏ã‡∏¥‡∏£‡πå‡∏ü (‡∏™‡∏µ‡πÄ‡∏Ç‡∏µ‡∏¢‡∏ß)
    for i, player in ipairs(players) do
        local numbered = string.format("%" .. numberWidth .. "d.   üü¢   ", i)
        local entry = numbered .. player.Name .. "    (  [    " .. player.DisplayName .. "    ]  )"
        table.insert(nameList, entry)
        
        -- ‡∏•‡∏ö‡∏≠‡∏≠‡∏Å‡∏à‡∏≤‡∏Å leavingPlayers ‡∏ñ‡πâ‡∏≤‡πÄ‡∏Ç‡∏≤‡∏Å‡∏•‡∏±‡∏ö‡πÄ‡∏Ç‡πâ‡∏≤‡∏°‡∏≤ (‡∏Å‡∏£‡∏ì‡∏µ reconnect ‡πÄ‡∏£‡πá‡∏ß)
        leavingPlayers[player] = nil
    end
    
    -- ‡πÄ‡∏û‡∏¥‡πà‡∏°‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡∏ó‡∏µ‡πà‡πÄ‡∏û‡∏¥‡πà‡∏á‡∏≠‡∏≠‡∏Å‡πÑ‡∏õ (‡πÅ‡∏ï‡πà‡∏¢‡∏±‡∏á‡πÑ‡∏°‡πà‡∏Ñ‡∏£‡∏ö 5 ‡∏ß‡∏¥) ‚Üí ‡πÅ‡∏™‡∏î‡∏á‡∏™‡∏µ‡πÅ‡∏î‡∏á
    for player, data in pairs(leavingPlayers) do
        if currentTime - data.timestamp <= 4 then
            local numbered = string.format("%" .. numberWidth .. "d.   üî¥   ", data.oldIndex or 999)
            local entry = numbered .. player.Name .. "    (  [    " .. player.DisplayName .. "    ]  )  <font color=\"rgb(255,100,100)\"> --- üí• ---</font>"
            table.insert(nameList, entry)
        else
            -- ‡∏Ñ‡∏£‡∏ö 5 ‡∏ß‡∏¥‡πÅ‡∏•‡πâ‡∏ß ‚Üí ‡∏•‡∏ö‡∏≠‡∏≠‡∏Å‡∏à‡∏≤‡∏Å‡∏ï‡∏≤‡∏£‡∏≤‡∏á
            leavingPlayers[player] = nil
        end
    end
    
    -- ‡πÄ‡∏£‡∏µ‡∏¢‡∏á‡∏•‡∏≥‡∏î‡∏±‡∏ö (‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡∏à‡∏£‡∏¥‡∏á‡∏Å‡πà‡∏≠‡∏ô, ‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡∏ó‡∏µ‡πà‡∏≠‡∏≠‡∏Å‡∏ï‡∏≤‡∏°‡∏´‡∏•‡∏±‡∏á)
    table.sort(nameList)
    
    local fullText
    if #nameList == 0 and next(leavingPlayers) == nil then
        fullText = "No Found Player In List"
    else
        fullText = table.concat(nameList, "\n\n\n")
    end
    
    -- ‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï Paragraph
    PlayerListParagraph:Set({
        Title = "                                                                                                                      (   " .. #players .. "   Player  )",
        Content = fullText
    })
end

-- ‡πÄ‡∏°‡∏∑‡πà‡∏≠‡∏°‡∏µ‡∏Ñ‡∏ô‡πÄ‡∏Ç‡πâ‡∏≤‡∏°‡∏≤
game.Players.PlayerAdded:Connect(function(player)
    UpdatePlayerList()
    
    if game.Players.LocalPlayer:IsFriendsWith(player.UserId) then
    end
end)

-- ‡πÄ‡∏°‡∏∑‡πà‡∏≠‡∏°‡∏µ‡∏Ñ‡∏ô‡∏≠‡∏≠‡∏Å
game.Players.PlayerRemoving:Connect(function(player)

    local players = game.Players:GetPlayers()
    local oldIndex = 1
    for i, p in ipairs(players) do
        if p == player then
            oldIndex = i
            break
        end
    end
    
    leavingPlayers[player] = {
        timestamp = tick(),
        oldIndex = oldIndex,
        DisplayName = player.DisplayName
    }
    
     Rayfield:Notify({
        Title = "[ üî¥ ]  Player Leave Game",
        Content = player.Name .. "  (  " .. player.DisplayName .. "  )",
        Duration = 4,
        Image = "door-closed",
    })
    
    -- ‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï‡∏ó‡∏±‡∏ô‡∏ó‡∏µ‡πÄ‡∏û‡∏∑‡πà‡∏≠‡πÅ‡∏™‡∏î‡∏á‡∏™‡∏µ‡πÅ‡∏î‡∏á
    UpdatePlayerList()
    
    -- ‡∏£‡∏≠ 5 ‡∏ß‡∏¥ ‡πÅ‡∏•‡πâ‡∏ß‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï‡∏≠‡∏µ‡∏Å‡∏ó‡∏µ‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏•‡∏ö‡∏≠‡∏≠‡∏Å‡∏à‡∏£‡∏¥‡∏á ‡πÜ
    task.delay(5, UpdatePlayerList)
end)

-- ‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï‡∏ó‡∏∏‡∏Å 1 ‡∏ß‡∏¥‡∏ô‡∏≤‡∏ó‡∏µ ‡πÄ‡∏û‡∏∑‡πà‡∏≠‡πÄ‡∏Ñ‡∏•‡∏µ‡∏¢‡∏£‡πå‡∏Ñ‡∏ô‡∏ó‡∏µ‡πà‡∏Ñ‡∏£‡∏ö 5 ‡∏ß‡∏¥‡πÅ‡∏•‡πâ‡∏ß (‡∏Å‡∏£‡∏ì‡∏µ‡πÑ‡∏°‡πà‡∏°‡∏µ event ‡∏≠‡∏∑‡πà‡∏ô)
task.spawn(function()
    while task.wait(1) do
        if next(leavingPlayers) then
            UpdatePlayerList()
        end
    end
end)

-- ‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï‡∏Ñ‡∏£‡∏±‡πâ‡∏á‡πÅ‡∏£‡∏Å
UpdatePlayerList()

