-- ‡πÄ‡∏û‡∏¥‡πà‡∏° GroupBox ‡πÉ‡∏´‡∏°‡πà‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö Infinite Jump ‡πÅ‡∏•‡∏∞ Noclip ‡πÑ‡∏ß‡πâ‡∏ó‡∏µ‡πà Right GroupBox
local ExtraGroupBox = Tabs['üôà 3']:AddRightGroupbox('‚öôÔ∏è [ Infinite Jump / Noclip ] ‚öôÔ∏è')

local infiniteJumpEnabled = false
local noclipEnabled = false

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô Noclip
local function noclipLoop()
    local player = game.Players.LocalPlayer
    while noclipEnabled do
        task.wait()
        if player.Character then
            for _, part in pairs(player.Character:GetDescendants()) do
                if part:IsA("BasePart") and part.CanCollide == true then
                    part.CanCollide = false
                end
            end
        end
    end
end

-- Infinite Jump
game:GetService("UserInputService").JumpRequest:Connect(function()
    if infiniteJumpEnabled then
        local character = game.Players.LocalPlayer.Character
        if character and character:FindFirstChildOfClass("Humanoid") then
            character.Humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
        end
    end
end)

ExtraGroupBox:AddToggle('InfiniteJumpToggle', {
    Text = 'ü¶ò [ ‡∏Å‡∏£‡∏∞‡πÇ‡∏î‡∏î‡πÑ‡∏°‡πà‡∏à‡∏≥‡∏Å‡∏±‡∏î ] ü¶ò',
    Default = false,
    Tooltip = 'ü¶ò [ ‡∏Å‡∏£‡∏∞‡πÇ‡∏î‡∏î‡πÑ‡∏°‡πà‡∏à‡∏≥‡∏Å‡∏±‡∏î ] ü¶ò',
    Callback = function(Value)
        infiniteJumpEnabled = Value
    end
})

ExtraGroupBox:AddToggle('NoclipToggle', {
    Text = 'üß± [ ‡∏ó‡∏∞‡∏•‡∏∏‡∏ó‡∏∏‡∏Å‡∏™‡∏¥‡πà‡∏á‡∏ó‡∏∏‡∏Å‡∏≠‡∏¢‡πà‡∏≤‡∏á ] üß±',
    Default = false,
    Tooltip = 'üß± [ 1 ‡∏Ñ‡∏ô 2 ‡∏™‡∏¥‡πà‡∏á‡∏Ç‡∏≠‡∏á 3 ‡∏Å‡∏≥‡πÅ‡∏û‡∏á ] üß±',
    Callback = function(Value)
        noclipEnabled = Value
        if Value then
            -- ‡∏£‡∏±‡∏ô loop Noclip
            task.spawn(noclipLoop)
        end
    end
})

-- Toggle Keybind ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡πÄ‡∏õ‡∏¥‡∏î/‡∏õ‡∏¥‡∏î Infinite Jump ‡πÅ‡∏•‡∏∞ Noclip
local UIS = game:GetService("UserInputService")
local toggleKey = Enum.KeyCode.V

UIS.InputBegan:Connect(function(input, gameProcessed)
    if gameProcessed then return end
    if input.KeyCode == toggleKey then
        infiniteJumpEnabled = not infiniteJumpEnabled
        noclipEnabled = not noclipEnabled

        -- ‡∏≠‡∏±‡∏û‡πÄ‡∏î‡∏ï Toggle UI
        Library:Notify('Toggle Keybind', ('Infinite Jump %s, Noclip %s'):format(tostring(infiniteJumpEnabled), tostring(noclipEnabled)), 3)

        Library.ToggleItems['InfiniteJumpToggle']:SetValue(infiniteJumpEnabled)
        Library.ToggleItems['NoclipToggle']:SetValue(noclipEnabled)

        if noclipEnabled then
            task.spawn(noclipLoop)
        end
    end
end)

-- ‡πÄ‡∏û‡∏¥‡πà‡∏° GroupBox ‡πÉ‡∏ô‡πÅ‡∏ó‡πá‡∏ö‡∏ö‡∏¥‡∏ô (‡∏≠‡∏¢‡∏π‡πà‡∏ã‡πâ‡∏≤‡∏¢‡∏Ç‡∏≠‡∏á‡πÅ‡∏ó‡πá‡∏ö‡∏ô‡∏±‡πâ‡∏ô)
local FlyGroupBox = Tabs['üôà 3']:AddRightGroupbox('üïäÔ∏è [ ‡πÇ‡∏ã‡∏ô‡∏ö‡∏¥‡∏ô ] üïäÔ∏è')

local flying = false
local speed = 50
local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local humanoid = character:WaitForChild("Humanoid")
local rootPart = character:WaitForChild("HumanoidRootPart")

local UserInputService = game:GetService("UserInputService")
local RunService = game:GetService("RunService")

local flyConnection

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡πÄ‡∏õ‡∏¥‡∏î/‡∏õ‡∏¥‡∏î‡∏ö‡∏¥‡∏ô
local function toggleFly(enable)
    flying = enable
    if flying then
        humanoid.PlatformStand = true -- ‡∏õ‡∏¥‡∏î‡∏ü‡∏¥‡∏™‡∏¥‡∏Å‡∏™‡πå humanoid ‡πÄ‡∏î‡∏¥‡∏°

        flyConnection = RunService.Heartbeat:Connect(function(deltaTime)
            local moveDirection = Vector3.new(0,0,0)
            if UserInputService:IsKeyDown(Enum.KeyCode.W) then
                moveDirection = moveDirection + workspace.CurrentCamera.CFrame.LookVector
            end
            if UserInputService:IsKeyDown(Enum.KeyCode.S) then
                moveDirection = moveDirection - workspace.CurrentCamera.CFrame.LookVector
            end
            if UserInputService:IsKeyDown(Enum.KeyCode.A) then
                moveDirection = moveDirection - workspace.CurrentCamera.CFrame.RightVector
            end
            if UserInputService:IsKeyDown(Enum.KeyCode.D) then
                moveDirection = moveDirection + workspace.CurrentCamera.CFrame.RightVector
            end
            if UserInputService:IsKeyDown(Enum.KeyCode.Space) then
                moveDirection = moveDirection + Vector3.new(0,1,0)
            end
            if UserInputService:IsKeyDown(Enum.KeyCode.LeftControl) then
                moveDirection = moveDirection - Vector3.new(0,1,0)
            end

            if moveDirection.Magnitude > 0 then
                moveDirection = moveDirection.Unit * speed
                rootPart.Velocity = moveDirection
            else
                rootPart.Velocity = Vector3.new(0,0,0)
            end
        end)
    else
        humanoid.PlatformStand = false
        if flyConnection then
            flyConnection:Disconnect()
            flyConnection = nil
        end
        rootPart.Velocity = Vector3.new(0,0,0)
    end
end

-- Toggle ‡πÄ‡∏õ‡∏¥‡∏î/‡∏õ‡∏¥‡∏î‡∏ö‡∏¥‡∏ô
FlyGroupBox:AddToggle('FlyToggle', {
    Text = 'üïäÔ∏è [ ‡∏ö‡∏¥‡∏ô‡πÅ‡∏ö‡∏ö‡∏ò‡∏£‡∏£‡∏°‡∏î‡∏≤ ] üïäÔ∏è',
    Default = false,
    Tooltip = 'üïäÔ∏è [ ‡∏Å‡∏î‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏ö‡∏¥‡∏ô ] üïäÔ∏è',
    Callback = function(value)
        toggleFly(value)
    end
})

-- Slider ‡∏õ‡∏£‡∏±‡∏ö‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß‡∏ö‡∏¥‡∏ô
FlyGroupBox:AddSlider('FlySpeed', {
    Text = ' [ ‡∏õ‡∏£‡∏±‡∏ö‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß‡∏ö‡∏¥‡∏ô ]',
    Default = speed,
    Min = 10,
    Max = 200,
    Rounding = 1,
    Tooltip = 'üïäÔ∏è [ ‡∏õ‡∏£‡∏±‡∏ö‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß‡∏ö‡∏¥‡∏ô ] üïäÔ∏è',
    Callback = function(value)
        speed = value
    end
})


local LeftGroupBox = Tabs['üôà 2']:AddLeftGroupbox('üåÄ ‡∏ß‡∏≤‡∏õ‡πÑ‡∏õ‡∏´‡∏≤‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏ô üåÄ')

-- ‡πÄ‡∏û‡∏¥‡πà‡∏°‡∏ï‡∏±‡∏ß‡πÅ‡∏õ‡∏£‡πÄ‡∏Å‡πá‡∏ö‡∏ä‡∏∑‡πà‡∏≠‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡∏ó‡∏µ‡πà‡πÄ‡∏•‡∏∑‡∏≠‡∏Å
local selectedPlayerName = nil

-- Dropdown ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô (‡∏≠‡∏±‡∏û‡πÄ‡∏î‡∏ï‡∏£‡∏≤‡∏¢‡∏ä‡∏∑‡πà‡∏≠)
local playerDropdown = LeftGroupBox:AddDropdown('üë• [ All Player ] üë•', {
    SpecialType = 'Player',
    Text = '',
    Tooltip = '‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡∏ó‡∏µ‡πà‡∏ï‡πâ‡∏≠‡∏á‡∏Å‡∏≤‡∏£‡πÄ‡∏ó‡πÄ‡∏•‡∏û‡∏≠‡∏£‡πå‡∏ï',
    Callback = function(Value)
        selectedPlayerName = Value
        print('[cb] Player dropdown got changed:', Value)
    end
})

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏≠‡∏±‡∏û‡πÄ‡∏î‡∏ï‡∏£‡∏≤‡∏¢‡∏ä‡∏∑‡πà‡∏≠‡πÉ‡∏ô Dropdown (‡∏ñ‡πâ‡∏≤ SpecialType = 'Player' ‡πÑ‡∏°‡πà‡∏≠‡∏±‡∏û‡πÄ‡∏î‡∏ï‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï‡∏¥)
local function updatePlayerList()
    local playerNames = {}
    for _, plr in pairs(game.Players:GetPlayers()) do
        if plr ~= game.Players.LocalPlayer then
            table.insert(playerNames, plr.Name)
        end
    end
    playerDropdown:SetValues(playerNames)
    if #playerNames > 0 then
        selectedPlayerName = playerNames[1]
        playerDropdown:SetValue(selectedPlayerName)
    else
        selectedPlayerName = nil
        playerDropdown:SetValue(nil)
    end
end

-- ‡πÄ‡∏£‡∏µ‡∏¢‡∏Å‡∏≠‡∏±‡∏û‡πÄ‡∏î‡∏ï‡∏£‡∏≤‡∏¢‡∏ä‡∏∑‡πà‡∏≠‡∏Ñ‡∏£‡∏±‡πâ‡∏á‡πÅ‡∏£‡∏Å
updatePlayerList()

-- ‡∏≠‡∏±‡∏û‡πÄ‡∏î‡∏ï‡∏£‡∏≤‡∏¢‡∏ä‡∏∑‡πà‡∏≠‡πÄ‡∏°‡∏∑‡πà‡∏≠‡∏°‡∏µ‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡πÄ‡∏Ç‡πâ‡∏≤‡∏≠‡∏≠‡∏Å
game.Players.PlayerAdded:Connect(updatePlayerList)
game.Players.PlayerRemoving:Connect(updatePlayerList)

-- ‡∏õ‡∏∏‡πà‡∏° Teleport ‡πÑ‡∏õ‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡∏ó‡∏µ‡πà‡πÄ‡∏•‡∏∑‡∏≠‡∏Å
LeftGroupBox:AddButton({
    Text = 'üöÄ ‡∏ß‡∏≤‡∏õ‡πÑ‡∏õ‡∏¢‡∏±‡∏á‡∏ó‡∏µ‡πà‡πÄ‡∏£‡∏≤‡πÄ‡∏•‡∏∑‡∏≠‡∏Å',
    Tooltip = '‡πÄ‡∏ó‡πÄ‡∏•‡∏û‡∏≠‡∏£‡πå‡∏ï‡πÑ‡∏õ‡∏¢‡∏±‡∏á‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡∏ó‡∏µ‡πà‡πÄ‡∏•‡∏∑‡∏≠‡∏Å',
    Func = function()
        if not selectedPlayerName then
            Library:Notify('Error', '‡∏Å‡∏£‡∏∏‡∏ì‡∏≤‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡∏Å‡πà‡∏≠‡∏ô‡πÄ‡∏ó‡πÄ‡∏•‡∏û‡∏≠‡∏£‡πå‡∏ï', 3)
            return
        end

        local targetPlayer = game.Players:FindFirstChild(selectedPlayerName)
        if not targetPlayer or not targetPlayer.Character or not targetPlayer.Character:FindFirstChild("HumanoidRootPart") then
            Library:Notify('Error', '‡πÑ‡∏°‡πà‡∏û‡∏ö‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡∏´‡∏£‡∏∑‡∏≠‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡πÑ‡∏°‡πà‡∏°‡∏µ‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£', 3)
            return
        end

        local localPlayer = game.Players.LocalPlayer
        if not localPlayer.Character or not localPlayer.Character:FindFirstChild("HumanoidRootPart") then
            Library:Notify('Error', '‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£‡∏Ç‡∏≠‡∏á‡∏Ñ‡∏∏‡∏ì‡∏¢‡∏±‡∏á‡πÑ‡∏°‡πà‡∏û‡∏£‡πâ‡∏≠‡∏°', 3)
            return
        end

        -- ‡πÄ‡∏ó‡πÄ‡∏•‡∏û‡∏≠‡∏£‡πå‡∏ï‡πÇ‡∏î‡∏¢‡πÄ‡∏•‡∏∑‡πà‡∏≠‡∏ô‡∏Ç‡∏∂‡πâ‡∏ô 5 studs
        localPlayer.Character.HumanoidRootPart.CFrame = targetPlayer.Character.HumanoidRootPart.CFrame + Vector3.new(0, 5, 0)
        Library:Notify('Success', '‡πÄ‡∏ó‡πÄ‡∏•‡∏û‡∏≠‡∏£‡πå‡∏ï‡∏™‡∏≥‡πÄ‡∏£‡πá‡∏à!', 3)
    end
})

-- ‡πÄ‡∏û‡∏¥‡πà‡∏°‡πÇ‡∏ã‡∏ô Highlight Player ‡πÉ‡∏ô Tab Main ‡∏î‡πâ‡∏≤‡∏ô‡∏Ç‡∏ß‡∏≤
local HighlightGroup = Tabs['üôà 4']:AddLeftGroupbox('[ üëÄ ] Menu ESP Players [ üëÄ ]')

local highlightEnabled = false
local rainbowEnabled = true

local function createHighlight(player)
    if not player.Character or not player.Character:FindFirstChild("HumanoidRootPart") then return end
    if not player.Character:FindFirstChild("Highlight") then
        local hl = Instance.new("Highlight")
        hl.Name = "Highlight"
        hl.FillColor = Color3.fromRGB(255, 0, 0)
        hl.OutlineColor = Color3.fromRGB(255, 255, 255)
        hl.FillTransparency = 0.5
        hl.OutlineTransparency = 0
        hl.Parent = player.Character
    end
end

local function removeHighlight(player)
    if player.Character then
        local hl = player.Character:FindFirstChild("Highlight")
        if hl then hl:Destroy() end
    end
end

HighlightGroup:AddToggle('ESP_Toggle', {
    Text = 'üëÄ ‡∏õ‡∏¥‡∏î/‡πÄ‡∏õ‡∏¥‡∏î‡∏Å‡∏≤‡∏£‡∏°‡∏≠‡∏á‡πÄ‡∏´‡πá‡∏ô‡∏Ñ‡∏ô üëÄ',
    Default = false,
    Tooltip = '‡πÄ‡∏õ‡∏¥‡∏î/‡∏õ‡∏¥‡∏î Highlight ‡∏ó‡∏±‡πâ‡∏á‡∏´‡∏°‡∏î',
    Callback = function(Value)
        highlightEnabled = Value
        for _, plr in pairs(game.Players:GetPlayers()) do
            if plr ~= game.Players.LocalPlayer then
                if Value then
                    createHighlight(plr)
                else
                    removeHighlight(plr)
                end
            end
        end
    end
})

HighlightGroup:AddToggle('Rainbow_Toggle', {
    Text = 'üåà ‡∏õ‡∏£‡∏±‡∏ö‡∏Å‡∏≤‡∏£‡∏°‡∏≠‡∏á‡πÄ‡∏´‡πá‡∏ô‡πÉ‡∏´‡πâ‡πÄ‡∏õ‡πá‡∏ô‡∏™‡∏µ‡∏£‡∏∏‡πà‡∏á',
    Default = true,
    Tooltip = '‡πÄ‡∏õ‡∏¥‡∏î/‡∏õ‡∏¥‡∏î‡∏Å‡∏≤‡∏£‡πÄ‡∏õ‡∏•‡∏µ‡πà‡∏¢‡∏ô‡∏™‡∏µ‡∏£‡∏∏‡πâ‡∏á‡∏Ç‡∏≠‡∏á Highlight',
    Callback = function(Value)
        rainbowEnabled = Value
    end
})

task.spawn(function()
    local hue = 0
    while task.wait(0.03) do
        if highlightEnabled and rainbowEnabled then
            hue = (hue + 0.01) % 1
            local color = Color3.fromHSV(hue, 1, 1)
            for _, player in pairs(game.Players:GetPlayers()) do
                if player ~= game.Players.LocalPlayer and player.Character then
                    local hl = player.Character:FindFirstChild("Highlight")
                    if hl then
                        hl.FillColor = color
                    end
                end
            end
        end
    end
end)

game.Players.PlayerAdded:Connect(function(plr)
    plr.CharacterAdded:Connect(function()
        task.wait(1)
        if highlightEnabled then
            createHighlight(plr)
        end
    end)
end)

local MenuGroup = Tabs['üôà']:AddLeftGroupbox('[ üôà ]  Exit Menu Ui [ üôà ]')

MenuGroup:AddButton('üôà [ Exit The Menu ] üôà', function() Library:Unload() end)
