local espEnabled = false
local espTraceEnabled = true
local espBoxes = {}

local function rainbow(speed, offset)
    local t = tick() * (speed or 1) + (offset or 0)
    return Color3.fromHSV(t % 1, 1, 1)
end

local function createESP(player)
    if not DrawingAvailable or espBoxes[player] then return end
    local box = {}
    box.main = Drawing.new("Square")
    box.main.Thickness = 2
    box.main.Filled = false
    box.main.Visible = false

    box.name = Drawing.new("Text")
    box.name.Size = 16
    box.name.Center = true
    box.name.Outline = true
    box.name.Visible = false

    box.tracer = Drawing.new("Line")
    box.tracer.Thickness = 2
    box.tracer.Visible = false

    espBoxes[player] = box
end

local function removeESP(player)
    local box = espBoxes[player]
    if box then
        pcall(function()
            box.main:Remove()
            box.name:Remove()
            box.tracer:Remove()
        end)
        espBoxes[player] = nil
    end
end

RunService.RenderStepped:Connect(function()
    if not DrawingAvailable then return end

    for _, player in pairs(Players:GetPlayers()) do
        if player ~= LocalPlayer and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
            local humanoid = player.Character:FindFirstChild("Humanoid")
            if humanoid and humanoid.Health > 0 then
                if espEnabled then
                    createESP(player)
                    local box = espBoxes[player]
                    local root = player.Character.HumanoidRootPart
                    local pos, onScreen = Camera:WorldToViewportPoint(root.Position)

                    if onScreen then
                        local scale = math.clamp(2000 / (Camera.CFrame.Position - root.Position).Magnitude, 20, 200)
                        local size = Vector2.new(scale * 0.6, scale)
                        local center = Vector2.new(pos.X, pos.Y)

                        box.main.Size = size
                        box.main.Position = center - size/2
                        box.main.Color = (autoTarget == player) and Color3.fromRGB(255, 0, 0) or rainbow(1, 0)
                        box.main.Visible = true

                        box.name.Position = center + Vector2.new(0, -size.Y/2 - 18)
                        box.name.Text = player.Name .. ((autoTarget == player) and "[ üéØ ‡∏•‡πá‡∏≠‡∏Ñ‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£‡∏ù‡∏±‡∏á‡∏ï‡∏£‡∏á‡∏Ç‡πâ‡∏≤‡∏°‡πÅ‡∏•‡πâ‡∏ß ]" or "")
                        box.name.Color = (autoTarget == player) and Color3.fromRGB(255, 0, 0) or rainbow(1, 0.15)
                        box.name.Visible = true

                        if espTraceEnabled then
                            local screenCenter = Vector2.new(Camera.ViewportSize.X/2, Camera.ViewportSize.Y/2)
                            box.tracer.From = screenCenter
                            box.tracer.To = center
                            box.tracer.Color = (autoTarget == player) and Color3.fromRGB(255, 0, 0) or rainbow(1, 0.25)
                            box.tracer.Visible = true
                        end
                    else
                        box.main.Visible = false
                        box.name.Visible = false
                        box.tracer.Visible = false
                    end
                else
                    removeESP(player)
                end
            else
                removeESP(player)
            end
        else
            removeESP(player)
        end
    end
end)

Players.PlayerRemoving:Connect(removeESP)

local EyeTab = Window:CreateTab("ESP Zone", "eye") -- Title, Image
EyeTab:CreateSection("[üîç] ‡πÇ‡∏ã‡∏ô‡∏°‡∏≠‡∏á‡∏î‡∏π‡∏Ñ‡∏ô‡πÅ‡∏ö‡∏ö‡πÄ‡∏™‡πá‡∏ô/‡πÑ‡∏°‡πà‡πÄ‡∏™‡πâ‡∏ô/‡∏°‡∏≠‡∏á‡∏ï‡∏±‡∏ß‡∏Ñ‡∏ô [üîç]")

-- ‡∏ï‡∏±‡∏ß‡πÅ‡∏õ‡∏£
local Highlights = {}
local HighlightEnabled = false
local RainbowSpeed = 1
local RainbowLoop = nil

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏™‡∏£‡πâ‡∏≤‡∏á Highlight
local function CreateHighlight(player)
    if player.Character and player.Character:FindFirstChild("HumanoidRootPart") and not Highlights[player] then
        local highlight = Instance.new("Highlight")
        highlight.Name = "RainbowHighlight"
        highlight.OutlineColor = Color3.fromRGB(255, 255, 255)
        highlight.OutlineTransparency = 0
        highlight.FillTransparency = 0.5
        highlight.Parent = player.Character
        Highlights[player] = highlight
    end
end

-- ‡∏•‡∏ö Highlight
local function RemoveHighlight(player)
    if Highlights[player] then
        Highlights[player]:Destroy()
        Highlights[player] = nil
    end
end

-- ‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï Highlight ‡∏ó‡∏∏‡∏Å‡∏Ñ‡∏ô
local function UpdateHighlights()
    for _, player in pairs(game.Players:GetPlayers()) do
        if player ~= game.Players.LocalPlayer and player.Character then
            if HighlightEnabled then
                CreateHighlight(player)
            else
                RemoveHighlight(player)
            end
        end
    end
end

-- Rainbow Loop (RunService)
local function StartRainbow()
    if RainbowLoop then RainbowLoop:Disconnect() end
    RainbowLoop = game:GetService("RunService").Heartbeat:Connect(function()
        if not HighlightEnabled then return end
        local time = tick() * RainbowSpeed
        local rainbow = Color3.fromHSV(time % 1, 1, 1) -- ‡∏™‡∏µ‡∏£‡∏∏‡πâ‡∏á‡πÄ‡∏ï‡πá‡∏°‡∏™‡∏π‡∏ö
        for _, highlight in pairs(Highlights) do
            if highlight and highlight.Parent then
                highlight.FillColor = rainbow
            end
        end
    end)
end

local function StopRainbow()
    if RainbowLoop then
        RainbowLoop:Disconnect()
        RainbowLoop = nil
    end
end

-- Toggle ‡πÄ‡∏õ‡∏¥‡∏î/‡∏õ‡∏¥‡∏î Rainbow
EyeTab:CreateToggle({
    Name = "üåà ‡πÄ‡∏õ‡∏¥‡∏î‡∏Å‡∏≤‡∏£‡∏°‡∏≠‡∏á‡∏ï‡∏±‡∏ß‡∏Ñ‡∏ô",
    CurrentValue = false,
    Flag = "RainbowToggle",
    Callback = function(Value)
        HighlightEnabled = Value
        if Value then
            UpdateHighlights()
            StartRainbow()
        else
            StopRainbow()
            for _, player in pairs(game.Players:GetPlayers()) do
                RemoveHighlight(player)
            end
        end
    end,
})

EyeTab:CreateToggle({
    Name = "üîç ‡πÄ‡∏õ‡∏¥‡∏î‡∏Å‡∏≤‡∏£‡∏°‡∏≠‡∏á‡πÄ‡∏´‡πá‡∏ô‡∏Ñ‡∏ô",
    CurrentValue = false,
    Callback = function(val)
        espEnabled = val
        if not val then
            for p,_ in pairs(espBoxes) do removeESP(p) end
        end
    end
})

EyeTab:CreateToggle({
    Name = "üîç ‡πÄ‡∏õ‡∏¥‡∏î‡πÄ‡∏™‡πâ‡∏ô‡πÅ‡∏ö‡∏ö‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï",
    CurrentValue = true,
    Callback = function(val)
        espTraceEnabled = val
    end
})

