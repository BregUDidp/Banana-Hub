local PTab = Window:CreateTab("[ - Player - ]", "contact")
local MainSection = PTab:CreateSection("[ üõ∏ ]  ‡πÇ‡∏ã‡∏ô‡∏ö‡∏¥‡∏ô‡πÅ‡∏•‡∏∞‡∏õ‡∏£‡∏±‡∏ö‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß‡∏ö‡∏¥‡∏ô")
 
local Players = game:GetService("Players")
