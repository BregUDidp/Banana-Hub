	if input.KeyCode == Enum.KeyCode.Space then
		ascend = false
	elseif input.KeyCode == Enum.KeyCode.LeftShift then
		descend = false
	end
end)

-- ‡∏ï‡∏±‡∏ß‡πÅ‡∏õ‡∏£‡∏£‡∏∞‡∏ö‡∏ö
local Players = game:GetService("Players")
local RS = game:GetService("RunService")
local UIS = game:GetService("UserInputService")
local player = Players.LocalPlayer
local char = player.Character or player.CharacterAdded:Wait()
local hrp = char:WaitForChild("HumanoidRootPart")
local humanoid = char:WaitForChild("Humanoid")
 
-- ‡∏ï‡∏±‡∏ß‡πÅ‡∏õ‡∏£‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô
local flying = false
local vv, gyro
local flySpeed = 50
local superSpeed = false
local runSpeed = 32
local noclipEnabled = false
local infJumpEnabled = false
 
-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏ö‡∏¥‡∏ô
local function startFlying()
    if flying then return end
    flying = true
    humanoid.PlatformStand = true
 
    vv = Instance.new("BodyVelocity")
    vv.MaxForce = Vector3.new(1e5,1e5,1e5)
    vv.P = 1250
    vv.Velocity = Vector3.zero
    vv.Parent = hrp
 
    gyro = Instance.new("BodyGyro")
    gyro.MaxTorque = Vector3.new(4e5,4e5,4e5)
    gyro.P = 3000
    gyro.CFrame = hrp.CFrame
    gyro.Parent = hrp
 
    Rayfield:Notify({
        Title = "üõ∏ ‡πÇ‡∏´‡∏°‡∏î‡∏ö‡∏¥‡∏ô‡πÄ‡∏õ‡∏¥‡∏î!",
        Content = "‡πÉ‡∏ä‡πâ W,A,S,D,Space,Ctrl ‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏Ñ‡∏ß‡∏ö‡∏Ñ‡∏∏‡∏°",
        Duration = 3,
        Image = 4483362458
    })
end
 
local function stopFlying()
    if not flying then return end
    flying = false
    if vv then vv:Destroy() vv=nil end
    if gyro then gyro:Destroy() gyro=nil end
    humanoid.PlatformStand = false
 
    Rayfield:Notify({
        Title = "üõ¨ ‡πÇ‡∏´‡∏°‡∏î‡∏ö‡∏¥‡∏ô‡∏õ‡∏¥‡∏î",
        Content = "‡∏•‡∏á‡∏à‡∏≠‡∏î‡πÄ‡∏£‡∏µ‡∏¢‡∏ö‡∏£‡πâ‡∏≠‡∏¢",
        Duration = 3,
        Image = 6026568198
    })
end
 
-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏ß‡∏¥‡πà‡∏á‡πÄ‡∏£‡πá‡∏ß
local function toggleSpeed(on)
    superSpeed = on
    humanoid.WalkSpeed = superSpeed and runSpeed or 16
    Rayfield:Notify({
        Title = superSpeed and "üèÉ‚Äç‚ôÇÔ∏è ‡∏ß‡∏¥‡πà‡∏á‡πÄ‡∏£‡πá‡∏ß ‚úÖ" or "üö∂‚Äç‚ôÇÔ∏è ‡∏ß‡∏¥‡πà‡∏á‡πÄ‡∏£‡πá‡∏ß üõë",
        Content = superSpeed and "‡∏ß‡∏¥‡πà‡∏á‡πÄ‡∏£‡πá‡∏ß‡∏™‡∏∏‡∏î‡∏Ç‡∏µ‡∏î!" or "‡∏Å‡∏•‡∏±‡∏ö‡∏™‡∏π‡πà‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß‡∏õ‡∏Å‡∏ï‡∏¥",
        Duration = 3,
        Image = 4483362458
    })
end

-- Services
local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer
local RunService = game:GetService("RunService")

-- Variables
local Character = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
local HumanoidRootPart = Character:WaitForChild("HumanoidRootPart")
local GodModeEnabled = false

-- Teleport Positions (Approximate; adjust based on your server/map)
local Positions = {
    Stronghold = Vector3.new(1500, 50, 1200),  -- Common spawn; check map diamond icon
    Campfire = Vector3.new(0, 10, 0),           -- Your base
    Traders = Vector3.new(-500, 20, 300)        -- Trader NPC area
}

-- TP Function
local function teleportTo(pos, name)
    if HumanoidRootPart then
        HumanoidRootPart.CFrame = CFrame.new(pos)
        Rayfield:Notify({
            Title = "‡∏ß‡∏≤‡∏õ‡∏™‡∏≥‡πÄ‡∏£‡πá‡∏à [‚úÖ]",
            Content = "Teleported to " .. name .. "!",
            Duration = 3,
            Image = 4483362458
        })
    else
        Rayfield:Notify({
            Title = "Error",
            Content = "Character not loaded!",
            Duration = 3,
            Image = 4483362458
        })
    end
end

PTab:CreateButton({
    Name = "[  ‚õ∫  ]  ‡∏ß‡∏≤‡∏õ‡πÑ‡∏õ‡πÅ‡∏Ñ‡∏°‡πÑ‡∏ü",
    Callback = function()
        teleportTo(Positions.Campfire, "Campfire")
    end
})

local MainSection = PTab:CreateSection("[ üõ∏ ]  ‡πÇ‡∏ã‡∏ô‡∏ö‡∏¥‡∏ô‡πÅ‡∏•‡∏∞‡∏õ‡∏£‡∏±‡∏ö‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß‡∏ö‡∏¥‡∏ô")

local flyToggle = PTab:CreateToggle({Name="[ üõ∏ ] ‡πÄ‡∏õ‡∏¥‡∏î/‡∏õ‡∏¥‡∏î ‡∏Å‡∏≤‡∏£‡∏ö‡∏¥‡∏ô", CurrentValue=false, Flag="FlyToggle", Callback=function(val) if val then startFlying() else stopFlying() end end})
local flySlider = PTab:CreateSlider({Name="[ üìè ] ‡∏õ‡∏£‡∏±‡∏ö‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß‡∏ö‡∏¥‡∏ô", Range={1,1000}, Increment=1, Suffix=" Speed", CurrentValue=1000, Flag="FlySpeed", Callback=function(val) flySpeed=val end})
 
local Section2 = PTab:CreateSection("[üèÉ‚Äç‚ôÄÔ∏è] ‡πÇ‡∏ã‡∏ô‡∏ß‡∏¥‡πà‡∏á‡πÄ‡∏£‡πá‡∏ß‡πÅ‡∏•‡∏∞‡∏õ‡∏£‡∏±‡∏ö‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß [üèÉ‚Äç‚ôÇÔ∏è]")

local speedToggle = PTab:CreateToggle({Name="[ üèÉ‚Äç‚ôÄÔ∏è ] ‡πÄ‡∏õ‡∏¥‡∏î/‡∏õ‡∏¥‡∏î ‡∏Å‡∏≤‡∏£‡∏ß‡∏¥‡πà‡∏á‡πÄ‡∏£‡πá‡∏ß", CurrentValue=false, Flag="SpeedToggle", Callback=toggleSpeed})
local speedSlider = PTab:CreateSlider({Name="[ üìè ] ‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß‡πÉ‡∏ô‡∏Å‡∏≤‡∏£‡∏ß‡∏¥‡πà‡∏á", Range={1,1000}, Increment=1, Suffix=" Speed", CurrentValue=32, Flag="RunSpeed", Callback=function(val) runSpeed=val if superSpeed then humanoid.WalkSpeed=runSpeed end end})
 
local Section4 = PTab:CreateSection("[ üß± ] ‡πÇ‡∏ã‡∏ô‡∏•‡πà‡∏≠‡∏á‡∏´‡∏ô‡πÅ‡∏•‡∏∞‡∏ó‡∏∞‡∏•‡∏∏‡∏Å‡∏≥‡πÅ‡∏û‡∏á‡πÅ‡∏•‡∏∞‡∏Å‡∏£‡∏∞‡πÇ‡∏î‡∏î‡πÑ‡∏°‡πà‡∏à‡∏≥‡∏Å‡∏±‡∏î")

--// ‡∏ï‡∏±‡πâ‡∏á‡∏Ñ‡πà‡∏≤‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏ï‡πâ‡∏ô
local player = game.Players.LocalPlayer
local defaultSpeed = 16
local boostedSpeed = 48
local invis_on = false
local isSpeedBoosted = false
local key = Enum.KeyCode.X

--// ‡∏™‡∏£‡πâ‡∏≤‡∏á‡πÄ‡∏™‡∏µ‡∏¢‡∏á
local sound = Instance.new("Sound")
sound.SoundId = "rbxassetid://942127495"
sound.Volume = 1
sound.Parent = player:WaitForChild("PlayerGui")

--// ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏ï‡∏±‡πâ‡∏á‡∏Ñ‡πà‡∏≤‡∏Ñ‡∏ß‡∏≤‡∏°‡πÇ‡∏õ‡∏£‡πà‡∏á‡πÉ‡∏™
local function setTransparency(character, transparency)
    for _, part in pairs(character:GetDescendants()) do
        if part:IsA("BasePart") or part:IsA("Decal") then
            part.Transparency = transparency
        end
    end
end

--// ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏•‡πà‡∏≠‡∏á‡∏´‡∏ô
local function toggleInvisibility()
    invis_on = not invis_on
    sound:Play()

    if invis_on then
        local character = player.Character or player.CharacterAdded:Wait()
        local root = character:WaitForChild("HumanoidRootPart")
        local savedpos = root.CFrame

        wait()
        root.CFrame = CFrame.new(-25.95, 84, 3537.55)

        wait(0.15)
        local Seat = Instance.new("Seat", workspace)
        Seat.Anchored = false
        Seat.CanCollide = false
        Seat.Name = "invischair"
        Seat.Transparency = 1
        Seat.Position = Vector3.new(-25.95, 84, 3537.55)

        local Weld = Instance.new("Weld", Seat)
        Weld.Part0 = Seat
        Weld.Part1 = character:FindFirstChild("Torso") or character:FindFirstChild("UpperTorso")

        wait()
        Seat.CFrame = savedpos
        setTransparency(character, 0.5)

        game.StarterGui:SetCore("SendNotification", {
            Title = "‡∏Å‡∏≤‡∏£‡∏•‡πà‡∏≠‡∏á‡∏´‡∏ô [ ‚úÖ ]",
            Text = "‡∏™‡∏ñ‡∏≤‡∏ô‡∏∞: ‡∏•‡πà‡∏≠‡∏á‡∏´‡∏ô üëª",
            Duration = 3
        })
    else
        local invisChair = workspace:FindFirstChild("invischair")
        if invisChair then
            invisChair:Destroy()
        end
        setTransparency(player.Character, 0)

        game.StarterGui:SetCore("SendNotification", {
            Title = "‡∏Å‡∏≤‡∏£‡∏•‡πà‡∏≠‡∏á‡∏´‡∏ô [ üõë ]",
            Text = "‡∏™‡∏ñ‡∏≤‡∏ô‡∏∞: ‡∏Ñ‡∏ô‡πÄ‡∏õ‡πá‡∏ô‡∏õ‡∏Å‡∏ï‡∏¥",
            Duration = 3
        })
    end
end

PTab:CreateButton({Name = "[ üëª ] ‡∏Å‡∏î‡∏õ‡∏∏‡πà‡∏°‡∏•‡πà‡∏≠‡∏á‡∏´‡∏ô [ .‡∏Å‡∏î‡∏ï‡∏±‡∏ß X. ]  [ .‡∏Å‡∏î‡∏≠‡∏µ‡∏Å‡∏ó‡∏µ‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏õ‡∏¥‡∏î. [ üõë ]", Callback = function() toggleInvisibility() end})

player.CharacterAdded:Connect(function(character)
    isSpeedBoosted = false
    local humanoid = character:WaitForChild("Humanoid")
    humanoid.WalkSpeed = defaultSpeed
end)

--// ‡∏Ñ‡∏µ‡∏¢‡πå‡∏•‡∏±‡∏î‡∏Å‡∏î X
game:GetService("UserInputService").InputBegan:Connect(function(input, gameProcessed)
    if gameProcessed then return end
    if input.KeyCode == key then
        toggleInvisibility()
    end
end)

local noclipToggle = PTab:CreateToggle({Name="[ üß± ] ‡πÄ‡∏õ‡∏¥‡∏î/‡∏õ‡∏¥‡∏î ‡∏Å‡∏≤‡∏£‡∏ó‡∏∞‡∏•‡∏∏‡∏Å‡∏≥‡πÅ‡∏û‡∏á", CurrentValue=false, Flag="NoclipToggle", Callback=function(val) noclipEnabled=val end})
local infJumpToggle = PTab:CreateToggle({Name="[ ü¶ò ] ‡πÄ‡∏õ‡∏¥‡∏î/‡∏õ‡∏¥‡∏î ‡∏Å‡∏£‡∏∞‡πÇ‡∏î‡∏î‡πÑ‡∏°‡πà‡∏à‡∏≥‡∏Å‡∏±‡∏î", CurrentValue=false, Flag="InfJumpToggle", Callback=function(val) infJumpEnabled=val end})

-- ‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï‡∏•‡∏π‡∏õ
RS.RenderStepped:Connect(function()
    if flying and vv and gyro and hrp and humanoid then
        local cam = workspace.CurrentCamera
        if not cam then return end
 
        local flatForward = Vector3.new(cam.CFrame.LookVector.X,0,cam.CFrame.LookVector.Z)
        local forward = flatForward.Magnitude > 0 and flatForward.Unit or Vector3.zero
        local right = Vector3.new(cam.CFrame.RightVector.X,0,cam.CFrame.RightVector.Z)
        if right.Magnitude > 0 then right = right.Unit end
 
        local moveVec = Vector3.zero
        if UIS:IsKeyDown(Enum.KeyCode.W) then moveVec = moveVec + Vector3.new(0,0,1) end
        if UIS:IsKeyDown(Enum.KeyCode.S) then moveVec = moveVec + Vector3.new(0,0,-1) end
        if UIS:IsKeyDown(Enum.KeyCode.A) then moveVec = moveVec + Vector3.new(-1,0,0) end
        if UIS:IsKeyDown(Enum.KeyCode.D) then moveVec = moveVec + Vector3.new(1,0,0) end
        if UIS:IsKeyDown(Enum.KeyCode.Space) then moveVec = moveVec + Vector3.new(0,1,0) end
        if UIS:IsKeyDown(Enum.KeyCode.LeftControl) then moveVec = moveVec + Vector3.new(0,-1,0) end
 
        local desired = forward*moveVec.Z + right*moveVec.X + Vector3.new(0,moveVec.Y,0)
        if desired.Magnitude>0 then desired = desired.Unit*flySpeed end
        vv.Velocity = desired
 
        if desired.Magnitude>1 then
            local flatDir = Vector3.new(desired.X,0,desired.Z)
            if flatDir.Magnitude>0 then gyro.CFrame = CFrame.new(hrp.Position,hrp.Position+flatDir) end
        end
    end
end)
 
RS.Stepped:Connect(function()
    if noclipEnabled and char then
        for _,part in pairs(char:GetDescendants()) do
            if part:IsA("BasePart") then part.CanCollide=false end
        end
    end
end)
 
UIS.JumpRequest:Connect(function()
    if infJumpEnabled and humanoid then
        humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
    end
end)

UIS.JumpRequest:Connect(function()
    if infJumpEnabled and humanoid then
        humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
    end
end)
 
-- ‡∏£‡∏µ‡πÄ‡∏ã‡πá‡∏ï‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£‡πÄ‡∏°‡∏∑‡πà‡∏≠ respawn
player.CharacterAdded:Connect(function(c)
    char=c
    hrp=c:WaitForChild("HumanoidRootPart")
    humanoid=c:WaitForChild("Humanoid")
    stopFlying()
    toggleSpeed(false)
    noclipEnabled=false
    infJumpEnabled=false
    Rayfield:LoadConfiguration()
end)

-- ====== ‡∏ï‡∏±‡∏ß‡πÅ‡∏õ‡∏£ Global ======
getgenv().AutoDragEnabled = false
local offsetDistance = 0
local delayBetweenItems = 0
local itemsFolder = Workspace:WaitForChild("Items")
local remoteEvents = ReplicatedStorage:WaitForChild("RemoteEvents")
local startDragging = remoteEvents:WaitForChild("RequestStartDraggingItem")
local stopDragging = remoteEvents:WaitForChild("StopDraggingItem")

-- ====== ‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£ ======
local player = Players.LocalPlayer
local function getCharacter()
    return player.Character or player.CharacterAdded:Wait()
end

-- ====== ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏•‡∏≤‡∏Å‡πÑ‡∏≠‡πÄ‡∏ó‡∏° ======
local function dragItemToPosition(itemToDrag, position)
    local clonedItem = cloneref(itemToDrag)
    startDragging:FireServer(clonedItem)
    task.spawn(function()
        if clonedItem:IsA("BasePart") then
            clonedItem.CFrame = CFrame.new(position)
        elseif clonedItem:IsA("Model") and clonedItem.PrimaryPart then
            clonedItem:SetPrimaryPartCFrame(CFrame.new(position))
        end
    end)
    task.wait(0.05)
    stopDragging:FireServer(clonedItem)
end

-- ====== ‡∏•‡∏≤‡∏Å‡∏ó‡∏∏‡∏Å‡πÑ‡∏≠‡πÄ‡∏ó‡∏°‡∏ó‡∏µ‡πà‡πÄ‡∏•‡∏∑‡∏≠‡∏Å ======
function dragAllItemsToPlayer()
    if #selectedItems == 0 then
        Rayfield:Notify({Title="‡∏ú‡∏¥‡∏î‡∏û‡∏•‡∏≤‡∏î", Content="‡∏Å‡∏£‡∏∏‡∏ì‡∏≤‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡πÑ‡∏≠‡πÄ‡∏ó‡∏°‡∏Å‡πà‡∏≠‡∏ô!", Duration=4})
        return
    end

    local character = getCharacter()
    local hrp = character:WaitForChild("HumanoidRootPart")
    local targetPos = (hrp.CFrame * CFrame.new(0, 0, -offsetDistance)).Position

    local itemsToDrag = {}
    for _, item in ipairs(itemsFolder:GetChildren()) do
        if table.find(selectedItems, item.Name) then
            table.insert(itemsToDrag, item)
        end
    end

    if #itemsToDrag == 0 then
        Rayfield:Notify({Title="‡πÑ‡∏°‡πà‡∏û‡∏ö‡πÑ‡∏≠‡πÄ‡∏ó‡∏°", Content="‡πÑ‡∏°‡πà‡∏û‡∏ö‡πÑ‡∏≠‡πÄ‡∏ó‡∏°‡∏ó‡∏µ‡πà‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡πÉ‡∏ô‡πÅ‡∏°‡∏û!", Duration=4})
        return
    end

    Rayfield:Notify({Title="‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏•‡∏≤‡∏Å", Content="‡∏û‡∏ö " .. #itemsToDrag .. " ‡πÑ‡∏≠‡πÄ‡∏ó‡∏°", Duration=3})

    for _, item in ipairs(itemsToDrag) do
        dragItemToPosition(item, targetPos)
        task.wait(delayBetweenItems)
    end

    Rayfield:Notify({Title="‡∏™‡∏≥‡πÄ‡∏£‡πá‡∏à", Content="‡∏•‡∏≤‡∏Å " .. #itemsToDrag .. " ‡πÑ‡∏≠‡πÄ‡∏ó‡∏°‡πÄ‡∏£‡∏µ‡∏¢‡∏ö‡∏£‡πâ‡∏≠‡∏¢!", Duration=5})
end

-- ====== Auto Drag Loop ======
function autoDragLoop()
    spawn(function()
        while getgenv().AutoDragEnabled do
            dragAllItemsToPlayer()
            task.wait(1)
        end
    end)
end

-- ====== ‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï‡∏Ñ‡πà‡∏≤ Slider ======
task.spawn(function()
    while true do
        offsetDistance = OffsetSlider.CurrentValue
        delayBetweenItems = DelaySlider.CurrentValue
        task.wait(0.1)
    end
end)

-- ====== ‡∏£‡∏µ‡πÄ‡∏ü‡∏£‡∏ä‡∏£‡∏≤‡∏¢‡∏Å‡∏≤‡∏£‡πÑ‡∏≠‡πÄ‡∏ó‡∏° (‡πÅ‡∏ö‡∏ö‡πÄ‡∏Å‡πà‡∏≤) ======
function refreshItemList()
    local found = {}
    for _, obj in ipairs(itemsFolder:GetChildren()) do
        local n = obj.Name
        if n ~= "" and not table.find(found, n) then
            table.insert(found, n)
        end
    end
    table.sort(found)
    Dropdown:Refresh(found, false)
    Rayfield:Notify({Title="‡∏£‡∏µ‡πÄ‡∏ü‡∏£‡∏ä‡∏™‡∏≥‡πÄ‡∏£‡πá‡∏à", Content="‡∏û‡∏ö " .. #found .. " ‡πÑ‡∏≠‡πÄ‡∏ó‡∏°", Duration=4})
end

-- ====== Explore Map (‡∏™‡πÅ‡∏Å‡∏ô‡πÅ‡∏°‡∏û + ‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï Dropdown) ======
function exploreMapAndRefresh()
    Rayfield:Notify({Title="‡∏Å‡∏≥‡∏•‡∏±‡∏á‡∏™‡πÅ‡∏Å‡∏ô‡πÅ‡∏°‡∏û...", Content="‡∏Ñ‡πâ‡∏ô‡∏´‡∏≤‡πÑ‡∏≠‡πÄ‡∏ó‡πá‡∏°‡∏ó‡∏±‡πâ‡∏á‡∏´‡∏°‡∏î", Duration=3})

    local foundItems = {}
    for _, obj in ipairs(itemsFolder:GetChildren()) do
        local name = obj.Name
        if name and name ~= "" and not table.find(foundItems, name) then
            table.insert(foundItems, name)
        end
    end

    table.sort(foundItems)

    if #foundItems == 0 then
        Rayfield:Notify({Title="‡πÑ‡∏°‡πà‡∏û‡∏ö‡πÑ‡∏≠‡πÄ‡∏ó‡πá‡∏°", Content="‡πÑ‡∏°‡πà‡∏°‡∏µ‡πÑ‡∏≠‡πÄ‡∏ó‡πá‡∏°‡πÉ‡∏ô Items Folder", Duration=5})
        return
    end

    Dropdown:Refresh(foundItems, false)

    Rayfield:Notify({
        Title = "‡∏™‡πÅ‡∏Å‡∏ô‡πÅ‡∏°‡∏û‡∏™‡∏≥‡πÄ‡∏£‡πá‡∏à!",
        Content = "‡∏û‡∏ö " .. #foundItems .. " ‡πÑ‡∏≠‡πÄ‡∏ó‡πá‡∏°\n‡∏ï‡∏±‡∏ß‡∏≠‡∏¢‡πà‡∏≤‡∏á: " .. table.concat(foundItems, ", ", 1, math.min(5, #foundItems)),
        Duration = 6
    })

    -- ‡∏ï‡∏±‡πâ‡∏á‡∏Ñ‡πà‡∏≤‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏ï‡πâ‡∏ô‡∏ñ‡πâ‡∏≤‡∏¢‡∏±‡∏á‡πÑ‡∏°‡πà‡πÄ‡∏•‡∏∑‡∏≠‡∏Å
    if #selectedItems == 0 and #foundItems > 0 then
        selectedItems = {foundItems[1]}
        Dropdown:Set(foundItems[1])
    end
end

-- ‡∏£‡∏±‡∏ô Explore Map ‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï‡∏¥‡∏ï‡∏≠‡∏ô‡πÄ‡∏£‡∏¥‡πà‡∏°
task.wait(1)
exploreMapAndRefresh()

-- ====== ‡∏ï‡∏±‡∏ß‡πÅ‡∏õ‡∏£ ======
local itemsFolder = Workspace:WaitForChild("Items")
local remoteEvents = ReplicatedStorage:WaitForChild("RemoteEvents")
local startDragging = remoteEvents:WaitForChild("RequestStartDraggingItem")
local stopDragging = remoteEvents:WaitForChild("StopDraggingItem")
local campfireCenter = Workspace:WaitForChild("Map"):WaitForChild("Campground"):WaitForChild("MainFire"):WaitForChild("Center")

local fuelItems = {"Biofuel","Coal","Fuel Canister","Oil Barrel"}
local heightOffset = 15
local distanceOffset = 1
local delayBetweenItems = 0

-- ====== ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏•‡∏≤‡∏Å‡πÑ‡∏≠‡πÄ‡∏ó‡∏° ======
local function dragItemToPosition(itemToDrag, position)
    local clonedItem = cloneref(itemToDrag)
    startDragging:FireServer(clonedItem)
    task.spawn(function()
        if clonedItem:IsA("BasePart") then
            clonedItem.CFrame = CFrame.new(position)
        elseif clonedItem:IsA("Model") and clonedItem.PrimaryPart then
            clonedItem:SetPrimaryPartCFrame(CFrame.new(position))
        end
    end)
    task.wait(0.05)
    stopDragging:FireServer(clonedItem)
end

-- ====== ‡πÄ‡∏ï‡∏¥‡∏°‡πÑ‡∏ü ======
function dragAllFuelToFire()
    local campfireCFrame = campfireCenter.CFrame
    local targetPosition = (campfireCFrame * CFrame.new(0, heightOffset, -distanceOffset)).Position
    
    local itemsToDrag = {}
    for _, item in ipairs(itemsFolder:GetChildren()) do
        if table.find(fuelItems, item.Name) then
            table.insert(itemsToDrag, item)
        end
    end
    
    if #itemsToDrag > 0 then
        for _, item in ipairs(itemsToDrag) do
            dragItemToPosition(item, targetPosition)
            task.wait(delayBetweenItems)
        end
    end
end

-- ====== Auto Fuel Loop ======
function autoFuelLoop()
    while autoFuelRunning do
        dragAllFuelToFire()
        task.wait(2) -- ‡∏ó‡∏∏‡∏Å 2 ‡∏ß‡∏¥‡∏ô‡∏≤‡∏ó‡∏µ
    end
end
