-- Variables
local Character = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
local HumanoidRootPart = Character:WaitForChild("HumanoidRootPart")
local GodModeEnabled = false

-- Teleport Positions (Approximate; adjust based on your server/map)
local Positions = {
    Stronghold = Vector3.new(1500, 50, 1200),  -- Common spawn; check map diamond icon
    Campfire = Vector3.new(0, 10, 0),           -- Your base
    Traders = Vector3.new(-500, 20, 300)        -- Trader NPC area
}

-- TP Function
local function teleportTo(pos, name)
    if HumanoidRootPart then
        HumanoidRootPart.CFrame = CFrame.new(pos)
        Rayfield:Notify({
            Title = "‡∏ß‡∏≤‡∏õ‡∏™‡∏≥‡πÄ‡∏£‡πá‡∏à [‚úÖ]",
            Content = "Teleported to " .. name .. "!",
            Duration = 3,
            Image = 4483362458
        })
    else
        Rayfield:Notify({
            Title = "Error",
            Content = "Character not loaded!",
            Duration = 3,
            Image = 4483362458
        })
    end
end

