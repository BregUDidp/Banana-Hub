local friendTab = Window:Tab({
    Title = "||   - Teleport Player",
    Icon = "circle-user-round",
    Locked = false,
})

-- ‡∏ï‡∏±‡∏ß‡πÅ‡∏õ‡∏£‡πÄ‡∏Å‡πá‡∏ö‡∏ä‡∏∑‡πà‡∏≠‡∏ó‡∏µ‡πà‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡∏™‡∏∏‡∏î‡∏ó‡πâ‡∏≤‡∏¢
local SelectedPlayer = nil

-- Input + Suggestions (‡∏Ñ‡πâ‡∏ô‡∏´‡∏≤‡∏ä‡∏∑‡πà‡∏≠‡πÑ‡∏°‡πà‡∏ï‡πâ‡∏≠‡∏á‡∏Ñ‡∏£‡∏ö)
local PlayerInput = friendTab:Input({
    Title = "[ üë• ]  Input Player",
    Desc = "[ - Dropdown Player - ]",
    Placeholder = "Selected Input",
    Callback = function(text)
        if text == "" then
            PlayerInput:Suggestions({})
            return
        end

        local lowerText = string.lower(text)
        local suggestions = {}

        for _, plr in pairs(game.Players:GetPlayers()) do
            if plr ~= game.Players.LocalPlayer then
                local name = plr.Name
                local display = plr.DisplayName or name

                if string.find(string.lower(name), lowerText, 1, true) or 
                   string.find(string.lower(display), lowerText, 1, true) then
                    table.insert(suggestions, name)
                end
            end
        end

        -- ‡πÄ‡∏£‡∏µ‡∏¢‡∏á‡∏ä‡∏∑‡πà‡∏≠‡∏ï‡∏≤‡∏°‡∏ï‡∏±‡∏ß‡∏≠‡∏±‡∏Å‡∏©‡∏£
        table.sort(suggestions, function(a, b) return string.lower(a) < string.lower(b) end)

        -- ‡∏à‡∏≥‡∏Å‡∏±‡∏î‡πÅ‡∏™‡∏î‡∏á‡πÅ‡∏Ñ‡πà 10 ‡∏ä‡∏∑‡πà‡∏≠ ‡πÄ‡∏û‡∏∑‡πà‡∏≠‡πÑ‡∏°‡πà‡πÉ‡∏´‡πâ‡∏¢‡∏≤‡∏ß‡πÄ‡∏Å‡∏¥‡∏ô‡πÑ‡∏õ
        if #suggestions > 10 then
            for i = 11, #suggestions do suggestions[i] = nil end
        end

        PlayerInput:Suggestions(suggestions)
    end
})

-- ‡πÄ‡∏°‡∏∑‡πà‡∏≠‡∏ú‡∏π‡πâ‡πÉ‡∏ä‡πâ‡∏Ñ‡∏•‡∏¥‡∏Å‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡∏à‡∏≤‡∏Å Suggestion
PlayerInput.OnSuggestionSelected = function(name)
    SelectedPlayer = name
    PlayerInput.Value = name  -- ‡πÅ‡∏™‡∏î‡∏á‡∏ä‡∏∑‡πà‡∏≠‡πÄ‡∏ï‡πá‡∏°‡πÉ‡∏ô‡∏ä‡πà‡∏≠‡∏á
end

-- ‡∏õ‡∏∏‡πà‡∏°‡∏ß‡∏≤‡∏õ
friendTab:Button({
    Title = "[ üìå ]  Selected Player",
    Desc = "[ - Teleport To Player Select - ]",
    Icon = "bolt",
    Callback = function()
        local inputName = PlayerInput.Value

        if not inputName or inputName == "" then
            
            return
        end

        local targetPlayer = game.Players:FindFirstChild(inputName)
        local lp = game.Players.LocalPlayer

        if not targetPlayer then
            
            return
        end

        if targetPlayer == lp then
            
            return
        end

        if targetPlayer.Character and targetPlayer.Character:FindFirstChild("HumanoidRootPart") then
            if lp.Character and lp.Character:FindFirstChild("HumanoidRootPart") then
                lp.Character.HumanoidRootPart.CFrame = targetPlayer.Character.HumanoidRootPart.CFrame * CFrame.new(0, 0, -3)
                
                -- ‡∏£‡∏µ‡πÄ‡∏ã‡πá‡∏ï‡∏ä‡πà‡∏≠‡∏á‡∏´‡∏•‡∏±‡∏á‡∏ß‡∏≤‡∏õ
                PlayerInput.Value = ""
                PlayerInput:Suggestions({})
            else
                
            end
        else
            
        end
    end
})

spawn(function()
    while task.wait(10) do
        local others = #game.Players:GetPlayers() - 1
        if others <= 0 then
            InfoLabel:Update({Text = "‡πÑ‡∏°‡πà‡∏°‡∏µ‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡∏≠‡∏∑‡πà‡∏ô‡πÉ‡∏ô‡πÄ‡∏ã‡∏¥‡∏£‡πå‡∏ü‡πÄ‡∏ß‡∏≠‡∏£‡πå"})
        else
            InfoLabel:Update({Text = "‡∏°‡∏µ‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡∏≠‡∏∑‡πà‡∏ô‡∏≠‡∏µ‡∏Å " .. others .. " ‡∏Ñ‡∏ô"})
        end
    end
end)
