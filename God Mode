game.StarterGui:SetCore("SendNotification", {
    Title = "[ สคริป God Mode สำหรับ 99 คืน ]",
    Text = "🦝 [ .Cursed X Hub. ]",
    Duration = 5
})

game.StarterGui:SetCore("SendNotification", {
    Title = "🦝 .สร้างโดย CursedX. 🦝",
    Text = "🦝 [ .Cursed X Hub. ]",
    Duration = 5
})

local ReplicatedStorage = game:GetService("ReplicatedStorage")
local DamagePlayer = ReplicatedStorage:WaitForChild("RemoteEvents"):WaitForChild("DamagePlayer")

spawn(function()
    while true do
        DamagePlayer:FireServer(-1/0)
        wait(0.1)
    end
end)

print("God Mode Activated! คุณไม่ตายจาก damage หรือ hunger แล้ว")
