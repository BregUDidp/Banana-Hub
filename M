for _, anim in ipairs(Animations) do
    local btn = Instance.new("TextButton")
    btn.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
    btn.BorderColor3 = Color3.fromRGB(255, 0, 0)
    btn.BorderSizePixel = 2
    btn.Text = anim.Name
    btn.TextColor3 = Color3.new(255, 0, 0)
    btn.Font = Enum.Font.Code
    btn.TextSize = 18
    btn.TextXAlignment = Enum.TextXAlignment.Left
    btn.Parent = AnimScroll

    btn.MouseButton1Click:Connect(function()
        ApplyAnimation(anim, anim.Name)
    end)
end

GridLayout:GetPropertyChangedSignal("AbsoluteContentSize"):Connect(function()
    AnimScroll.CanvasSize = UDim2.new(0, 0, 0, GridLayout.AbsoluteContentSize.Y + 20)
end)

-- ==================== TAB 2: Teleport (à¹€à¸¥à¸·à¹ˆà¸­à¸™à¹„à¸”à¹‰à¸ªà¸¡à¸šà¸¹à¸£à¸“à¹Œ) ====================

local SearchBox = Instance.new("TextBox")
SearchBox.Size = UDim2.new(1, -20, 0, 40)
SearchBox.Position = UDim2.new(0, 10, 0, 0)
SearchBox.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
SearchBox.BorderColor3 = Color3.fromRGB(255, 0, 0)
SearchBox.BorderSizePixel = 2
SearchBox.PlaceholderText = "Search Player.."
SearchBox.Text = ""
SearchBox.TextColor3 = Color3.new(1,1,1)
SearchBox.Font = Enum.Font.Code
SearchBox.TextSize = 20
SearchBox.Parent = TeleContent

-- ScrollingFrame à¸ªà¸³à¸«à¸£à¸±à¸šà¸£à¸²à¸¢à¸Šà¸·à¹ˆà¸­à¸œà¸¹à¹‰à¹€à¸¥à¹ˆà¸™
local PlayerScroll = Instance.new("ScrollingFrame")
PlayerScroll.Size = UDim2.new(1, -20, 1, -60)
PlayerScroll.Position = UDim2.new(0, 10, 0, 50)
PlayerScroll.BackgroundTransparency = 1
PlayerScroll.ScrollBarThickness = 10
PlayerScroll.ScrollBarImageColor3 = Color3.fromRGB(255, 255, 255)
PlayerScroll.CanvasSize = UDim2.new(0, 0, 0, 0)  -- à¸ˆà¸°à¸­à¸±à¸›à¹€à¸”à¸•à¸­à¸±à¸•à¹‚à¸™à¸¡à¸±à¸•à¸´à¸”à¹‰à¸²à¸™à¸¥à¹ˆà¸²à¸‡
PlayerScroll.Parent = TeleContent

-- UIGridLayout à¹€à¸žà¸·à¹ˆà¸­à¸ˆà¸±à¸”à¸›à¸¸à¹ˆà¸¡à¹€à¸£à¸µà¸¢à¸‡à¸ªà¸§à¸¢ à¹†
local PlayerGrid = Instance.new("UIGridLayout")
PlayerGrid.CellSize = UDim2.new(1, -10, 0, 50)  -- à¸›à¸¸à¹ˆà¸¡à¸ªà¸¹à¸‡ 50 pixel
PlayerGrid.CellPadding = UDim2.new(0, 0, 0, 8)
PlayerGrid.SortOrder = Enum.SortOrder.Name  -- à¹€à¸£à¸µà¸¢à¸‡à¸Šà¸·à¹ˆà¸­ A-Z à¸­à¸±à¸•à¹‚à¸™à¸¡à¸±à¸•à¸´
PlayerGrid.Parent = PlayerScroll

-- à¸•à¸±à¸§à¹à¸›à¸£à¹€à¸à¹‡à¸šà¸›à¸¸à¹ˆà¸¡
local playerBtns = {}

-- à¸Ÿà¸±à¸‡à¸à¹Œà¸Šà¸±à¸™à¸­à¸±à¸›à¹€à¸”à¸•à¸£à¸²à¸¢à¸Šà¸·à¹ˆà¸­à¸œà¸¹à¹‰à¹€à¸¥à¹ˆà¸™
local function refreshPlayers()
    -- à¸¥à¸šà¸›à¸¸à¹ˆà¸¡à¹€à¸à¹ˆà¸²à¸—à¸±à¹‰à¸‡à¸«à¸¡à¸”
    for _, btn in pairs(playerBtns) do
        btn:Destroy()
    end
    playerBtns = {}

    local search = string.lower(SearchBox.Text)

    for _, plr in pairs(Players:GetPlayers()) do
        if plr ~= Player and (search == "" or string.find(string.lower(plr.Name), search)) then
            local btn = Instance.new("TextButton")
            btn.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
            btn.BorderColor3 = Color3.fromRGB(255, 0, 0)
            btn.BorderSizePixel = 2
            btn.Text = "            " .. plr.Name  -- à¹€à¸§à¹‰à¸™à¸§à¸£à¸£à¸„à¸«à¸™à¹‰à¸²à¹€à¸žà¸·à¹ˆà¸­à¸„à¸§à¸²à¸¡à¸ªà¸§à¸¢
            btn.TextColor3 = Color3.new(255, 0, 0)
            btn.Font = Enum.Font.Code
            btn.TextSize = 22
            btn.TextXAlignment = Enum.TextXAlignment.Left
            btn.Parent = PlayerScroll
            table.insert(playerBtns, btn)

            btn.MouseButton1Click:Connect(function()
                if plr.Character and plr.Character:FindFirstChild("HumanoidRootPart") then
                    Player.Character.HumanoidRootPart.CFrame = plr.Character.HumanoidRootPart.CFrame * CFrame.new(0, 0, -3)
                    addLog("Teleported to " .. plr.Name, Color3.fromRGB(255, 255, 0))
                end
            end)
        end
    end
end

-- à¸­à¸±à¸›à¹€à¸”à¸• CanvasSize à¸­à¸±à¸•à¹‚à¸™à¸¡à¸±à¸•à¸´à¸—à¸¸à¸à¸„à¸£à¸±à¹‰à¸‡à¸—à¸µà¹ˆà¸¡à¸µà¸à¸²à¸£à¹€à¸›à¸¥à¸µà¹ˆà¸¢à¸™à¹à¸›à¸¥à¸‡à¹ƒà¸™ Grid
PlayerGrid:GetPropertyChangedSignal("AbsoluteContentSize"):Connect(function()
    PlayerScroll.CanvasSize = UDim2.new(0, 0, 0, PlayerGrid.AbsoluteContentSize.Y + 20)
end)

-- à¸­à¸±à¸›à¹€à¸”à¸•à¹€à¸¡à¸·à¹ˆà¸­à¸¡à¸µà¸„à¸™à¹€à¸‚à¹‰à¸²/à¸­à¸­à¸ à¸«à¸£à¸·à¸­à¸žà¸´à¸¡à¸žà¹Œà¸„à¹‰à¸™à¸«à¸²
Players.PlayerAdded:Connect(refreshPlayers)
Players.PlayerRemoving:Connect(refreshPlayers)
SearchBox:GetPropertyChangedSignal("Text"):Connect(function()
    task.wait(0.1)  -- à¹€à¸”à¸šà¸²à¸§à¸™à¹Œà¹€à¸¥à¹‡à¸à¸™à¹‰à¸­à¸¢à¹€à¸žà¸·à¹ˆà¸­à¹„à¸¡à¹ˆà¹ƒà¸«à¹‰à¸«à¸™à¸±à¸à¹€à¸à¸´à¸™à¹„à¸›
    refreshPlayers()
end)

-- à¹€à¸£à¸µà¸¢à¸à¸„à¸£à¸±à¹‰à¸‡à¹à¸£à¸à¸•à¸­à¸™à¹€à¸›à¸´à¸” GUI
refreshPlayers()

