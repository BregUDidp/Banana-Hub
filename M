-- à¸Ÿà¸±à¸‡à¸à¹Œà¸Šà¸±à¸™à¹€à¸žà¸´à¹ˆà¸¡ log
local function addLog(text, color)
    local label = Instance.new("TextLabel")
    label.Size = UDim2.new(1, 0, 0, 20)
    label.BackgroundTransparency = 1
    label.Text = os.date("[%H:%M:%S] ") .. text
    label.TextColor3 = color or Color3.new(1,1,1)
    label.Font = Enum.Font.Code
    label.TextSize = 16
    label.TextXAlignment = Enum.TextXAlignment.Left
    label.Parent = LoggerScroll

    LoggerScroll.CanvasSize = UDim2.new(0, 0, 0, LoggerLayout.AbsoluteContentSize.Y + 10)
    LoggerScroll.CanvasPosition = Vector2.new(0, LoggerScroll.AbsoluteCanvasSize.Y)
end

-- à¹à¸ˆà¹‰à¸‡à¸„à¸™à¹€à¸‚à¹‰à¸²/à¸­à¸­à¸
Players.PlayerAdded:Connect(function(plr)
    if plr ~= Player then
        addLog(plr.Name .. " Joined the game", Color3.fromRGB(0, 255, 0))
    end
end)

Players.PlayerRemoving:Connect(function(plr)
    addLog(plr.Name .. " Left the game", Color3.fromRGB(255, 50, 50))
end)

-- à¹à¸ªà¸”à¸‡à¸„à¸™à¸—à¸µà¹ˆà¸­à¸¢à¸¹à¹ˆà¹ƒà¸™à¹€à¸‹à¸´à¸£à¹Œà¸Ÿà¹€à¸§à¸­à¸£à¹Œà¸•à¸­à¸™à¹€à¸›à¸´à¸”
for _, plr in pairs(Players:GetPlayers()) do
    if plr ~= Player then
        addLog(plr.Name .. " Is in server", Color3.fromRGB(138, 43, 179))
    end
end

-- Tab Switching
local function switchTab(show)
    AnimContent.Visible = show == AnimContent
    TeleContent.Visible = show == TeleContent
    CheatsContent.Visible = show == CheatsContent
    
    AnimTab.BackgroundColor3 = show == AnimContent and Color3.fromRGB(100, 0, 0) or Color3.fromRGB(50, 0, 0)
    TeleTab.BackgroundColor3 = show == TeleContent and Color3.fromRGB(100, 0, 0) or Color3.fromRGB(50, 0, 0)
    CheatsTab.BackgroundColor3 = show == CheatsContent and Color3.fromRGB(100, 0, 0) or Color3.fromRGB(50, 0, 0)
end

AnimTab.MouseButton1Click:Connect(function() switchTab(AnimContent) end)
TeleTab.MouseButton1Click:Connect(function() switchTab(TeleContent) end)
CheatsTab.MouseButton1Click:Connect(function() switchTab(CheatsContent) end)

