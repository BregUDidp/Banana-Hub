local ESPEnabled = false
local ESPData = {}  -- à¹€à¸à¹‡à¸š Highlight + BillboardGui à¸‚à¸­à¸‡à¹à¸•à¹ˆà¸¥à¸° player
local RunService = game:GetService("RunService")
local Players = game:GetService("Players")

-- à¸›à¸¸à¹ˆà¸¡ Toggle
local ESPToggle = Instance.new("TextButton")
ESPToggle.Size = UDim2.new(1, -20, 0, 40)
ESPToggle.Position = UDim2.new(0, 10, 0, 240)
ESPToggle.BackgroundColor3 = Color3.fromRGB(200, 0, 0)
ESPToggle.BorderColor3 = Color3.fromRGB(255, 255, 255)
ESPToggle.BorderSizePixel = 2
ESPToggle.Text = "ESP Player: OFF"
ESPToggle.TextColor3 = Color3.new(1, 1, 1)
ESPToggle.TextScaled = false
ESPToggle.Font = Enum.Font.Code
ESPToggle.Parent = CheatsContent

-- Glow Effect à¹€à¸¡à¸·à¹ˆà¸­à¹€à¸›à¸´à¸”
local Glow = Instance.new("ImageLabel")
Glow.Size = UDim2.new(1, 10, 1, 10)
Glow.Position = UDim2.new(0, -5, 0, -5)
Glow.BackgroundTransparency = 1
Glow.Image = "rbxassetid://4996891970"
Glow.ImageColor3 = Color3.fromRGB(1, 1, 1)
Glow.ImageTransparency = 0.6
Glow.Visible = false
Glow.ZIndex = ESPToggle.ZIndex - 1
Glow.Parent = ESPToggle

-- à¸­à¸±à¸›à¹€à¸”à¸•à¸›à¸¸à¹ˆà¸¡
local function updateToggleButton()
    if ESPEnabled then
        ESPToggle.Text = "ESP Player: ON"
        ESPToggle.BackgroundColor3 = Color3.fromRGB(0, 150, 0)
        ESPToggle.BorderColor3 = Color3.fromRGB(255, 255, 255)
        ESPToggle.TextColor3 = Color3.new(1, 1, 1)
        Glow.Visible = true
    else
        ESPToggle.Text = "ESP Player: OFF"
        ESPToggle.BackgroundColor3 = Color3.fromRGB(200, 0, 0)
        ESPToggle.BorderColor3 = Color3.fromRGB(255, 255, 255)
        ESPToggle.TextColor3 = Color3.new(1, 1, 1)
        Glow.Visible = false
    end
end

-- à¸ªà¸£à¹‰à¸²à¸‡ ESP
local function createESP(character, player)
    if not character or not character:FindFirstChild("HumanoidRootPart") or not character:FindFirstChild("Head") then return end
    
    -- à¸¥à¹‰à¸²à¸‡à¸‚à¸­à¸‡à¹€à¸à¹ˆà¸²à¸–à¹‰à¸²à¸¡à¸µ
    if ESPData[player] then
        if ESPData[player].Highlight then ESPData[player].Highlight:Destroy() end
        if ESPData[player].Billboard then ESPData[player].Billboard:Destroy() end
    end
    
    -- Highlight (à¹€à¸«à¹‡à¸™à¸œà¹ˆà¸²à¸™à¸à¸³à¹à¸žà¸‡)
    local highlight = Instance.new("Highlight")
    highlight.FillColor = Color3.fromRGB(255, 0, 0)
    highlight.OutlineColor = Color3.fromRGB(255, 0, 0)
    highlight.FillTransparency = 0.4
    highlight.OutlineTransparency = 0
    highlight.DepthMode = Enum.HighlightDepthMode.AlwaysOnTop
    highlight.Adornee = character
    highlight.Parent = character
    highlight.Enabled = ESPEnabled
    
    -- BillboardGui (à¹€à¸žà¸´à¹ˆà¸¡à¸„à¸§à¸²à¸¡à¸ªà¸¹à¸‡à¹€à¸žà¸·à¹ˆà¸­à¸£à¸­à¸‡à¸£à¸±à¸š 2 à¸šà¸£à¸£à¸—à¸±à¸”à¸Šà¸·à¹ˆà¸­)
    local billboard = Instance.new("BillboardGui")
    billboard.Adornee = character.Head
    billboard.Size = UDim2.new(0, 200, 0, 90)  -- à¸ªà¸¹à¸‡à¸‚à¸¶à¹‰à¸™à¹€à¸žà¸·à¹ˆà¸­à¹ƒà¸«à¹‰à¸¡à¸µà¸—à¸µà¹ˆà¸§à¸²à¸‡à¸Šà¸·à¹ˆà¸­ 2 à¸šà¸£à¸£à¸—à¸±à¸”
    billboard.StudsOffset = Vector3.new(0, 3.8, 0)
    billboard.AlwaysOnTop = true
    billboard.Enabled = ESPEnabled
    billboard.Parent = character.Head
    
    -- DisplayName (à¸šà¸£à¸£à¸—à¸±à¸”à¸šà¸™)
    local displayLabel = Instance.new("TextLabel")
    displayLabel.Size = UDim2.new(1, 0, 0.35, 0)
    displayLabel.Position = UDim2.new(0, 0, 0, 0)
    displayLabel.BackgroundTransparency = 1
    displayLabel.TextColor3 = Color3.new(1, 1, 1)
    displayLabel.TextStrokeTransparency = 0
    displayLabel.TextStrokeColor3 = Color3.fromRGB(255, 0, 0)
    displayLabel.Font = Enum.Font.Code
    displayLabel.TextScaled = true
    displayLabel.Parent = billboard
    
    -- Username (à¸šà¸£à¸£à¸—à¸±à¸”à¸¥à¹ˆà¸²à¸‡)
    local usernameLabel = Instance.new("TextLabel")
    usernameLabel.Size = UDim2.new(1, 0, 0.35, 0)
    usernameLabel.Position = UDim2.new(0, 0, 0.35, 0)
    usernameLabel.BackgroundTransparency = 1
    usernameLabel.TextColor3 = Color3.new(0.8, 0.8, 0.8)  -- à¹€à¸—à¸²à¸­à¹ˆà¸­à¸™à¹€à¸žà¸·à¹ˆà¸­à¹à¸¢à¸à¸ˆà¸²à¸ DisplayName
    usernameLabel.TextStrokeTransparency = 0
    usernameLabel.TextStrokeColor3 = Color3.fromRGB(255, 0, 0)
    usernameLabel.Font = Enum.Font.Code
    usernameLabel.TextScaled = true
    usernameLabel.Parent = billboard
    
    -- à¸•à¸±à¹‰à¸‡à¸„à¹ˆà¸²à¸Šà¸·à¹ˆà¸­
    if player.DisplayName ~= player.Name then
        displayLabel.Text = player.DisplayName
        usernameLabel.Text = "( " .. player.Name .. " )"
        usernameLabel.Visible = true
    else
        displayLabel.Text = "( " .. player.Name .. " )"
        usernameLabel.Visible = false  -- à¸‹à¹ˆà¸­à¸™à¸šà¸£à¸£à¸—à¸±à¸”à¸¥à¹ˆà¸²à¸‡à¸–à¹‰à¸²à¸Šà¸·à¹ˆà¸­à¸‹à¹‰à¸³
    end
    
    -- à¸à¸¥à¹ˆà¸­à¸‡à¹à¸–à¸šà¹€à¸¥à¸·à¸­à¸”
    local healthBG = Instance.new("Frame")
    healthBG.Size = UDim2.new(1, 0, 0.3, 0)
    healthBG.Position = UDim2.new(0, 0, 0.7, 0)
    healthBG.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
    healthBG.BorderSizePixel = 2
    healthBG.BorderColor3 = Color3.fromRGB(255, 0, 0)
    healthBG.Parent = billboard
    
    -- à¹à¸–à¸šà¹€à¸¥à¸·à¸­à¸”
    local healthBar = Instance.new("Frame")
    healthBar.Size = UDim2.new(1, 0, 1, 0)
    healthBar.BackgroundColor3 = Color3.fromRGB(0, 255, 0)
    healthBar.BorderSizePixel = 0
    healthBar.ZIndex = healthBG.ZIndex + 1
    healthBar.Parent = healthBG
    
    -- à¸‚à¹‰à¸­à¸„à¸§à¸²à¸¡ HP à¸•à¸£à¸‡à¸à¸¥à¸²à¸‡à¹à¸–à¸šà¹€à¸¥à¸·à¸­à¸”
    local healthText = Instance.new("TextLabel")
    healthText.Size = UDim2.new(1, 0, 1, 0)
    healthText.BackgroundTransparency = 1
    healthText.Text = "100/100 HP"
    healthText.TextColor3 = Color3.new(255, 0, 0)
    healthText.TextStrokeTransparency = 0
    healthText.TextStrokeColor3 = Color3.new(0, 0, 0)
    healthText.Font = Enum.Font.Code
    healthText.TextScaled = true
    healthText.ZIndex = healthBG.ZIndex + 2
    healthText.Parent = healthBG
    
    -- à¸­à¸±à¸›à¹€à¸”à¸•à¹€à¸¥à¸·à¸­à¸”
    local humanoid = character:FindFirstChild("Humanoid")
    if humanoid then
        local function updateHealth()
            local health = math.floor(humanoid.Health)
            local maxHealth = humanoid.MaxHealth
            local healthPercent = math.clamp(health / maxHealth, 0, 1)
            
            healthBar.Size = UDim2.new(healthPercent, 0, 1, 0)
            healthText.Text = health .. "/" .. maxHealth .. " HP"
            
            if healthPercent > 0.6 then
                healthBar.BackgroundColor3 = Color3.fromRGB(0, 255, 0)
            elseif healthPercent > 0.3 then
                healthBar.BackgroundColor3 = Color3.fromRGB(255, 255, 0)
            else
                healthBar.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
            end
        end
        
        updateHealth()
        humanoid.HealthChanged:Connect(updateHealth)
    end
    
    ESPData[player] = {
        Highlight = highlight,
        Billboard = billboard
    }
end

-- à¸Ÿà¸±à¸‡à¸à¹Œà¸Šà¸±à¸™à¹€à¸žà¸´à¹ˆà¸¡ ESP
local function addESP(player)
    local function onCharacterAdded(char)
        task.delay(0.5, function()
            if char and char.Parent then
                createESP(char, player)
            end
        end)
    end
    
    if player.Character then
        onCharacterAdded(player.Character)
    end
    
    player.CharacterAdded:Connect(onCharacterAdded)
    player.CharacterRemoving:Connect(function()
        if ESPData[player] then
            if ESPData[player].Highlight then ESPData[player].Highlight:Destroy() end
            if ESPData[player].Billboard then ESPData[player].Billboard:Destroy() end
            ESPData[player] = nil
        end
    end)
end

-- à¹€à¸žà¸´à¹ˆà¸¡à¹ƒà¸«à¹‰à¸—à¸¸à¸ player
for _, player in pairs(Players:GetPlayers()) do
    addESP(player)
end

Players.PlayerAdded:Connect(addESP)
Players.PlayerRemoving:Connect(function(player)
    if ESPData[player] then
        if ESPData[player].Highlight then ESPData[player].Highlight:Destroy() end
        if ESPData[player].Billboard then ESPData[player].Billboard:Destroy() end
        ESPData[player] = nil
    end
end)

-- Fallback à¹€à¸ªà¸–à¸µà¸¢à¸£
RunService.Heartbeat:Connect(function()
    if not ESPEnabled then return end
    for player, data in pairs(ESPData) do
        if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
            if not data.Highlight or not data.Highlight.Parent then
                addESP(player)
            end
        end
    end
end)

-- à¸›à¸¸à¹ˆà¸¡ Toggle
ESPToggle.MouseButton1Click:Connect(function()
    ESPEnabled = not ESPEnabled
    updateToggleButton()
    
    if ESPLabel then
        ESPLabel.Text = ESPEnabled and "ESP Player: ON" or "ESP Player: OFF"
        ESPLabel.TextColor3 = ESPEnabled and Color3.fromRGB(1, 1, 1) or Color3.fromRGB(255, 100, 100)
    end
    
    for _, data in pairs(ESPData) do
        if data.Highlight then data.Highlight.Enabled = ESPEnabled end
        if data.Billboard then data.Billboard.Enabled = ESPEnabled end
    end
end)

-- à¹€à¸£à¸´à¹ˆà¸¡à¸•à¹‰à¸™
updateToggleButton()

-- Teleport Tools Label (à¸ˆà¸°à¹à¸ªà¸”à¸‡à¸ªà¸–à¸²à¸™à¸°)
local ToolLabel = Instance.new("TextLabel")
