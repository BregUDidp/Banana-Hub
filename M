-- Noclip
local NoclipToggle = Instance.new("TextButton")
NoclipToggle.Size = UDim2.new(1, -20, 0, 40)
NoclipToggle.Position = UDim2.new(0, 10, 0, 120)
NoclipToggle.BackgroundColor3 = Color3.fromRGB(200, 0, 0)
NoclipToggle.BorderColor3 = Color3.fromRGB(255, 255, 255)
NoclipToggle.BorderSizePixel = 2
NoclipToggle.Text = "Noclip Walling: OFF"
NoclipToggle.TextColor3 = Color3.new(1,1,1)
NoclipToggle.Font = Enum.Font.Code
NoclipToggle.Parent = CheatsContent

-- Noclip
NoclipToggle.MouseButton1Click:Connect(function()
    NoclipEnabled = not NoclipEnabled
    NoclipToggle.Text = NoclipEnabled and "Noclip Walling: ON" or "Noclip Walling: OFF"
    NoclipToggle.BackgroundColor3 = NoclipEnabled and Color3.fromRGB(0, 150, 0) or Color3.fromRGB(200, 0, 0)

    if NoclipEnabled then
        NoclipConn = RunService.Stepped:Connect(function()
            if Player.Character then
                for _, part in pairs(Player.Character:GetDescendants()) do
                    if part:IsA("BasePart") then
                        part.CanCollide = false
                    end
                end
            end
        end)
    else
        if NoclipConn then NoclipConn:Disconnect() end
        if Player.Character then
            for _, part in pairs(Player.Character:GetDescendants()) do
                if part:IsA("BasePart") and part.Name ~= "HumanoidRootPart" then
                    part.CanCollide = true
                end
            end
        end
    end
end)

