-- Teleport Tools Toggle Button (à¸›à¸¸à¹ˆà¸¡à¸à¸”à¹€à¸›à¸´à¸”/à¸›à¸´à¸”)
local ToolToggle = Instance.new("TextButton")
ToolToggle.Size = UDim2.new(1, -20, 0, 40)
ToolToggle.Position = UDim2.new(0, 10, 0, 360)
ToolToggle.BackgroundColor3 = Color3.fromRGB(200, 0, 0)
ToolToggle.BorderColor3 = Color3.fromRGB(255, 255, 255)
ToolToggle.BorderSizePixel = 2
ToolToggle.Text = "Teleport Tools Toggle"  -- à¸‚à¹‰à¸­à¸„à¸§à¸²à¸¡à¸›à¸¸à¹ˆà¸¡à¸„à¸‡à¸—à¸µà¹ˆ à¸«à¸£à¸·à¸­à¸ˆà¸°à¹ƒà¸ªà¹ˆ "à¹€à¸›à¸´à¸”/à¸›à¸´à¸” Teleport" à¸à¹‡à¹„à¸”à¹‰
ToolToggle.TextColor3 = Color3.new(1, 1, 1)
ToolToggle.Font = Enum.Font.Code
ToolToggle.TextScaled = false
ToolToggle.Parent = CheatsContent

-- à¸•à¸±à¸§à¹à¸›à¸£à¸ªà¸–à¸²à¸™à¸°
local ToolEnabled = false
local TeleportTool = nil
local Player = game.Players.LocalPlayer

-- à¸Ÿà¸±à¸‡à¸à¹Œà¸Šà¸±à¸™à¸­à¸±à¸žà¹€à¸”à¸—à¸‚à¹‰à¸­à¸„à¸§à¸²à¸¡à¹ƒà¸™ Label
local function updateLabelStatus()
    if ToolEnabled then
        ToolLabel.Text = "Teleport Tools: ON"
        ToolLabel.TextColor3 = Color3.fromRGB(0, 255, 0)  -- à¹€à¸‚à¸µà¸¢à¸§à¹€à¸¡à¸·à¹ˆà¸­à¹€à¸›à¸´à¸”
        ToolToggle.BackgroundColor3 = Color3.fromRGB(0, 150, 0)
    else
        ToolLabel.Text = "Teleport Tools: OFF"
        ToolLabel.TextColor3 = Color3.fromRGB(255, 100, 100)  -- à¹à¸”à¸‡à¸­à¹ˆà¸­à¸™à¹€à¸¡à¸·à¹ˆà¸­à¸›à¸´à¸”
        ToolToggle.BackgroundColor3 = Color3.fromRGB(200, 0, 0)
    end
end

-- à¸Ÿà¸±à¸‡à¸à¹Œà¸Šà¸±à¸™à¸ªà¸£à¹‰à¸²à¸‡ Teleport Tool (à¹€à¸«à¸¡à¸·à¸­à¸™à¹€à¸”à¸´à¸¡)
local function createTeleportTool()
    -- [à¹‚à¸„à¹‰à¸”à¸ªà¸£à¹‰à¸²à¸‡ Tool à¸—à¸±à¹‰à¸‡à¸«à¸¡à¸”à¹€à¸«à¸¡à¸·à¸­à¸™à¹€à¸”à¸´à¸¡ à¹„à¸¡à¹ˆà¹€à¸›à¸¥à¸µà¹ˆà¸¢à¸™à¹à¸›à¸¥à¸‡]
    local tool = Instance.new("Tool")
    tool.Name = "ðŸ­"
    tool.ToolTip = "[ ðŸ­ ] Teleport Tool"
    tool.RequiresHandle = true

    local handle = Instance.new("Part")
    handle.Name = "Handle"
    handle.Size = Vector3.new(2.2, 2.2, 2.2)
    handle.Shape = Enum.PartType.Ball
    handle.Material = Enum.Material.ForceField
    handle.Color = Color3.fromRGB(138, 43, 179)
    handle.Transparency = 0.15
    handle.CanCollide = false
    handle.Parent = tool

    local sparkles = Instance.new("Sparkles")
    sparkles.SparkleColor = Color3.fromRGB(138, 43, 179)
    sparkles.Enabled = false
    sparkles.Parent = handle

    local particles = Instance.new("ParticleEmitter")
    particles.Texture = "rbxassetid://241650934"
    particles.Color = ColorSequence.new(Color3.fromRGB(138, 43, 179), Color3.fromRGB(180, 100, 200))
    particles.Size = NumberSequence.new(0.4)
    particles.Lifetime = NumberRange.new(1.5, 2.5)
    particles.Rate = 20
    particles.SpreadAngle = Vector2.new(45, 45)
    particles.Speed = NumberRange.new(2)
    particles.Parent = handle

    local light = Instance.new("PointLight")
    light.Color = Color3.fromRGB(138, 43, 179)
    light.Brightness = 2
    light.Range = 10
    light.Parent = handle

    local teleportSound = Instance.new("Sound")
    teleportSound.SoundId = "rbxassetid://4861638982"
    teleportSound.Volume = 0.7
    teleportSound.Parent = handle

    tool.Equipped:Connect(function()
        sparkles.Enabled = false
        particles.Rate = 50
        light.Brightness = 0
    end)

    tool.Unequipped:Connect(function()
        sparkles.Enabled = false
        particles.Rate = 20
        light.Brightness = 2
    end)

    tool.Activated:Connect(function()
        teleportSound:Play()
        local character = Player.Character
        if character and character:FindFirstChild("HumanoidRootPart") then
            local mouse = Player:GetMouse()
            local targetPos = mouse.Hit.Position + Vector3.new(0, 5, 0)
            character.HumanoidRootPart.CFrame = CFrame.new(targetPos)
        end
    end)

    tool.Equipped:Connect(function()
        local equipSound = Instance.new("Sound")
        equipSound.SoundId = "rbxassetid://2865227271"
        equipSound.Volume = 0.5
        equipSound.Parent = Player.Character:FindFirstChild("HumanoidRootPart")
        if equipSound.Parent then
            equipSound:Play()
            equipSound.Ended:Connect(function()
                equipSound:Destroy()
            end)
        end
    end)

    return tool
end

-- à¸„à¸¥à¸´à¸à¸›à¸¸à¹ˆà¸¡ Toggle
ToolToggle.MouseButton1Click:Connect(function()
    ToolEnabled = not ToolEnabled

    if ToolEnabled then
        -- à¸ªà¸£à¹‰à¸²à¸‡ Tool
        TeleportTool = createTeleportTool()
        TeleportTool.Parent = Player:WaitForChild("Backpack")
    else
        -- à¸¥à¸š Tool
        if TeleportTool then
            TeleportTool:Destroy()
            TeleportTool = nil
        end
    end

    -- à¸­à¸±à¸žà¹€à¸”à¸—à¸ªà¸–à¸²à¸™à¸°à¹ƒà¸™ Label à¹à¸¥à¸°à¸ªà¸µà¸›à¸¸à¹ˆà¸¡
    updateLabelStatus()
end)

-- à¸£à¸­à¸‡à¸£à¸±à¸šà¸à¸²à¸£à¹€à¸à¸´à¸”à¹ƒà¸«à¸¡à¹ˆ
Player.CharacterAdded:Connect(function()
    if ToolEnabled and TeleportTool == nil then
        task.wait(1)
        TeleportTool = createTeleportTool()
        TeleportTool.Parent = Player:WaitForChild("Backpack")
    end
end)

-- à¸•à¸±à¹‰à¸‡à¸„à¹ˆà¸²à¸ªà¸–à¸²à¸™à¸°à¹€à¸£à¸´à¹ˆà¸¡à¸•à¹‰à¸™
updateLabelStatus()

-- à¸Ÿà¸±à¸‡à¸à¹Œà¸Šà¸±à¸™ Speed Hack
SpeedToggle.MouseButton1Click:Connect(function()
    SpeedEnabled = not SpeedEnabled
    if SpeedEnabled then
        SpeedLabel.Text = "Speed Hack: ON ( Value: " .. SpeedValue .. " )"
        SpeedLabel.TextColor3 = Color3.fromRGB(0, 255, 0)
    else
        SpeedLabel.Text = "Speed Hack: OFF ( Value: " .. SpeedValue .. " )"
        SpeedLabel.TextColor3 = Color3.fromRGB(255, 100, 100)
        if Player.Character and Player.Character:FindFirstChild("Humanoid") then
            Player.Character.Humanoid.WalkSpeed = 16
        end
    end
end)

SpeedPlus.MouseButton1Click:Connect(function()
    SpeedValue = SpeedValue + 1
    SpeedLabel.Text = SpeedEnabled and "Speed Hack: ON ( Value: " .. SpeedValue .. " )" or "Speed Hack: OFF ( Value: " .. SpeedValue .. " )"
end)

SpeedMinus.MouseButton1Click:Connect(function()
    if SpeedValue > 16 then
        SpeedValue = SpeedValue - 1
        SpeedLabel.Text = SpeedEnabled and "Speed Hack: ON ( Value: " .. SpeedValue .. " )" or "Speed Hack: OFF ( Value: " .. SpeedValue .. " )"
    end
end)

RunService.Stepped:Connect(function()
    if SpeedEnabled and Player.Character and Player.Character:FindFirstChild("Humanoid") then
        Player.Character.Humanoid.WalkSpeed = SpeedValue
    end
end)

-- Infinite Jump
InfJumpToggle.MouseButton1Click:Connect(function()
    InfJumpEnabled = not InfJumpEnabled
    InfJumpToggle.Text = InfJumpEnabled and "Infinite Jump: ON" or "Infinite Jump: OFF"
    InfJumpToggle.BackgroundColor3 = InfJumpEnabled and Color3.fromRGB(0, 150, 0) or Color3.fromRGB(200, 0, 0)

    if InfJumpEnabled then
        InfJumpConn = UserInputService.JumpRequest:Connect(function()
            if Player.Character and Player.Character:FindFirstChild("Humanoid") then
                Player.Character.Humanoid:ChangeState("Jumping")
            end
        end)
    else
        if InfJumpConn then InfJumpConn:Disconnect() end
    end
end)
