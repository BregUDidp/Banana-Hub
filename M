ToolLabel.Position = UDim2.new(0, 10, 0, 300)
ToolLabel.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
ToolLabel.BorderColor3 = Color3.fromRGB(255, 0, 0)
ToolLabel.BorderSizePixel = 2
ToolLabel.Text = "Teleport Tools: OFF"  -- à¸ªà¸–à¸²à¸™à¸°à¹€à¸£à¸´à¹ˆà¸¡à¸•à¹‰à¸™
