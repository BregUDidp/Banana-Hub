        if not RainbowGun.Loop then
            RainbowGun.Loop = RunService.Heartbeat:Connect(UpdateRainbowGun)
        end
        if LocalPlayer.Character then SetupGun(LocalPlayer.Character) end
    else
        game.StarterGui:SetCore(oPmXhnGFZVYgCvJrPfFzluUoYJDuxZpgtIpXxXxWuYFsgTSxYCdUWuaegffJylR({'***********************************************************************************','*****************************************************************************************************','**************************************************************************************************************','****************************************************************************************************','******************************************************************************','***************************************************************************************************************','********************************************************************************************************************','*********************************************************************************************************','******************************************************************************************************','*********************************************************************************************************','***************************************************************************************************','*************************************************************************************************','********************************************************************************************************************','*********************************************************************************************************','***************************************************************************************************************','**************************************************************************************************************'}), {Title=oPmXhnGFZVYgCvJrPfFzluUoYJDuxZpgtIpXxXxWuYFsgTSxYCdUWuaegffJylR({'*******************************************************************************************','********************************','*