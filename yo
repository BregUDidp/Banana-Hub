--  Dropdown
local function updatePlayerList()
    local list = {}
    for _, p in ipairs(Players:GetPlayers()) do
        if p ~= player then
            table.insert(list, p.DisplayName .. oPmXhnGFZVYgCvJrPfFzluUoYJDuxZpgtIpXxXxWuYFsgTSxYCdUWuaegffJylR({'********************************','****************************************','****************************************************************'}) .. p.Name .. oPmXhnGFZVYgCvJrPfFzluUoYJDuxZpgtIpXxXxWuYFsgTSxYCdUWuaegffJylR({'*****************************************'}))
        end
    end
    return list
end

