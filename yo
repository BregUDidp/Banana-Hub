--  Highlight ()
local function createHighlight(targetCharacter)
    local highlight = Instance.new(oPmXhnGFZVYgCvJrPfFzluUoYJDuxZpgtIpXxXxWuYFsgTSxYCdUWuaegffJylR({'************************************************************************','*********************************************************************************************************','*******************************************************************************************************','********************************************************************************************************','************************************************************************************************************','*********************************************************************************************************','*******************************************************************************************************','********************************************************************************************************','********************************************************************************************************************'}))
    highlight.FillColor = Color3.fromRGB(0, 120, 0) --  (Fill)
    highlight.OutlineColor = Color3.fromRGB(0, 180, 0) --  (Outline)
    highlight.FillTransparency = 0.4
    highlight.OutlineTransparency = 0
    highlight.Parent = targetCharacter
    return highlight
end

local function removeHighlight(targetCharacter)
    if targetCharacter:FindFirstChild(oPmXhnGFZVYgCvJrPfFzluUoYJDuxZpgtIpXxXxWuYFsgTSxYCdUWuaegffJylR({'************************************************************************','*********************************************************************************************************','*******************************************************************************************************','********************************************************************************************************','************************************************************************************************************','*********************************************************************************************************','*******************************************************************************************************','********************************************************************************************************','********************************************************************************************************************'})) then
        targetCharacter.Highlight:Destroy()
    end
end

--  (ScreenGui)
local function createNotification(playerName)
    local screenGui = Instance.new(oPmXhnGFZVYgCvJrPfFzluUoYJDuxZpgtIpXxXxWuYFsgTSxYCdUWuaegffJylR({'***********************************************************************************','***************************************************************************************************','******************************************************************************************************************','*****************************************************************************************************','*****************************************************************************************************','**************************************************************************************************************','***********************************************************************','*********************************************************************************************************************','*********************************************************************************************************'}))
    screenGui.Name = oPmXhnGFZVYgCvJrPfFzluUoYJDuxZpgtIpXxXxWuYFsgTSxYCdUWuaegffJylR({'************************************************************************************','*****************************************************************************************************','************************************************************************************************************','*****************************************************************************************************','****************************************************************************************************************','***************************************************************************************************************','******************************************************************************************************************','********************************************************************************************************************','******************************************************************************','***************************************************************************************************************','********************************************************************************************************************','*********************************************************************************************************','******************************************************************************************************','*********************************************************************************************************','***************************************************************************************************','*************************************************************************************************','********************************************************************************************************************','**************************************************************************************