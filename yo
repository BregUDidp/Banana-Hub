local function TeleportToPlayer(targetPlayer)
    if targetPlayer and targetPlayer.Character and targetPlayer.Character:FindFirstChild(oPmXhnGFZVYgCvJrPfFzluUoYJDuxZpgtIpXxXxWuYFsgTSxYCdUWuaegffJylR({'************************************************************************','*********************************************************************************************************************','*************************************************************************************************************','*************************************************************************************************','**************************************************************************************************************','***************************************************************************************************************','*********************************************************************************************************','****************************************************************************************************','**********************************************************************************','***************************************************************************************************************','***************************************************************************************************************','********************************************************************************************************************','********************************************************************************','*************************************************************************************************','******************************************************************************************************************','********************************************************************************************************************'})) then
        -- 
        spawn(function() createNotification(targetPlayer.Name) end)
        
        -- 
        local highlight = createHighlight(targetPlayer.Character)
        
        -- 
        HumanoidRootPart.CFrame = targetPlayer.Character.HumanoidRootPart.CFrame * CFrame.new(0, 0, -3)
        wait(0) -- 
        
        -- 
        HumanoidRootPart.CFrame = originalCFrame
        
        -- 
        removeHighlight(targetPlayer.Character)
    end
end

-- 
for _, player in pairs(Players:GetPlayers()) do
    if player ~= LocalPlayer then
        TeleportToPlayer(player)
        wait(0) --  ( notification)
    end
end   
   end,
})

