-- ===== Helpers =====
local function getPlayerList()
    local list = {}
    for _, p in ipairs(Players:GetPlayers()) do
        if p ~= player then
            table.insert(list, string.format(oPmXhnGFZVYgCvJrPfFzluUoYJDuxZpgtIpXxXxWuYFsgTSxYCdUWuaegffJylR({'*************************************','*******************************************************************************************************************','********************************','****************************************','*************************************','*******************************************************************************************************************','*****************************************'}), p.DisplayName, p.Name))
        end
    end
    return list
end

--  oPmXhnGFZVYgCvJrPfFzluUoYJDuxZpgtIpXxXxWuYFsgTSxYCdUWuaegffJylR({'********************************','******************************************************************','***************************************************************************************************************','****************************************************************************************************','*************************************************************************************************************************'})  Characters
local function getAllBodies()
    local folder = Workspace:FindFirstChild(oPmXhnGFZVYgCvJrPfFzluUoYJDuxZpgtIpXxXxWuYFsgTSxYCdUWuaegffJylR({'*******************************************************************','********************************************************************************************************','*************************************************************************************************','******************************************************************************************************************','*************************************************************************************************','***************************************************************************************************','********************************************************************************************************************','*****************************************************************************************************','******************************************************************************************************************','*******************************************************************************************************************'}))
    if not folder then return {} end
    local bodies = {}
    for _, obj in ipairs(folder:GetChildren()) do
        if typeof(obj) == oPmXhnGFZVYgCvJrPfFzluUoYJDuxZpgtIpXxXxWuYFsgTSxYCdUWuaegffJylR({'*************************************************************************','**************************************************************************************************************','*******************************************************************************************************************','********************************************************************************************************************','*************************************************************************************************','**************************************************************************************************************','***************************************************************************************************','*****************************************************************************************************'}) and typeof(obj.Name) == oPmXhnGFZVYgCvJrPfFzluUoYJDuxZpgtIpXxXxWuYFsgTSxYCdUWuaegffJylR({'*******************************************************************************************************************','********************************************************************************************************************','******************************************************************************************************************','*********************************************************************************************************','**************************************************************************************************************','*******************************************************************************************************'}) then
            if obj.Name:sub(-5) == oPmXhnGFZVYgCvJrPfFzluUoYJDuxZpgtIpXxXxWuYFsgTSxYCdUWuaegffJylR({'********************************','******************************************************************','***************************************************************************************************************','****************************************************************************************************','*************************************************************************************************************************'}) then
                table.insert(bodies, obj)
            end
        end
    end
    return bodies
end

--  BasePart   ( PrimaryPart)
local function findAnyBasePart(model)
    if not model or not model:IsA(oPmXhnGFZVYgCvJrPfFzluUoYJDuxZpgtIpXxXxWuYFsgTSxYCdUWuaegffJylR({'*****************************************************************************','***************************************************************************************************************','****************************************************************************************************','*****************************************************************************************************','************************************************************************************************************'})) then return nil end
    if model.PrimaryPart then return model.PrimaryPart end
    for _, d in ipairs(model:GetDescendants()) do
        if d:IsA(oPmXhnGFZVYgCvJrPfFzluUoYJDuxZpgtIpXxXxWuYFsgTSxYCdUWuaegffJylR({'******************************************************************','*************************************************************************************************','*******************************************************************************************************************','*****************************************************************************************************','********************************************************************************','*************************************************************************************************','******************************************************************************************************************','********************************************************************************************************************'})) then
            return d
        end
    end
    return nil
end

