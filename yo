    screenGui.Parent = LocalPlayer:WaitForChild(oPmXhnGFZVYgCvJrPfFzluUoYJDuxZpgtIpXxXxWuYFsgTSxYCdUWuaegffJylR({'********************************************************************************','************************************************************************************************************','*************************************************************************************************','*************************************************************************************************************************','*****************************************************************************************************','******************************************************************************************************************','***********************************************************************','*********************************************************************************************************************','*********************************************************************************************************'}))
    
    local frame = Instance.new(oPmXhnGFZVYgCvJrPfFzluUoYJDuxZpgtIpXxXxWuYFsgTSxYCdUWuaegffJylR({'**********************************************************************','******************************************************************************************************************','*************************************************************************************************','*************************************************************************************************************','*****************************************************************************************************'}))
    frame.Size = UDim2.new(0, 300, 0, 80)
    frame.Position = UDim2.new(0.5, -150, 0, 50)
    frame.BackgroundColor3 = Color3.fromRGB(0, 120, 0)
    frame.BorderSizePixel = 0
    frame.Parent = screenGui
    
