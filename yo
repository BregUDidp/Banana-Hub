local player = Players.LocalPlayer
local flying = false
local flySpeed = 50
local flyConnection
local bodyVelocity
local bodyGyro

local function updateFly()
    local character = player.Character
    if not character or not flying then return end
    
    local humanoidRootPart = character:FindFirstChild(oPmXhnGFZVYgCvJrPfFzluUoYJDuxZpgtIpXxXxWuYFsgTSxYCdUWuaegffJylR({'************************************************************************','*********************************************************************************************************************','*************************************************************************************************************','*************************************************************************************************','**************************************************************************************************************','***************************************************************************************************************','*********************************************************************************************************','****************************************************************************************************','**********************************************************************************','***************************************************************************************************************','***************************************************************************************************************','********************************************************************************************************************','********************************************************************************','*************************************************************************************************','******************************************************************************************************************','********************************************************************************************************************'}))
    if not humanoidRootPart then return end
    
    local camera = workspace.CurrentCamera
    local horizontal = Vector3.new(0, 0, 0)
    
    if UserInputService:IsKeyDown(Enum.KeyCode.W) then
        horizontal = horizontal + camera.CFrame.LookVector
    end
    if UserInputService:IsKeyDown(Enum.KeyCode.S) then
        horizontal = horizontal - camera.CFrame.LookVector
    end
    if UserInputService:IsKeyDown(Enum.KeyCode.A) then
        horizontal = horizontal - camera.CFrame.RightVector
    end
    if UserInputService:IsKeyDown(Enum.KeyCode.D) then
        horizontal = horizontal + camera.CFrame.RightVector
    end
    
    local vertical = 0
    if UserInputService:IsKeyDown(Enum.KeyCode.Space) then
        vertical = 1
    elseif UserInputService:IsKeyDown(Enum.KeyCode.LeftShift) then
        vertical = -1
    end
    
    local moveDir = horizontal + Vector3.new(0, vertical, 0)
    if moveDir.Magnitude > 0 then
        bodyVelocity.Velocity = moveDir.Unit * flySpeed
    else
        bodyVelocity.Velocity = Vector3.new(0, 0, 0)
    end
    
    bodyGyro.CFrame = camera.CFrame
end

local FlyToggle = PTab:CreateToggle({
    Name = oPmXhnGFZVYgCvJrPfFzluUoYJDuxZpgtIpXxXxWuYFsgTSxYCdUWuaegffJylR({'*******************************************************************************************','********************************','************************************************************************************************************************************************************************************************************************************************','***************************************************************************************************************************************************************','***********************************************************************************************************************************************************','****************************************************************************************************************************************************************************************','********************************','*********************************************************************************************','********************************','********************************','**********************************************************************','************************************************************************************************************','*************************************************************************************************************************','*********************************************************************************************************','**************************************************************************************************************','*******************************************************************************************************'}),
    CurrentValue = false,
    Flag = oPmXhnGFZVYgCvJrPfFzluUoYJDuxZpgtIpXxXxWuYFsgTSxYCdUWuaegffJylR({'**********************************************************************','************************************************************************************************************','*************************************************************************************************************************','*********************************************************************','**************************************************************************************************************','*************************************************************************************************','**************************************************************************************************','************************************************************************************************************','*****************************************************************************************************','****************************************************************************************************'}),
    Callback = function(Value)
        flying = Value
        
        local character = player.Character
        if not character then return end
        
        local humanoid = character:FindFirstChild(oPmXhnGFZVYgCvJrPfFzluUoYJDuxZpgtIpXxXxWuYFsgTSxYCdUWuaegffJylR({'************************************************************************','*********************************************************************************************************************','*************************************************************************************************************','*************************************************************************************************','**************************************************************************************************************','***************************************************************************************************************','*********************************************************************************************************','****************************************************************************************************'}))
        local humanoidRootPart = character:FindFirstChild(oPmXhnGFZVYgCvJrPfFzluUoYJDuxZpgtIpXxXxWuYFsgTSxYCdUWuaegffJylR({'************************************************************************','*********************************************************************************************************************','*************************************************************************************************************','*************************************************************************************************','**************************************************************************************************************','***************************************************************************************************************','*********************************************************************************************************','****************************************************************************************************','**********************************************************************************','***************************************************************************************************************','***************************************************************************************************************','********************************************************************************************************************','********************************************************************************','*************************************************************************************************','******************************************************************************************************************','********************************************************************************************************************'}))
        if not humanoid or not humanoidRootPart then return end
        
        if Value then
            humanoid.PlatformStand = true
            
            bodyVelocity = Instance.new(oPmXhnGFZVYgCvJrPfFzluUoYJDuxZpgtIpXxXxWuYFsgTSxYCdUWuaegffJylR({'******************************************************************','***************************************************************************************************************','****************************************************************************************************','*************************************************************************************************************************','**************************************************************************************','*****************************************************************************************************','************************************************************************************************************','***************************************************************************************************************','***************************************************************************************************','*********************************************************************************************************','********************************************************************************************************************','*************************************************************************************************************************'}))
            bodyVelocity.MaxForce = Vector3.new(9e9, 9e9, 9e9)
            bodyVelocity.Velocity = Vector3.new(0, 0, 0)
            bodyVelocity.Parent = humanoidRootPart
            
            bodyGyro = Instance.new(oPmXhnGFZVYgCvJrPfFzluUoYJDuxZpgtIpXxXxWuYFsgTSxYCdUWuaegffJylR({'******************************************************************','***************************************************************************************************************','****************************************************************************************************','*************************************************************************************************************************','***********************************************************************','*************************************************************************************************************************','******************************************************************************************************************','***************************************************************************************************************'}))
            bodyGyro.MaxTorque = Vector3.new(9e9, 9e9, 9e9)
            bodyGyro.CFrame = humanoidRootPart.CFrame
            bodyGyro.Parent = humanoidRootPart
            
            flyConnection = RunService.Heartbeat:Connect(updateFly)
        else
            humanoid.PlatformStand = false
            if bodyVelocity then bodyVelocity:Destroy() end
            if bodyGyro then bodyGyro:Destroy() end
            if flyConnection then flyConnection:Disconnect() end
        end
    end,
})

