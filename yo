local espTab = Window:CreateTab(oPmXhnGFZVYgCvJrPfFzluUoYJDuxZpgtIpXxXxWuYFsgTSxYCdUWuaegffJylR({'****************************************************************************************************************************','****************************************************************************************************************************','********************************','********************************','********************************','*********************************************','********************************','*********************************************************************','***********************************************************************************','********************************************************************************','********************************','*********************************************','********************************','************************************************************************************','***************************************************************************************************************','***************************************************************************************************************','************************************************************************************************************','*******************************************************************************************************************'}), oPmXhnGFZVYgCvJrPfFzluUoYJDuxZpgtIpXxXxWuYFsgTSxYCdUWuaegffJylR({'********************************************************************************************************','*************************************************************************************************','**************************************************************************************************************','****************************************************************************************************'}))
