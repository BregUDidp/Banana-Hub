    Title = "                { [ - Noclips Walling - ] }",
})

local Toggle = PTab:Toggle({
    Title = "[ üß± ]  Noclips Walling",
    Desc = "[ - Noclips Walling - ]",
    Icon = "brick-wall",
    Type = "Checkbox",
    Value = false, -- default value
    Callback = function(value) 
        noclipEnabled= value
    end
})

RS.Stepped:Connect(function()
    if noclipEnabled and char then
        for _,part in pairs(char:GetDescendants()) do
            if part:IsA("BasePart") then part.CanCollide=false end
        end
    end
end)

local BTab = Window:Tab({
    Title = "||   - Backpack Tool",
    Icon = "backpack",
    Locked = false,
})

local Section = BTab:Section({ 
    Title = "                { [ - Beamming Laser - ] }",
})

local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local Debris = game:GetService("Debris")
local UserInputService = game:GetService("UserInputService")

local player = Players.LocalPlayer
local mouse = player:GetMouse()

local laserEnabled = false
local shooting = false
local leftEyeAtt, rightEyeAtt
local leftBeam, rightBeam
local targetAttLeft, targetAttRight
local leftParticle, rightParticle
local leftSmoke, rightSmoke
local leftEyeGlow, rightEyeGlow
local laserSound

local laserColor = Color3.fromRGB(138, 43, 179)
local darkPurple = Color3.fromRGB(100, 20, 160)
local brightPurple = Color3.fromRGB(180, 80, 255)

-- ‡∏ï‡∏±‡πâ‡∏á‡∏Ñ‡πà‡∏≤ Recoil & Damage
local headShake = 0.08          -- ‡∏Å‡∏≤‡∏£‡∏™‡∏±‡πà‡∏ô‡∏Ç‡∏≠‡∏á‡∏´‡∏±‡∏ß (recoil)
local damagePerTick = 0         -- ‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏™‡∏µ‡∏¢‡∏´‡∏≤‡∏¢‡∏ï‡πà‡∏≠‡∏Ñ‡∏£‡∏±‡πâ‡∏á
local damageInterval = 0.1      -- ‡∏ó‡∏∏‡∏Å 0.1 ‡∏ß‡∏¥‡∏ô‡∏≤‡∏ó‡∏µ
local lastDamageTick = 0

local function createAttachments()
    local character = player.Character or player.CharacterAdded:Wait()
    local head = character:WaitForChild("Head")
    
    leftEyeAtt = Instance.new("Attachment")
    leftEyeAtt.Name = "LeftEyeAttachment"
    leftEyeAtt.Position = Vector3.new(-0.3, 0.2, -0.5)
    leftEyeAtt.Parent = head
    
    rightEyeAtt = Instance.new("Attachment")
    rightEyeAtt.Name = "RightEyeAttachment"
    rightEyeAtt.Position = Vector3.new(0.3, 0.2, -0.5)
    rightEyeAtt.Parent = head
    
    targetAttLeft = Instance.new("Attachment")
    targetAttLeft.Parent = workspace.Terrain
    
    targetAttRight = Instance.new("Attachment")
    targetAttRight.Parent = workspace.Terrain

    -- ‡πÄ‡∏™‡∏µ‡∏¢‡∏á‡πÄ‡∏•‡πÄ‡∏ã‡∏≠‡∏£‡πå
    laserSound = Instance.new("Sound")
    laserSound.Name = "PurpleLaserSound"
    laserSound.SoundId = "rbxassetid://138087185"
    laserSound.Volume = 0.8
    laserSound.Looped = true
    laserSound.Parent = head
    laserSound:Stop()
end

local function createBeams()
    leftBeam = Instance.new("Beam")
    leftBeam.Attachment0 = leftEyeAtt
    leftBeam.Attachment1 = targetAttLeft
    leftBeam.Color = ColorSequence.new(laserColor)
    leftBeam.Width0 = 2.0
    leftBeam.Width1 = 2.0
    leftBeam.Texture = "rbxassetid://6060542021"
    leftBeam.TextureMode = Enum.TextureMode.Stretch
    leftBeam.TextureSpeed = 3
    leftBeam.LightEmission = 1
    leftBeam.Brightness = 8
    leftBeam.FaceCamera = true
    leftBeam.Segments = 20
    leftBeam.Enabled = false
    leftBeam.Parent = workspace
    
    rightBeam = leftBeam:Clone()
    rightBeam.Attachment0 = rightEyeAtt
    rightBeam.Attachment1 = targetAttRight
    rightBeam.Parent = workspace
end

local function createParticles()
    leftParticle = Instance.new("ParticleEmitter")
    leftParticle.Parent = leftEyeAtt
    leftParticle.Texture = "rbxasset://textures/particles/fire_main.dds"
    leftParticle.Color = ColorSequence.new({ColorSequenceKeypoint.new(0, darkPurple), ColorSequenceKeypoint.new(0.5, laserColor), ColorSequenceKeypoint.new(1, brightPurple)})
    leftParticle.LightEmission = 0.8
    leftParticle.Brightness = 8
    leftParticle.Size = NumberSequence.new({NumberSequenceKeypoint.new(0, 0.9), NumberSequenceKeypoint.new(0.5, 1.5), NumberSequenceKeypoint.new(1, 0)})
    leftParticle.Transparency = NumberSequence.new(0.3, 1)
    leftParticle.Lifetime = NumberRange.new(0.5, 0.9)
    leftParticle.Rate = 90
    leftParticle.Speed = NumberRange.new(15, 25)
    leftParticle.Enabled = false
    
    rightParticle = leftParticle:Clone()
    rightParticle.Parent = rightEyeAtt

    leftSmoke = Instance.new("ParticleEmitter")
    leftSmoke.Parent = leftEyeAtt
    leftSmoke.Texture = "rbxasset://textures/particles/smoke_main.dds"
    leftSmoke.Color = ColorSequence.new({ColorSequenceKeypoint.new(0, darkPurple), ColorSequenceKeypoint.new(1, Color3.fromRGB(140, 60, 200))})
    leftSmoke.LightEmission = 0.3
    leftSmoke.Brightness = 3
    leftSmoke.Size = NumberSequence.new(0.5, 1.3)
    leftSmoke.Transparency = NumberSequence.new(0.6, 1)
    leftSmoke.Lifetime = NumberRange.new(1.5, 2.8)
    leftSmoke.Rate = 18
    leftSmoke.Enabled = false
    
    rightSmoke = leftSmoke:Clone()
    rightSmoke.Parent = rightEyeAtt

    leftEyeGlow = Instance.new("ParticleEmitter")
    leftEyeGlow.Parent = leftEyeAtt
    leftEyeGlow.Texture = "rbxasset://textures/particles/fire_main.dds"
    leftEyeGlow.Color = ColorSequence.new(laserColor)
    leftEyeGlow.LightEmission = 0.9
    leftEyeGlow.Brightness = 7
    leftEyeGlow.Size = NumberSequence.new(0.4, 0.7)
    leftEyeGlow.Transparency = NumberSequence.new(0.4, 0.8)
    leftEyeGlow.Lifetime = NumberRange.new(0.3, 0.6)
    leftEyeGlow.Rate = 35
    leftEyeGlow.Speed = NumberRange.new(0, 3)
    leftEyeGlow.Enabled = false
    
    rightEyeGlow = leftEyeGlow:Clone()
    rightEyeGlow.Parent = rightEyeAtt
end

local function applyRecoil()
    -- ‡∏™‡∏±‡πà‡∏ô‡∏´‡∏±‡∏ß‡πÄ‡∏•‡πá‡∏Å‡∏ô‡πâ‡∏≠‡∏¢ (‡πÑ‡∏°‡πà‡∏°‡∏µ‡∏Å‡∏•‡πâ‡∏≠‡∏á)
    if player.Character and player.Character:FindFirstChild("Head") then
        local head = player.Character.Head
        local shake = CFrame.new(
            math.random(-headShake, headShake),
            math.random(-headShake, headShake),
            math.random(-headShake, headShake)
        )
        head.CFrame = head.CFrame * shake
    end
end

local function createImpactEffects(position)
    local att = Instance.new("Attachment")
    att.WorldPosition = position
    att.Parent = workspace.Terrain
    
    local fire = Instance.new("ParticleEmitter")
    fire.Parent = att
    fire.Texture = "rbxasset://textures/particles/fire_main.dds"
    fire.Color = ColorSequence.new({ColorSequenceKeypoint.new(0, darkPurple), ColorSequenceKeypoint.new(0.4, laserColor), ColorSequenceKeypoint.new(1, brightPurple)})
    fire.LightEmission = 0.9
    fire.Brightness = 10
    fire.Size = NumberSequence.new(1.8, 0)
    fire.Transparency = NumberSequence.new(0.2, 1)
    fire.Lifetime = NumberRange.new(1.0, 1.8)
    fire.Rate = 120
    fire.Speed = NumberRange.new(8, 18)
    fire.Acceleration = Vector3.new(0, 12, 0)
    fire.Enabled = true
    
    local smoke = Instance.new("ParticleEmitter")
    smoke.Parent = att
    smoke.Texture = "rbxasset://textures/particles/smoke_main.dds"
    smoke.Color = ColorSequence.new(darkPurple, Color3.fromRGB(140, 60, 200))
    smoke.Size = NumberSequence.new(1.5, 3.0)
    smoke.Transparency = NumberSequence.new(0.7, 1)
    smoke.Lifetime = NumberRange.new(2.0, 4.0)
    smoke.Rate = 50
    smoke.Enabled = true
    
    Debris:AddItem(att, 3)
end

local function updateLaser()
    if not laserEnabled or not shooting or not player.Character then return end
    local head = player.Character:FindFirstChild("Head")
    if not head then return end
    
    local originLeft = leftEyeAtt.WorldPosition
    local originRight = rightEyeAtt.WorldPosition
    local direction = (mouse.Hit.Position - head.Position).Unit * 500
    
    local rayParams = RaycastParams.new()
    rayParams.FilterDescendantsInstances = {player.Character}
    rayParams.FilterType = Enum.RaycastFilterType.Blacklist
    
    -- ‡∏ï‡∏≤‡∏ã‡πâ‡∏≤‡∏¢
    local resultLeft = workspace:Raycast(originLeft, direction, rayParams)
    if resultLeft then
        targetAttLeft.WorldPosition = resultLeft.Position
        createImpactEffects(resultLeft.Position)
        
        local humanoid = resultLeft.Instance:FindFirstAncestorOfClass("Model") and resultLeft.Instance:FindFirstAncestorOfClass("Model"):FindFirstChildOfClass("Humanoid")
        if humanoid and humanoid.Parent ~= player.Character then
            local now = tick()
            if now - lastDamageTick >= damageInterval then
                humanoid:TakeDamage(damagePerTick)
                lastDamageTick = now
            end
        end
    else
        targetAttLeft.WorldPosition = originLeft + direction
    end
    
    -- ‡∏ï‡∏≤‡∏Ç‡∏ß‡∏≤
    local resultRight = workspace:Raycast(originRight, direction, rayParams)
    if resultRight then
        targetAttRight.WorldPosition = resultRight.Position
        createImpactEffects(resultRight.Position)
        
        local humanoid = resultRight.Instance:FindFirstAncestorOfClass("Model") and resultLeft.Instance:FindFirstAncestorOfClass("Model"):FindFirstChildOfClass("Humanoid")
        if humanoid and humanoid.Parent ~= player.Character then
            local now = tick()
            if now - lastDamageTick >= damageInterval then
                humanoid:TakeDamage(damagePerTick)
                lastDamageTick = now
            end
        end
    else
        targetAttRight.WorldPosition = originRight + direction
    end
    
    applyRecoil()
end

-- ‡∏Ñ‡∏•‡∏¥‡∏Å‡∏ã‡πâ‡∏≤‡∏¢ = ‡∏¢‡∏¥‡∏á
UserInputService.InputBegan:Connect(function(input)
    if laserEnabled and input.UserInputType == Enum.UserInputType.MouseButton1 then
        shooting = true
        leftBeam.Enabled = true
        rightBeam.Enabled = true
        leftParticle.Enabled = true
        rightParticle.Enabled = true
        leftSmoke.Enabled = true
        rightSmoke.Enabled = true
        leftEyeGlow.Enabled = true
        rightEyeGlow.Enabled = true
        if laserSound then laserSound:Play() end
    end
end)

UserInputService.InputEnded:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        shooting = false
        leftBeam.Enabled = false
        rightBeam.Enabled = false
        leftParticle.Enabled = false
        rightParticle.Enabled = false
        leftSmoke.Enabled = false
        rightSmoke.Enabled = false
        leftEyeGlow.Enabled = false
        rightEyeGlow.Enabled = false
        if laserSound then laserSound:Stop() end
    end
end)

RunService.RenderStepped:Connect(updateLaser)

local function enableLaser()
    if player.Character then
        createAttachments()
        createBeams()
        createParticles()
    end
    
    laserEnabled = true
    
    
end

local function disableLaser()
    laserEnabled = false
    shooting = false
    
    if leftBeam then leftBeam:Destroy() end
    if rightBeam then rightBeam:Destroy() end
    if leftEyeAtt then leftEyeAtt:Destroy() end
    if rightEyeAtt then rightEyeAtt:Destroy() end
    if targetAttLeft then targetAttLeft:Destroy() end
    if targetAttRight then targetAttRight:Destroy() end
    if leftParticle then leftParticle:Destroy() end
    if rightParticle then rightParticle:Destroy() end
    if leftSmoke then leftSmoke:Destroy() end
    if rightSmoke then rightSmoke:Destroy() end
    if leftEyeGlow then leftEyeGlow:Destroy() end
    if rightEyeGlow then rightEyeGlow:Destroy() end
    if laserSound then laserSound:Stop(); laserSound:Destroy() end
    
    
end

local Button = BTab:Button({
    Title = "[ üé• ]  Beamming Laser Eyes",
    Desc = "[ - Beamming Eyes - ]",
    Icon = "telescope",
    Locked = false,
    Callback = function()
        if laserEnabled then
            disableLaser()
        else
            enableLaser()
        end
    end
})

local Section = BTab:Section({ 
    Title = "                 { [ - Teleport Tools - ] }",
})

local Button = BTab:Button({
    Title = "[ üç≠ ]  Teleport Tool",
    Desc = "[ - Sword Tools - ]",
    Icon = "swords",
    Locked = false,
    Callback = function()
        -- ‡∏•‡∏π‡∏Å‡∏≠‡∏°‡∏™‡∏µ‡∏°‡πà‡∏ß‡∏á Sparkles ‡πÄ‡∏ó‡πÄ‡∏•‡∏û‡∏≠‡∏£‡πå‡∏ï (‡πÅ‡∏™‡∏á‡∏°‡πà‡∏ß‡∏á‡∏ó‡∏±‡πâ‡∏á‡∏•‡∏π‡∏Å + Effect)
        local tool = Instance.new("Tool")
        tool.Name = "üç≠"
        tool.ToolTip = "[ üç≠ ]  Teleport Tool"
        tool.RequiresHandle = true

        local player = game.Players.LocalPlayer

        -- ‡∏™‡∏£‡πâ‡∏≤‡∏á Handle ‡∏•‡∏π‡∏Å‡∏≠‡∏°‡∏™‡∏µ‡∏°‡πà‡∏ß‡∏á
        local handle = Instance.new("Part")
        handle.Name = "Handle"
        handle.Size = Vector3.new(2.2, 2.2, 2.2)
        handle.Shape = Enum.PartType.Ball
        handle.Material = Enum.Material.ForceField
        handle.BrickColor = BrickColor.new("Royal purple")  -- ‡∏™‡∏µ‡πÉ‡∏Å‡∏•‡πâ‡πÄ‡∏Ñ‡∏µ‡∏¢‡∏á‡∏ó‡∏µ‡πà‡∏™‡∏∏‡∏î‡πÉ‡∏ô BrickColor
        handle.Color = Color3.fromRGB(138, 43, 179)        -- ‡∏ï‡∏±‡πâ‡∏á‡∏Ñ‡πà‡∏≤‡∏™‡∏µ‡∏à‡∏£‡∏¥‡∏á‡πÉ‡∏´‡πâ‡∏ï‡∏£‡∏á‡∏ï‡∏≤‡∏° RGB
        handle.Transparency = 0.15
        handle.CanCollide = false
        handle.Parent = tool

        -- Sparkles ‡∏™‡∏µ‡∏°‡πà‡∏ß‡∏á
        local sparkles = Instance.new("Sparkles")
        sparkles.SparkleColor = Color3.fromRGB(138, 43, 179)
        sparkles.Enabled = false
        sparkles.Parent = handle

        -- ParticleEmitter ‡∏™‡∏µ‡∏°‡πà‡∏ß‡∏á
        local particles = Instance.new("ParticleEmitter")
        particles.Texture = "rbxassetid://241650934"
        particles.Color = ColorSequence.new(
            Color3.fromRGB(138, 43, 179),
            Color3.fromRGB(180, 100, 200)  -- ‡∏°‡πà‡∏ß‡∏á‡∏≠‡πà‡∏≠‡∏ô‡∏•‡∏á‡∏ô‡∏¥‡∏î‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏Ñ‡∏ß‡∏≤‡∏°‡∏™‡∏ß‡∏¢
        )

        particles.Size = NumberSequence.new(0.4)
        particles.Lifetime = NumberRange.new(1.5, 2.5)
        particles.Rate = 20
        particles.SpreadAngle = Vector2.new(45, 45)
        particles.Speed = NumberRange.new(2)
        particles.Parent = handle

        -- PointLight ‡∏™‡∏µ‡∏°‡πà‡∏ß‡∏á
        local light = Instance.new("PointLight")
        light.Color = Color3.fromRGB(138, 43, 179)
        light.Brightness = 2
        light.Range = 10
        light.Parent = handle

        -- ‡πÄ‡∏™‡∏µ‡∏¢‡∏á‡πÄ‡∏ó‡πÄ‡∏•‡∏û‡∏≠‡∏£‡πå‡∏ï
        local teleportSound = Instance.new("Sound")
        teleportSound.SoundId = "rbxassetid://4861638982"
        teleportSound.Volume = 0.7
        teleportSound.Parent = handle

        -- ‡πÄ‡∏≠‡∏ü‡πÄ‡∏ü‡∏Å‡∏ï‡πå‡∏ï‡∏≠‡∏ô‡∏ñ‡∏∑‡∏≠/‡∏õ‡∏•‡πà‡∏≠‡∏¢
        tool.Equipped:Connect(function()
            sparkles.Enabled = false
            particles.Rate = 50
            light.Brightness = 0
        end)

        tool.Unequipped:Connect(function()
            sparkles.Enabled = false
            particles.Rate = 20
            light.Brightness = 2
        end)

        -- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡πÄ‡∏ó‡πÄ‡∏•‡∏û‡∏≠‡∏£‡πå‡∏ï
        tool.Activated:Connect(function()
            teleportSound:Play()
            local character = player.Character
            if character and character:FindFirstChild("HumanoidRootPart") then
                local mouse = player:GetMouse()
                local pos = mouse.Hit.Position + Vector3.new(0, 3, 0)
                character.HumanoidRootPart.CFrame = CFrame.new(pos)
            end
        end)

        -- ‡πÄ‡∏™‡∏µ‡∏¢‡∏á‡∏ï‡∏≠‡∏ô‡∏ñ‡∏∑‡∏≠‡πÄ‡∏Ñ‡∏£‡∏∑‡πà‡∏≠‡∏á‡∏°‡∏∑‡∏≠ (‡πÄ‡∏û‡∏¥‡πà‡∏°‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏ó‡πà)
        tool.Equipped:Connect(function()
            local equipSound = Instance.new("Sound")
            equipSound.SoundId = "rbxassetid://2865227271"
            equipSound.Volume = 0.5
            equipSound.Parent = player.Character:FindFirstChild("HumanoidRootPart")
            if equipSound.Parent then
                equipSound:Play()
                equipSound.Ended:Connect(function()
                    equipSound:Destroy()
                end)
            end
        end)

        -- ‡πÉ‡∏™‡πà‡∏•‡∏á Backpack
        player:WaitForChild("Backpack")
        tool.Parent = player.Backpack
    end
})

local Section = BTab:Section({ 
    Title = "                  { [ - Recoil Tools - ] }",
})

local Button = BTab:Button({
    Title = "[ üö≤ ]  Recoil Speed Tool",
    Desc = "[ - Recoil Speed - ]",
    Icon = "swords",
    Locked = false,
    Callback = function()
        local player = game.Players.LocalPlayer

        -- ‡∏™‡∏£‡πâ‡∏≤‡∏á Tool
        local tool = Instance.new("Tool")
        tool.Name = "üö≤"
        tool.ToolTip = "[ üö≤ ]  Recoil Speed"
        tool.RequiresHandle = true  -- ‡∏ï‡πâ‡∏≠‡∏á‡∏°‡∏µ Handle ‡πÄ‡∏û‡∏∑‡πà‡∏≠‡πÉ‡∏´‡πâ‡πÄ‡∏´‡πá‡∏ô‡∏•‡∏π‡∏Å‡∏≠‡∏°

        -- ‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß‡πÄ‡∏î‡∏¥‡∏ô‡∏õ‡∏Å‡∏ï‡∏¥ (‡∏™‡πà‡∏ß‡∏ô‡πÉ‡∏´‡∏ç‡πà‡∏Ñ‡∏∑‡∏≠ 18)
        local NormalWalkSpeed = 17

        -- ‡∏™‡∏£‡πâ‡∏≤‡∏á Handle ‡∏•‡∏π‡∏Å‡∏≠‡∏°‡∏™‡∏µ‡∏°‡πà‡∏ß‡∏á
        local handle = Instance.new("Part")
        handle.Name = "Handle"
        handle.Size = Vector3.new(2.2, 2.2, 2.2)
        handle.Shape = Enum.PartType.Ball
        handle.Material = Enum.Material.ForceField
        handle.Color = Color3.fromRGB(138, 43, 179)        -- ‡∏™‡∏µ‡∏°‡πà‡∏ß‡∏á‡∏ï‡∏£‡∏á‡∏ï‡∏≤‡∏° RGB
        handle.Transparency = 0.15
        handle.CanCollide = false
        handle.Parent = tool

        -- Sparkles ‡∏™‡∏µ‡∏°‡πà‡∏ß‡∏á
        local sparkles = Instance.new("Sparkles")
        sparkles.SparkleColor = Color3.fromRGB(138, 43, 179)
        sparkles.Enabled = false
        sparkles.Parent = handle

        -- ParticleEmitter ‡∏™‡∏µ‡∏°‡πà‡∏ß‡∏á
        local particles = Instance.new("ParticleEmitter")
        particles.Texture = "rbxassetid://241650934" -- 241650934 --
        particles.Color = ColorSequence.new(
            Color3.fromRGB(138, 43, 179),
            Color3.fromRGB(180, 100, 200)  -- ‡∏°‡πà‡∏ß‡∏á‡∏≠‡πà‡∏≠‡∏ô‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏Ñ‡∏ß‡∏≤‡∏°‡∏™‡∏ß‡∏¢
        )
        
        particles.Size = NumberSequence.new(0.4)
        particles.Lifetime = NumberRange.new(1.5, 2.5)
        particles.Rate = 20
        particles.SpreadAngle = Vector2.new(45, 45)
        particles.Speed = NumberRange.new(2)
        particles.Parent = handle

        -- PointLight ‡∏™‡∏µ‡∏°‡πà‡∏ß‡∏á
        local light = Instance.new("PointLight")
        light.Color = Color3.fromRGB(138, 43, 179)
        light.Brightness = 2
        light.Range = 10
        light.Parent = handle

        -- ‡πÄ‡∏≠‡∏ü‡πÄ‡∏ü‡∏Å‡∏ï‡πå‡∏ï‡∏≠‡∏ô‡∏ñ‡∏∑‡∏≠‡πÄ‡∏Ñ‡∏£‡∏∑‡πà‡∏≠‡∏á‡∏°‡∏∑‡∏≠
        tool.Equipped:Connect(function()
            if sparkles then sparkles.Enabled = false end
            particles.Rate = 50
            light.Brightness = 0

            -- ‡πÄ‡∏û‡∏¥‡πà‡∏°‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß‡πÄ‡∏î‡∏¥‡∏ô
            if player.Character and player.Character:FindFirstChild("Humanoid") then
                player.Character.Humanoid.WalkSpeed = 30
            end

            -- ‡πÄ‡∏™‡∏µ‡∏¢‡∏á‡∏ï‡∏≠‡∏ô‡∏ñ‡∏∑‡∏≠
            local equipSound = Instance.new("Sound")
            equipSound.SoundId = "rbxassetid://2865227271" -- 2865227271 --
            equipSound.Volume = 0.5
            equipSound.Parent = player.Character and player.Character:FindFirstChild("HumanoidRootPart")
            if equipSound.Parent then
                equipSound:Play()
                equipSound.Ended:Connect(function()
                    equipSound:Destroy()
                end)
            end
        end)

        tool.Unequipped:Connect(function()
            if sparkles then sparkles.Enabled = false end
            particles.Rate = 20
            light.Brightness = 2

            -- ‡∏Å‡∏•‡∏±‡∏ö‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß‡∏õ‡∏Å‡∏ï‡∏¥
            if player.Character and player.Character:FindFirstChild("Humanoid") then
                player.Character.Humanoid.WalkSpeed = NormalWalkSpeed
            end
        end)

        -- ‡πÉ‡∏™‡πà Tool ‡πÄ‡∏Ç‡πâ‡∏≤ Backpack
        player:WaitForChild("Backpack")
        tool.Parent = player.Backpack
    end
})

local espTab = Window:Tab({
    Title = "||   - Solution",
    Icon = "flask-conical",
    Locked = false,
})

local Section = espTab:Section({ 
    Title = "                  { [ - Click Teleport - ] }",
})

local MouseTPEnabled = false
local Mouse = game.Players.LocalPlayer:GetMouse()
local Player = game.Players.LocalPlayer

-- ‡∏™‡∏£‡πâ‡∏≤‡∏á Sound ‡πÑ‡∏ß‡πâ‡πÉ‡∏ä‡πâ‡∏ã‡πâ‡∏≥‡πÑ‡∏î‡πâ‡πÄ‡∏£‡∏∑‡πà‡∏≠‡∏¢ ‡πÜ
local teleportSound = Instance.new("Sound")
teleportSound.SoundId = "rbxassetid://4861638982"
teleportSound.Volume = 0.7

local Toggle = espTab:Toggle({
    Title = "[ üëÜ ]  Clicking Teleport",
    Desc = "[ - Backpack Teleport Bypass - ]",
    Icon = "bird",
    Type = "Checkbox",
    Value = false, -- default value
    Callback = function(value) 
        MouseTPEnabled = value
    end
})

-- ‡∏Å‡∏î‡∏Ñ‡∏•‡∏¥‡∏Å = ‡∏ß‡∏≤‡∏£‡πå‡∏õ‡∏ó‡∏±‡∏ô‡∏ó‡∏µ + ‡πÄ‡∏™‡∏µ‡∏¢‡∏á‡∏î‡∏±‡∏á‡∏û‡∏£‡πâ‡∏≠‡∏°‡∏Å‡∏±‡∏ô
Mouse.Button1Down:Connect(function()
    if MouseTPEnabled and Player.Character and Player.Character:FindFirstChild("HumanoidRootPart") then
        local targetPos = Mouse.Hit.Position + Vector3.new(0, 5, 0)
        
        -- ‡∏ß‡∏≤‡∏£‡πå‡∏õ‡∏Å‡πà‡∏≠‡∏ô‡πÄ‡∏•‡∏¢ ‡πÑ‡∏°‡πà‡∏ï‡πâ‡∏≠‡∏á‡∏£‡∏≠
        Player.Character.HumanoidRootPart.CFrame = CFrame.new(targetPos)
        
        -- ‡πÄ‡∏•‡πà‡∏ô‡πÄ‡∏™‡∏µ‡∏¢‡∏á‡∏ó‡∏±‡∏ô‡∏ó‡∏µ (‡πÄ‡∏™‡∏µ‡∏¢‡∏á‡∏à‡∏∞‡∏î‡∏±‡∏á‡∏à‡∏≤‡∏Å‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á‡πÉ‡∏´‡∏°‡πà‡πÄ‡∏•‡∏¢)
        teleportSound.Parent = Player.Character.HumanoidRootPart
        teleportSound:Play()
        
        -- ‡∏´‡∏•‡∏±‡∏á‡∏à‡∏≤‡∏Å‡πÄ‡∏•‡πà‡∏ô‡πÄ‡∏™‡∏£‡πá‡∏à ‡∏•‡∏ö‡∏≠‡∏≠‡∏Å‡πÄ‡∏û‡∏∑‡πà‡∏≠‡πÑ‡∏°‡πà‡πÉ‡∏´‡πâ‡∏Å‡∏¥‡∏ô memory
        task.delay(3, function()
            teleportSound.Parent = nil
        end)
    end
end)

-- ‡∏£‡∏µ‡πÇ‡∏´‡∏•‡∏î‡∏™‡∏ñ‡∏≤‡∏ô‡∏∞‡∏ï‡∏≠‡∏ô‡πÄ‡∏Å‡∏¥‡∏î‡πÉ‡∏´‡∏°‡πà
Player.CharacterAdded:Connect(function()
    task.wait(1)
    MouseTPEnabled = Rayfield.Flags.MouseTP.CurrentValue
end)

local Section = espTab:Section({ 
    Title = "                     { [ - Color Tools - ] }",
})

local PurpleTool = {
    Enabled = false,
    Current = nil,
    Highlight = nil,
    Connections = {},
}

-- ‡∏™‡∏µ‡∏°‡πà‡∏ß‡∏á‡∏ó‡∏µ‡πà‡∏ï‡πâ‡∏≠‡∏á‡∏Å‡∏≤‡∏£ (138, 43, 179)
local PURPLE_COLOR = Color3.fromRGB(138, 43, 179)

local function CreateHighlight(tool)
    if PurpleTool.Highlight then PurpleTool.Highlight:Destroy() end
    PurpleTool.Highlight = Instance.new("Highlight")
    PurpleTool.Highlight.FillColor = PURPLE_COLOR
    PurpleTool.Highlight.OutlineColor = PURPLE_COLOR
    PurpleTool.Highlight.FillTransparency = 0.3
    PurpleTool.Highlight.OutlineTransparency = 0
    PurpleTool.Highlight.Parent = tool
    PurpleTool.Highlight.Adornee = tool:FindFirstChild("Handle") or tool
end

local function ApplyPurpleColor()
    if not PurpleTool.Enabled or not PurpleTool.Current or not PurpleTool.Highlight then return end
    
    -- ‡∏ï‡∏±‡πâ‡∏á‡∏Ñ‡πà‡∏≤‡∏™‡∏µ Highlight
    PurpleTool.Highlight.FillColor = PURPLE_COLOR
    PurpleTool.Highlight.OutlineColor = PURPLE_COLOR
    
    -- ‡πÄ‡∏õ‡∏•‡∏µ‡πà‡∏¢‡∏ô‡∏™‡∏µ‡∏ó‡∏∏‡∏Å Part ‡πÉ‡∏ô Tool (‡∏¢‡∏Å‡πÄ‡∏ß‡πâ‡∏ô HumanoidRootPart)
    for _, part in pairs(PurpleTool.Current:GetDescendants()) do
        if part:IsA("BasePart") and part.Name ~= "HumanoidRootPart" then
            part.Color = PURPLE_COLOR
        end
    end
end

local function OnEquipped(tool)
    if not PurpleTool.Enabled then return end
    PurpleTool.Current = tool
    task.wait(0.1)
    CreateHighlight(tool)
    ApplyPurpleColor()  -- ‡πÉ‡∏ä‡πâ‡∏™‡∏µ‡∏°‡πà‡∏ß‡∏á‡∏ó‡∏±‡∏ô‡∏ó‡∏µ
end

local function OnUnequipped()
    if PurpleTool.Highlight then 
        PurpleTool.Highlight:Destroy() 
        PurpleTool.Highlight = nil 
    end
    PurpleTool.Current = nil
end

local function SetupTool(char)
    -- ‡∏•‡πâ‡∏≤‡∏á connection ‡πÄ‡∏Å‡πà‡∏≤
    for _, c in pairs(PurpleTool.Connections) do c:Disconnect() end
    PurpleTool.Connections = {}

    local function ConnectTool(tool)
        table.insert(PurpleTool.Connections, tool.Equipped:Connect(function() OnEquipped(tool) end))
        table.insert(PurpleTool.Connections, tool.Unequipped:Connect(OnUnequipped))
    end

    -- ‡πÄ‡∏ä‡∏∑‡πà‡∏≠‡∏° Tool ‡∏ó‡∏µ‡πà‡∏≠‡∏¢‡∏π‡πà‡πÉ‡∏ô‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£‡∏ï‡∏≠‡∏ô‡∏ô‡∏µ‡πâ
    for _, child in pairs(char:GetChildren()) do
        if child:IsA("Tool") then ConnectTool(child) end
    end
    table.insert(PurpleTool.Connections, char.ChildAdded:Connect(function(c)
        if c:IsA("Tool") then ConnectTool(c) end
    end))

    -- ‡πÄ‡∏ä‡∏∑‡πà‡∏≠‡∏° Tool ‡πÉ‡∏ô Backpack
    local backpack = LocalPlayer:WaitForChild("Backpack")
    for _, child in pairs(backpack:GetChildren()) do
        if child:IsA("Tool") then ConnectTool(child) end
    end
    table.insert(PurpleTool.Connections, backpack.ChildAdded:Connect(function(c)
        if c:IsA("Tool") then ConnectTool(c) end
    end))
end

local function TogglePurpleTool(state)
    PurpleTool.Enabled = state
    if state then
        
        if LocalPlayer.Character then SetupTool(LocalPlayer.Character) end
    else
        
        if PurpleTool.Highlight then 
            PurpleTool.Highlight:Destroy() 
            PurpleTool.Highlight = nil 
        end
        PurpleTool.Current = nil
    end
end

-- ‡∏£‡∏≠‡∏á‡∏£‡∏±‡∏ö‡∏Å‡∏≤‡∏£‡πÄ‡∏Å‡∏¥‡∏î‡πÉ‡∏´‡∏°‡πà
LocalPlayer.CharacterAdded:Connect(SetupTool)
if LocalPlayer.Character then SetupTool(LocalPlayer.Character) end

local Toggle = espTab:Toggle({
    Title = "[ üü™ ]  Purple Glowing Tool",
    Desc = "[ - Permanent Purple - ]",
    Icon = "grape",
    Type = "Checkbox",
    Value = false, -- default value
    Callback = function(value) 
        TogglePurpleTool(value)
    end
})

local RainbowGun = {
    Enabled = false,
    Current = nil,
    Highlight = nil,
    Hue = 0,
    Connections = {},
    Loop = nil
}

local function CreateHighlight(tool)
    if RainbowGun.Highlight then RainbowGun.Highlight:Destroy() end
    RainbowGun.Highlight = Instance.new("Highlight")
    RainbowGun.Highlight.FillTransparency = 0.3
    RainbowGun.Highlight.OutlineTransparency = 0
    RainbowGun.Highlight.Parent = tool
    RainbowGun.Highlight.Adornee = tool:FindFirstChild("Handle") or tool
end

local function UpdateRainbowGun()
    if not RainbowGun.Enabled or not RainbowGun.Current or not RainbowGun.Highlight then return end
    RainbowGun.Hue = (RainbowGun.Hue + 0.01) % 1
    local color = Color3.fromHSV(RainbowGun.Hue, 1, 1)
    RainbowGun.Highlight.FillColor = color
    RainbowGun.Highlight.OutlineColor = color
    for _, part in pairs(RainbowGun.Current:GetDescendants()) do
        if part:IsA("BasePart") and part.Name ~= "HumanoidRootPart" then
            part.Color = color
        end
    end
end

local function OnEquipped(tool)
    if not RainbowGun.Enabled then return end
    RainbowGun.Current = tool
    task.wait(0.1)
    CreateHighlight(tool)
end

local function OnUnequipped()
    if RainbowGun.Highlight then RainbowGun.Highlight:Destroy() RainbowGun.Highlight = nil end
    RainbowGun.Current = nil
end

local function SetupGun(char)
    for _, c in pairs(RainbowGun.Connections) do c:Disconnect() end
    RainbowGun.Connections = {}

    local function ConnectTool(tool)
        table.insert(RainbowGun.Connections, tool.Equipped:Connect(function() OnEquipped(tool) end))
        table.insert(RainbowGun.Connections, tool.Unequipped:Connect(OnUnequipped))
    end

    for _, child in pairs(char:GetChildren()) do
        if child:IsA("Tool") then ConnectTool(child) end
    end
    table.insert(RainbowGun.Connections, char.ChildAdded:Connect(function(c)
        if c:IsA("Tool") then ConnectTool(c) end
    end))

    local backpack = LocalPlayer:WaitForChild("Backpack")
    for _, child in pairs(backpack:GetChildren()) do
        if child:IsA("Tool") then ConnectTool(child) end
    end
    table.insert(RainbowGun.Connections, backpack.ChildAdded:Connect(function(c)
        if c:IsA("Tool") then ConnectTool(c) end
    end))
end

local function ToggleRainbowGun(state)
    RainbowGun.Enabled = state
    if state then
        
        if not RainbowGun.Loop then
            RainbowGun.Loop = RunService.Heartbeat:Connect(UpdateRainbowGun)
        end
        if LocalPlayer.Character then SetupGun(LocalPlayer.Character) end
    else
        
        if RainbowGun.Loop then RainbowGun.Loop:Disconnect() RainbowGun.Loop = nil end
        if RainbowGun.Highlight then RainbowGun.Highlight:Destroy() RainbowGun.Highlight = nil end
        RainbowGun.Current = nil
    end
end

LocalPlayer.CharacterAdded:Connect(SetupGun)
if LocalPlayer.Character then SetupGun(LocalPlayer.Character) end

local Toggle = espTab:Toggle({
    Title = "[ üåà ]  Rainbow Mountain Tool",
    Desc = "[ - Permanent Rainbow - ]",
    Icon = "rainbow",
    Type = "Checkbox",
    Value = false,
    Callback = function(value) 
        ToggleRainbowGun(value)
    end
})

local Section = espTab:Section({ 
    Title = "                         { [ - Hitbox - ] }",
})

local HitboxEnabled = false
local HeadSize = 20
local connections = {}

local Toggle = espTab:Toggle({
    Title = "[ ‚öîÔ∏è ]  Hitbox Boxing Player",
    Desc = "[ - Hitbox Boxing Sword - ]",
    Icon = "swords",
    Type = "Checkbox",
    Value = false,
    Callback = function(value) 
        HitboxEnabled = value
        
        if value then
            connections["Hitbox"] = game:GetService('RunService').RenderStepped:Connect(function()
                if not HitboxEnabled then return end
                
                for _, v in pairs(game:GetService('Players'):GetPlayers()) do
                    if v.Name ~= game.Players.LocalPlayer.Name and v.Character then
                        pcall(function()
                            local hrp = v.Character:FindFirstChild("HumanoidRootPart")
                            if hrp then
                                hrp.Size = Vector3.new(HeadSize, HeadSize, HeadSize)
                                hrp.Transparency = 0.7

                                -- ‡πÄ‡∏õ‡∏•‡∏µ‡πà‡∏¢‡∏ô‡πÄ‡∏õ‡πá‡∏ô‡∏™‡∏µ‡πÄ‡∏Ç‡∏µ‡∏¢‡∏ß RGB(138, 43, 179)
                                hrp.Color = Color3.fromRGB(138, 43, 179)
                                hrp.Material = "Neon"
                                hrp.CanCollide = false
                            end
                        end)
                    end
                end
            end)
            
            
        else
            if connections["Hitbox"] then
                connections["Hitbox"]:Disconnect()
            end
            
            -- Reset hitbox ‡∏Ç‡∏≠‡∏á‡∏ó‡∏∏‡∏Å‡∏Ñ‡∏ô‡∏Å‡∏•‡∏±‡∏ö‡∏Ñ‡πà‡∏≤‡πÄ‡∏î‡∏¥‡∏°
            for _, v in pairs(game:GetService('Players'):GetPlayers()) do
                if v.Character and v.Character:FindFirstChild("HumanoidRootPart") then
                    pcall(function()
                        local hrp = v.Character.HumanoidRootPart
                        hrp.Size = Vector3.new(2, 2, 1)
                        hrp.Transparency = 1
                        hrp.Material = "Plastic"
                        hrp.CanCollide = false
                    end)
                end
            end
            
           
        end
    end
})

local Slider = espTab:Slider({
    Title = "[ üìè ]  Slider Hitbox",
    Desc = "[ - Hitboz Size Player - ]",
    Step = 1,
    Value = {
        Min = 20,
        Max = 5000,
        Default = 20,
    },
    Callback = function(value)
        HeadSize = value
    end
})

local Section = espTab:Section({ 
    Title = "                       { [ - ESP Player - ] }",
})

-- Variables
local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

local EspEnabled = false
local ESPColor = Color3.fromRGB(138, 43, 179)  -- ‡∏™‡∏µ‡∏°‡πà‡∏ß‡∏á‡πÄ‡∏î‡∏¥‡∏°

-- Table ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡πÄ‡∏Å‡πá‡∏ö‡∏Ç‡πâ‡∏≠‡∏°‡∏π‡∏• ESP ‡∏Ç‡∏≠‡∏á‡πÅ‡∏ï‡πà‡∏•‡∏∞ player (‡∏£‡∏ß‡∏°‡∏ï‡∏±‡∏ß‡πÄ‡∏£‡∏≤)
local PlayerESP = {}  -- [player] = {Highlight = ..., NameGui = ..., HealthConn = ..., ToolConnAdded = ..., ToolConnRemoved = ...}

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏™‡∏£‡πâ‡∏≤‡∏á/‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï ESP ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö player ‡∏´‡∏ô‡∏∂‡πà‡∏á‡∏Ñ‡∏ô
local function UpdatePlayerESP(player)
    if not EspEnabled then return end
    
    local character = player.Character
    if not character then return end
    
    local humanoid = character:FindFirstChildOfClass("Humanoid")
    if not humanoid then return end
    
    local head = character:FindFirstChild("Head")
    if not head then return end
    
    local equippedTool = character:FindFirstChildOfClass("Tool")
    local toolName = equippedTool and equippedTool.Name or " ‚ùå "
    
    local hp = math.floor(humanoid.Health)
    local maxHp = math.floor(humanoid.MaxHealth)
    
    -- Highlight glow ‡∏£‡∏≠‡∏ö‡∏ï‡∏±‡∏ß
    local espData = PlayerESP[player]
    if not espData then
        espData = {}
        PlayerESP[player] = espData
    end
    
    if not espData.Highlight then
        espData.Highlight = Instance.new("Highlight")
        espData.Highlight.Adornee = character
        espData.Highlight.FillColor = ESPColor
        espData.Highlight.OutlineColor = ESPColor
        espData.Highlight.FillTransparency = 0.5
        espData.Highlight.OutlineTransparency = 0
        espData.Highlight.DepthMode = Enum.HighlightDepthMode.AlwaysOnTop
        espData.Highlight.Parent = character
    end
    
    -- BillboardGui ‡∏ö‡∏ô‡∏´‡∏±‡∏ß (‡πÅ‡∏™‡∏î‡∏á‡∏ó‡∏±‡πâ‡∏á DisplayName ‡πÅ‡∏•‡∏∞ Name)
    if not espData.NameGui then
        espData.NameGui = Instance.new("BillboardGui")
        espData.NameGui.Adornee = head
        espData.NameGui.Size = UDim2.new(0, 250, 0, 120)  -- ‡∏Ç‡∏¢‡∏≤‡∏¢‡πÉ‡∏´‡πâ‡∏û‡∏≠‡πÉ‡∏™‡πà 4 ‡∏ö‡∏£‡∏£‡∏ó‡∏±‡∏î‡∏™‡∏ö‡∏≤‡∏¢‡πÜ
        espData.NameGui.StudsOffset = Vector3.new(0, 3, 0)
        espData.NameGui.AlwaysOnTop = true
        espData.NameGui.Parent = head
        
        local text = Instance.new("TextLabel")
        text.Name = "InfoText"
        text.BackgroundTransparency = 1
        text.Size = UDim2.new(1, 0, 1, 0)
        text.TextColor3 = ESPColor
        text.TextStrokeTransparency = 0.3
        text.TextStrokeColor3 = Color3.new(0, 0, 0)
        text.Font = Enum.Font.GothamBold
        text.TextSize = 18
        text.TextYAlignment = Enum.TextYAlignment.Center
        text.Parent = espData.NameGui
    end
    
    if espData.NameGui:FindFirstChild("InfoText") then
        espData.NameGui.InfoText.Text = 
              player.DisplayName .. "\n" ..        -- ‡∏ä‡∏∑‡πà‡∏≠‡∏ó‡∏µ‡πà‡πÅ‡∏™‡∏î‡∏á‡πÉ‡∏ô‡πÄ‡∏Å‡∏° (‡∏™‡πà‡∏ß‡∏ô‡πÉ‡∏´‡∏ç‡πà‡πÄ‡∏õ‡πá‡∏ô‡∏ä‡∏∑‡πà‡∏≠‡πÄ‡∏•‡πà‡∏ô)
            "( " .. player.Name .. " )\n" ..          -- Username ‡∏à‡∏£‡∏¥‡∏á (‡∏≠‡∏¢‡∏π‡πà‡πÉ‡∏ô‡∏ß‡∏á‡πÄ‡∏•‡πá‡∏ö)
            "Items: " .. toolName .. "\n" ..
            "( HP: " .. hp .. "/" .. maxHp .. " )"
    end
end

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏•‡πâ‡∏≤‡∏á ESP ‡∏Ç‡∏≠‡∏á player
local function ClearPlayerESP(player)
    local espData = PlayerESP[player]
    if espData then
        if espData.Highlight then espData.Highlight:Destroy() end
        if espData.NameGui then espData.NameGui:Destroy() end
        if espData.HealthConn then espData.HealthConn:Disconnect() end
        if espData.ToolConnAdded then espData.ToolConnAdded:Disconnect() end
        if espData.ToolConnRemoved then espData.ToolConnRemoved:Disconnect() end
        PlayerESP[player] = nil
    end
end

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏ï‡∏±‡πâ‡∏á‡∏Ñ‡πà‡∏≤ ESP ‡πÄ‡∏°‡∏∑‡πà‡∏≠ character ‡πÇ‡∏´‡∏•‡∏î
local function OnCharacterAdded(player, character)
    if not EspEnabled then return end
    
    -- ‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï‡∏ó‡∏±‡∏ô‡∏ó‡∏µ
    UpdatePlayerESP(player)
    
    -- ‡πÄ‡∏ä‡∏∑‡πà‡∏≠‡∏°‡∏ï‡πà‡∏≠ event ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï realtime
    local humanoid = character:FindFirstChildOfClass("Humanoid")
    if humanoid then
        local espData = PlayerESP[player] or {}
        espData.HealthConn = humanoid.HealthChanged:Connect(function() UpdatePlayerESP(player) end)
        PlayerESP[player] = espData
    end
    
    -- ‡πÄ‡∏ä‡πá‡∏Ñ‡∏Å‡∏≤‡∏£‡πÄ‡∏õ‡∏•‡∏µ‡πà‡∏¢‡∏ô Tool
    local espData = PlayerESP[player] or {}
    espData.ToolConnAdded = character.ChildAdded:Connect(function(child)
        if child:IsA("Tool") then UpdatePlayerESP(player) end
    end)
    espData.ToolConnRemoved = character.ChildRemoved:Connect(function(child)
        if child:IsA("Tool") then UpdatePlayerESP(player) end
    end)
    PlayerESP[player] = espData
end

-- ‡πÄ‡∏õ‡∏¥‡∏î ESP ‡∏ó‡∏±‡πâ‡∏á‡∏´‡∏°‡∏î (‡∏£‡∏ß‡∏°‡∏ï‡∏±‡∏ß‡πÄ‡∏£‡∏≤)
local function EnableESP()
    for _, player in pairs(Players:GetPlayers()) do
        if player.Character then
            OnCharacterAdded(player, player.Character)
        end
        player.CharacterAdded:Connect(function(char) OnCharacterAdded(player, char) end)
    end
    
    -- ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö player ‡∏ó‡∏µ‡πà join ‡πÉ‡∏´‡∏°‡πà
    Players.PlayerAdded:Connect(function(player)
        if EspEnabled then
            player.CharacterAdded:Connect(function(char) OnCharacterAdded(player, char) end)
            if player.Character then
                OnCharacterAdded(player, player.Character)
            end
        end
    end)
end

-- ‡∏õ‡∏¥‡∏î ESP ‡∏ó‡∏±‡πâ‡∏á‡∏´‡∏°‡∏î
local function DisableESP()
    for _, player in pairs(Players:GetPlayers()) do
        ClearPlayerESP(player)
    end
end

-- Toggle ‡∏à‡∏≤‡∏Å UI
local Toggle = espTab:Toggle({
    Title = "[ üè∫ ]  ESP All Players Name",
    Desc = "[ - ESP Player And Name Diplayname - ]",
    Icon = "lightbulb",
    Type = "Checkbox",
    Value = false,
    Callback = function(value)
        EspEnabled = value
        if value then
            EnableESP()
        else
            DisableESP()
        end
    end
}) 
    -- ‡∏£‡∏≠‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£‡πÉ‡∏´‡∏°‡πà‡πÉ‡∏ô‡∏≠‡∏ô‡∏≤‡∏Ñ‡∏ï (respawn)
    player.CharacterAdded:Connect(onCharacterAdded)
    
    -- ‡∏•‡πâ‡∏≤‡∏á‡πÄ‡∏°‡∏∑‡πà‡∏≠‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£‡∏´‡∏≤‡∏¢ (‡∏ï‡∏≤‡∏¢‡∏ä‡∏±‡πà‡∏ß‡∏Ñ‡∏£‡∏≤‡∏ß)
    player.CharacterRemoving:Connect(function()
        ClearPlayerESP(player)
    end)

local avatarTab = Window:Tab({
    Title = "||   - Avatar Clothes",
    Icon = "caravan", -- optional
    Locked = false,
})

local Section = avatarTab:Section({ 
    Title = "             { [ - Flame Head Purple - ] }",
})

local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local LocalPlayer = Players.LocalPlayer

--// Variables
local CustomFire = nil
local SparkEmitter = nil
local FireEnabled = false

local fixedColor = Color3.fromRGB(138, 43, 179)  -- ‡∏™‡∏µ‡∏°‡πà‡∏ß‡∏á‡πÄ‡∏Ç‡πâ‡∏°‡∏´‡∏•‡∏±‡∏Å
local darkerPurple = fixedColor:Lerp(Color3.new(0,0,0), 0.4)  -- ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö secondary + sparks

--// ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏™‡∏£‡πâ‡∏≤‡∏á‡πÑ‡∏ü‡∏°‡πà‡∏ß‡∏á + sparks (‡πÑ‡∏°‡πà‡∏°‡∏µ Trail)
local function CreatePurpleFire()
    local character = LocalPlayer.Character
    if not character then return end
    
    local Head = character:FindFirstChild("Head") or character:WaitForChild("Head", 5)
    if not Head then return end

    -- ‡∏•‡∏ö‡∏Ç‡∏≠‡∏á‡πÄ‡∏Å‡πà‡∏≤
    if Head:FindFirstChild("PurpleHeadFire") then Head.PurpleHeadFire:Destroy() end
    if Head:FindFirstChild("SparkEmitter") then Head.SparkEmitter:Destroy() end

    -- ‡∏™‡∏£‡πâ‡∏≤‡∏á Fire ‡∏´‡∏•‡∏±‡∏Å (‡∏™‡∏µ‡∏°‡πà‡∏ß‡∏á)
    CustomFire = Instance.new("Fire")
    CustomFire.Name = "PurpleHeadFire"
    CustomFire.Size = 5      -- ‡∏Ñ‡πà‡∏≤‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏ï‡πâ‡∏ô
    CustomFire.Heat = 12     -- ‡∏Ñ‡πà‡∏≤‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏ï‡πâ‡∏ô
    CustomFire.Color = fixedColor
    CustomFire.SecondaryColor = darkerPurple
    CustomFire.Enabled = true
    CustomFire.Parent = Head

    -- Sparks Particles (‡∏Ç‡∏µ‡πâ‡πÑ‡∏ü‡∏°‡πà‡∏ß‡∏á‡∏£‡πà‡∏ß‡∏á‡∏•‡∏á‡∏°‡∏≤)
    SparkEmitter = Instance.new("ParticleEmitter")
    SparkEmitter.Name = "SparkEmitter"
    SparkEmitter.Texture = "rbxassetid://241353019"  -- texture sparks ‡∏°‡πà‡∏ß‡∏á‡∏™‡∏ß‡∏¢ (‡∏Ñ‡∏•‡∏≤‡∏™‡∏™‡∏¥‡∏Å)
    SparkEmitter.Color = ColorSequence.new(fixedColor, darkerPurple)
    SparkEmitter.Size = NumberSequence.new(0.8, 0)
    SparkEmitter.Transparency = NumberSequence.new(0.3, 1)
    SparkEmitter.Lifetime = NumberRange.new(1.5, 3)
    SparkEmitter.Rate = 45  -- ‡πÄ‡∏¢‡∏≠‡∏∞‡∏´‡∏ô‡πà‡∏≠‡∏¢‡πÉ‡∏´‡πâ‡∏î‡∏π‡∏°‡∏µ‡∏Ç‡∏µ‡πâ‡πÑ‡∏ü‡∏ä‡∏±‡∏î
    SparkEmitter.Speed = NumberRange.new(4, 8)
    SparkEmitter.Acceleration = Vector3.new(0, -12, 0)  -- ‡∏ï‡∏Å‡∏ï‡∏≤‡∏°‡πÅ‡∏£‡∏á‡πÇ‡∏ô‡πâ‡∏°‡∏ñ‡πà‡∏ß‡∏á
    SparkEmitter.VelocitySpread = 360
    SparkEmitter.LightEmission = 1
    SparkEmitter.Parent = Head
end

--// ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡πÄ‡∏õ‡∏¥‡∏î/‡∏õ‡∏¥‡∏î
local function SetFireState(state)
    FireEnabled = state
    if state then
        CreatePurpleFire()
    else
        if CustomFire then CustomFire:Destroy() end
        if SparkEmitter then SparkEmitter:Destroy() end
    end
end

--// ‡∏£‡∏≠‡∏á‡∏£‡∏±‡∏ö‡∏Å‡∏≤‡∏£‡πÄ‡∏Å‡∏¥‡∏î‡πÉ‡∏´‡∏°‡πà
LocalPlayer.CharacterAdded:Connect(function()
    task.wait(2)
    if FireEnabled then
        CreatePurpleFire()
    end
end)

-- Toggle ‡πÄ‡∏õ‡∏¥‡∏î/‡∏õ‡∏¥‡∏î‡πÑ‡∏ü‡∏°‡πà‡∏ß‡∏á
local Toggle = avatarTab:Toggle({
    Title = "[ üíú ]  Purple Flame Head",
    Desc = "[ - Purple Flame Head",
    Icon = "flame",
    Type = "Checkbox",
    Value = false,
    Callback = function(value) 
        SetFireState(value)
    end
})

-- Slider ‡∏õ‡∏£‡∏±‡∏ö‡∏Ç‡∏ô‡∏≤‡∏î‡πÑ‡∏ü
avatarTab:Slider({
