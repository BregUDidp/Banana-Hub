    Step = 5,
    Value = {
        Min = 16,
        Max = 5000,
        Default = 16,
    },
    Callback = function(value)
        runSpeed= value
        if superSpeed then humanoid.WalkSpeed= runSpeed end
    end
})

local Section = PTab:Section({ 
    Title = "          { [ - Airwalk - Infinite Jump - ] }",
})

local AirWalkEnabled = false
local Platform = nil

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡πÄ‡∏õ‡∏¥‡∏î/‡∏õ‡∏¥‡∏î Air Walk
local function ToggleAirWalk(enabled)
    AirWalkEnabled = enabled
    
    if enabled then
        if not Platform then
            Platform = Instance.new("Part")
            Platform.Size = Vector3.new(5, 1, 5)
            Platform.Transparency = 1 -- ‡πÉ‡∏™‡∏´‡∏°‡∏î ‡∏°‡∏≠‡∏á‡πÑ‡∏°‡πà‡πÄ‡∏´‡πá‡∏ô
            Platform.Anchored = true
            Platform.CanCollide = true
            Platform.Parent = workspace
        end
        
        -- ‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á‡πÅ‡∏ú‡πà‡∏ô‡∏û‡∏∑‡πâ‡∏ô‡∏ó‡∏∏‡∏Å‡πÄ‡∏ü‡∏£‡∏°
        game:GetService("RunService").Heartbeat:Connect(function()
            if AirWalkEnabled and game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
                local hrp = game.Players.LocalPlayer.Character.HumanoidRootPart
                Platform.CFrame = hrp.CFrame * CFrame.new(0, -3.5, 0) -- ‡∏ß‡∏≤‡∏á‡πÉ‡∏ï‡πâ‡πÄ‡∏ó‡πâ‡∏≤‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£
            end
        end)
        
        
    else
        if Platform then
            Platform:Destroy()
            Platform = nil
        end
        
        
    end
end

local Toggle = PTab:Toggle({
    Title = "[ üí® ]  Platform Airstyle Walking",
    Desc = "[ - Platform Invisible Airstyle - ]",
    Icon = "cloudy",
    Type = "Checkbox",
    Value = false, -- default value
    Callback = function(value) 
        ToggleAirWalk(value)
    end
})

