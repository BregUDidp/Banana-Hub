    },
    Callback = function(value)
         maxWebDistance = value
    end
})

local Slider = PTab:Slider({
    Title = "[ ‚õèÔ∏è ]  Swinging Gravity",
    Desc = "[ - Swinging Gravity - ]",
    Step = 5,
    Value = {
        Min = 20,
        Max = 5000,
        Default = 50,
    },
    Callback = function(value)
        swingGravity = value
    end
})

