            player.Character.Humanoid.JumpPower = value
        end
    end
})

game:GetService("UserInputService").JumpRequest:Connect(function()
    if InfiniteJumpEnabled then
        local player = game.Players.LocalPlayer
        if player.Character and player.Character:FindFirstChildOfClass("Humanoid") then
            player.Character.Humanoid:ChangeState("Jumping")
        end
    end
end)

local Section = PTab:Section({ 
    Title = "              { [ - Sitting Automatic - ] }",
})

local SittingEnabled = false
local SitConnection = nil
local player = game.Players.LocalPlayer

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏ö‡∏±‡∏á‡∏Ñ‡∏±‡∏ö‡∏ô‡∏±‡πà‡∏á
local function ForceSit()
    local character = player.Character or player.CharacterAdded:Wait()
    local humanoid = character:FindFirstChildOfClass("Humanoid")
    
    if humanoid then
        -- ‡∏ö‡∏±‡∏á‡∏Ñ‡∏±‡∏ö‡πÉ‡∏´‡πâ Humanoid ‡∏ô‡∏±‡πà‡∏á (‡πÅ‡∏°‡πâ‡πÑ‡∏°‡πà‡∏°‡∏µ Seat)
        humanoid.Sit = true
        
        -- ‡∏õ‡πâ‡∏≠‡∏á‡∏Å‡∏±‡∏ô‡πÑ‡∏°‡πà‡πÉ‡∏´‡πâ‡∏•‡∏∏‡∏Å‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï‡∏¥
        if humanoid.SeatPart then
            humanoid.SeatPart = nil
        end
    end
end

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡πÄ‡∏õ‡∏¥‡∏î/‡∏õ‡∏¥‡∏î ‡∏Å‡∏≤‡∏£‡∏ô‡∏±‡πà‡∏á‡∏Ñ‡πâ‡∏≤‡∏á
local function ToggleSit(enabled)
    SittingEnabled = enabled
    
    if enabled then
        ForceSit()
        
        -- ‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï‡∏ó‡∏∏‡∏Å‡∏Ñ‡∏£‡∏±‡πâ‡∏á‡∏ó‡∏µ‡πà‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£‡πÄ‡∏Å‡∏¥‡∏î‡πÉ‡∏´‡∏°‡πà ‡∏´‡∏£‡∏∑‡∏≠‡πÄ‡∏õ‡∏•‡∏µ‡πà‡∏¢‡∏ô‡∏™‡∏ñ‡∏≤‡∏ô‡∏∞
        SitConnection = game:GetService("RunService").Heartbeat:Connect(function()
            if SittingEnabled then
                local character = player.Character
                if character then
                    local humanoid = character:FindFirstChildOfClass("Humanoid")
                    if humanoid and humanoid.Sit == false then
                        humanoid.Sit = true
                    end
                end
            end
        end)
        
        -- ‡∏ñ‡πâ‡∏≤‡∏ï‡∏≤‡∏¢‡πÅ‡∏•‡πâ‡∏ß‡πÄ‡∏Å‡∏¥‡∏î‡πÉ‡∏´‡∏°‡πà ‡πÉ‡∏´‡πâ‡∏ô‡∏±‡πà‡∏á‡∏ï‡πà‡∏≠‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï‡∏¥
        player.CharacterAdded:Connect(function()
            if SittingEnabled then
                task.wait(0.5) -- ‡∏£‡∏≠‡πÉ‡∏´‡πâ‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£‡πÇ‡∏´‡∏•‡∏î‡πÄ‡∏™‡∏£‡πá‡∏à
                ForceSit()
            end
        end)
        
        
        
    else
        -- ‡∏õ‡∏¥‡∏î‡∏Å‡∏≤‡∏£‡∏ô‡∏±‡πà‡∏á
        if SitConnection then
            SitConnection:Disconnect()
            SitConnection = nil
        end
        
        local character = player.Character
        if character then
            local humanoid = character:FindFirstChildOfClass("Humanoid")
            if humanoid then
                humanoid.Sit = false
            end
        end
        
        
    end
end

local Toggle = PTab:Toggle({
    Title = "[ ü™ë ]  Forced To Sitting",
    Desc = "[ - Auto Sit To Click Toggle - ]",
    Icon = "rocking-chair",
    Type = "Checkbox",
    Value = false,
    Callback = function(value) 
        ToggleSit(value)
    end
})

local Button = PTab:Button({
    Title = "[ üßç ]  Stand Immediately",
    Desc = "[ - Auto Stand Up To Click Button - ]",
    Icon = "person-standing",
    Locked = false,
    Callback = function()
        ToggleSit(false)
    end
})

local Section = PTab:Section({ 
    Title = "                 { [ - Infinite Zoom - ] }",
})

-- ‡∏ï‡∏±‡∏ß‡πÅ‡∏õ‡∏£‡∏Ñ‡∏ß‡∏ö‡∏Ñ‡∏∏‡∏°
local InfiniteZoomEnabled = false
local MaxZoomDistance = 10000  -- ‡∏Ñ‡πà‡∏≤‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏ï‡πâ‡∏ô
local player = game.Players.LocalPlayer

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï‡∏£‡∏∞‡∏¢‡∏∞‡∏ã‡∏π‡∏°
local function UpdateZoom()
    if player then
        player.CameraMaxZoomDistance = MaxZoomDistance
        player.CameraMinZoomDistance = 0.5  -- ‡∏ã‡∏π‡∏°‡πÄ‡∏Ç‡πâ‡∏≤‡πÉ‡∏Å‡∏•‡πâ‡∏™‡∏∏‡∏î
    end
end

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡πÄ‡∏õ‡∏¥‡∏î/‡∏õ‡∏¥‡∏î Infinite Zoom
local function ToggleInfiniteZoom(enabled)
    InfiniteZoomEnabled = enabled
    
    if enabled then
        UpdateZoom()
        
        -- ‡∏£‡∏≠‡∏á‡∏£‡∏±‡∏ö‡∏ï‡∏≠‡∏ô‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£‡πÄ‡∏Å‡∏¥‡∏î‡πÉ‡∏´‡∏°‡πà (‡∏ï‡∏≤‡∏¢‡πÅ‡∏•‡πâ‡∏ß‡πÄ‡∏Å‡∏¥‡∏î)
        player.CharacterAdded:Connect(function()
            task.wait(0.1)
            if InfiniteZoomEnabled then
                UpdateZoom()
            end
        end)
        
        
    else
        -- ‡∏Ñ‡∏∑‡∏ô‡∏Ñ‡πà‡∏≤‡πÄ‡∏î‡∏¥‡∏°‡∏Ç‡∏≠‡∏á Roblox (‡∏õ‡∏Å‡∏ï‡∏¥‡∏Ñ‡∏∑‡∏≠ 128)
        player.CameraMaxZoomDistance = 128
        player.CameraMinZoomDistance = 0.5
        
        
    end
end

local Toggle = PTab:Toggle({
    Title = "[ üîé ]  Infinite Zoom",
    Desc = "[ - Infinite Zoom Altra - ]",
    Icon = "zoom-in",
    Type = "Checkbox",
    Value = false, -- default value
    Callback = function(value) 
        ToggleInfiniteZoom(value)
    end
})

local Slider = PTab:Slider({
    Title = "[ üìè ]  Slider Zoom",
    Desc = "[ - Infinite Zoom Slider - ]",
    Step = 100,
    Value = {
        Min = 400,
        Max = 1000,
        Default = 400,
    },
    Callback = function(value)
        MaxZoomDistance = Value
        
        if InfiniteZoomEnabled then
            UpdateZoom()
       end
    end
})

