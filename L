    Desc = "[ - Slider Flame Size - ]",
    Step = 1,
    Value = {
        Min = 1,
        Max = 5000,
        Default = 5,
    },
    Callback = function(value)
        if CustomFire then
            CustomFire.Size = value
        end
    end
})

-- Slider ‡∏õ‡∏£‡∏±‡∏ö‡∏Ñ‡∏ß‡∏≤‡∏°‡∏£‡πâ‡∏≠‡∏ô (‡∏ó‡∏≥‡πÉ‡∏´‡πâ‡πÑ‡∏ü‡∏™‡∏π‡∏á/‡πÅ‡∏£‡∏á‡∏Ç‡∏∂‡πâ‡∏ô)
avatarTab:Slider({
    Title = "[ üî• ]  Slider Heater",
    Desc = "[ - Slider Flame Heater - ]",
    Step = 1,
    Value = {
        Min = 1,
        Max = 5000,
        Default = 12,
    },
    Callback = function(value)
        if CustomFire then
            CustomFire.Heat = value
        end
    end
})

