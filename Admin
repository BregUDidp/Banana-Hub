local Tab = Window:NewTab("[ üï∏Ô∏è ] Speeds Walk")
local Section = Tab:NewSection("üï∏Ô∏è Speed Slider Walk")

Section:NewSlider("Slider Run", "SliderInfo", 500, 0, function(s) -- 500 (MaxValue) | 0 (MinValue)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
end)

local Section = Tab:NewSection("üï∏Ô∏è Speed TextBox Walk")

Section:NewTextBox("Number For Run", "TextboxInfo", function(txt)
	print(txt)
	game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = txt
end)

local Tab = Window:NewTab("[ üï∏Ô∏è ] KeyBlinds")
local Section = Tab:NewSection("üï∏Ô∏è KeyBlinds")

Section:NewKeybind("KeybindText", "KeybindInfo", Enum.KeyCode.G, function()
	Library:ToggleUI()
end)
